	var DIV_NO	= decode(fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"DIV_NO"),null,"",fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"DIV_NO")) ;
	var BRNCH_NM = fcArray2Value(ARRAY2_GBL_RET_VALUE,"BRNCH_NM");
	
	var TMS = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"TMS");
	var DAY_ORD = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"DAY_ORD");
	
	var COMM_NO ="";
	
	
	
	var nTms = parseInt(TMS);
	var nDayOrd = parseInt(DAY_ORD);
	
	var sBrnchNmTitle = BRNCH_NM+" "+nTms+"회 "+nDayOrd+" 일차";
	
	grdSalesViewDiv.SetCellProp("head",0,"text", "("+BRNCH_NM+") 매출액 비교");
//	grdSalesViewDiv.SetCellProp("head",2,"text",(nTms-1)+"회");	
//	grdSalesViewDiv.SetCellProp("head",5,"text",sBrnchNmTitle);
//	grdSalesViewDiv.SetCellProp("head",8,"text",(nTms-2)+"회");
	
	
    var sServiceName = "rsm2011-service:searchSalesViewDiv";  // 지점별 매출액 비교 조회 (전체)
    
    if( RACE_DAY == fcd_GetSysdate("YYYYMMDD"))
	{
		sServiceName = "rsm2011-service:searchSalesViewDivToday";  // 지점별 매출액 비교 조회 오늘
	}
	
	var sInDataSet   = "";
	var sOutDataSet  = "dsSalesViewDiv=dsSalesViewDiv ";
	
	// 매출현황 파라미터
	
	fcd_AddParam("RACE_DAY", RACE_DAY);  
	fcd_AddParam("MEET_CD", MEET_CD);  
	fcd_AddParam("COMM_NO", sCOMM_NO); 
	fcd_AddParam("DIV_NO", DIV_NO);        
	fcd_AddParam("BRNCH_NM", BRNCH_NM);  
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack",false);		
}



// 콤보박스 바꾸면 검색
function fcOnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	switch ( obj.ID ) {
		
		case "rdoAutoOPT" : // 수동 자동 구분
			if(strCode=="2")
			{
				cboTimeBox.Enable = true;
				SetTimer(1, parseInt(cboTimeBox.Value) *60 * 1000 ); // Timer구동
			}
			else
			{
				cboTimeBox.Enable = false;
				KillTimer(1); // Timer해제
			}
	
			break;
		
		case "cboTimeBox":
			KillTimer(1); // Timer해제
			SetTimer(1, parseInt(strCode) *60 *1000 ); // Timer구동	
			break;
	}


//	fcSearch();
}





