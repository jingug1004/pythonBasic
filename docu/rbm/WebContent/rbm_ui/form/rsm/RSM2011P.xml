/***************************************************************************************************
*                                         공통 Event 처리 부분                                     *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 Load時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/

var ARRAY2_GBL_RET_VALUE = Array();	// 이전페이지에서 넘어온 변수
var RACE_DAY  = ""; // 경주일
var MEET_CD  = ""; // 경주 구분 코드
function fcFormLoadSetting(obj) {  
	
    var nCXScreen	= GetDeviceInfo("CXScreen");			
	var nCYScreen	= GetDeviceInfo("CYScreen");
	
	RSM2011P.MoveWindow(0,0,nCXScreen,nCYScreen);
	divBtnLst.Left = nCXScreen-120;
	stUnit.Left= nCXScreen-100;
	grdSalesViewDiv.Width	= nCXScreen-30;
	dsSalesViewDiv.ClearData();
	
	var sGBL_RET_VALUE = decode(GBL_RET_VALUE,null,'',GBL_RET_VALUE);
	ARRAY2_GBL_RET_VALUE = split2(GBL_RET_VALUE," ","=");
	
	GBL_RET_VALUE ="";
	
	if(sGBL_RET_VALUE!='')
	{
		fcSearchInfo();
	}
	
}

/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 UNLoad時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormUnloadProcess(obj) {
	//자료 변경 여부를 체크하고자 하는  DataSet Object를 넘긴다.
	var sDataset = "dsSalesViewDiv";
	
	if (   !fnc_FormUnloadCheck(sDataset  , sFormId) ) 
		return false;
}

/*-------------------------------------------------------------------------------------------------+
>>  화면 실행할 때마다 실행
+-------------------------------------------------------------------------------------------------*/
function fcFormOnActivite(){
	
	var sGBL_RET_VALUE = decode(GBL_RET_VALUE,null,'',GBL_RET_VALUE);
	ARRAY2_GBL_RET_VALUE = split2(GBL_RET_VALUE," ","=");
	
	GBL_RET_VALUE ="";
	
}

/*-------------------------------------------------------------------------------------------------+
>>  단축키 처리(Form KeyDown)
+-------------------------------------------------------------------------------------------------*/
function fcHotKey(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

	//Return Key Press時 Next Component focus
	if ( nChar == ENTER_KEY ) {
		var oFocusComponent = GetNextComponent(true);
		oFocusComponent.setfocus();
		return;
	}

}

