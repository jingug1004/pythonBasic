// 지점별 상세확정목록 팝업을 호출한다.
function grdPcGita_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if( nRow < 0) return;
	
	var pv_meet_cd	= dsPoGitaGrp.GetColumn(nRow , "MEET_CD");
	var pv_pay_mm	= dsPoGitaGrp.GetColumn(nRow , "PAY_MM");
	var pv_pay_year	= dsPoGitaGrp.GetColumn(nRow , "PAY_YEAR");
	var pv_sell_cd	= dsPoGitaGrp.GetColumn(nRow , "SELL_CD");
	var pv_comm_no	= dsPoGitaGrp.GetColumn(nRow , "COMM_NO");
	
	var sFormId = "frm_rsm::RSM5070P.xml";
	var sArgs = "pv_meet_cd="	+ quote(pv_meet_cd)	
			  + " pv_sell_cd="	+ quote(pv_sell_cd)
			  + " pv_pay_year=" + quote(pv_pay_year)
			  + " pv_pay_mm="	+ quote(pv_pay_mm)
			  + " pv_comm_no="	+ quote(pv_comm_no);
	Dialog(sFormId, sArgs, 900, 464, "");
}
	
	
