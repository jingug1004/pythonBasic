function fcSetResize(obj,nX)
{	
	var arrObj = array(-1);
	arrObj[0] = "grdCntntList";
	
	var nWidth = nX-8;
	if( nX <= 820){
		grdCntntList.Autofit = false;		
		grdCntntList.right = 801;

		Shape1.Width = 801;	
		divBtnLst.Left = this.width - 393;			
		btn_verifyLisFile.Left = this.width - 115;			
		
		return;
	}
	
	grdCntntList.right = nWidth;
	if( nX > 1280){
		grdCntntList.Autofit = true;		
	}else{
		grdCntntList.Autofit = false;		
	}
		
	// ∞¯≈Î
	Shape1.Width = nWidth-8;
	divBtnLst.Left = this.width - 393;	
	btn_verifyLisFile.Left = this.width - 115;
	
}

