// 특정콤포넌트(그리드) 리사이즈
function fcSetResize(obj,nX)
{	
	var arrObj = array(-1);
	arrObj[0] = "grdCntntList";
	
	var nWidth = nX-8;
	if( nX <= 820){
		grdCntntList.Autofit = false;		
		grdCntntList.right = 801;

		Shape1.Width = 801;	
		divBtnLst.Left = this.width - 393;			
		btn_verifyLisFile.Left = this.width - 115;			
		
		return;
	}
	
	grdCntntList.right = nWidth;
	if( nX > 1280){
		grdCntntList.Autofit = true;		
	}else{
		grdCntntList.Autofit = false;		
	}
		
	// 공통
	Shape1.Width = nWidth-8;
	divBtnLst.Left = this.width - 393;	
	btn_verifyLisFile.Left = this.width - 115;
	
}

// 해당 폼사이즈 변경시
function fcOnSize(obj,nCx,nCy,nState)
{
	
	fcSetResize(obj,nCx);
}


function fcVerifyLisFile(obj)
{
    // LIS FILE 날짜 검색
	var sServiceName = "rsm1010-service:VerifyLISFile";
    var sInDataSet   = "";
    var sOutDataSet  = "";
    
	fcd_AddParam("RACE_DAY", dsRaceDayFileInfo.GetColumn(dsRaceDayFileInfo.row, "RACE_DAY"));	                         
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}

function SetVisibleReVerify()
{
	var VeryfiTF = dsRaceDayFileInfo.GetColumn(dsRaceDayFileInfo.row , "TF");
	if (VeryfiTF == "001") { 		// 일치
		btn_verifyLisFile.Visible = false;	// 일치일 경우 재검증 버튼을 보여줄 필요가 없음
	} else if (VeryfiTF == "002") { // 불일치
		btn_verifyLisFile.Visible = true;
	} else if (dsRaceDayFileInfo.GetColumn(dsRaceDayFileInfo.row , "RACE_DAY") != today()) { // 미검증이라도 당일이 아닌 경우
		// CRON작업이 미실행된 경우에도 재검증을 보여줘야 함
		btn_verifyLisFile.Visible = true;
	} else {
		btn_verifyLisFile.Visible = false;	// 기본값은 안보여줌
	}
}
	


function fcOnLoadCompleted(obj,nErrorCode,strErrorMsg,nReason)
{
	switch (obj.id) {
		case "dsCntntList":
			if( nReason == 0 ) { //데이터가 로드되었을때
				obj.row = -1;
			}
			break;
	}
}

