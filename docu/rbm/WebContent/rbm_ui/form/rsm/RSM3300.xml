function fcOnChar(obj,strPreText,nChar,strPostText,nLLParam,nHLParam)
{
	switch ( obj.ID ) {		
		// 경주 검색 시작 일자에서 ENTER_KEY 눌렀을 경우
		case "calRaceDaySt" :	
			if ( nChar == ENTER_KEY ) {
				fcSearch();
				return;
			}
		break;
		// 경주 검색 마지막 일자에서 ENTER_KEY 눌렀을 경우
		case "calRaceDayEnd" :	
			if ( nChar == ENTER_KEY ) {
				fcSearch();
				return;
			}
		break;
		
		// 지점에서 ENTER_KEY 눌렀을 경우
		case "cboSearchBrncCd" :	
