function fcSearch(obj) {
    dsSales.ClearData();
        
    var sServiceName = "";
    var sInDataSet   = "";
    var sOutDataSet  = "";	
        
    if(cboSearchBrncCd.Value == "01"){
		sServiceName = "rsm3300-service:searchSales1";
		sInDataSet   = "";
		sOutDataSet  = "dsSales=dsSales ";
    }
    else{
		sServiceName = "rsm3300-service:searchSales";
		sInDataSet   = "";
		sOutDataSet  = "dsSales=dsSales ";
    }
	
	fcd_AddParam("RACE_DAY_ST", 	calRaceDaySt.Value);
	fcd_AddParam("RACE_DAY_END", 	calRaceDayEnd.Value);
	fcd_AddParam("BRNC_CD", 		cboSearchBrncCd.Value);
	fcd_AddParam("MEET_CD", 		rdoMeetCd.Value);
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
	
}


