function fccalOnKeyDown(obj,nChar,bShift,bCtrl,bAlt,nLLParam,nHLParam)
{
	switch ( obj.ID ) {		
		// Á¾·áÀÏÀÚ¿¡¼­ ENTER_KEY ´­·¶À» °æ¿ì
