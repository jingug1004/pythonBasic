function fcFormLoadSetting(obj) {  
    sBUTTONLIST   = 'TTFFTFT';
  
 	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
    
    //공통코드 조회
    fcd_GetComboDs(dsInComCd);
    
    fcd_GetAlarmDs("dsAlarm", "005");
    
    /*
	 메인에서 이동될 경우 
    var sRetValue = decode(GBL_RET_VALUE,null,'',GBL_RET_VALUE);
	if(sRetValue!='')
	{		
		// 이전 페이지에서 파라미터 받음	
		ARRAY2_GBL_RET_VALUE = split(GBL_RET_VALUE,"=","webstyle");
		GBL_RET_VALUE="";
		sSearchRecvUserId = ARRAY2_GBL_RET_VALUE[1];

	}
	*/
	
    //조회 조건의 날짜(today~ today)
    calToDt  .Value = sCurDate;
    calFromDt.Value = sCurDate;
    
    if (G_ds_param.rowcount() > 0) return;
    
    // 권한 설정	
