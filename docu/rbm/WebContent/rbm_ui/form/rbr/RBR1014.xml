}

/*-------------------------------------------------------------------------------------------------+
>>  신규일 때는 기본키 입력/수정 가능, 기존 자료일 때는 수정불가  
+-------------------------------------------------------------------------------------------------*/
function fcDsOnRowPosChanged(obj,nOldRow,nRow)
{	
	//용역구분, 직책코드
	if(dsServMana.getRowType(nRow) == "insert")
	{
		cboServCd   .Enable = true;
		cboWorkTryCd.Enable = true;
		cboJobTitCd .Enable = true;
	} else {
		cboServCd   .Enable = false;
		cboWorkTryCd.Enable = false;
		cboJobTitCd .Enable = false;
	}
	
	if( dsServMana.GetColumn(nRow, "SERV_CD").length == 0 ) {
		var sFilter = "CD_TERM1 = " + quote("");
		dsJobTitCd.Filter(sFilter);
	}
}

