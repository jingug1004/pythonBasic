/*-------------------------------------------------------------------------------------------------+
>>  행추가 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcAddRow(obj) {
	//DataSet의 Header 정보가 있을 때  Add 처리
	if ( fnc_DatasetChangeCheck(dsEvntMana) ) {
		//fnc_Message(L_COM_ALT_0024, "N", "현재 작업중인 행이 존재하기 때문에\r\n 행을 추가할 수 ");
		//return;
	}
	
	nRow = dsEvntMana.AddRow();
	
	//자동 Setting
	dsEvntMana.SetColumn(nRow, "BRNC_CD", dsBrncCd.GetColumn(0, "CD"));
	dsEvntMana.SetColumn(nRow, "GEN_DT" , sCurDate);
	dsEvntMana.SetColumn(nRow, "MNG"    , gdsUser.GetColumn(gdsUser.row, "USER_NM"));
	dsEvntMana.SetColumn(nRow, "END_YN" , "N");
	
	
	calGenDt.SetFocus(); // 입력 첫 항목으로 Focus

}

/*-------------------------------------------------------------------------------------------------+
>>  행삭제 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcDelRow(obj) {

	sMsg = "";
	
	//사건이력이 있는 사건은 삭제가 되어서는 않된다.
	for(var i = 0 ; i < dsEvntMana.rowcount ; i++) {
		if( dsEvntMana.GetColumn(i, "CHK") == "1" && 
		    dsEvntMana.GetColumn(i, "DELETE_CD") == "N") {	
			sMsg += (i+1) + ", ";
			dsEvntMana.SetColumn(i, "CHK", "0");
		}
	}
	
	if( sMsg == "" ) {
		if ( !fnc_DeleteCheck(dsEvntMana, "chk", "1") ) return;
	} else {
		sMsg = LEFT(sMsg, LENGTH(sMsg)-2); 
		fnc_Message(SNIS_RBM_B001, "N", sMsg, "사건이력");
	}
}


