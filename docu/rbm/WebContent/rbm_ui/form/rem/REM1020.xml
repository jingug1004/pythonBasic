	fcInit(obj);
	return true;
}

function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;
    
	var sServiceName = "rem1020-service:searchEntRefuReport";
    var sInDataSet   = "";
    var sOutDataSet  = "dsEntRefuReport=dsEntRefuReport ";     
    
    //조회조건
    fcd_AddParam("BRNC_CD"     , cboBrncCdSearch.Value);
	fcd_AddParam("RACE_DT_FROM", calFromDt.Value);
	fcd_AddParam("RACE_DT_TO"  , calToDt.Value);
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}

