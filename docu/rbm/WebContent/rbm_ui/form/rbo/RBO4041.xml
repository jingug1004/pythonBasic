/***************** 20111130 추가 *********************/

function fcCombo_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	switch( obj.Id ) {
		case "cboSchPartsLcd" :
			if( length(strCode) > 0){
				var strFilter="";
				if( strCode == "001") strFilter = "CD_TERM1=='001'";
				if( strCode == "002") strFilter = "CD_TERM2=='002'";
				if( strCode == "003") strFilter = "CD_TERM3=='003'";		
				dsPartsMcd.Filter(strFilter);
				cboSchPartsMcd.Enable 	= true;
				cboSchPartsMcd.Index 	= 0;
			}
			else{
				strFilter = "CD_TERM1=='001'";
				dsPartsMcd.Filter(strFilter);
				cboSchPartsMcd.Enable 	= true;
				cboSchPartsMcd.Index 	= 0;
			}
			fcSearch();
		break;
		case "cboSchPartsMcd" :
			fcSearch();
		break;
		case "cboSchBrncCd" :
			fcSearch();
		break;
	}
}


// 발매기 '전체' 삽입
function fcDsOnLoadCompleted(obj,nErrorCode,strErrorMsg,nReason)
{
	switch ( obj.Id ) {
		case "dsPartsLcd" :
		
			if( nReason == 0){
				//데이터셋 복사( 조회조건용 / 입력용 )
				//dsLcd.Copy(obj);
				
				obj.fireEvent = false;
				obj.insertRow(0);
				obj.setColumn(0,"CD","");
				obj.setColumn(0,"CD_NM","전체");
				obj.fireEvent = true;
				cboSchPartsLcd.Index = 0;
			}
		break;
		
		case "dsPartsMcd" :
		
			if( nReason == 0){
				//데이터셋 복사( 조회조건용 / 입력용 )
				//dsMcd.Copy(obj);
			
				obj.fireEvent = false;
				obj.insertRow(0);
				obj.setColumn(0,"CD","");
				obj.setColumn(0,"CD_NM","전체");
				obj.setColumn(0,"CD_TERM1","001");
				obj.setColumn(0,"CD_TERM2","002");
				obj.setColumn(0,"CD_TERM3","003");
				obj.fireEvent = true;
				cboSchPartsMcd.Index = 0;
			}
		break;
	}
}

/*-------------------------------------------------------------------------------------------------+
>>  해당 부서코드 선택
+-------------------------------------------------------------------------------------------------*/
function fcChoice(obj) {
	
	
	var arrReturn    = array();               //결과 반환 할 Array

	if(dsList.GetRowCount() <=0 ) return;
	
	if(fnc_GetColumnValueCount(dsList, "CHK", "1") <=0){
	
	    fnc_Message(L_COM_ALT_0027,"N");
	    
		return;
	}
	
	var rtnStr ="";
	var nSeq = 0;
	
	for(var i =0;  i <  dsList.GetRowCount() ; i++){
		
		if(dsList.GetColumn(i,"CHK") == "1"){
			rtnStr = dsList.GetColumn(i, "PARTS_LCD") ;
			rtnStr += "|"+dsList.GetColumn(i, "PARTS_MCD") ;
			rtnStr += "|"+dsList.GetColumn(i, "PARTS_CD") ;
			rtnStr += "|"+dsList.GetColumn(i, "REQ_STK_CNT") ;
			
			rtnStr += "|"+dsList.GetColumn(i, "MADE_GBN") ;
				
			arrReturn[nSeq] = rtnStr;
			nSeq++;
	   }
	}

	
    Close(arrReturn); 
}



