		sOutDataSet  = "dsTotPlan=dsTotPlan dsTotRslt=dsTotRslt dsTotRsltComp=dsTotRsltComp ";
		SPlanType = "N";
	} else {
		SPlanType = "Y";
	}
	fcd_AddParam("PLAN_TYPE", SPlanType);
	fcd_AddParam("MEET_CD",   rdoMeetCd.Value);
	fcd_AddParam("STND_YEAR", cboYear.Value);

	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

