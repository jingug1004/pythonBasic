function fcSave(obj) {
	if ( !fcSaveCheck() ) return;
	var SPlanType = "";
	var sServiceName = "rbs9010-service:saveNetPlan";
	var sInDataSet   = "dsNetPlanSave=dsNetPlanSave:u ";
	var sOutDataSet  = "dsNetPlan=dsNetPlan dsNetRslt=dsNetRslt dsNetRsltComp=dsNetRsltComp ";

	if (tab0.TabIndex == 1) {
		sServiceName = "rbs9010-service:saveTotPlan";
