*                          CALL BACK 처리 부분                                                     *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
	
	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	if (sServiceName == "common-service:searchCode") {
		cboYear.Index = 0;
		fcSearch();
	}	
	
	// 조회 완료 후
	if ( sServiceName == "rbs9010-service:searchNetPlan" ) {
		dsNetRsltComp.row = -1;	
		if (dsNetPlan.rowcount() == 0) {
			dsNetPlan.addrow();
			dsNetPlan.SetColumn(0, "MEET_CD",   rdoMeetCd.Value);
			dsNetPlan.SetColumn(0, "STND_YEAR", cboYear.Value);
			dsNetPlan.SetColumn(0, "PLAN_TYPE", "Y");	//순매출액
		}
	} else if ( sServiceName == "rbs9010-service:searchTotPlan" ) {// 총매출액 완료 후
		dsTotRsltComp.row = -1;	
		if (dsTotPlan.rowcount() == 0) {
			dsTotPlan.addrow();		
			dsTotPlan.SetColumn(0, "MEET_CD",   rdoMeetCd.Value);
			dsTotPlan.SetColumn(0, "STND_YEAR", cboYear.Value);
			dsTotPlan.SetColumn(0, "PLAN_TYPE", "N");	//총매출액
		}
	} else if ( sServiceName == "rbs9010-service:saveNetPlan" ) {// 저장 완료 후
		
	}			
	
	
}


