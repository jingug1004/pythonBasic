	if (!fcSearchCheck(obj)) return;// 필수조건 체크 및 그리드 초기화
	
	var sServiceName = "rbs9010-service:searchNetPlan";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsNetPlan=dsNetPlan dsNetRslt=dsNetRslt dsNetRsltComp=dsNetRsltComp ";
	
