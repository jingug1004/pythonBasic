>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
	
	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	if (sServiceName == "common-service:searchCode") {
		cboYear.Index = 0;
		fcSearch();
	}	
	
	// 조회 완료 후
	if ( sServiceName == "rbs9050-service:searchGrnCdSalesRate" ) {
		if (dsGrnCdSalesRate.rowcount() > 0) {
			grdGrnCdSalesRate.SetCellProp("head", 1, "text", ToString(toNumber(cboYear.value) -1)+"년도");
			grdGrnCdSalesRate.SetCellProp("head", 2, "text", cboYear.value+"년도");
			dsGrnCdSalesRate.row = -1;
		}
	}			
	
	
}


