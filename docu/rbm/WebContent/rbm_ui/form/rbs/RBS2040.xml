	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	// 조회 완료 후
	if ( sServiceName == "rbs2040-service:searchList" ) {

		grdList.SetCellProp("head",3,"text", cboYear.value);
		grdList.SetCellProp("head",4,"text", parseInt(cboYear.value) - 1);
	}			
}


