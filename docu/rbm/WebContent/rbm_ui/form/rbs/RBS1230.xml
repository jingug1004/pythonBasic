/*-------------------------------------------------------------------------------------------------+
>>  Transaction ÈÄ Ã³¸® ÇØ¾ß ÇÒ ³»¿ë!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
	
	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	if (sServicename == "common-service:searchCode") {	// °øÅëÄÚµå Á¶È¸ÈÄ
		if (dsMeetCd.rowcount() > 0) rdoMeetCd.Value = "001";
				
	} else if ( sServiceName == "rsm2020-service:searchTms" ) { // TMS °ª °Ë»ö
		
		if(dsTms.GetRowCount()==0) {		
			dsTms.SetColumn(0,"TMS","");
			dsTms.SetColumn(0,"TMS_DESC","¾øÀ½");
			cboTms.Index=0;
		} else {
			cboTms.Value = dsTms.GetColumn(0,"MAX_TMS");		
		}
		fcSearchDayOrd();
		
	} else if ( sServiceName == "rsm2020-service:searchDayOrd" ) {	//  ÀÏÂ÷ 
		fcSearch();
		
	} else if ( sServiceName == "rbs1230-service:searchList" ) {	// ³¯Â¥º° ¸®½ºÆ®
		fcSearchYesSangRslt();
		
	} else if ( sServiceName == "rbs1230-service:searchYesSangRslt" ) {	// ¿¹»ó³»¿ª  Á¶È¸ ¿Ï·á ÈÄ
		dsYesSangRslt.row = -1;
	}	else if ( sServiceName == "rbs1230-service:saveYesSangRslt" ) { // ÀúÀå ¿Ï·á ÈÄ
		dsRaceNoList.row = nLastRow;
		fcSearchYesSangRslt();
	}		
}


