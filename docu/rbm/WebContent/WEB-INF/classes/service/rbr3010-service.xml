<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbr3010" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile" 	value="resultkey.properties" />

    <property name="resultKeyList" 			value="dsLeaseMana|dsLeaseBond" />

    <transition name="success" 				value="EventRouter" />

  </activity>

  

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">

    <transition name="searchLeaseMana"  	value="searchLeaseMana" />
    
    <transition name="searchLeaseBond"      value="searchLeaseBond" />

    <transition name="saveLeaseMana"   		value="saveLeaseMana" />    

    <transition name="success" 				value="end" />

  </activity>


  

  <!-- 임대  조회 -->  
  
  <activity name="searchLeaseMana" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success" 			value="end" />

    <property name="resultkey" 			value="dsLeaseMana" />

    <property name="sqlkey"				value="rbr3010_s01" />

    <property name="param-count" 		value="9" />

    <property name="param0" 			value="BRNC_CD" />
	<property name="param1"             value="CNTR_STR_DT" />
	<property name="param2"             value="CNTR_END_DT" />
	<property name="param3"             value="CNTR_STR_DT" />
    <property name="param4"             value="CNTR_END_DT" />
    <property name="param5"             value="CNTR_STR_DT" />
    <property name="param6"             value="CNTR_END_DT" />
    <property name="param7"             value="CNTR_STR_DT" />
    <property name="param8"             value="CNTR_END_DT" />
      
    <property name="dao" 				value="rbmdao" />

  </activity>  
  
 <!-- 임대  채권현황 조회 -->  
  
  <activity name="searchLeaseBond"      class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success"          value="end" />

    <property name="resultkey"          value="dsLeaseBond" />

    <property name="sqlkey"             value="rbr3010_s02" />

    <property name="param-count"        value="2" />

    <property name="param0"             value="BRNC_CD" />
    <property name="param1"             value="CNTR_STR_DT" />
\
      
    <property name="dao"                value="rbmdao" />

  </activity>  

  <!-- 임대  저장 -->

  <activity name="saveLeaseMana" 		class="snis.rbm.business.rbr3010.activity.SaveLeaseMana">
    <transition name="success" 			value="end" />
  </activity>


  <transaction-manager id="tx_rbm" commit="true" />

</service>



