<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm3050" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsDIVTMS|dsDivMonths|dsDIVTMSGsum|dsDivMonthsGsum" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchDIVTMS01"     value="searchDIVTMS01" />		<!-- 지점별 매출액(회차별) -->
    	<transition name="searchDIVMONTH01"   value="searchDIVMONTH01" />	<!-- 지점별 매출액(월별) -->
    	<transition name="searchDIVTMS02"     value="searchDIVTMS02" />		<!-- 지점별 매출액(회차별)-그린카드합산 -->
    	<transition name="searchDIVMONTH02"   value="searchDIVMONTH02" />	<!-- 지점별 매출액(월별)-그린카드합산 -->
		<transition name="success" 			  value="end" />
	</activity>
 	
 	
 	<!-- 지점별 매출액(전체, 회차별) -->
	<activity name="searchDIVTMS01" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 		  value="end" />
	  <property name="resultkey" 		  value="dsDIVTMS" />
	  <property name="sqlkey" 		  value="rsm3050_s01" />
	  <property name="param-count" 	  value="6" />
	  <property name="param0" 		  value="MEET_CD" />	<!-- 경주 구분 코드 -->
	  <property name="param1" 		  value="RACE_DAY_ST" />	<!-- 검색 기간 시작일 -->
	  <property name="param2" 		  value="RACE_DAY_END" />	<!-- 검색 기간 마지막 일 -->
	  <property name="param3" 		  value="MEET_CD" />		<!-- 경주 구분 코드 -->
	  <property name="param4" 		  value="RACE_DAY_ST" />	<!-- 검색 기간 시작일 -->
	  <property name="param5" 		  value="RACE_DAY_END" />	<!-- 검색 기간 마지막 일 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	
	
 	<!-- 지점별 매출액(전체, 월별) -->
	<activity name="searchDIVMONTH01" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	  value="end" />
	  <property name="resultkey" 	  value="dsDivMonths" />
	  <property name="sqlkey" 		  value="rsm3050_s02" />
	  <property name="param-count" 	  value="6" />
	  <property name="param0" 		  value="MEET_CD" />		<!-- 경주 구분 코드 -->
	  <property name="param1" 		  value="RACE_DAY_ST" />	<!-- 검색 기간 시작일 -->
	  <property name="param2" 		  value="RACE_DAY_END" />	<!-- 검색 기간 마지막 일 -->
	  <property name="param3" 		  value="MEET_CD" />		<!-- 경주 구분 코드 -->
	  <property name="param4" 		  value="RACE_DAY_ST" />	<!-- 검색 기간 시작일 -->
	  <property name="param5" 		  value="RACE_DAY_END" />	<!-- 검색 기간 마지막 일 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
					
 	<!-- 지점별 매출액(전체, 회차별)-그린카드합산 -->
	<activity name="searchDIVTMS02" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 		  value="end" />
	  <property name="resultkey" 		  value="dsDIVTMSGsum" />
	  <property name="sqlkey" 		  value="rsm3050_s03" />
	  <property name="param-count" 	  value="6" />
	  <property name="param0" 		  value="MEET_CD" />	<!-- 경주 구분 코드 -->
	  <property name="param1" 		  value="RACE_DAY_ST" />	<!-- 검색 기간 시작일 -->
	  <property name="param2" 		  value="RACE_DAY_END" />	<!-- 검색 기간 마지막 일 -->
	  <property name="param3" 		  value="MEET_CD" />		<!-- 경주 구분 코드 -->
	  <property name="param4" 		  value="RACE_DAY_ST" />	<!-- 검색 기간 시작일 -->
	  <property name="param5" 		  value="RACE_DAY_END" />	<!-- 검색 기간 마지막 일 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	
	
 	<!-- 지점별 매출액(전체, 월별)-그린카드합산 -->
	<activity name="searchDIVMONTH02" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	  value="end" />
	  <property name="resultkey" 	  value="dsDivMonthsGsum" />
	  <property name="sqlkey" 		  value="rsm3050_s04" />
	  <property name="param-count" 	  value="6" />
	  <property name="param0" 		  value="MEET_CD" />		<!-- 경주 구분 코드 -->
	  <property name="param1" 		  value="RACE_DAY_ST" />	<!-- 검색 기간 시작일 -->
	  <property name="param2" 		  value="RACE_DAY_END" />	<!-- 검색 기간 마지막 일 -->
	  <property name="param3" 		  value="MEET_CD" />		<!-- 경주 구분 코드 -->
	  <property name="param4" 		  value="RACE_DAY_ST" />	<!-- 검색 기간 시작일 -->
	  <property name="param5" 		  value="RACE_DAY_END" />	<!-- 검색 기간 마지막 일 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
							
  <transaction-manager id="tx_rbm" commit="true" />

</service>



