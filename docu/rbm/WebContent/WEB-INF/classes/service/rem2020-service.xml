<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem2020" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsThrOutBrnc|dsThrOutReport" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchThrOutReport"  	value="searchThrOutReport" />
    <transition name="searchThrOutBrnc"  	value="searchThrOutBrnc" />
    <transition name="success" 				value="end" />
  </activity>
  
  <!-- »ïÁø¾Æ¿ô È°µ¿  Áý°èÇ¥ -->
  <activity name="searchThrOutReport" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="searchThrOutBrnc" />
    <property name="resultkey" 			value="dsThrOutReport" />
    <property name="sqlkey"				value="rem2020_s02" />
    <property name="param-count" 		value="2" />
    <property name="param0" 			value="ACT_YEAR" />
    <property name="param1" 			value="BRNC_CD" />
    <property name="dao" 				value="rbmdao" />
  </activity>  
  

  <!-- »ïÁø¾Æ¿ô È°µ¿  Á¶È¸ -->
  <activity name="searchThrOutBrnc"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsThrOutBrnc" />
    <property name="sqlkey"				value="rem2020_s01" />
    <property name="param-count" 		value="3" />
    <property name="param0" 			value="OUT_GBN" />
    <property name="param1" 			value="ACT_YEAR" />
    <property name="param2" 			value="BRNC_CD" />
    <property name="dao" 				value="rbmdao" />
  </activity>  
  
  <transaction-manager id="tx_rbm" commit="true" />
</service>



