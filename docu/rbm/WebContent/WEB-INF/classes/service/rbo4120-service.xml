<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbo4120" initial="SetResultKeys">

    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
        <property name="staticResultKeyFile"  value="resultkey.properties" />
        <property name="resultKeyList"        value="dsInOutList" />
        <transition name="success"            value="EventRouter" />
    </activity>
  
    <activity name="EventRouter"     class="com.posdata.glue.biz.activity.PosDefaultRouter">            
        <transition name="searchInOutList"         value="searchInOutList" />   <!-- 입출고내역  조회 -->
        <transition name="success"            value="end" />
    </activity>
    
  <!-- 입출고내역  조회 -->
  <activity name="searchInOutList"         class="com.posdata.glue.miplatform.biz.activity.PosSearch">  
    
    <transition name="success"        value="end" />
    
    <property name="resultkey"        value="dsInOutList" />
    <property name="sqlkey"           value="rbo4120_s01" />
    <property name="param-count"      value="12" />  
    
    <property name="param0"           value="PARTS_CD" />   <!-- 부품코드  -->
    <property name="param1"           value="PARTS_LCD" />  <!-- 발매기코드   -->
    <property name="param2"           value="PARTS_MCD" />  <!-- 모델코드  -->
    <property name="param3"           value="INOUT_FROM" /> <!-- 조회기간 시작일 -->
    <property name="param4"           value="INOUT_TO" />   <!-- 조회기간 종료일  -->
    
    <property name="param5"           value="PARTS_CD" />    <!-- 부품코드  -->
    <property name="param6"           value="PARTS_LCD" />   <!-- 발매기코드   -->
    <property name="param7"           value="PARTS_MCD" />   <!-- 모델코드  -->  
    <property name="param8"           value="INOUT_FROM" />  <!-- 조회기간 시작일 -->
    <property name="param9"           value="INOUT_TO" />    <!-- 조회기간 종료일  -->
    
    <property name="param10"           value="BRNC_CD" />    <!-- 지점코드 -->
    <property name="param11"           value="INOUT_GBN" />  <!-- 입출고 구분  -->
      
    <property name="dao"              value="rbmdao" />

  </activity>
  
  <transaction-manager id="tx_rbm" commit="true" />

</service>