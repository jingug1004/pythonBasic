<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm3160" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsHighLow" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchHighLow"         value="searchHighLow" /> 	<!-- 승식별 최고/최저 -->
		<transition name="success" 			  value="end" />
	
	</activity>
	
	
	<!-- 승식별 최고/최저 -->
	<activity name="searchHighLow" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 		  value="end" />
	  <property name="resultkey" 		  value="dsHighLow" />
	  <property name="sqlkey" 		  value="rsm3160_s01" />
	  <property name="param-count" 	  value="3" />
	  <property name="param0" 		  value="MEET_CD" />	<!-- 개최지 코드    -->
	  <property name="param1" 		  value="RACE_DAY_ST" />	<!-- 경주검색 시작일 -->
	  <property name="param2" 		  value="RACE_DAY_END" />	<!-- 경주검색 마지막 일 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	
 	
  <transaction-manager id="tx_rbm" commit="true" />

</service>



