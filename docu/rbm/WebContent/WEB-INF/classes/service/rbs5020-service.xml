<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbs5020" initial="SetResultKeys">
  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsContestType" />
    <transition name="success"              value="EventRouter" />
  </activity>  

  <activity name="EventRouter"              	class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchContestTypeList"  	value="searchContestTypeList" />   	<!-- 대회분류  조회 -->
    <transition name="saveContestType"  		value="saveContestType" /> 			<!-- 대회분류  저장  -->
    <transition name="success"              	value="end" />
  </activity>

  <!-- 대회분류 조회 -->
  <activity name="searchContestTypeList"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsContestType" />
    <property name="sqlkey"                 value="rbs5020_s01" />    
    <property name="param-count"            value="1" />        
    <property name="param0"                 value="GAME_CD" />
    <property name="dao"                    value="rbmdao" />    
  </activity>
  
  
  <!-- 대회분류  저장  --> 
  <activity name="saveContestType"          class="snis.rbm.business.rbs5020.activity.SaveContestType">
    <transition name="success"              value="searchContestTypeList" />
  </activity>    

  <transaction-manager id="tx_rbm" commit="true" />
</service>
