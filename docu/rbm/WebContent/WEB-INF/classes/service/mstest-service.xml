<?xml version="1.0" encoding="EUC-KR"?>

<service name="mstest" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile" 	value="resultkey.properties" />

    <property name="resultKeyList" 			value="dsSale|dsProfitMana_0" />

    <transition name="success" 				value="EventRouter" />

  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    
    <transition name="searchTest"  			value="searchTest"  />  <!-- mssql -->
    <transition name="searchTest1"  		value="searchTest1" />  <!-- oracle -->

  </activity>
  
    <!-- mssql -->
   <activity name="searchTest"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success" 			value="end" />

    <property name="resultkey" 			value="dsSale" />

    <property name="sqlkey"				value="mstest_s01" />

    <property name="param-count" 		value="0" />
    
    <property name="dao" 				value="venisrbmdao" />

  </activity>
  
  <!-- oracle -->
   <activity name="searchTest1"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success" 			value="end" />

    <property name="resultkey" 			value="dsProfitMana_0" />

    <property name="sqlkey"				value="mstest_s02" />

    <property name="param-count" 		value="0" />
    
    <property name="dao" 				value="rbmdao" />

  </activity>
  
  <transaction-manager id="tx_venus" commit="true" />

</service>



