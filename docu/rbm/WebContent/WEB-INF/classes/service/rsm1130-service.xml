<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm1130" initial="SetResultKeys">
    <activity name="SetResultKeys"   				class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  		value="resultkey.properties" />	    <property name="resultKeyList" 		  		value="dsList|dsListDetl|dsListEx" />	    <transition name="success" 			  		value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchEventHitStat"  		value="searchEventHitStat" />		<!--  기간별 적중현황  -->
    	<transition name="searchEventHitExStat"  	value="searchEventHitExStat" />		<!--  기간별 연속적중현황  -->
    	<transition name="searchEventHitList"  		value="searchPartiList" />		<!--  기간별 적중내역  -->
		<transition name="success" 		  			value="end" />	</activity> 	
 	
 	<!--  기간별 이벤트 적중현황  -->
	<activity name="searchEventHitStat" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  <transition name="success" 		value="end" />
	  <property name="resultkey" 		value="dsList" />
	  <property name="sqlkey" 			value="rsm1130_s01" />
	  <property name="param-count" 		value="2" />
	  <property name="param0" 			value="STR_DT" />	<!-- 시작일자  -->
	  <property name="param1" 			value="END_DT" />	<!-- 종료일자 -->
	  <property name="dao" 				value="rbmdao" />
	</activity>
	
 	<!--  기간별 이벤트 적중현황  -->
	<activity name="searchPartiList" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  <transition name="success" 		value="end" />
	  <property name="resultkey" 		value="dsListDetl" />
	  <property name="sqlkey" 			value="rsm1130_s02" />
	  <property name="param-count" 		value="1" />
	  <property name="param0" 			value="EVENT_DT" />	<!-- 응모일자 -->
	  <property name="dao" 				value="rbmdao" />
	</activity>
	
 	<!--  기간별 이벤트 적중현황  -->
	<activity name="searchEventHitExStat" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  <transition name="success" 		value="searchEventHitStat" />
	  <property name="resultkey" 		value="dsListEx" />
	  <property name="sqlkey" 			value="rsm1130_s03" />
	  <property name="param-count" 		value="2" />
	  <property name="param0" 			value="STR_DT" />	<!-- 시작일자  -->
	  <property name="param1" 			value="END_DT" />	<!-- 종료일자 -->
	  <property name="dao" 				value="rbmdao" />
	</activity>
	
	<transaction-manager id="tx_rbm" commit="true" />
  </service>