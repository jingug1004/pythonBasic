<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs1050" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsList1|dsList2|dsList3" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
        <transition name="searchList1"        value="searchList1" />	
	    <transition name="searchList2"        value="searchList2" />		    
        <transition name="searchList3"        value="searchList3" />		
        <transition name="saveBusiProgReg"    value="saveBusiProgReg" />		<transition name="success" 			  value="end" />	</activity> 		
  <!-- 집행계획 조회 -->  <activity name="searchList1" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		  value="searchList2" />
    <property name="resultkey" 		  value="dsList1" />
    <property name="sqlkey" 		  value="rbs1050_s01" />
    <property name="param-count" 	  value="0" />   
    <property name="dao" 			  value="rbmdao" />
  </activity>
  
  
  <!-- 집행내역 조회 -->
  <activity name="searchList2" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		  value="searchList3" />
    <property name="resultkey" 		  value="dsList2" />
    <property name="sqlkey" 		  value="rbs1050_s02" />
    <property name="param-count" 	  value="0" />  
    <property name="dao" 			  value="rbmdao" />
  </activity>
  
  
  <!-- 계획대비 집행 조회 -->
  <activity name="searchList3" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		  value="end" />
    <property name="resultkey" 		  value="dsList3" />
    <property name="sqlkey" 		  value="rbs1050_s03" />
    <property name="param-count" 	  value="0" />   
    <property name="dao" 			  value="rbmdao" />
  </activity>	
  	  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

