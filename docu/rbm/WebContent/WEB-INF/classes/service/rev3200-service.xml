<?xml version="1.0" encoding="EUC-KR"?>

<service name="rev3200" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />   
    <property name="resultKeyList" 			value="dsEscEmp|dsTeller|dsEstmDate|dsDwTelmp" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEscEmp"  		value="searchEscEmp" />	 <!-- 발매제외기간 해당 사원  조회 -->
    <transition name="searchTeller"  		value="searchTeller" />	 <!-- 발매제외기간 대상 사원  조회 -->
    <transition name="saveEscEmp"   		value="saveEscEmp" />    <!-- 발매제외기간 저장  -->
    <transition name="saveTemlp"   		    value="saveTemlp" />     <!-- 발매실적 초기화  --> 
  </activity>
 
  <!-- 발매제외기간 해당 사원  조회 -->
   <activity name="searchEscEmp" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="searchEstmDate" />
    <property name="resultkey" 			value="dsEscEmp" />
    <property name="sqlkey"				value="rev3200_s01" />
    <property name="param-count" 	    value="6" />
    <property name="param0"             value="ESTM_YEAR" />   <!-- 평가년도 -->
    <property name="param1"             value="ESTM_NUM" />    <!-- 평가차수 -->
    <property name="param2"			    value="BRNC_CD" />     <!-- 지점코드 -->
    <property name="param3"             value="BRNC_CD" />     <!-- 지점코드 -->
    <property name="param4"             value="BRNC_CD" />     <!-- 지점코드 -->
    <property name="param5" 		    value="EMP_NM" />      <!-- 사원명    -->
    <property name="dao" 				value="rbmdao" />
  </activity>
  
  <!-- 평가기간  조회 -->
   <activity name="searchEstmDate" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsEstmDate" />
    <property name="sqlkey"				value="rev3200_s02" />
    <property name="param-count" 	    value="2" />
    <property name="param0"             value="ESTM_YEAR" />   <!-- 평가년도 -->
    <property name="param1"             value="ESTM_NUM" />    <!-- 평가차수 -->
    <property name="dao" 				value="rbmdao" />
  </activity>
  
  <!-- 발매제외기간 대상 사원  조회 -->
   <activity name="searchTeller" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="searchEstmDate" />
    <property name="resultkey" 			value="dsTeller" />
    <property name="sqlkey"				value="rev3201_s01" />
    <property name="param-count" 	    value="6" />
    <property name="param0"             value="ESTM_YEAR" />   <!-- 평가년도 -->
    <property name="param1"             value="ESTM_NUM" />    <!-- 평가차수 -->
    <property name="param2"			    value="BRNC_CD" />     <!-- 지점코드 -->
    <property name="param3"             value="BRNC_CD" />     <!-- 지점코드 -->
    <property name="param4"             value="BRNC_CD" />     <!-- 지점코드 -->
    <property name="param5" 		    value="EMP_NM" />      <!-- 사원명    -->
    <property name="dao" 				value="rbmdao" />
  </activity>

  <!-- 발매실적 조회 -->
    <activity name="searchDwTelmp"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
        <transition name="success"        value="end" />
        <property name="resultkey"        value="dsDwTelmp" />
        <property name="sqlkey"           value="rev1010_s11" />
        <property name="param-count"      value="9" />
        <property name="param0"           value="PRM_STR_DT" /> 
        <property name="param1"           value="PRM_END_DT" />
        <property name="param2"           value="ESTM_YEAR" /> 
        <property name="param3"           value="ESTM_NUM" />
        <property name="param4"           value="DEPT_CD" />
        <property name="param5"           value="DEPT_CD" />
        <property name="param6"           value="DEPT_CD" />
        <property name="param7"           value="ESTM_YEAR" /> 
        <property name="param8"           value="ESTM_NUM" />
        <property name="dao"              value="rbmdao" />
    </activity>
    
  <!-- 발매제외기간 저장 -->
  <activity name="saveEscEmp" 		class="snis.rbm.business.rev3200.activity.SaveEscEmp">
    <transition name="success" 			value="end" />
  </activity>
  
  <!-- 발매실적 초기화 -->
  <activity name="saveTemlp" 		class="snis.rbm.business.rev3200.activity.SaveTelmp">
    <transition name="success" 			value="end" />
  </activity>  
  <transaction-manager id="tx_rbm" commit="true" />
</service>