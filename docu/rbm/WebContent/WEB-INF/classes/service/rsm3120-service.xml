<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm3120" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsTelmpTeller" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchTelmpTeller01"	value="searchTelmpTeller01" />	<!-- 개인별 발매 -->
    	<transition name="searchTelmpTellerView01"	value="searchTelmpTellerView01" />	<!--  개인별 발매 View -->    	    		
		<transition name="success" 			  value="end" />
	</activity>
	
 	<!-- 개인별 발매 -->
	<activity name="searchTelmpTeller01" 	  class="snis.rbm.business.rsm3120.activity.SearchTelmpTeller">
	  <transition name="success" 		  value="end" />
	</activity>
	
	
	<!-- 개인별 발매  View -->
	<activity name="searchTelmpTellerView01" 	  class="snis.rbm.business.rsm3120.activity.SearchTelmpTeller">
	  <transition name="success" 		  value="end" />
	</activity>
	
  <transaction-manager id="tx_rbm" commit="true" />

</service>



