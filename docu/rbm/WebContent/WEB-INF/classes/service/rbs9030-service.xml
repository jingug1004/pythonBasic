<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbs9030" initial="SetResultKeys">
  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsCrssRate|dsCrssCnt" />
    <transition name="success"              value="EventRouter" />
  </activity>  

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchCrssRate"      	value="searchCrssRate" />  		<!-- 교차투표비율  조회  -->
    <transition name="searchCrssCnt"      	value="searchCrssCnt" />  		<!-- 교차투표건수  조회  -->
  </activity>

  <!-- 교차투표비율  조회 -->
  <activity name="searchCrssRate"       		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="searchCrssCnt" />
    <property name="resultkey"              value="dsCrssRate" />
    <property name="sqlkey"                 value="rbs9030_s01" />    
    <property name="param-count"            value="14" />        
    <property name="param0" 				value="STND_YEAR" /> 
    <property name="param1" 				value="STND_YEAR" /> 
    <property name="param2" 				value="STND_YEAR" /> 
    <property name="param3" 				value="STND_YEAR" /> 
    <property name="param4" 				value="STND_YEAR" />
    <property name="param5" 				value="STND_YEAR" /> 
    <property name="param6" 				value="STND_YEAR" /> 
    <property name="param7" 				value="STND_YEAR" /> 
    <property name="param8" 				value="STND_YEAR" />
    <property name="param9" 				value="STND_YEAR" /> 
    <property name="param10" 				value="STND_YEAR" /> 
    <property name="param11" 				value="STND_YEAR" /> 
    <property name="param12" 				value="STND_YEAR" /> 
    <property name="param13" 				value="STND_YEAR" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>

  <!-- 교차투표건수  조회 -->
  <activity name="searchCrssCnt"       		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsCrssCnt" />
    <property name="sqlkey"                 value="rbs9030_s02" />    
    <property name="param-count"            value="4" />        
    <property name="param0" 				value="STND_YEAR" /> 
    <property name="param1" 				value="STND_YEAR" /> 
    <property name="param2" 				value="STND_YEAR" /> 
    <property name="param3" 				value="STND_YEAR" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
    
  <transaction-manager id="tx_rbm" commit="true" />
</service>
