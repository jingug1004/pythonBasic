<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbr5030" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsBrncInfo|dsOrganExamBrnc|dsOrganExamVeri" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchBrncCd"  		value="searchBrncCd" />     	<!-- 지점코드 조회  -->
	<transition name="searchOrganExamBrnc"  value="searchOrganExamBrnc" />  <!-- 출주표 검수내역 조회 -->
    <transition name="searchOrganExamVeri"  value="searchOrganExamVeri" />  <!-- 출주표 검수상태 조회  -->
    <transition name="saveOrganExamBrnc"   	value="saveOrganExamBrnc" />	<!-- 검수내역 저장  -->
    <transition name="saveOrganExamVeri"   	value="saveOrganExamVeri" />	<!-- 검수상태 저장  -->
  </activity>
  

  <!-- 지점코드 조회 -->
  <activity name="searchBrncCd" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsBrncInfo" />
    <property name="sqlkey"				value="rbr5030_s01" />
    <property name="param-count" 		value="1" />
    <property name="param0" 			value="USER_ID" />   <!-- 사번 -->
    <property name="dao" 				value="rbmdao" />
  </activity>  
 
   <!-- 출주표 검수상태 조회 -->
  <activity name="searchOrganExamVeri" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="searchOrganExamBrnc" />
    <property name="resultkey" 			value="dsOrganExamVeri" />
    <property name="sqlkey"				value="rbr5030_s02" />
    <property name="param-count" 		value="11" />
    <property name="param0" 			value="STND_YEAR" /> <!-- 기준년도 -->
    <property name="param1" 			value="STND_MM" />   <!-- 기준월 -->
    <property name="param2" 			value="BRNC_CD" />   <!-- 지점코드 -->
    <property name="param3" 			value="USER_ID" />   <!-- 사번 -->
    <property name="param4" 			value="STND_YEAR" /> <!-- 기준년도 -->
    <property name="param5" 			value="STND_MM" />   <!-- 기준월 -->
    <property name="param6" 			value="BRNC_CD" />   <!-- 지점코드 -->
    <property name="param7" 			value="USER_ID" />   <!-- 사번 -->
    <property name="param8" 			value="USER_ID" />   <!-- 사번 -->        
    <property name="param9" 			value="USER_ID" />   <!-- 사번 -->
    <property name="param10" 			value="USER_ID" />   <!-- 사번 -->
    <property name="dao" 				value="rbmdao" />
  </activity> 
  
  <!-- 출주표 제작수량 조회 -->
  <activity name="searchOrganExamBrnc" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsOrganExamBrnc" />
    <property name="sqlkey"				value="rbr5030_s03" />
    <property name="param-count" 		value="4" />
    <property name="param0" 			value="STND_YEAR" /> <!-- 기준년도 -->
    <property name="param1" 			value="STND_MM" />   <!-- 기준월 -->
    <property name="param2" 			value="BRNC_CD" />   <!-- 지점코드 -->
    <property name="param3" 			value="BRNC_CD" />   <!-- 지점코드 -->
    <property name="dao" 				value="rbmdao" />
  </activity>
 
  <!-- 출주표 검수내역 저장 -->
  <activity name="saveOrganExamBrnc" 	class="snis.rbm.business.rbr5030.activity.SaveOrganExamBrnc">
    <transition name="success" 			value="end" />
  </activity>
  
  <!-- 출주표 검수상태 저장 -->
  <activity name="saveOrganExamVeri" 	class="snis.rbm.business.rbr5030.activity.SaveOrganExamVeri">
    <transition name="success" 			value="end" />
  </activity>
  
  <transaction-manager id="tx_rbm" commit="true" />

</service>