<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbr1040" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsConInfoSum|dsConInfo|dsConInfo2|dsConEquip|dsConEquip2" />
    <transition name="searchConEquip"       value="searchConEquip" />    <!-- 장비및시설  조회 -->
    <transition name="searchConEquip2"      value="searchConEquip2" />    <!-- 장비및시설  조회 -->   
    <transition name="success"              value="EventRouter" />
  </activity>


  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchConInfoSum"     value="searchConInfoSum" />    <!-- 장비및시설  조회 -->
    <transition name="searchConInfo"        value="searchConInfo" />    <!-- 장비및시설  조회 -->
    <transition name="searchConInfo2"       value="searchConInfo2" />    <!-- 장비및시설  조회 -->
    <transition name="success"              value="end" />
  </activity>

  <!--  장비및시설  조회 -->
  <activity name="searchConInfoSum"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsConInfoSum" />
    <property name="sqlkey"                 value="rbr1030_s01" />   
    <property name="param-count"            value="2" />      
    <property name="param0"                 value="STND_YEAR" />       <!-- 지점코드  -->    
    <property name="param1"                 value="BRNC_CD" /> 
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  <!--  장비및시설  조회 -->
  <activity name="searchConInfo"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsConInfo" />
    <property name="sqlkey"                 value="rbr1030_s02" />   
    <property name="param-count"            value="1" />      
    <property name="param0"                 value="STND_YEAR" />       <!-- 지점코드  -->    
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  
  <!--  장비및시설  조회 -->
  <activity name="searchConInfo2"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsConInfo2" />
    <property name="sqlkey"                 value="rbr1030_s03" />   
    <property name="param-count"            value="1" />      
    <property name="param0"                 value="STND_YEAR" />       <!-- 지점코드  -->    
    <property name="dao"                    value="rbmdao" />
  </activity>  
  

  <!--  장비및시설  조회 -->
  <activity name="searchConEquip"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="searchConEquip2" />
    <property name="resultkey"              value="dsConEquip" />
    <property name="sqlkey"                 value="rbr1030_s04" />   
    <property name="param-count"            value="1" />      
    <property name="param0"                 value="STND_YEAR" />       <!-- 지점코드  -->    
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  <!--  장비및시설  조회 -->
  <activity name="searchConEquip2"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsConEquip2" />
    <property name="sqlkey"                 value="rbr1030_s05" />    
    <property name="param-count"            value="1" />      
    <property name="param0"                 value="STND_YEAR" />       <!-- 지점코드  -->  
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  
<transaction-manager id="tx_rbm" commit="true" />

</service>



