<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs2020" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsList|dsHealBusi" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchList"         value="searchList" />	
	    <transition name="searchHealBusiList"     value="searchHealBusiList" />    
	    <transition name="saveHealExec"       value="saveHealExec" />			<transition name="success" 			  value="end" />	</activity> 		
  <!-- 건전레저인원등록 조회 -->  <activity name="searchList" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		  value="end" />
    <property name="resultkey" 		  value="dsList" />
    <property name="sqlkey" 		  value="rbs2020_s_healexec" />
    <property name="param-count" 	  value="8" />
    <property name="param0" 		  value="EXEC_YEAR" />    
    <property name="param1" 		  value="DPRT_CD" /> 
    <property name="param2" 		  value="EXEC_MM" /> 
    
    <property name="param3"           value="BUSI_CNTNT_CD" /> 
    <property name="param4"           value="BUSI_CNTNT_CD" /> 
    <property name="param5"           value="BUSI_CNTNT_CD" /> 
    
    <property name="param6" 		  value="EXEC_YEAR" /> 
    <property name="param7" 		  value="EXEC_MM" />
    <property name="dao" 			  value="rbmdao" />
  </activity>	
  <!-- 건전레저사업명  조회 -->
  <activity name="searchHealBusiList"     class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"        value="end" />
    <property name="resultkey"        value="dsHealBusi" />
    <property name="sqlkey"           value="rbs2020_s02" />
    <property name="param-count"      value="2" />    
    <property name="param0"           value="DPRT_CD" /> 
    <property name="param1"           value="BUSI_YM_STR" /> 
    <property name="dao"              value="rbmdao" />
  </activity>
  
	<!-- 건전레저인원등록 저장 -->
  	<activity name="saveHealExec" 		class="snis.rbm.business.rbs2020.activity.SaveHealLeisuPrsnNumReg">
    	<transition name="success" 			value="end" />
  	</activity>
  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

