<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs1110" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsYear|dsHnrRefreTms" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 			class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchYearList"     	value="searchYearList" />	
	    <transition name="searchHnrRefreTms"  	value="searchHnrRefreTms" />	
	    <transition name="saveHnrRefreTms"  	value="saveHnrRefreTms" />
		<transition name="success" 			  	value="end" />	</activity> 		
  	<!-- 명예심판 기준년도 조회 -->
  	<activity name="searchYearList" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsYear" />
    	<property name="sqlkey" 		  value="rbs1110_s01" />
    	<property name="param-count" 	  value="1" />
    	<property name="param0" 		  value="MEET_CD" />    
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
  	<!-- 명예심판 차수 조회 -->
  	<activity name="searchHnrRefreTms" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsHnrRefreTms" />
    	<property name="sqlkey" 		  value="rbs1110_s02" />
    	<property name="param-count" 	  value="2" />
    	<property name="param0" 		  value="MEET_CD" />    
    	<property name="param1" 		  value="STND_YEAR" />
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
	
	<!-- 명예심판 차수 저장 -->
  	<activity name="saveHnrRefreTms" 	  class="snis.rbm.business.rbs1110.activity.SaveHnrRefreTms">
    	<transition name="success" 	  	  value="end" />
  	</activity>
  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

