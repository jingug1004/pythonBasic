<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm2041" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsEtc|dsTMSEtc|dsRemark" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchEtc"         value="searchEtc" />    	<transition name="searchTMSEtc"      value="searchTMSEtc" />    	<transition name="searchGraphEtc"    value="searchGraphEtc" />
    	<transition name="saveRaceEtc"       value="saveRaceEtc" />		<transition name="success" 			 value="end" />	</activity>
	
	<!-- 특이사항 조회-->
	<activity name="searchEtc" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	  value="end" />
	  <property name="resultkey" 	  value="dsEtc" />
	  <property name="sqlkey" 		  value="rsm2041_s01" />
	  <property name="param-count" 	  value="2" />
	  <property name="param0" 		  value="MEET_CD" />
	  <property name="param1" 		  value="RACE_DAY" />
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	<!-- 회차 특이사항 조회-->	<activity name="searchTMSEtc" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">	  <transition name="success" 	  value="end" />	  <property name="resultkey" 	  value="dsTMSEtc" />	  <property name="sqlkey" 		  value="rsm2041_s02" />	  <property name="param-count" 	  value="2" />	  <property name="param0" 		  value="MEET_CD" />	  <property name="param1" 		  value="RACE_DAY" />	  <property name="dao" 			  value="rbmdao" />	</activity>	<!-- 년도 회차 특이사항 전체조회-->	<activity name="searchGraphEtc" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">	  <transition name="success" 	  value="end" />	  <property name="resultkey" 	  value="dsRemark" />	  <property name="sqlkey" 		  value="rsm2041_s03" />	  <property name="param-count" 	  value="2" />	  <property name="param0" 		  value="MEET_CD" />	  <property name="param1" 		  value="RACE_DAY" />	  <property name="dao" 			  value="rbmdao" />	</activity>	
	<!-- 특이사항 수정 -->
	<activity name="saveRaceEtc" 	  class="snis.rbm.business.rsm2041.activity.SaveRaceEtc">
    	<transition name="success" 	  value="end" />
  	</activity>
	
  
  <transaction-manager id="tx_rbm" commit="true" />

  </service>

