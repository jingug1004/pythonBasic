<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbo2050" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsAppoSearMana" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchAppoSearMana"	value="searchAppoSearMana" /> <!-- 월별매표세부 조회  -->
  </activity>
  

  <!-- 월별매표세부 조회  -->
   <activity name="searchAppoSearMana"	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsAppoSearMana" />

    <property name="sqlkey"				value="rbo2050_s01" />

    <property name="param-count" 		value="6" />
    
    <property name="param0" 			value="BRNC_CD"       /> <!-- 지점  -->
    <property name="param1" 			value="SALES_DT_FROM" /> <!-- 판매일자FROM -->
    <property name="param2"             value="SALES_DT_TO"   /> <!-- 판매일자TO -->
    
    <property name="param3"             value="MEET_CD"   /> <!-- 시행처구분  -->
    <property name="param4"             value="MEET_CD"   /> <!-- 시행처구분 -->
    <property name="param5"             value="MEET_CD"   /> <!-- 시행처구분 -->
    
    <property name="dao" 				value="rbmdao" />
  </activity>   
  
  <transaction-manager id="tx_rbm" commit="true" />
</service>