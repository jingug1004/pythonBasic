<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem3011" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsStayEndMana" />
    <transition name="success"              value="EventRouter" />

  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">\
    <transition name="searchStayEndMana"       value="searchStayEndMana" /> <!--  체류인원취소요청   조회  -->
    <transition name="saveStayEndMana"         value="saveStayEndMana" />   <!--  체류인원취소요청   저장  -->
    <transition name="success"              value="end" />

  </activity>

  <!--  체류인원취소요청  조회 -->
  
  <activity name="searchStayEndMana"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsStayEndMana" />
    <property name="sqlkey"                 value="rem3011_s01" />
    
    <property name="param-count"            value="4" />
    
    <property name="param0"                 value="RACE_DT" />      <!--  경주일자 -->
    <property name="param1"                 value="BRNC_CD" />      <!--  지점 -->
    <property name="param2"                 value="MEET_CD" />      <!--  경륜장코드  -->
    <property name="param3"                 value="SEQ" />          <!--  순번  -->

   
   <property name="dao"                    value="rbmdao" />       

  </activity>  



  <!-- 체류인원취소요청  저장 -->

  <activity name="saveStayEndMana"             class="snis.rbm.business.rem3011.activity.SaveStayEndMana">
    <transition name="success"              value="end" />
  </activity>


  <transaction-manager id="tx_rbm" commit="true" />

</service>



