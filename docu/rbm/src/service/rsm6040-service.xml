<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm6040" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsTsnRcptResult" />
    <transition name="success"              value="EventRouter" />
  </activity>

  <activity name="EventRouter"          class="com.posdata.glue.biz.activity.PosDefaultRouter">    
    <transition name="searchTsnRcpt"    value="searchTsnRcpt" />   		<!-- 분실신고서 온라인접수 목록조회 	-->
    <transition name="saveReq"      	value="saveReq" />     			<!-- 분실신고서 온라인접수  			-->    
    <transition name="saveReqCancel"    value="saveReqCancel" />		<!-- 분실신고서 온라인접수취소 		-->
    <transition name="saveRefund"      	value="saveRefund" />   		<!-- 분실신고서 온라인접수기환급 		-->
    <transition name="saveRcptNo"      	value="saveRcptNo" />   		<!-- 분실신고서 온라인접수해당없음 	-->
    <transition name="saveDataEx"      	value="saveDataEx" />   		<!-- 분실신고서 온라인접수자료존재	-->
  </activity>
  
  <!-- 분실신고서 온라인접수 목록조회  -->
  <activity name="searchTsnRcpt"       		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"       		value="end" />   
    <property name="resultkey"        		value="dsTsnRcptResult" />
    <property name="sqlkey"           		value="rsm6040_s01" />    
    
    <property name="param-count"      		value="7" />    
    <property name="param0"          		value="rcptstatcd" />         
    <property name="param1"          		value="rcptstatcd" />
    <property name="param2"          		value="rcptstatcd" />
    <property name="param3"          		value="brnccd" />
    <property name="param4"          		value="brnccd" />
    <property name="param5"          		value="brnccd" />
    <property name="param6"          		value="custnm" />
    
    <property name="dao"              		value="rbmdao" />   
  </activity>

  <!-- 분실신고서 온라인접수   -->
  <activity name="saveReq"         	 		class="snis.rbm.business.rsm6040.activity.SaveStopPayOnline">
    <transition name="success"             	value="end" />
  </activity>

  <!-- 분실신고서 온라인접수취소   -->
  <activity name="saveReqCancel"         	class="snis.rbm.business.rsm6040.activity.SaveStopPayOnline">
    <transition name="success"             	value="end" />
  </activity>

  <!-- 분실신고서 온라인접수기환급    -->
  <activity name="saveRefund"         	 	class="snis.rbm.business.rsm6040.activity.SaveStopPayOnline">
    <transition name="success"             	value="end" />
  </activity>

  <!-- 분실신고서 온라인접수해당없음   -->
  <activity name="saveRcptNo"         	 	class="snis.rbm.business.rsm6040.activity.SaveStopPayOnline">
    <transition name="success"             	value="end" />
  </activity>

  <!-- 분실신고서 온라인접수자료존재   -->
  <activity name="saveDataEx"         	 	class="snis.rbm.business.rsm6040.activity.SaveStopPayOnline">
    <transition name="success"             	value="end" />
  </activity>
 
  <transaction-manager id="tx_rbm" commit="true" />
</service>