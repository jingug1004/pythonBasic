<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsy4010" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile" 	value="resultkey.properties" />

    <property name="resultKeyList" 			value="dsInfraSw" />

    <transition name="success" 				value="EventRouter" />

  </activity>

  

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">

    <transition name="searchInfraSw"  		value="searchInfraSw" />

    <transition name="saveInfraSw"   		value="saveInfraSw" />    

    <transition name="success" 				value="end" />

  </activity>


  

  <!--  응용시스템 조회 -->
  
  <activity name="searchInfraSw" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success" 				value="end" />

    <property name="resultkey" 				value="dsInfraSw" />

    <property name="sqlkey"					value="rsy4010_s01" />

    <property name="param-count" 			value="4" />

    <property name="param0" 				value="BUILD_YEAR" />
    
    <property name="param1" 				value="SW_NM" />
    
    <property name="param2" 				value="DEV_LANG" />
    
    <property name="param3" 				value="DEV_COMP" />

    <property name="dao" 					value="rbmdao" />

  </activity>  
  

  <!-- 응용시스템 저장 -->

  <activity name="saveInfraSw" 				class="snis.rbm.system.rsy4010.activity.SaveInfraSw">
    <transition name="success" 				value="end" />
  </activity>



  <transaction-manager id="tx_rbm" commit="true" />

</service>



