<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem3040" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsStayStatByBrnc" />
    <transition name="success"              value="EventRouter" />

  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">

    <transition name="searchStayStatByBrnc"       value="searchStayStatByBrnc" />         <!-- 체류인원  지점별 현황 조회 -->

    <transition name="success"              value="end" />

  </activity>


  <!--  체류인원  지점별 현황 조회  -->
  
  <activity name="searchStayStatByBrnc"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsStayStatByBrnc" />
    <property name="sqlkey"                 value="rem3040_s01" />
    
    <property name="param-count"            value="8" />
        
    <property name="param0"                 value="RACE_STR_DT" />      <!--  경주일자(시작) -->
    <property name="param1"                 value="RACE_END_DT" />      <!--  경주일자(종료) -->
    <property name="param2"                 value="MEET_CD" />          <!--  경륜장구분  -->
    <property name="param3"                 value="BRNC_CD" />          <!--  지점  -->
    
    <property name="param4"                 value="OPT" />      <!--  경주일자(시작)  -->
    <property name="param5"                 value="RACE_STR_DT" />      <!--  경주일자(시작)  -->
    <property name="param6"                 value="RACE_END_DT" />      <!--  경주일자(종료)  -->
    
    <property name="param7"                 value="MEET_CD" />          <!--  경륜장구분-->
   
   <property name="dao"                    value="rbmdao" />       

  </activity>  

  <transaction-manager id="tx_rbm" commit="true" />

</service>



