<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs2010" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsHeal" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchHeal"         value="searchHeal" />	
	    <transition name="saveHeal"           value="saveHeal" />			<transition name="success" 			  value="end" />	</activity> 		
  <!-- 건전레저사업 조회 -->  <activity name="searchHeal" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		  value="end" />
    <property name="resultkey" 		  value="dsHeal" />
    <property name="sqlkey" 		  value="rbs2010_s_heal" />
    <property name="param-count" 	  value="1" />        
    <property name="param0" 		  value="BUSI_STAT_CD" /> 
    <property name="dao" 			  value="rbmdao" />
  </activity>	
	
	<!-- 건전레저사업등록 저장 -->
  	<activity name="saveHeal" 		class="snis.rbm.business.rbs2010.activity.SaveHealLeisuBusiReg">
    	<transition name="success" 			value="end" />
  	</activity>
  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

