<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm2110" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsDivGoalSalesRaw|dsTmsProcessRatio" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchDivGoalSalesRslt" value="searchDivGoalSalesRslt" />					<!-- 사업장별 매출목표 및 실적 검색 -->
    	<transition name="searchTmsProcessRatio"  value="searchTmsProcessRatio" />					<!-- 경주일 진행률 -->
		<transition name="success" 			  	  value="end" />	</activity> 	
 	<!-- 사업장별 매출목표 및 실적 검색 -->
	<activity name="searchDivGoalSalesRslt" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	  value="searchTmsProcessRatio" />
	  <property name="resultkey" 	  value="dsDivGoalSalesRaw" />
	  <property name="sqlkey" 		  value="rsm2110_s01" />
	  <property name="param-count" 	  value="10" />
	  <property name="param0" 		  value="DAY_END" />
	  <property name="param1" 		  value="DAY_START" />
	  <property name="param2" 		  value="DAY_END" />
	  
	  <property name="param3" 		  value="DAY_END" />
	  <property name="param4" 		  value="DAY_START" />
	  <property name="param5" 		  value="DAY_END" />
	  
	  <property name="param6" 		  value="DAY_END" />
	  
	  <property name="param7" 		  value="DAY_START" />
	  <property name="param8" 		  value="DAY_START" />
	  <property name="param9" 		  value="DAY_END" />
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	
	<!-- 경주일 진행률 -->
	<activity name="searchTmsProcessRatio" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	  value="end" />
	  <property name="resultkey" 	  value="dsTmsProcessRatio" />
	  <property name="sqlkey" 		  value="rsm2110_s02" />
	  <property name="param-count" 	  value="1" />
	  <property name="param0" 		  value="DAY_END" />	<!-- 경주일 yyyymmdd  -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
		
  <transaction-manager id="tx_rbm" commit="true" /></service>

