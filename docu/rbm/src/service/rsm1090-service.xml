<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm1090" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsGreenWndBal|dsGreenCdPwd" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 			class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchGreenWndBal" 	value="searchGreenWndBal" />		<!--  그린카드 창구별 수지명세  -->
    	<transition name="searchGreenCdPwd" 	value="searchGreenCdPwd" />		<!--  그린카드비밀번호 조회  -->
		<transition name="success" 		  		value="end" />	</activity> 	
 	
 	<!--  그린카드 창구별 수지명세  -->
	<activity name="searchGreenWndBal" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  <transition name="success" 			value="end" />
	  <property name="resultkey" 			value="dsGreenWndBal" />
	  <property name="sqlkey" 				value="rsm1090_s01" />
	  <property name="param-count" 			value="0" />
	  <property name="dao" 					value="rbmdao" />
	</activity>
	
 	<!--  그린카드 비밀번호 조회  -->
	<activity name="searchGreenCdPwd" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  <transition name="success" 			value="end" />
	  <property name="resultkey" 			value="dsGreenCdPwd" />
	  <property name="sqlkey" 				value="rsm1090_s02" />
	  <property name="param-count" 			value="1" />
      <property name="param0"               value="GREEN_CARD_NO" />   <!-- 지점코드 -->
	  <property name="dao" 					value="rbmdao" />
	</activity>
	
	<transaction-manager id="tx_rbm" commit="true" />
  	
  </service>

