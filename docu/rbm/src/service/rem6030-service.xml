<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem6030" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsDivDate|dsDivTms|dsDivMonth" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 			class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchDivDate"		value="searchDivDate" />		<!-- 기간별 매출액(일자별) -->
    	<transition name="searchDivTms"			value="searchDivTms" />			<!-- 기간별 매출액(회차별) -->
    	<transition name="searchDivMonth"		value="searchDivMonth" />		<!-- 지점별 매출액(월별) -->
		<transition name="success" 				value="end" />
	</activity>
 	
 	
 	<!-- 기간별 매출액(일자별) -->
	<activity name="searchDivDate"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success"		value="end" />
	  <property name="resultkey"		value="dsDivDate" />
	  <property name="sqlkey" 		  	value="rem6030_s01" />
	  <property name="param-count"      value="20" />    
	  <property name="param0"           value="FIXED_SEAT" />	<!--  영등포 천안 지정좌석실 포함 여부  -->
	  <property name="param1"           value="STR_DT" />  		<!--  시작일자  -->
	  <property name="param2"           value="END_DT" />  		<!--  종료일자  -->
	  <property name="param3"           value="STR_DT" />  		<!--  시작일자  -->
	  <property name="param4"           value="END_DT" />  		<!--  종료일자  -->
	  <property name="param5"           value="STR_DT" />  		<!--  시작일자  -->
	  <property name="param6"           value="END_DT" />  		<!--  종료일자  -->
	  <property name="param7"           value="BOAT_COUNTRY" />	<!--  지방경정 포함 여부  -->
	  <property name="param8"           value="MEET_CD" />  	<!--  경주구분  -->
	  <property name="param9"           value="MEET_CD" />  	<!--  경주구분  -->
	  <property name="param10"          value="STR_DT" />  		<!--  시작일자  -->
	  <property name="param11"          value="END_DT" />  		<!--  종료일자  -->
	  <property name="param12"          value="T_ONLY" />	    <!--  광명본장 입장권 여부  -->
	  <property name="param13"          value="MSRCASH" />	    <!--  미사리본장 입장권 여부  -->
	  
	  <property name="param14"           value="FIXED_SEAT" />	<!--  영등포 천안 지정좌석실 포함 여부  -->
	  
	  <property name="param15"          value="MEET_CD" />  	<!--  경주구분  -->
	  <property name="param16"          value="MEET_CD" />  	<!--  경주구분  -->
	  <property name="param17"          value="EXCEPT_KM" />	<!--  본장 제외 여부  -->
	  <property name="param18"          value="STR_DT" />  		<!--  시작일자  -->
	  <property name="param19"          value="END_DT" />  		<!--  종료일자  -->
	  <property name="dao" 			  	value="rbmdao" />
	</activity>
	
	
