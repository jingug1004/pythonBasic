<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs1220" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsYesCom|dsYesSangList|dsYesSang" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchYesComList"  	value="searchYesComList" />
	    <transition name="searchList"         	value="searchList" />	
	    <transition name="searchYesSang"        value="searchYesSang" />	
v	    <transition name="saveYesSang"  		value="saveYesSang" />
		<transition name="success" 			  	value="end" />	</activity> 		
  	<!-- 예상지 업체 조회 -->
  	<activity name="searchYesComList" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsYesCom" />
    	<property name="sqlkey" 		  value="rbs1220_s01" />
    	<property name="param-count" 	  value="0" />    
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
  	<!-- 날짜별 등록 내역 조회 -->
  	<activity name="searchList" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsYesSangList" />
    	<property name="sqlkey" 		  value="rbs1220_s02" />
    	<property name="param-count" 	  value="3" />
    	<property name="param0" 		  value="YES_COM_SEQ" />  
    	<property name="param1" 		  value="STND_YEAR" />    
    	<property name="param2" 		  value="MEET_CD" />    
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
  	<!-- 날짜별 등록내역 조회 -->
  	<activity name="searchYesSang" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsYesSang" />
    	<property name="sqlkey" 		  value="rbs1220_s03" />
    	<property name="param-count" 	  value="5" />
    	<property name="param0" 		  value="YES_COM_SEQ" />  
    	<property name="param1" 		  value="STND_YEAR" />    
    	<property name="param2" 		  value="MEET_CD" />    
    	<property name="param3" 		  value="TMS" />    
    	<property name="param4" 		  value="DAY_ORD" />    
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
	
	<!-- 날짜별 등록내역 저장 -->
  	<activity name="saveYesSang" 	  class="snis.rbm.business.rbs1220.activity.SaveYesSang">
    	<transition name="success" 	  value="searchList" />
  	</activity>
  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

