    <transition name="success" 			value="searchEvEmpMult" />
    <property name="resultkey" 			value="dsEvEmpSrv" />
    <property name="sqlkey"				value="rev4050_s03" />
    <property name="param-count" 	    value="9" />
    <property name="param0"             value="ESTM_YEAR" />   <!-- 평가년도 -->
    <property name="param1"             value="ESTM_NUM" />    <!-- 평가차수 -->
    <property name="param2"			    value="DEPT_CD" />     <!-- 부서코드 -->
    <property name="param3"             value="DEPT_CD" />     <!-- 부서코드 -->
    <property name="param4"             value="DEPT_CD" />     <!-- 부서코드 -->
    <property name="param5"			    value="WK_JOB" />      <!-- 보직코드 -->
    <property name="param6"             value="WK_JOB" />      <!-- 보직코드 -->
    <property name="param7"             value="WK_JOB" />      <!-- 보직코드 -->
    <property name="param8"			    value="EMP_NM" />      <!-- 사원명    -->
    <property name="dao" 				value="rbmdao" />
  </activity>
  
 
  <!-- 개인별 서비스평가 결과 조회 -->
   <activity name="searchEvEmpMult" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsEvEmpMult" />
