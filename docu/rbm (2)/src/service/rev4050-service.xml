<?xml version="1.0" encoding="EUC-KR"?>

<service name="rev4050" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />   
    <property name="resultKeyList" 			value="dsEvEmp|dsEvEmpPrm|dsEvEmpMnr|dsEvEmpSrv|dsEvEmpMult" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEvEmp"  		value="searchEvEmp" />		          <!-- 개인별세부평가결과  조회 -->
    <transition name="searchEvEmpPrm"  		value="searchEvEmpPrm" />		          <!-- 개인별세부평가결과  조회 -->
    <transition name="searchEvEmpMnr"  		value="searchEvEmpMnr" />		          <!-- 개인별세부평가결과  조회 -->
    <transition name="searchEvEmpSrv"  		value="searchEvEmpSrv" />		          <!-- 개인별세부평가결과  조회 -->
    <transition name="searchEvEmpMult"  	value="searchEvEmpMult" />		          <!-- 개인별세부평가결과  조회 -->
  </activity>
 
  <!-- 개인별  업무수행평가 결과 조회 -->
   <activity name="searchEvEmp" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsEvEmp" />
    <property name="sqlkey"				value="rev4050_s05" />
    <property name="param-count" 	    value="7" />
    <property name="param0"             value="ESTM_YEAR" />   <!-- 평가년도 -->
    <property name="param1"             value="ESTM_NUM" />    <!-- 평가차수 -->
    <property name="param2"             value="ESTM_YEAR" />   <!-- 평가년도 -->
    <property name="param3"             value="ESTM_NUM" />    <!-- 평가차수 -->
    <property name="param4"			    value="DEPT_CD" />     <!-- 부서코드 -->
    <property name="param5"			    value="WK_JOB" />      <!-- 보직코드 -->
    <property name="param6"			    value="EMP_NM" />      <!-- 사원명    -->
    <property name="dao" 				value="rbmdao" />
  </activity>
  
  <!-- 개인별  업무수행평가 결과 조회 -->
   <activity name="searchEvEmpPrm" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="searchEvEmpMnr" />
    <property name="resultkey" 			value="dsEvEmpPrm" />
    <property name="sqlkey"				value="rev4050_s01" />
