<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm3090" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsMRADIVTMS" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchMRADIVTMS"         value="searchMRADIVTMS" />	<!-- 경정교차 회차별 -->
		<transition name="success" 			  value="end" />
	</activity>
 	
 	
 	<!-- 경정교차 회차별 -->
	<activity name="searchMRADIVTMS" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 		  value="end" />
	  <property name="resultkey" 		  value="dsMRADIVTMS" />
	  <property name="sqlkey" 		  value="rsm3090_s01" />
	  <property name="param-count" 	  value="3" />
	  <property name="param0" 		  value="RACE_DAY_ST" />	<!-- 경주 검색 시작 일 -->
	  <property name="param1" 		  value="RACE_DAY_END" />	<!-- 경주 검색 마지막 일 -->
	  <property name="param2" 		  value="RACE_DAY_ST" />	<!-- 경주 검색 시작 일 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>

  <transaction-manager id="tx_rbm" commit="true" />

</service>



