<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm3180" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  	value="resultkey.properties" />	    <property name="resultKeyList" 			value="dsMyCat|dsMyCatSales|dsMyCatTelmp|dsMyCatVerify" />	    <transition name="success" 	  			value="EventRouter" />    </activity>  
    <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchMyCat"  		value="searchMyCat" />
    	<transition name="searchMyCatSales"  	value="searchMyCatSales" />
    	<transition name="searchMyCatTelmp"  	value="searchMyCatTelmp" />
    	<transition name="searchMyCatVerify"  	value="searchMyCatVerify" />
    	<transition name="success" 				value="end" />
  	</activity>
  	
  
	<!-- 마이켓 : 지점별 입출력 -->	
	<activity name="searchMyCat" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  	<transition name="success" 				value="end" />
	  	<property name="resultkey" 				value="dsMyCat" />
	  	<property name="sqlkey" 				value="rsm3180_s01" />
	  	<property name="param-count" 			value="6" />
	  	<property name="param0" 				value="RACE_DAY_START" />	<!-- 경주일:조회시작일 -->
	  	<property name="param1" 				value="RACE_DAY_END" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param2" 				value="RACE_DAY_START" />	<!-- 경주일:조회시작일 -->
	  	<property name="param3" 				value="RACE_DAY_END" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param4" 				value="RACE_TYPE" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param5" 				value="DIV_NO" />		<!-- 지점 -->
	  	<property name="dao" 					value="rbmdao" />
	</activity>
	
	<!-- 마이켓 : 투표소별 매출액 -->	
	<activity name="searchMyCatSales" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  	<transition name="success" 				value="end" />
	  	<property name="resultkey" 				value="dsMyCatSales" />
	  	<property name="sqlkey" 				value="rsm3180_s02" />
	  	<property name="param-count" 			value="6" />
	  	<property name="param0" 				value="RACE_TYPE" />		<!-- 시행처코드 -->
	  	<property name="param1" 				value="RACE_DAY_START" />	<!-- 경주일:조회시작일 -->
	  	<property name="param2" 				value="RACE_DAY_END" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param3" 				value="BRNC_CD" />		<!-- 지점 -->
	  	<property name="param4" 				value="MEET_CD" />		<!-- 시행처코드 -->
	  	<property name="param5" 				value="GRN_DIV_NO" />		<!-- 투표소 -->
	  	<property name="dao" 					value="rbmdao" />
	</activity>
	
  
	<!-- 마이켓 : 단말기별 매출액 -->	
	<activity name="searchMyCatTelmp" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  	<transition name="success" 				value="end" />
	  	<property name="resultkey" 				value="dsMyCatTelmp" />
	  	<property name="sqlkey" 				value="rsm3180_s03" />
	  	<property name="param-count" 			value="5" />
	  	<property name="param0" 				value="RACE_DAY_START" />	<!-- 경주일:조회시작일 -->
	  	<property name="param1" 				value="RACE_DAY_END" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param2" 				value="RACE_TYPE" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param3" 				value="MEET_CD" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param4" 				value="DIV_NO" />		<!-- 지점 -->
	  	<property name="dao" 					value="rbmdao" />
	</activity>
	
	<!-- 마이켓 : 매출액 검증 -->	
	<activity name="searchMyCatVerify" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  	<transition name="success" 				value="end" />
	  	<property name="resultkey" 				value="dsMyCatVerify" />
	  	<property name="sqlkey" 				value="rsm3180_s04" />
	  	<property name="param-count" 			value="8" />
	  	<property name="param0" 				value="RACE_DAY_START" />	<!-- 경주일:조회시작일 -->
	  	<property name="param1" 				value="RACE_DAY_END" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param2" 				value="RACE_DAY_START" />	<!-- 경주일:조회시작일 -->
	  	<property name="param3" 				value="RACE_DAY_END" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param4" 				value="RACE_DAY_START" />	<!-- 경주일:조회시작일 -->
	  	<property name="param5" 				value="RACE_DAY_END" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param6" 				value="RACE_TYPE" />	<!-- 경주일 ;조회종료일-->
	  	<property name="param7" 				value="MEET_CD" />	<!-- 경주일 ;조회종료일-->
	  	<property name="dao" 					value="rbmdao" />
	</activity>
	
  <transaction-manager id="tx_rbm" commit="true" /></service>

