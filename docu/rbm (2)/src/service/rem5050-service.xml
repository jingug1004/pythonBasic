<?xml version="1.0" encoding="EUC-KR"?>
<service name="rem5050" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsGoodHist|dsYearItemHist" />
    <transition name="success"              value="EventRouter" />
  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchGoodHist"  		value="searchGoodHist" />       <!--  기간별 상품입출내역 조회 -->
    <transition name="searchYearItemHist"  	value="searchYearItemHist" />   <!--  기간별 연간단가계약상품 상품입출내역 조회 -->
    <transition name="success"              value="end" />
  </activity>

  <!--  지점별 상품 입/출고내역 조회 -->  
  <activity name="searchGoodHist"       	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsGoodHist" />
    <property name="sqlkey"                 value="rem5050_s02" />    
    <property name="param-count"            value="3" />    
    <property name="param0"                 value="BRNC_CD" />     <!--  지점코드 -->
    <property name="param1"                 value="STR_DT" />      <!--  출고일 -->
    <property name="param2"                 value="END_DT" />      <!--  출고일 -->
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  
  <!--  지점별 연간단가계약상품 입/출고내역 조회 -->  
  <activity name="searchYearItemHist"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsYearItemHist" />
    <property name="sqlkey"                 value="rem5050_s03" />    
    <property name="param-count"            value="7" />    
    <property name="param0"                 value="BRNC_CD" />     <!--  지점코드 -->
    <property name="param1"                 value="STR_DT" />      <!--  출고일 -->
    <property name="param2"                 value="END_DT" />      <!--  출고일 -->
    <property name="param3"                 value="BRNC_CD" />     <!--  지점코드 -->
    <property name="param4"                 value="STR_DT" />      <!--  출고일 -->
    <property name="param5"                 value="END_DT" />      <!--  출고일 -->
    <property name="param6"                 value="BRNC_CD" />     <!--  지점코드 -->
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  
  <transaction-manager id="tx_rbm" commit="true" />
</service>
