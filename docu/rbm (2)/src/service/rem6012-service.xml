<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem6012" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsCash" />
    <transition name="success"              value="EventRouter" />
  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchCash"  			value="searchCash" />	<!-- 미사리 현금 입장인원 조회 -->
    <transition name="saveCash"      		value="saveCash" />     <!-- 미사리 현금 입장인원 저장  -->
    <transition name="success"              value="end" />
  </activity>


  <!-- 무료입장인원 조회 -->  
  <activity name="searchCash"       		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"         		value="end" />
    <property name="sqlkey"            		value="rem6012_s01" />
    <property name="resultkey"         		value="dsCash" />
    <property name="param-count"       		value="2" />    
    <property name="param0"            		value="STR_DT" />	<!-- 입장시작일 -->
    <property name="param1"            		value="END_DT" />	<!-- 입장종료일 -->
    <property name="dao"               		value="rbmdao" />
  </activity>  
  
  <!-- 무료 입장인원 저장  -->
  <activity name="saveCash"            		class="snis.rbm.business.rem6012.activity.SaveCash">
    <transition name="success"              value="end" />
  </activity>
    
  <transaction-manager id="tx_rbm" commit="true" />

</service>



