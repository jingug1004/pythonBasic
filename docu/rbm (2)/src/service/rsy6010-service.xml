<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsy6010" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsBtcLog" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchBtcLog"  		value="searchBtcLog" />      <!-- 배치로그 조회 -->
  </activity>
  
  <!-- 배치로그 조회 -->
  <activity name="searchBtcLog" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsBtcLog" />
    <property name="sqlkey"				value="rsy6010_s01" />
    <property name="param-count" 		value="5" />
    <property name="param0" 			value="LOG_DT_FROM" />  <!-- 로그일자 From -->
    <property name="param1" 			value="LOG_DT_TO" />    <!-- 로그일자To -->
    <property name="param2" 			value="BTC_CD" />       <!-- 배치코드  -->
    <property name="param3" 			value="LOG_CD" />       <!-- 로그구분  -->    
    <property name="param4" 			value="SRCH_STR" />     <!-- 검색문자열  -->    
    <property name="dao" 				value="rbmdao" />
  </activity>  
 
  <transaction-manager id="tx_rbm" commit="true" />

</service>