<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs1210" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsYesCom" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchList"         	value="searchList" />	
	    <transition name="saveYesCom"  			value="saveYesCom" />
v	    <transition name="chagePswd"  			value="chagePswd" />
		<transition name="success" 			  	value="end" />	</activity> 		
  	<!-- 예상지 업체 조회 -->  	<activity name="searchList" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsYesCom" />
    	<property name="sqlkey" 		  value="rbs1210_s01" />
    	<property name="param-count" 	  value="2" />
    	<property name="param0" 		  value="MEET_CD" />    
    	<property name="param1" 		  value="YES_COM_NM" />  
    	<property name="dao" 			  value="rbmdao" />
  	</activity>	
	
	<!-- 예상지 업체 저장 -->
  	<activity name="saveYesCom" 	  class="snis.rbm.business.rbs1210.activity.SaveYesCom">
    	<transition name="success" 	  value="searchList" />
  	</activity>
  	
	<!-- 예상지 업체 담당자 암호 저장 -->
  	<activity name="chagePswd" 	  class="snis.rbm.business.rbs1210.activity.ChagePswd">
    	<transition name="success" 	  value="searchList" />
  	</activity>
  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

