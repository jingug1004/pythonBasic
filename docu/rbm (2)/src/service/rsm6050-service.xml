<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm6050" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsTsnRcptResult|dsTsnRcptResult_S|dsUploadFile" />
    <transition name="success"              value="EventRouter" />
  </activity>

  <activity name="EventRouter"             	class="com.posdata.glue.biz.activity.PosDefaultRouter">
	<transition name="saveReceiptData"     value="saveReceiptData" />   	<!-- 분실신고서 데이타저장  -->
    <transition name="searchTsnReceipt"     value="searchTsnReceipt" />   	<!-- 분실신고서 접수 목록조회 -->    
    <transition name="saveReceipt"   		value="saveReceipt" />   		<!-- 분실신고서 접수 -->
    <transition name="saveReceiptCancel"   	value="saveReceiptCancel" />   	<!-- 분실신고서 접수취소 -->
    <transition name="saveCnclReq"   		value="saveCnclReq" />   		<!-- 지급정지 해지요청 -->
    <transition name="saveCnclReqCancel"   	value="saveCnclReqCancel" />   	<!-- 지급정지 해지요청취소 -->
    
    <transition name="searchFileMana"    value="searchFileMana" />    <!-- 첨부파일조회-->
         
  </activity>
  
  <!-- 분실신고서 접수 대상목록조회  -->
  <activity name="searchTsnReceipt"       	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"       		value="end" />   
    <property name="resultkey"        		value="dsTsnRcptResult" />
    <property name="sqlkey"           		value="rsm6050_s01" />    
    
    <property name="param-count"      		value="10" />
    <property name="param0"          		value="ticstas" />   
    <property name="param1"          		value="ticstas" />
    <property name="param2"          		value="ticstas" />
    <property name="param3"          		value="daytype" />         
    <property name="param4"          		value="fromdate" />         
    <property name="param5"          		value="todate" />    
    <property name="param6"          		value="tnsno" />
    <property name="param7"          		value="brnccd" />
    <property name="param8"          		value="brnccd" />
    <property name="param9"          		value="brnccd" />
    
    <property name="dao"              		value="rbmdao" />
  </activity>
  
  
    <!-- 첨부파일 조회   -->
  <activity name="searchFileMana"         class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />

    <property name="resultkey"              value="dsUploadFile" />

    <property name="sqlkey"                 value="common_file_s01" />

    <property name="param-count"            value="1" />

    <property name="param0"                 value="ATT_FILE_KEY" />

    <property name="dao"                    value="rbmdao" />
  </activity>
  
  <!-- 분실신고서 접수 -->
  <activity name="saveReceipt"    			class="snis.rbm.business.rsm6050.activity.SaveReceiptProcess">
    <transition name="success"             	value="end" />
  </activity>
  
  <!-- 분실신고서 접수취소 -->
  <activity name="saveReceiptCancel"    	class="snis.rbm.business.rsm6050.activity.SaveReceiptProcess">
    <transition name="success"             	value="end" />
  </activity>
  
  <!-- 지급정지 해지요청 -->
  <activity name="saveCnclReq"    			class="snis.rbm.business.rsm6050.activity.SaveReceiptProcess">
    <transition name="success"             	value="end" />
  </activity>
  
  <!-- 지급정지 해지요청취소 -->
  <activity name="saveCnclReqCancel"    	class="snis.rbm.business.rsm6050.activity.SaveReceiptProcess">
    <transition name="success"             	value="end" />
  </activity>
  
  <!-- 분실신고서 데이타저 -->
  <activity name="saveReceiptData"    	class="snis.rbm.business.rsm6050.activity.SaveLossReturnsReceipt">
    <transition name="success"             	value="end" />
  </activity>
  
  <transaction-manager id="tx_rbm" commit="true" />
</service>