<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem6020" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsDIVCnt" />
    <transition name="success"              value="EventRouter" />
  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchDIVCnt"  		value="searchDIVCnt" />         <!--  기간별 지점별 입장현황 조회 -->
    <transition name="success"              value="end" />
  </activity>


  <!--  기간별 지점별 입장현황 조회 -->  
  <activity name="searchDIVCnt"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"         value="end" />
    <property name="resultkey"         value="dsDIVCnt" />
    <property name="sqlkey"            value="rem6020_s01" />    
    <property name="param-count"       value="12" />    
    <property name="param0"            value="BOAT_COUNTRY" />	<!--  지방경정 포함 여부  -->
    <property name="param1"            value="MEET_CD" />  		<!--  경주구분  -->
    <property name="param2"            value="MEET_CD" />  		<!--  경주구분  -->
    <property name="param3"            value="STR_DT" />  		<!--  시작일자  -->
    <property name="param4"            value="END_DT" />  		<!--  종료일자  -->
    <property name="param5"            value="T_ONLY" />	    <!--  광명본장 입장권 여부  -->
    <property name="param6"            value="MSRCASH" />	    <!--  미사리본장 입장권 여부  -->
    <property name="param7"            value="MEET_CD" />  		<!--  경주구분  -->
    <property name="param8"            value="MEET_CD" />  		<!--  경주구분  -->
    <property name="param9"           value="EXCEPT_KM" />		<!--  본장 제외 여부  -->
    <property name="param10"           value="STR_DT" />  		<!--  시작일자  -->
    <property name="param11"           value="END_DT" />  		<!--  종료일자  -->
    <property name="dao"               value="rbmdao" />
  </activity>  
  
  <transaction-manager id="tx_rbm" commit="true" />

</service>



