<?xml version="1.0" encoding="EUC-KR"?>
<service name="rbs9230" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsList|dsListCnt|dsListAmt" />
    <transition name="success" 				value="EventRouter" />
  </activity>  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
	<transition name="searchList"  			value="searchList" />      <!-- 전자카드 금액대별 이용률 조회 -->    <transition name="searchListCnt"  		value="searchListCnt" />   <!-- 전자카드 금액대별 이용률 (금액) 조회 -->    <transition name="searchListAmt"  		value="searchListAmt" />   <!-- 전자카드 금액대별 이용률 (건수) 조회 -->    <transition name="searchListCntBonjang" value="searchListCntBonjang" />   <!-- 전자카드 금액대별 이용률 (금액) 조회 /본장 -->    <transition name="searchListAmtBonjang" value="searchListAmtBonjang" />   <!-- 전자카드 금액대별 이용률 (건수) 조회 /본장 -->       
    <transition name="success" 				value="end" />
  </activity>

  <!-- 전자카드 금액대별 이용률 조회 -->  <activity name="searchList" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success" 				value="end" />    <property name="resultkey" 				value="dsList" />    <property name="sqlkey"					value="rbs9230_s01"/>    <property name="param-count" 			value="8" />    <property name="param0" 				value="CAL_FROM" />    <property name="param1" 				value="CAL_TO" />    <property name="param2" 				value="GUBUN" />    <property name="param3" 				value="BRNC_CD" />    <property name="param4" 				value="CAL_FROM" />    <property name="param5" 				value="CAL_TO" />    <property name="param6" 				value="GUBUN" />    <property name="param7" 				value="BRNC_CD" />    <property name="dao" 					value="rbmdao" />  </activity>      <!-- 전자카드 금액대별 이용률 (금액) 조회 -->
