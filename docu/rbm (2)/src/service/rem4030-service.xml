<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem4030" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsStayTax|dsFreeBtnChk" />
    <transition name="success"              value="EventRouter" />
  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchStayTax"  		value="searchStayTax" />         <!--  기간별 지점별 입장현황 조회 -->
    <transition name="searchFreeBtnChk"  	value="searchFreeBtnChk" />      <!--  무료입장인원 버튼권한 조회 -->
    <transition name="success"              value="end" />
  </activity>


  <!--  기간별 지점별 입장현황 조회 -->  
  <activity name="searchStayTax"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsStayTax" />
    <property name="sqlkey"                 value="rem4030_s01" />    
    <property name="param-count"            value="25" />
    <property name="param0"                 value="T_ONLY" />	    <!--  광명본장 입장권 발매기 제외 여부  -->
    <property name="param1"                	value="BOAT_COUNTRY" />	<!--  지방경정 포함 여부  -->
    <property name="param2"                 value="MEET_CD" />  	<!--  경주구분  -->
    <property name="param3"                value="EXCEPT_KM" />	<!--  본장 제외 여부  -->
    <property name="param4"                value="STR_DT" />  		<!--  시작일자  -->
    <property name="param5"                value="END_DT" />  		<!--  종료일자  -->
    <property name="param6"                value="T_ONLY" />	    <!--  광명본장 입장권 발매기 제외 여부  -->
    <property name="param7"                value="BOAT_COUNTRY" />	<!--  지방경정 포함 여부  -->
    <property name="param8"                value="MEET_CD" />  	<!--  경주구분  -->
    <property name="param9"                value="EXCEPT_KM" />	<!--  본장 제외 여부  -->
    <property name="param10"                value="STR_DT" />  		<!--  시작일자  -->
    <property name="param11"                value="END_DT" />  		<!--  종료일자  -->
    <property name="param12"                value="BOAT_COUNTRY" />	<!--  지방경정 포함 여부  -->
    <property name="param13"                value="MEET_CD" />  	<!--  경주구분  -->
    <property name="param14"                value="MEET_CD" />  	<!--  경주구분  -->
    <property name="param15"                value="STR_DT" />  		<!--  시작일자  -->
    <property name="param16"                value="END_DT" />  		<!--  종료일자  -->
    <property name="param17"                value="T_ONLY" />	    <!--  광명본장 입장권 여부  -->
    <property name="param18"                value="MSRCASH" />	    <!--  미사리본장 입장권 여부  -->
    <property name="param19"                value="BOAT_COUNTRY" />	<!--  지방경정 포함 여부  -->
    <property name="param20"                value="MEET_CD" />  	<!--  경주구분  -->
    <property name="param21"                value="MEET_CD" />  	<!--  경주구분  -->
    <property name="param22"                value="EXCEPT_KM" />	<!--  본장 제외 여부  -->
    <property name="param23"                value="STR_DT" />  		<!--  시작일자  -->
    <property name="param24"                value="END_DT" />  		<!--  종료일자  -->
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  <!--  무료입장인원 버튼권한 조회 -->  
  <activity name="searchFreeBtnChk"       	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsFreeBtnChk" />
    <property name="sqlkey"                 value="rem4030_s02" />    
    <property name="param-count"            value="2" />
    <property name="param0"                 value="USER_ID" />	    <!--  사용자ID  -->
    <property name="param1"                	value="MN_ID" />		<!--  화면ID  -->
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  <transaction-manager id="tx_rbm" commit="true" />

</service>



