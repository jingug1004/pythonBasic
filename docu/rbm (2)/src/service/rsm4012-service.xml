<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm4012" initial="SetResultKeys">
    <activity name="SetResultKeys"   				class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  		value="resultkey.properties" />	    <property name="resultKeyList" 		  		value="dsGitaBonbu||dsGitaChangwon||dsGitaBusan" />	    <transition name="success" 			  		value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchGitaBonbu"			value="searchGitaBonbu" />	<!-- 기타소득세 정보 / 경륜 -->    	<transition name="searchGitaChangwon"		value="searchGitaChangwon" />	<!-- 기타소득세 정보 / 경륜 -->    	<transition name="searchGitaBusan"			value="searchGitaBusan" />	<!-- 기타소득세 정보 / 경륜 -->    	<transition name="searchGitaBonbuBoat"		value="searchGitaBonbuBoat" />	<!-- 기타소득세 정보 / 경정 -->    	<transition name="searchGitaChangwonBoat"	value="searchGitaChangwonBoat" />	<!-- 기타소득세 정보 / 경정 -->    	<transition name="searchGitaBusanBoat"		value="searchGitaBusanBoat" />	<!-- 기타소득세 정보 / 경정 -->		<transition name="success" 			  		value="end" />	</activity>
	
	<!-- 기타소득세 정보 본부 / 경륜-->
	<activity name="searchGitaBonbu" 	  			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 		  			value="searchGitaChangwon" />
	  <property name="resultkey" 		  			value="dsGitaBonbu" />
	  <property name="sqlkey" 		  				value="rsm4012_s01" />
	  <property name="param-count" 	  				value="2" />
	  <property name="param0" 		  				value="MEET_CD" />	  <property name="param1" 		  				value="RACE_DAY" />
	  <property name="dao" 			  				value="rbmdao" />
	</activity>		<!-- 기타소득세 정보 창원 / 경륜-->	<activity name="searchGitaChangwon" 	  		class="com.posdata.glue.miplatform.biz.activity.PosSearch">	  <transition name="success" 		  			value="searchGitaBusan" />	  <property name="resultkey" 		  			value="dsGitaChangwon" />	  <property name="sqlkey" 		  				value="rsm4012_s02" />	  <property name="param-count" 	  				value="2" />	  <property name="param0" 		  				value="MEET_CD" />	  <property name="param1" 		  				value="RACE_DAY" />	  <property name="dao" 			  				value="rbmdao" />	</activity>		<!-- 기타소득세 정보 부산 / 경륜-->	<activity name="searchGitaBusan" 	  			class="com.posdata.glue.miplatform.biz.activity.PosSearch">	  <transition name="success" 		  			value="end" />	  <property name="resultkey" 		  			value="dsGitaBusan" />	  <property name="sqlkey" 		  				value="rsm4012_s03" />	  <property name="param-count" 	  				value="2" />	  <property name="param0" 		  				value="MEET_CD" />	  <property name="param1" 		  				value="RACE_DAY" />	  <property name="dao" 			  				value="rbmdao" />	</activity>		<!-- 기타소득세 정보 본부 / 경정-->	<activity name="searchGitaBonbuBoat" 	  		class="com.posdata.glue.miplatform.biz.activity.PosSearch">	  <transition name="success" 		  			value="searchGitaChangwonBoat" />	  <property name="resultkey" 		  			value="dsGitaBonbu" />	  <property name="sqlkey" 		  				value="rsm4012_s04" />	  <property name="param-count" 	  				value="2" />	  <property name="param0" 		  				value="MEET_CD" />	  <property name="param1" 		  				value="RACE_DAY" />	  <property name="dao" 			  				value="rbmdao" />	</activity>		<!-- 기타소득세 정보 창원 / 경정-->	<activity name="searchGitaChangwonBoat" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">	  <transition name="success" 		  			value="searchGitaBusanBoat" />	  <property name="resultkey" 		  			value="dsGitaChangwon" />	  <property name="sqlkey" 		  				value="rsm4012_s05" />	  <property name="param-count" 	  				value="2" />	  <property name="param0" 		  				value="MEET_CD1" />	  <property name="param1" 		  				value="RACE_DAY" />	  <property name="dao" 			  				value="rbmdao" />	</activity>		<!-- 기타소득세 정보 부산 / 경정-->	<activity name="searchGitaBusanBoat" 	  		class="com.posdata.glue.miplatform.biz.activity.PosSearch">	  <transition name="success" 		  			value="end" />	  <property name="resultkey" 		  			value="dsGitaBusan" />	  <property name="sqlkey" 		  				value="rsm4012_s06" />	  <property name="param-count" 	  				value="2" />	  <property name="param0" 		  				value="MEET_CD1" />	  <property name="param1" 		  				value="RACE_DAY" />	  <property name="dao" 			  				value="rbmdao" />	</activity>
  
  <transaction-manager id="tx_rbm" commit="true" /></service>

