  </activity>
  
  <!-- 작년도현황 저장  -->            
  <activity name="saveLastYear"         class="snis.rbm.business.rbr1010.activity.SaveLastYear">
    <transition name="success"          value="end" />
  </activity>
 
  <transaction-manager id="tx_rbm" commit="true" />

