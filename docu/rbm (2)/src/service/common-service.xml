<?xml version="1.0" encoding="EUC-KR"?>
<service name="common" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />    <property name="resultKeyList" value="dsOutSysdate|dsYear|dsUserListValue|dsList|dsBzInfo|dsOutDept|ds_post|dsEmp|dsTms|dsMenuGrpValue|dsAlarm|dsRecvUser|dsPersonalMn|dsPersonalIdChk" />    <transition name="success" value="EventRouter" />  </activity>

  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">    <transition name="searchCode"    				value="searchCode" />    <transition name="searchSysdate" 				value="searchSysdate" />    <transition name="searchYear"    				value="searchYear" />    <transition name="searchUserAll"  				value="searchUserAll" />
    <transition name="searchOrgan"  				value="searchOrgan" />    <transition name="success"       				value="end" />    <transition name="searchDivCode"    			value="searchDivCode" />
    <transition name="saveApproval" 				value="saveApproval" /> 
    <transition name="searchAlarm" 					value="searchAlarm" />
    <transition name="saveAlarm" 					value="saveAlarm" />
    <transition name="searchBzInfo" 				value="searchBzInfo" />    
    <transition name="saveCopy" 					value="saveCopy" />
    <transition name="SearchPost" 					value="SearchPost" />
    <transition name="SearchEmp"    				value="SearchEmp" />
    <transition name="SearchEmpAll"    				value="SearchEmpAll" />
    <transition name="SearchEmpHolOff"  			value="SearchEmpHolOff" />
    <transition name="SearchTms"    				value="SearchTms" />
    <transition name="SearchMenuGrp"    			value="SearchMenuGrp" />
    <transition name="searchAlarmCd"    			value="searchAlarmCd" />
    <transition name="searchAlarmUser"  			value="searchAlarmUser" />
    <transition name="SearchEmpTemp"   				value="SearchEmpTemp" />
    <transition name="SavePrivInfo"    				value="SavePrivInfo" />    <transition name="SearchPersonalMn" 			value="SearchPersonalMn" />		<!-- 개인정보처리메뉴 여부 조회 -->    <transition name="SearchPersonalIdChk"			value="SearchPersonalIdChk" />	<!-- 개인정보처리메뉴 권한 조회 -->
  </activity>
   
  <!-- 코드 조회 -->
  <activity name="searchCode" class="snis.rbm.common.util.SearchCode">
    <transition name="success" value="end" />
  </activity>  
  <!-- System일시 조회 -->
  <activity name="searchSysdate" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutSysdate" />
    <property name="sqlkey" value="common_sysdate" />
    <property name="param-count" value="1" />
    <property name="dao" value="rbmdao" />
    <property name="param0" value="CUSTOM" />
  </activity>
  
    <!-- 조회연도 조회 -->
  <activity name="searchYear" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsYear" />
    <property name="sqlkey" value="common_year" />
    <property name="dao" value="rbmdao" />
  </activity>
  
  <!-- 사용자 조회 -->
  <activity name="searchUserAll" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 					value="end" />
    <property name="resultkey" 					value="dsUserListValue" />
    <property name="sqlkey" 					value="common_user" />    
    <property name="param-count" 				value="2"    />
    <property name="param0"  					value="USER_NM"  />      
    <property name="param1"  					value="TEAM_NM"  />          
    <property name="dao"     					value="rbmdao"   />
  </activity>
  
  
  <!-- 조직도 조회 -->
  <activity name="searchOrgan" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 					value="end" />
    <property name="resultkey" 					value="dsOutDept" />
    <property name="sqlkey" 					value="common_organ" />    
    <property name="param-count" 				value="1"    />
    <property name="param0"  					value="DEPT_ID"  />      
   <property name="dao"     					value="rbmdao"   />
  </activity>


  <activity name="searchDivCode" class="snis.rbm.common.activity.SearchDivCode">
    <transition name="success" value="end" />
  </activity>  


  <activity name="errorActivity" 			class="snis.rbm.common.activity.ErrorActivity">
    <transition name="success" 				value="end" />
  </activity>  
  
  
  <!-- 결재일련번호 조회 -->
  <activity name="saveApproval" class="snis.rbm.common.util.saveApproval">
    <transition name="success" value="end" />
  </activity>
  
  <!-- 확정관리 대상 리스트 조회 -->
  <activity name="searchAlarm" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 					value="end" />
    <property name="resultkey" 					value="dsAlarm" />
    <property name="sqlkey" 					value="common_Alarm_List_Search" />    
    <property name="param-count" 				value="2"    />
    <property name="param0"  					value="ALARM_CD"  /> 
    <property name="param1"  					value="ALARM_CD"  />        
    <property name="dao"     					value="rbmdao"   />
  </activity>  
  
  <!-- 확정관리 저장 -->
  <activity name="saveAlarm" class="snis.rbm.common.util.saveAlarm">
    <transition name="success" value="end" />
  </activity>
    
  <!-- 사업자정보 조회 -->
  <activity name="searchBzInfo" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 					value="end" />
    <property name="resultkey" 					value="dsBzInfo" />
    <property name="sqlkey" 					value="common_search_bz_info" />    
    <property name="param-count" 				value="0"    />
    <property name="dao"     					value="rbmdao"   />
  </activity> 

  <!-- 자료복사  -->
  <activity name="saveCopy" class="snis.rbm.common.activity.SaveCopy">
    <transition name="success" value="end" />
  </activity>
  
  <!-- 우편번호 조회 -->
  <activity name="SearchPost" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="ds_post" />
    <property name="sqlkey" value="common_search_post" />
    <property name="param-count" value="1" />
    <property name="dao" value="rbmdao" />
    <property name="param0" value="DONG" />
  </activity>
  
  <!-- 사원 조회 -->
  <activity name="SearchEmp" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"   value="end" />
    <property name="resultkey"   value="dsEmp" />
    <property name="sqlkey"      value="common_search_emp" />
    <property name="param-count" value="2" />
    <property name="param0"      value="EMP_NO" />
    <property name="param1"      value="EMP_NM" />
    <property name="dao"         value="rbmdao" />
  </activity>
  
  <!-- 사원 세부내역 조회 -->
  <activity name="SearchEmpAll" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"   value="end" />
    <property name="resultkey"   value="dsEmp" />
    <property name="sqlkey"      value="common_search_emp_all" />
    <property name="param-count" value="3" />
    <property name="param0"      value="DEPT_NM" />
    <property name="param1"      value="DEPT_NM" />
    <property name="param2"      value="EMP_NM" />
    <property name="dao"         value="rbmdao" />
  </activity>
  
  <!-- 퇴사자 세부내역 조회 -->
  <activity name="SearchEmpHolOff" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"   value="end" />
    <property name="resultkey"   value="dsEmp" />
    <property name="sqlkey"      value="common_search_emp_HolOff" />
    <property name="param-count" value="2" />
    <property name="param0"      value="DEPT_NM" />
    <property name="param1"      value="EMP_NM" />
    <property name="dao"         value="venisrbmdao" />
  </activity>
  
  <!-- 회차  조회 -->
  <activity name="SearchTms" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"   value="end" />
    <property name="resultkey"   value="dsTms" />
    <property name="sqlkey"      value="common_search_tms" />
    <property name="param-count" value="4" />
    <property name="param0"      value="STND_YEAR" />
    <property name="param1"      value="MEET_CD" />
    <property name="param2"      value="STND_YEAR" />
    <property name="param3"      value="RACE_DAY" />
    <property name="dao"         value="rbmdao" />
  </activity>
 

  <!-- 사원(지원직 및 일용직) 조회 -->
  <activity name="SearchEmpTemp" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"   value="end" />
    <property name="resultkey"   value="dsEmp" />
    <property name="sqlkey"      value="common_search_emp_temp" />
    <property name="param-count" value="4" />
    <property name="param0"      value="COMM_NO" />
    <property name="param1"      value="WK_JOB_CD" />
    <property name="param2"      value="EMP_NO" />
    <property name="param3"      value="EMP_NM" /> 
    <property name="dao"         value="rbmdao" />
  </activity>
  
  <!-- 메뉴그룹  조회 -->

  <activity name="SearchMenuGrp"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsMenuGrpValue" />
    <property name="sqlkey"                 value="common_menu_grp" />
    <property name="param-count"            value="6" />
    <property name="param0"                 value="MN_GRP" />
    <property name="param1"                 value="MN_GRP" />
    <property name="param2"                 value="USER_ID" />
    <property name="param3"                 value="USER_ID" />    <property name="param4"                 value="USER_ID" />    <property name="param5"                 value="USER_IP" />
    <property name="dao"                    value="rbmdao" />    

  </activity>


  <!-- 확정 코드   조회 -->
  <activity name="searchAlarmCd"           class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsAlarm" />
    <property name="sqlkey"                 value="common_alarm" />
    <property name="param-count"            value="1" />
    <property name="param0"                 value="ALARM_CD" /> 

    <property name="dao"                    value="rbmdao" />    

  </activity>
  
 <!-- 메세지 전송 사용자목록    조회 -->
  <activity name="searchAlarmUser"           class="snis.rbm.common.activity.SearchAlarmUser">
    <transition name="success"              value="end" />
  </activity>

 <!-- 사용자 접속정보 저장 -->
  <activity name="updateUserTrace"           class="snis.rbm.common.activity.SearchAlarmUser">
    <transition name="success"              value="end" />
  </activity>

 <!-- 개인정보 파일 삭제 정보 저장 -->
  <activity name="SavePrivInfo"           class="snis.rbm.common.activity.SavePrivfileInfo">
    <transition name="success"            value="end" />
  </activity>
    <!-- 개인정보처리메뉴 조회 -->  <activity name="SearchPersonalMn"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success"              value="end" />    <property name="resultkey"              value="dsPersonalMn" />    <property name="sqlkey"                 value="personal_mn" />    <property name="param-count"            value="2" />    <property name="param0"                 value="FORM_ID" />    <property name="param1"                 value="FORM_ID" />    <property name="dao"                    value="rbmdao" />   </activity>    <!-- 개인정보처리메뉴 접근권한 조회 -->  <activity name="SearchPersonalIdChk"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success"              value="end" />    <property name="resultkey"              value="dsPersonalIdChk" />    <property name="sqlkey"                 value="personal_id_chk" />    <property name="param-count"            value="2" />    <property name="param0"                 value="FORM_ID" />    <property name="param1"                 value="USER_ID" />    <property name="dao"                    value="rbmdao" />   </activity>    
  <transaction-manager id="tx_rbm" commit="true" />
</service>

