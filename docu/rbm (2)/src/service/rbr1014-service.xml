<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbr1014" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	<property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsServMana" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchServMana"  		value="searchServMana" /> <!-- 侩开包府 炼雀  -->																
  	<transition name="saveServMana"   		value="saveServMana" />   <!-- 侩开包府 历厘  -->
  </activity>
 
   <!-- 侩开包府  炼雀  -->
  <activity name="searchServMana" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsServMana" />

    <property name="sqlkey"				value="rbr1014_s01" />

    <property name="param-count" 		value="2" />
    
    <property name="param0" 			value="BRNC_CD" />   <!-- 瘤痢内靛  -->
    <property name="param1" 			value="STND_YEAR" /> <!-- 斥档        -->
    
    <property name="dao" 				value="rbmdao" />
  </activity>
  
  <!-- 侩开包府 历厘  -->			   
  <activity name="saveServMana" 		class="snis.rbm.business.rbr1014.activity.SaveServMana">
    <transition name="success" 			value="end" />
  </activity>
  	
  <transaction-manager id="tx_rbm" commit="true" />

</service>