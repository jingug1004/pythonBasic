function fcInit(obj) {

	if (tab0.TabIndex == 0) dsStayStatByDay.ClearData();
	else if (tab0.TabIndex == 1) dsStayStatByRaceDay.ClearData();
	else if (tab0.tabindex == 2) dsStayStatByTms.ClearData();
	
}

