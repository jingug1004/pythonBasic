+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {

	fcInit(obj);

	return true;
}



/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {

	var sServiceName = "";
	var sInDataSet   = "";
	var sOutDataSet  = "";
	if ( !fcSearchCheck(obj) ) return;

	if (tab0.TabIndex == 0) {
		sServiceName = "rem4080-service:searchStayStatByDay";
		sOutDataSet  = "dsStayStatByDay=dsStayStatByDay ";
		
		if (tab0.tab1.cboRowType.Value == "02") {	// 회차별
			sServiceName = "rem4080-service:searchStayStatByDayTms";
			tab0.tab1.grdStayStatByDay.SetCellProp("head",0,"text","회차");
		} else {
			tab0.tab1.grdStayStatByDay.SetCellProp("head",0,"text","지점");
		}
		
		//조회조건 : 
		fcd_AddParam("MEET_CD", tab0.tab1.cboMeetCd.Value);	 
		fcd_AddParam("STR_DT",  tab0.tab1.calRaceStrDt.Value);
		fcd_AddParam("END_DT",  tab0.tab1.calRaceEndDt.Value);
		fcd_AddParam("T_ONLY",  tab0.tab1.chkTOnly.Value);
		fcd_AddParam("FS",  	tab0.tab1.chkFS.Value);
		fcd_AddParam("MSR",  	tab0.tab1.chkMsrCash.Value);
		
	} else if (tab0.TabIndex == 1) {
		sServiceName = "rem4080-service:searchStayStatByRaceDay";
		sOutDataSet  = "dsStayStatByRaceDay=dsStayStatByRaceDay ";
		
		//조회조건 : 
		fcd_AddParam("MEET_CD", tab0.tab2.cboMeetCd.Value);	 
		fcd_AddParam("STR_DT",  tab0.tab2.calRaceStrDt.Value);
		fcd_AddParam("END_DT",  tab0.tab2.calRaceEndDt.Value);
		fcd_AddParam("BRNC_CD", tab0.tab2.cboSearchBrncCd.value);
		fcd_AddParam("T_ONLY",  tab0.tab2.chkTOnly.Value);
		fcd_AddParam("MSR",  	tab0.tab2.chkMsrCash.Value);
	} else if (tab0.TabIndex == 2) {
		sServiceName = "rem4080-service:searchStayStatByTms";
		sOutDataSet  = "dsStayStatByTms=dsStayStatByTms ";
		
		//조회조건 : 
		fcd_AddParam("MEET_CD", tab0.tab3.cboMeetCd.Value);	 
		fcd_AddParam("STR_DT",  tab0.tab3.calRaceStrDt.Value);
		fcd_AddParam("END_DT",  tab0.tab3.calRaceEndDt.Value);
		fcd_AddParam("BRNC_CD", tab0.tab3.cboSearchBrncCd.value);
		fcd_AddParam("T_ONLY",  tab0.tab3.chkTOnly.Value);
		fcd_AddParam("MSR",  	tab0.tab3.chkMsrCash.Value);
	} 
	
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}



