function setActYear(){

	var sCurYear  = ParseInt(fcd_GetSysdate("YYYY"));

	
	for(var i= 2010; i <= sCurYear ; i++){
		nRow = dsActYear.AddRow();
		
		dsActYear.SetColumn(nRow,"CD",i);
		dsActYear.SetColumn(nRow,"CD_NM",i);		
	}
	cboActYear.Value = sCurYear;

	fcSearch();
}


