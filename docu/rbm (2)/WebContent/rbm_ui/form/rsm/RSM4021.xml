// ∆Ø¡§ƒﬁ∆˜≥Õ∆Æ(±◊∏ÆµÂ) ∏ÆªÁ¿Ã¡Ó
function fcSetResize(obj,nX)
{	// «ÿ¥Á»≠∏È¿∫ ±‚¡ÿªÁ¿Ã¡Ó∫∏¥Ÿ Height∞° ≈©±‚∂ßπÆø° Ω∫≈©∑—πŸ ª˝º∫πﬂª˝.±◊ø° µ˚∂Û ±◊∏ÆµÂªÁ¿Ã¡Ó ¡∂¡§
	var arrObj = array(-1);
	arrObj[0] = "grdCommi";
	arrObj[1] = "grdRace";
	arrObj[2] = "grdETC";
	
	var nWidth = nX-28;
	if( nX <= 820){
		for( var i=0; i<length(arrObj); i++){
			object(arrObj[i]).Autofit = false;		
			object(arrObj[i]).width = 780;
			object(arrObj[i]).Redraw;
		}
		Shape1.Width 	= 780;	
		divBtnLst.Left 	= this.width - 405;			
		Static0.Left 	= this.width - 111;		
		return;
	}
	
	for( var i=0; i<length(arrObj); i++){
		object(arrObj[i]).right = nWidth;
		
		if( i == 0 || i == 2) object(arrObj[i]).Autofit = true;	
		else{
			if( nX > 2400 && i==1)	{
				object(arrObj[i]).Autofit = true;		
			}else{
				object(arrObj[i]).Autofit = false;	
			}
		}
	}	
		
	// ∞¯≈Î
	Shape1.Width 	= nWidth-8;
	divBtnLst.Left 	= this.width - 405;	
	Static0.Left 	= this.width - 111;	
	
}
// «ÿ¥Á ∆˚ªÁ¿Ã¡Ó ∫Ø∞ÊΩ√
function fcOnSize(obj,nCx,nCy,nState)
{
	fcSetResize(obj,nCx);
