/*-------------------------------------------------------------------------------------------------+
>>  조회후 각지방자치단체의 "경주예수계"를 계산
+-------------------------------------------------------------------------------------------------*/
function fcCalYesuSum() 
{
	if (dsTaxBonjang.GetRowCount() == 0) return;
	if (dsTaxJijum.GetRowCount() == 0) return;
	var nLsTax = 0;
	var nLetax = 0;
	var nSptax = 0;
	
	nLsTax = dsTaxBonjang.GetColumn(dsTaxBonjang.RowCount() -1, "LSTAX") + dsTaxJijum.GetColumn(dsTaxJijum.RowCount() -2, "LSTAX");
	nLeTax = dsTaxBonjang.GetColumn(dsTaxBonjang.RowCount() -1, "LETAX") + dsTaxJijum.GetColumn(dsTaxJijum.RowCount() -2, "LETAX");
	nSpTax = dsTaxBonjang.GetColumn(dsTaxBonjang.RowCount() -1, "SPTAX") + dsTaxJijum.GetColumn(dsTaxJijum.RowCount() -2, "SPTAX");
	
	dsTaxJijum.SetColumn(dsTaxJijum.RowCount() -1, "LSTAX", nLsTax);
	dsTaxJijum.SetColumn(dsTaxJijum.RowCount() -1, "LETAX", nLeTax);
	dsTaxJijum.SetColumn(dsTaxJijum.RowCount() -1, "SPTAX", nSpTax);
	dsTaxJijum.SetColumn(dsTaxJijum.RowCount() -1, "TOTAL", nLsTax + nLeTax + nSpTax);
}]]></Script>
