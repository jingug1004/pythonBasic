function fcSearch(obj) {	
	var sServiceName = "rsm2031-service:searchRaceResultDetail";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsRaceResultDetail=dsRaceResultDetail ";     
	
	var MEET_CD = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"MEET_CD");
	var STND_YEAR = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"STND_YEAR");
	var TMS = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"TMS");
	var DAY_ORD = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"DAY_ORD");
	var RACE_NO = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"RACE_NO");
	
	
	fcd_AddParam("MEET_CD", MEET_CD);    
	fcd_AddParam("STND_YEAR", STND_YEAR);    
	fcd_AddParam("TMS", TMS);    
	fcd_AddParam("DAY_ORD", DAY_ORD);    
	fcd_AddParam("RACE_NO", RACE_NO);    
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}



