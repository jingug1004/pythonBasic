>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
    if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
    
    if ( sServiceName == "rsm2031-service:searchRaceResultDetail" )
    {
		var RACE_DAY = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"RACE_DAY");
		
		var sMM = substr(RACE_DAY,4,2); // 경주 월
		var sDD = substr(RACE_DAY,6,2); // 경주 일
		
		var MEET_CD = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"MEET_CD");
		var sMeetNm = decode(MEET_CD,'003','경정','경륜'); 
		
		var STND_YEAR = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"STND_YEAR");
		var TMS = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"TMS");
		var DAY_ORD = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"DAY_ORD");
		var RACE_NO = fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"RACE_NO");		
		
		var sRaceDayInfo = STND_YEAR+"년 "+sMM+"월 "+sDD+"일 "+TMS+"회차 "+DAY_ORD+"일차 "+RACE_NO+"경주";
		
		stRaceInfo.Text = "경주별현황 "+sMeetNm+" "+sRaceDayInfo;
		
	}
	
	
}




