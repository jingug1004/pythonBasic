function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

    if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
    
    
	if ( sServiceName == "rsm1110-service:searchEventStat" )
	{
		dsList.row = -1;
		if (dsListDetl.rowcount() > 0) {
			var sMinRaceNo = dsListDetl.GetColumn(0, "MIN_RACE_NO");
			for (var i=7;i<=13;i++) {
				grdListDetl.SetCellProp("head", i, "text", toString(ToNumber(sMinRaceNo) + i - 7) + "경주");
			}
		}
		return;
	}
	
}


