function fccalOnKeyDown(obj,nChar,bShift,bCtrl,bAlt,nLLParam,nHLParam)
{
	switch ( obj.ID ) {		
		// 종료일자에서 ENTER_KEY 눌렀을 경우
		case "calStrDt" :	
			if ( nChar == ENTER_KEY ) {
				calEndDt.Value = calStrDt.Value;
				calEndDt.SetFocus();
				return;
			}
		break;
		case "calEndDt" :	
			if ( nChar == ENTER_KEY ) {
				fcSearch();	
				return;
			}
		break;
	}
}

function fcgrdOnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if (obj.id == "grdList") {
		var sFilter = "";
		var sCommNm = dsList.Getcolumn(nRow, nCell -1);
		if (sCommNm != "" && nCell > 0) {
			sFilter = "COMM_NM='"+sCommNm+"'";
			dsListDetl.Filter(sFilter);
		} else {
			dsListDetl.UnFilter();
		}
	}
}


