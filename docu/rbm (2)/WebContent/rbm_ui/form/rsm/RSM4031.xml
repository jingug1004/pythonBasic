// 특정콤포넌트(그리드) 리사이즈
function fcSetResize(obj,nX,nY)
{	
	var nHeight = ny-151;   //기존 151
	if (nY <= 584) {
		TabDiv.tabDt.grdDiv.height = 433;
		
	} else {
		TabDiv.tabDt.grdDiv.height = nHeight;		
	}
	
	TabDiv.tabSum.grdDiv.Height = TabDiv.tabDt.grdDiv.height;
	TabDiv.Height = TabDiv.tabDt.grdDiv.height + 40;	
	
	return;
	
	//가로 폭 조정
	var arrObj = array(-1);
	arrObj[0] = "TabDiv.tabDt.grdDiv";
		
	var nWidth = nX-8;
	if( nX <= 820){
		for( var i=0; i<length(arrObj); i++){
			object(arrObj[i]).Autofit = false;		
			object(arrObj[i]).width = 801;
			object(arrObj[i]).Redraw;
		}
		Shape1.Width 	= 801;	
		divBtnLst.Left 	= this.width - 393;
	} else {
		for( var i=0; i<length(arrObj); i++){
			object(arrObj[i]).right = nWidth;
			object(arrObj[i]).Autofit = true;		
		}	
	}
		
	// 공통
	Shape1.Width 	= nWidth-8;
	divBtnLst.Left 	= this.width - 393;		
}

