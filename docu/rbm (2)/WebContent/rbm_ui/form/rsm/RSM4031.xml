/*-------------------------------------------------------------------------------------------------+
>>  내용 검색
+-------------------------------------------------------------------------------------------------*/
function fcSearchInfo(obj) {
    if ( !fcSearchCheck(obj) ) return;
    
	var sServiceName = "rsm4031-service:searchRaceInfo"; // 경륜 조회
		
    var sInDataSet   = "";
    var sOutDataSet  = "dsRaceInfo=dsRaceInfo ";
		sOutDataSet  += "dsDiv=dsDiv ";
		sOutDataSet  += "dsDivSum=dsDivSum ";
		
		
    // TMS ,DAY_ORD	전페이지 파라미터
    var STND_YEAR	= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"STND_YEAR");
    var MEET_CD	= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"MEET_CD");
    var RACE_DAY	= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"RACE_DAY");
    
	fcd_AddParam("MEET_CD", MEET_CD);				// 경주일
	fcd_AddParam("RACE_DAY", RACE_DAY);				// 경주일
	
	if(rdoMeetCd.Value =="001"){
		fcd_AddParam("SRCH_MEET_CD", MEET_CD);				// 경주일
    }else{
		fcd_AddParam("SRCH_MEET_CD", rdoMeetCd.Value);				// 경주일
    }
	
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
	
}



