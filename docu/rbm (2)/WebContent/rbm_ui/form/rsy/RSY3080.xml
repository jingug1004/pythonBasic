    dsAuthList.ClearData();
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 체크 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck() {
    //조회조건 체크및 trim처리
	fcInit();    //기존 데이터셋 초기화
	
	return true;
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch() {
	
    if ( !fcSearchCheck() ) return;
       
	var sServiceName = "rsy3080-service:searchAuthList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsAuthList=dsAuthList";
	
	var sMnGrpCd = "";
	if (length(cboMnGrpCd.Value) != 7) {
        sMnGrpCd = "R000000";
    }else{    
        sMnGrpCd=cboMnGrpCd.Value;
    }
    
	fcd_AddParam("MN_GRP", 	sMnGrpCd);
	fcd_AddParam("TEAM_CD", cboTeamNm.Value);
	fcd_AddParam("USER_ID", edSearchTxt.Text);
	fcd_AddParam("AUTH_CD", cboAuth.Value);
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

