function fcAddRow(obj) {

	if ( pv_inDt != substr(getDate(), 0, 8) ) {
		// 금일데이터만 수정 가능합니다.
		fnc_Message(SNIS_RBM_B009, "N", "수정");
		return false;
	
	} else {

		var nRow = dsConEquip.AddRow(); // Record Add
		
		dsConEquip.SetColumn(nRow, "BRNC_CD"		, pv_brncCd			);	// 발매기
		//dsList.SetColumn(nRow, "PARTS_MCD"		, pv_partsMcd			);	// 부품분류
		dsConEquip.SetColumn(nRow, "BUY_RMK"		, pv_buyRmk			);	// 부품코드
		dsConEquip.SetColumn(nRow, "CON_MONEY"		, pv_conMoney			);	// 부품명
		//dsList.SetColumn(nRow, "MADE_GBN"		, pv_madeGbn			);	// 원산지구분
		//dsList.SetColumn(nRow, "IN_DT"			, pv_inDt				);	// 입고일자
		//dsList.SetColumn(nRow, "INPUT_DT"		, substr(getDate(), 0, 8));	// 입력일자	
		//dsList.SetColumn(nRow, "DEL_YN"			, "N"					);	// 삭제 " Y "	아니면, " N " (이력관리)
		//dsList.SetColumn(nRow, "TODT"			, substr(getDate(), 0, 8));	// TODAY	
	
	
		grdConEquip.SetFocus(nRow);		// 포커스준 Row의 첫번째 Cell에 포지션을 둔다.
		grdConEquip.SetCellPos(0);
	}
}


