>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
	
	if ( !fnc_ResultNP(ErrorCode, ErrorMSG) ) return;
	
	// 조회 완료 후
	if ( sServiceName == "rbr1013-service:searchEmpStat" ) {
		if( dsEmpStat.rowcount > 0 ) {
			mskDate.Text = dsEmpStat.GetColumn(dsEmpStat.row, "INST_DT");
		} else {
			mskDate.Text = "";
		}
	}			
	
	// 조회 완료 후
	if ( sServiceName == "rbr1013_b-service:searchEmpStatCurr" ) {
		mskDate.Text = sCurDate;
	}	
	
	// 저장 완료 후
	if ( sServiceName == "rbr1013-service:saveEmpStat" ) {
		fcSearch();		
	}	
}

