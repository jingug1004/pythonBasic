function fcSearch(obj) {
	
	// 올해로 조회되었을 경우만 '현황저장' 버튼을 보여준다.
	if( parent.cboStndYearSearch ==sCurYear) {
		btnCurrYeartSave.Visible = true;
	} else {
		btnCurrYeartSave.Visible = false;
	}
	
	if ( !fcSearchCheck(obj) ) return;
	
	var sServiceName, sInDataSet, sOutDataSet;
	
	sServiceName = "rbr1013-service:searchEmpStat";
	sInDataSet   = "";
	sOutDataSet  = "dsEmpStat=dsEmpStat ";
	
	fcd_AddParam("BRNC_CD"  , parent.cboBrncCdSearch.Value);
	fcd_AddParam("STND_YEAR", parent.cboStndYearSearch.Value);
	

		
		
	/*
	 <=
	 <= RBM DB 에서 무조건 가져오도록 변경 함 , 아래는 주석처리 
	 
	if( parent.cboStndYearSearch ==sCurYear) {
		//현재년도 조회		
		sServiceName = "rbr1013_b-service:searchEmpStatCurr";
		sInDataSet   = "";
		sOutDataSet  = "dsEmpStat=dsEmpStat ";
		
		var nFindRow = parent.dsBrncCd.FindRowNF("CD", parent.cboBrncCdSearch.Value);
		var sBrncCd  = parent.dsBrncCd.GetColumn(nFindRow, "CD_TERM2");

		sBrncCd = substr(sBrncCd, 4, 8);

		fcd_AddParam("BRNC_CD", sBrncCd);		
	} else {
		//과거년도 조회
		sServiceName = "rbr1013-service:searchEmpStat";
		sInDataSet   = "";
		sOutDataSet  = "dsEmpStat=dsEmpStat ";
		
		fcd_AddParam("BRNC_CD"  , parent.cboBrncCdSearch.Value);
		fcd_AddParam("STND_YEAR", parent.cboStndYearSearch.Value);
	}
	*/
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

