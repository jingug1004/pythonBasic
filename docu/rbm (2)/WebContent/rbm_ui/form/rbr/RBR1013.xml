function fcBtnOnClick(obj) {		
    if(fnc_Message(SNIS_COM_1012, "Y")) {
        
        //저장하기 전 올해 년도인지를 체크
        if( parent.cboStndYearSearch != sCurYear )	return;
        
		var sServiceName = "rbr1013-service:saveEmpStat";
		var sInDataSet   = "dsEmpStat=dsEmpStat ";
		var sOutDataSet  = "dsEmpStat=dsEmpStat "; 
	    
	    fcd_AddParam("BRNC_CD"  , cboBrncCdSearch.Value);	//지점코드
		fcd_AddParam("STND_YEAR", sCurYear);				//현재년도
		
		var nFindRow = parent.dsBrncCd.FindRowNF("CD", parent.cboBrncCdSearch.Value);
		var sBrncCd  = parent.dsBrncCd.GetColumn(nFindRow, "CD_TERM2");
	
		sBrncCd = substr(sBrncCd, 4, 8);
	
		fcd_AddParam("VW_BRNC_CD", sBrncCd);
	 
		fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
    }
}


