/*-------------------------------------------------------------------------------------------------+
>>  행삭제 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcDelRow(obj) {
	//DataSet의 Header 정보가 있을 때  Add 처리 	

	// 상세 등록 정보 존재 여부
	for(var i = 0; i < dsList.GetRowCount(); i++)
	{
		if(dsList.GetColumn(i, "chk") == 1)
		{
			var busiCntntPrps = dsList.GetColumn(i, "BUSI_CNTNT_PRPS");
			var busiCntntSmry = dsList.GetColumn(i, "BUSI_CNTNT_SMRY");
			
			if( !fnc_IsNull(busiCntntPrps) || !fnc_IsNull(busiCntntSmry) )
			{
				//alert("상세정보가 존재합니다. 그래도 삭제 하시겠습니까?");
				var rtn = fnc_Message(SNIS_RBM_E019, "Y");
				
				if(rtn == 0) return false;	// "닫기"를 누르면 삭제처리로직 없이 종료.
			}
		}
	}

	if ( !fnc_DeleteCheck(dsList, "chk", "1") ) return;
        //처리로직
		break;
}


