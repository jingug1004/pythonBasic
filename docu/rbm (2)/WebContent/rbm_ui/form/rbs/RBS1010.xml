function fcSaveCheck(obj) {
	
	// 변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsList) ) {
		// 저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");
		return false;
	}

	// 필수항목체크
	oDataSet          = dsList;
	sMandatoryColID   = "BUSI_YEAR|CONFIRM_TMS|ACC_BGN_CD|BUSI_NM|CONFIRM_AMT|DPRT_CD|MNG_ID";
	sMandatoryColName = "년도|승인차수|회계구분코드|사업명|승인금액|부서코드|담당자ID";
	oGrid             = grdList;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
		return false;
	}

	return true;
}


