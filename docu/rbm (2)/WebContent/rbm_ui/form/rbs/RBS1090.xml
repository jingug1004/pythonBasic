/*-------------------------------------------------------------------------------------------------+
>>  적립금액을 입력할때마다 누적잔액을 갱신한다.
+-------------------------------------------------------------------------------------------------*/
function fcDsList_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	// 적립금액이 변할 때
	if( strColumnID == "SAV_AMT" ) {

		var nLastAmuAmt = ToFloat(dsList.GetColumn(nRow, "LAST_AMU_AMT"));   // 전년누적잔액(A)
		var nPlanSum     = ToFloat(dsList.GetColumn(nRow, "PLAN_SUM"));       // 소계(B)
		var nSavAmt      = ToFloat(dsList.GetColumn(nRow, "SAV_AMT"));        // 적립금액(C)
			
		var nAmuAmt = nLastAmuAmt - nPlanSum + nSavAmt;   // 누적잔액(
		
		
		dsList.SetColumn(nRow, "AMU_AMT", nAmuAmt);   //  분기 합 셋팅
	}
	
}

