/***************************************************************************************************
*                                      공통 버튼 Event 처리 부분                                   *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  취소 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInit(obj) {
    dsApplyList.ClearData();
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
	fcInit(obj);
	return true;
}

function fcSearch(obj)
{
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "";
	var sInDataSet   = "";
	var sOutDataSet  = "";     
	
	if (tab0.TabIndex == 0) {
		sServiceName = "rbs8030-service:searchWinnerList";
		sInDataSet   = "";
		sOutDataSet  = "dsWinnerList=dsWinnerList";  
	} else {
		sServiceName = "rbs8030-service:searchAllEmpList";
		sInDataSet   = "";
		sOutDataSet  = "dsAllEmpList=dsAllEmpList";  
	}
    
    //조회조건
    fcd_AddParam("STR_DT", calStrDt.Value); 
	fcd_AddParam("END_DT", calEndDt.Value); 
	fcd_AddParam("USER_NM", edUserNm.text); 
	fcd_AddParam("WK_JOB_CD", cboWorkJob.Value); 
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
	
}

