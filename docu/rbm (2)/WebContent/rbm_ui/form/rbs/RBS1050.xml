function fcOnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
		switch ( obj.id ) {
			case "grdList2" :	
			
				if(nCell == 0){  
						
					var sParam = " sACCOUNT="+dsList2.GetColumn(nRow,"ACC_BGN_CD") ;		
						sParam += " sYEAR="+ParseInt(fcd_GetSysdate("YYYY"));
						
						
						GBL_RET_VALUE=sParam; // 값 생성
						

						global.FrameBottom.fcOpenMdiForm("RBS1060", "사업별 진행내역", "frm_rbs::RSM1060.xml");
		
			    }
			    
			break;
		 case "grdList3" :	
			
				if(nCell == 0){  
						
					var sParam = " sAccount="+dsList3.GetColumn(nRow,"ACC_BGN_CD") ;		
						sParam += " sYear="+ParseInt(fcd_GetSysdate("YYYY"));
						
						
						GBL_RET_VALUE=sParam; // 값 생성
						

						global.FrameBottom.fcOpenMdiForm("RBS1070", "사업별 계획 대비 집행내역", "frm_rbs::RSM1070.xml");
		
			    }
			    
			break;
		}
}
