+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {
   
	if (!fcSearchCheck(obj)) return;// 필수조건 체크 및 그리드 초기화
			
	var sServiceName = "rbs2020-service:searchList";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsList=dsList ";     
				
	fcd_AddParam("EXEC_YEAR" , cboYear.Value  );  // 년도  
	fcd_AddParam("EXEC_MM"   , cboMonth.Value );  // 월
		
	// 20111028_장한너울 (사용안함) -- dsOutTeamNm 조회로 교체(combo 사용)
