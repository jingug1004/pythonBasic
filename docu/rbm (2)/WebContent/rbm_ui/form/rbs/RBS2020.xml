/*-------------------------------------------------------------------------------------------------+
>>  Combo가 바뀌면 조회한다.  
>>                             집행년도
+-------------------------------------------------------------------------------------------------*/
function combo_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{

	
	switch ( obj.id ) {
		case "cboHealBusi":
		
			if(cboMonth.Value == null || cboMonth.Value == ""){
				dsList.GroupKey="HEAL_LEISU_CD:BUSI_NM";
			}else{
				dsList.GroupKey="";
			}
			
			
			fcSearch();	
		
			break;
		default:
			if(cboMonth.Value == null || cboMonth.Value == ""){
				dsList.GroupKey="HEAL_LEISU_CD:BUSI_NM";
			}else{
				dsList.GroupKey="";
			}
			
			fcSearchHealBusi();
			
			fcSearch();	
			
			break;
	}
	
	
	
}

