	}

	var nRow = 0;
	if (tab0.TabIndex == 0) {
		dsNetPlanSave.ClearData();
		for(var i=1;i<=12;i++) {
			nRow = dsNetPlanSave.AddRow();
			dsNetPlanSave.SetColumn(nRow, "MEET_CD",    dsNetPlan.GetColumn(0, "MEET_CD"));
			dsNetPlanSave.SetColumn(nRow, "STND_YEAR",  dsNetPlan.GetColumn(0, "STND_YEAR"));
