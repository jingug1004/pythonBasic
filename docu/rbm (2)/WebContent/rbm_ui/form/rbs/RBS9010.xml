			dsNetPlanSave.SetColumn(nRow, "PLAN_TYPE",  dsNetPlan.GetColumn(0, "PLAN_TYPE"));
			dsNetPlanSave.SetColumn(nRow, "NET_SALES_AMT", dsNetPlan.GetColumn(0, "NET"+Lpad(toString(i), "0", 2)));
		}
	} else if(tab0.TabIndex == 1) {
		dsTotPlanSave.ClearData();
		for(var i=1;i<=12;i++) {
			nRow = dsTotPlanSave.AddRow();
			dsTotPlanSave.SetColumn(nRow, "MEET_CD",    dsTotPlan.GetColumn(0, "MEET_CD"));
			dsTotPlanSave.SetColumn(nRow, "STND_YEAR",  dsTotPlan.GetColumn(0, "STND_YEAR"));
			dsTotPlanSave.SetColumn(nRow, "STND_MONTH", Lpad(toString(i), "0", 2));
			dsTotPlanSave.SetColumn(nRow, "PLAN_TYPE",  dsTotPlan.GetColumn(0, "PLAN_TYPE"));
			dsTotPlanSave.SetColumn(nRow, "NET_SALES_AMT", dsTotPlan.GetColumn(0, "NET"+Lpad(toString(i), "0", 2)));
		}
	} else {
		return false;
	}

	return true;
}


