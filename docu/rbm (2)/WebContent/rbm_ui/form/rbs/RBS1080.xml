/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {
   
	if (!fcSearchCheck(obj)) return;// 필수조건 체크 및 그리드 초기화
			
	var sServiceName = "rbs1080-service:searchList";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsList=dsList ";     
	
				
	fcd_AddParam("BUSI_YEAR"     , cboYear.Value);        // 사업년도  	
	fcd_AddParam("ACC_BGN_CD"    , cboAccount.Value);     // 회계구분 
	//fcd_AddParam("TRAN_STAT"     , cboTranStat.Value);    // 이월상태 	
	fcd_AddParam("BUSI_NM"       , edBusiName.Value);     // 사업명 
	fcd_AddParam("EXEC_DT_FROM"  , calFromDt.Value);      // 집행기간 시작
	fcd_AddParam("EXEC_DT_TO"    , calToDt.Value);        // 집행기간 종료
	
	if(Length(sPrepAmtCd) >0){
		fcd_AddParam("PREP_AMT_CD"    , sPrepAmtCd);    // 사업 
		
	}

	sPrepAmtCd = "";
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);

	
}

