/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 Load時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormLoadSetting(obj) {  
    sBUTTONLIST   = 'FTFFTFT';     //초기화,조회,저장,삭제,엑셀,인쇄,닫기
    //sBUTTONLIST   = 'TTTTTTT'; 

 	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
	

    
    //공통코드 조회
	fcd_GetComboDs(dsInComCd);
	
	
	/*
		Div 가 Loading 시간이 걸린다. 
		반드시 timer 로 처리해야함.
	*/
	
	
	// 년도 설정
	fcSetYear();
	
	
	// Combo 에 전체 추가 : 회계구분
	dsAccount.InsertRow(0);		
	dsAccount.SetColumn(0,"CD"   , "" );
	dsAccount.SetColumn(0,"CD_NM", "전체");
	
	cboAccount.Index = 0;
	
	
    //조회 조건의 날짜(올해 1월1일 ~ today)
    calToDt  .Value = sCurDate;
    calFromDt.Value = sCurYear + '0101';
	
	
	//메뉴이동 처리 
	var sRetValue = decode(GBL_RET_VALUE,null,'',GBL_RET_VALUE);
	if(sRetValue!='')
	{		
		// 이전 페이지에서 파라미터 받음	
		ARRAY2_GBL_RET_VALUE = split2(GBL_RET_VALUE," ","=");
		GBL_RET_VALUE="";
		cboAccount.Value 	= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"sAccBgnCd");		
		cboYear.Value		= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"sYear");
		sPrepAmtCd       	= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"sPrepAmtCd");
		
	    calToDt  .Value = sCurDate;
		calFromDt.Value = cboYear.Value + '0101';
    
	}
	
	
	// 화면 열리자마자 조회
	fcSearch();
	
	GBL_RET_VALUE="";
}

/*-------------------------------------------------------------------------------------------------+
>>  화면 실행할 때마다 실행
+-------------------------------------------------------------------------------------------------*/
function fcFormOnActivite(){

	var sRetValue = decode(GBL_RET_VALUE,null,'',GBL_RET_VALUE);

	//메뉴이동 처리 

	if(sRetValue!='')
	{		
		// 이전 페이지에서 파라미터 받음	
		ARRAY2_GBL_RET_VALUE = split2(GBL_RET_VALUE," ","=");
		GBL_RET_VALUE="";
		cboAccount.Value 	= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"sAccBgnCd");		
		cboYear.Value		= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"sYear");
		sPrepAmtCd       	= fnc_Array2Value(ARRAY2_GBL_RET_VALUE,"sPrepAmtCd");
		
	    calToDt  .Value = sCurDate;
		calFromDt.Value = cboYear.Value + '0101';
		
		fcSearch();
	}
	
}


