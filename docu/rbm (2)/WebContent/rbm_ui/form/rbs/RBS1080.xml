var gnGrpRow        = 0;

var sCurDate = fcd_GetSysdate("YYYYMMDD");				// 시스템의 오늘날짜
var sCurYear = substr(sCurDate, 0, 4);	


//메뉴이동 처리 
var ARRAY2_GBL_RET_VALUE = Array();	// 이전페이지에서 넘어온 변수
var sAccBgnCd = "";
var sYear = "";
var sPrepAmtCd = "";
/***************************************************************************************************
*                                         공통 Event 처리 부분                                     *
