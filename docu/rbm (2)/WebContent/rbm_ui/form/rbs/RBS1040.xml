/*-------------------------------------------------------------------------------------------------+
>>  행추가 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcAddRow(obj) {
	var sFormId 	= "";
	var sArgs 		= "";
	var arrReturn   = [];	
	var fRow = dsList.row;
	
	sFormId = "frm_rbs::RBS1040P.xml";
	// 해당 부서에 속해있는 세부 집행 내역만 검색 허용하기 위해 부서코드와 부서명을 넘긴다.
	sArgs = "pv_dept_cd=" + quote(dsList.GetColumn(fRow, "DPRT_CD"		))+
			" pv_dept_nm="+ quote(dsList.GetColumn(fRow, "DPRT_NAME"	))+
			" pv_mng_id=" + quote(dsList.GetColumn(fRow, "MNG_ID"		));	
	
	// 통합재무 팝업 열기 
	arrReturn = Dialog(sFormId, sArgs, 700, 600, "");
		
	
	// LOOP :  팝업에서 선택한 Row 수만큼 반복한다
	for( var j=0; j < arrReturn.length ; j++ ) 
	{
	    var temp = arrReturn[j];
	    		
	    // 결의서번호, 집행일자, 집행내역, 집행금액
		var sOutDecisId = temp[0];   // 결의서번호   
		var sOutDy      = temp[1];   // 집행일자
		var sDecisRmk   = temp[2];   // 집행내역
		var sDecisAmt   = temp[3];   // 집행금액
		
			    
	    // dsExec 에 이미 있는 Row 인지 검사한다. 이미 있으면 addRow 하지 않는다.
	    var nFirstRow = dsExec.findRow("REPORT_NO" , sOutDecisId);
	    
	    if(nFirstRow == -1)  
	    {	
			// ADD ROW
			dsExec.AddRow(); // Record Add
			var nRow = dsExec.currow;	
						
						
			/*  필수입력값 셋팅
			 * 1. 시개금코드   - dsList 에서 가져온다.
			 */	 
			dsExec.SetColumn(nRow, "PREP_AMT_CD" , dsList.getColumn(fRow, "PREP_AMT_CD")); 
			
			
			/*  
			 * 초기값 셋팅 : 
			 */    		 		 
			dsExec.SetColumn(nRow, "REPORT_NO"  , sOutDecisId);	 // 결의서번호  
			dsExec.SetColumn(nRow, "EXEC_DT"    , sOutDy     );  // 집행일자
			dsExec.SetColumn(nRow, "EXEC_CNTNT" , sDecisRmk  );  // 집행내역
			dsExec.SetColumn(nRow, "EXEC_AMT"   , sDecisAmt  );  // 집행금액
		}
			
	}	
	// LOOP END
		

}


