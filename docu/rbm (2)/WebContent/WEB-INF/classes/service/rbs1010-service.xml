<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs1010" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsList" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchList"         	value="searchList" />	
	    <transition name="saveBusiPlanDetlReg"  value="saveBusiPlanDetlReg" />
	    <transition name="saveBusiProgReg"    	value="saveBusiProgReg" />
	    <transition name="saveBusiPlanReg"    	value="saveBusiPlanReg" />			<transition name="success" 			  	value="end" />	</activity> 		
  	<!-- 사업계획등록 조회 -->  	<activity name="searchList" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsList" />
    	<property name="sqlkey" 		  value="rbs1010_s01" />
    	<property name="param-count" 	  value="3" />
    	<property name="param0" 		  value="BUSI_YEAR" />    
    	<property name="param1" 		  value="ACC_BGN_CD" />
    	<property name="param2" 		  value="CONFIRM_TMS" />  
    	<property name="dao" 			  value="rbmdao" />
  	</activity>	
	
	<!-- 사업계획등록 정보 삭제시 Relation 되어 있는 자식 테이블부터 삭제 후 마스터 정보를 삭제 한다. (무결성 제약조건 위배 이유) -->
	
	<!-- 사업계획등록 저장 -->
  	<activity name="saveBusiPlanReg" 	  class="snis.rbm.business.rbs1010.activity.SaveBusiPlanReg">
    	<transition name="success" 	  value="end" />
  	</activity>
  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

