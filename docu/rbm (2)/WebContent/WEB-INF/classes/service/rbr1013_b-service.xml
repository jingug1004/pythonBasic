	<property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsEmpStat" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEmpStatCurr"  	value="searchEmpStatCurr" />    <!-- 직원현황 조회 -->
  </activity>
  
  <!-- 직원현황 조회  -->
  <activity name="searchEmpStatCurr" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsEmpStat" />

    <property name="sqlkey"				value="rbr1013_s02" />

    <property name="param-count" 		value="1" />
    
    <property name="param0" 			value="BRNC_CD" />      <!-- 지점코드  -->
    
    <property name="dao" 				value="venisrbmdao" />
  </activity>  
  	
  <transaction-manager id="tx_venus" commit="true" />

</service>