<?xml version="1.0" encoding="EUC-KR"?>
<service name="rbs9220" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsListYy|dsListMm" />
    <transition name="success" 				value="EventRouter" />
  </activity>  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchListMm"  		value="searchListMm" />   <!-- 지점별 전자카드 발매건수 월별 조회 -->    <transition name="searchListYy"  		value="searchListYy" />   <!-- 지점별 전자카드 발매건수 연도별 조회 -->    <transition name="saveList"   			value="saveList" />       <!-- 지점별 전자카드 발매건수 저장 -->   
    <transition name="success" 				value="end" />
  </activity>

  <!-- 지점별 전자카드 발매건수 월별 조회 -->  <activity name="searchListMm" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success" 				value="end" />    <property name="resultkey" 				value="dsListMm" />    <property name="sqlkey"					value="rbs9220_s01"/>    <property name="param-count" 			value="3" />    <property name="param0" 				value="STND_YEAR" />    <property name="param1" 				value="STND_MM" />    <property name="param2" 				value="BRNC_CD" />    <property name="dao" 					value="rbmdao" />  </activity>     <!-- 지점별 전자카드 발매건수 연도별 조회 -->
  <activity name="searchListYy" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsListYy" />
    <property name="sqlkey"					value="rbs9220_s02"/>
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="STND_YEAR" />    <property name="param1" 				value="BRNC_CD" />    <property name="param2" 				value="STND_YEAR" />    <property name="param3" 				value="BRNC_CD" />
    <property name="dao" 					value="rbmdao" />
  </activity>    <!-- 지점별 전자카드 발매건수 저장 -->  <activity name="saveList" 				class="snis.rbm.business.rbs9220.activity.GreencardCntSaveList">    <transition name="success" 				value="end" />  </activity>  
  <transaction-manager id="tx_rbm" commit="true" />
</service>