<?xml version="1.0" encoding="EUC-KR"?>
<service name="rsy3020" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">     <property name="staticResultKeyFile" 	value="resultkey.properties" />    <property name="resultKeyList" 			value="dsUserListValue|dsAuthListValue" />    <transition name="success" 				value="EventRouter" />  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">    <transition name="searchUserList"  		value="searchUserList" />        <transition name="searchMenuAuthList"  	value="searchMenuAuthList" />
    <transition name="saveAuth"   			value="saveAuth" />        <transition name="success" 				value="end" />  </activity>

  <!-- 사용자 조회 -->  <activity name="searchUserList" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success" 				value="end" />    <property name="resultkey" 				value="dsUserListValue" />    <property name="sqlkey" 				value="rsy3020_s01" />    <property name="param-count" 			value="4" />
    <property name="param0" 				value="MN_ID" />
    <property name="param1" 				value="MN_ID" />
    <property name="param2" 				value="USER_NM" />
    <property name="param3" 				value="TEAM_CD" />    <property name="dao" 					value="rbmdao" />      </activity>
  <!-- 메뉴권한리스트  조회 -->  <activity name="searchMenuAuthList" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success" 				value="end" />    <property name="resultkey" 				value="dsAuthListValue" />    <property name="sqlkey" 				value="rsy3020_s02" />    <property name="param-count" 			value="2" />    <property name="param0" 				value="USER_ID" />
    <property name="param1"                 value="ADMIN_USER_ID" />       <property name="dao" 					value="rbmdao" />      </activity>
    <activity name="saveAuth" 				class="snis.rbm.system.rsy3020.activity.SaveAuth">
    <transition name="success" 				value="end" />
  </activity>    
  <transaction-manager id="tx_rbm" commit="true" /></service>
