<?xml version="1.0" encoding="EUC-KR"?>
<service name="rsy4080" initial="SetResultKeys">
  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsDutyList" />
    <transition name="success"              value="EventRouter" />
  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchDutyList"        value="searchDutyList" />
    <transition name="saveDutyList"          value="saveDutyList" /> 
    <transition name="success"              value="end" />
  </activity>


  <!-- 업무일지 조회 -->
  <activity name="searchDutyList"         class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsDutyList" />
    <property name="sqlkey"                 value="rsy4080_s01" />
    <property name="param-count"            value="9" />
    <property name="param0"                 value="USER_ID" />
    <property name="param1"                 value="STR_DT" />
    <property name="param2"                 value="END_DT" />
    <property name="param3"                 value="STAT_CD" />
    <property name="param4"                 value="END_YN" />
    <property name="param5"                 value="END_YN" />
    <property name="param6"                 value="SEARCH_WORD" />
    <property name="param7"                 value="SEARCH_WORD" />
    <property name="param8"                 value="SEARCH_WORD" />
    <property name="dao"                    value="rbmdao" />
  </activity>  

  <!-- 업무일지 저장 -->
  <activity name="saveDutyList"             class="snis.rbm.system.rsy4080.activity.SaveDutyList">
    <transition name="success"              value="searchDutyList" />
  </activity>
  <transaction-manager id="tx_rbm" commit="true" />
</service>