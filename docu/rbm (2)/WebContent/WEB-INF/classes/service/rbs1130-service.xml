<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs1130" initial="SetResultKeys">
    <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsHnrRefreWnr" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 			class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchHnrRefreWnr"    value="searchHnrRefreWnr" />	
	    <transition name="saveHnrRefreWnr"  	value="saveHnrRefreWnr" />
		<transition name="success" 			  	value="end" />	</activity> 		
  	<!-- 명예심판  선정자 조회 -->
  	<activity name="searchHnrRefreWnr" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsHnrRefreWnr" />
    	<property name="sqlkey" 		  value="rbs1130_s01" />
    	<property name="param-count" 	  value="3" />
    	<property name="param0" 		  value="MEET_CD" />    
    	<property name="param1" 		  value="STND_YEAR" />    
    	<property name="param2" 		  value="TMS" />    
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
	<!-- 명예심판 직접 입력자료 저장 -->
  	<activity name="saveHnrRefreWnr" 	  class="snis.rbm.business.rbs1130.activity.SaveHnrRefreWnr">
    	<transition name="success" 	  	  value="end" />
  	</activity>
  	  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

