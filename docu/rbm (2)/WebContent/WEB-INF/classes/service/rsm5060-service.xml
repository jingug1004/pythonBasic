<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm5060" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsPcGita|dsCnt|dsCntChkValid" />
    <transition name="success"              value="EventRouter" />
  </activity>

  <activity name="EventRouter"                class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPcGita"           value="searchPcGita" />    	<!-- 기타소득세 검증 조회 -->
    <transition name="searchCntChkValid"      value="searchCntChkValid" />  <!-- 기타소득세 일치 불일치 카운트 -->
    <transition name="saveDbGitaFile"         value="saveDbGitaFile" />  	<!-- 기타소득세 DB읽기 -->
    <transition name="saveGitaCfmCntnt"       value="saveGitaCfmCntnt" />  	<!-- 기타소득세 확정하기 -->
    <transition name="deleteGitaCfmCntnt"     value="deleteGitaCfmCntnt" /> <!-- 기타소득세 확정취소 -->
    <transition name="searchCfmCnt"     	  value="searchCfmCnt" />  		<!-- 확정여부 조회 -->
  </activity>


  <!-- 확정여부 조회 -->
  <activity name="searchCfmCnt"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"        value="end" />  
    <property name="resultkey"        value="dsCnt" />
    <property name="sqlkey"           value="rsm5060_s06" />

    <property name="param-count"      value="2" />    
    <property name="param0"           value="PAY_YEAR" />
    <property name="param1"           value="PAY_MM" />   
    
    <property name="dao"              value="rbmdao" />
  </activity>
    
  <!-- 기타소득세 검증 조회 -->
  <activity name="searchPcGita"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"        value="searchCntChkValid" />  
    <property name="resultkey"        value="dsPcGita" />
    <property name="sqlkey"           value="rsm5060_s01" />
    <property name="param-count"      value="8" />    
    <property name="param0"           value="PAY_YEAR" />
    <property name="param1"           value="PAY_MM" />
    <property name="param2"           value="MEET_CD" />
    <property name="param3"           value="SELL_CD" />  
    <property name="param4"           value="PAY_YEAR" />
    <property name="param5"           value="PAY_MM" />
    <property name="param6"           value="MEET_CD" />
    <property name="param7"           value="SELL_CD" />
    <property name="dao"              value="rbmdao" />
  </activity>
  
	<!-- 기타소득세 일치 불일치 카운트 -->
	<activity name="searchCntChkValid"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success"        value="end" />  
		<property name="resultkey"        value="dsCntChkValid" />
		<property name="sqlkey"           value="rsm5060_s08" />
		<property name="param-count"      value="8" />    
		<property name="param0"           value="PAY_YEAR" />
		<property name="param1"           value="PAY_MM" />
		<property name="param2"           value="MEET_CD" />
		<property name="param3"           value="SELL_CD" />  
		<property name="param4"           value="PAY_YEAR" />
		<property name="param5"           value="PAY_MM" />
		<property name="param6"           value="MEET_CD" />
		<property name="param7"           value="SELL_CD" />	    
		<property name="dao"              value="rbmdao" />
	</activity>

	<!-- 기타소득세 검증 조회 -->
	 <activity name="searchPcGita(X)"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	   <transition name="success"        value="end" />  
	   <property name="resultkey"        value="dsPcGita" />
	   <property name="sqlkey"           value="rsm5060_s01" />
	
	   <property name="param-count"      value="8" />
	   
	   <property name="param0"           value="PAY_YEAR" />
	   <property name="param1"           value="PAY_MM" />
	   <property name="param2"           value="MEET_CD" />
	   <property name="param3"           value="SELL_CD" />  
	   <property name="param4"           value="PAY_YEAR" />
	   <property name="param5"           value="PAY_MM" />
	   <property name="param6"           value="MEET_CD" />
	   <property name="param7"           value="SELL_CD" />
	     
	   <property name="dao"              value="rbmdao" />
	 </activity>

  <!-- 기타소득세 DB읽기 -->  
  <activity name="saveDbGitaFile"          class="snis.rbm.business.rsm5060.activity.SaveDbGitaFile"> 
    <transition name="success"             value="end" />
  </activity>
  
  <!-- 기타소득세 DB읽기 -->  
  <activity name="saveGitaCfmCntnt"        class="snis.rbm.business.rsm5060.activity.SaveGitaCfmCntnt">
    <transition name="success"             value="end" />
  </activity>
  
    <!-- 기타소득세 확정취소 -->  
  <activity name="deleteGitaCfmCntnt"      class="snis.rbm.business.rsm5060.activity.DeleteGitaCfmCntnt">
    <transition name="success"             value="end" />
  </activity>
  
  <transaction-manager id="tx_rbm" commit="true" />
</service>