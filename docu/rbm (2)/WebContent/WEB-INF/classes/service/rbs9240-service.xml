<?xml version="1.0" encoding="EUC-KR"?>
<service name="rbs9240" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsLeaseMana" />
    <transition name="success" 				value="EventRouter" />
  </activity>  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchLeaseMana"  	value="searchLeaseMana" />   <!-- 전자카드 인프라 조회 -->   
    <transition name="saveLeaseMana"   		value="saveLeaseMana" />     <!-- 전자카드 인프라 저장 -->
    <transition name="success" 				value="end" />
  </activity>

  <!-- 적발건수 조회 -->
  <activity name="searchLeaseMana" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsLeaseMana" />
    <property name="sqlkey"					value="rbs9240_s01"/>
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="ACT_YEAR" />    <property name="param1" 				value="BRNC_CD" />
    <property name="dao" 					value="rbmdao" />
  </activity>  
  <!-- 적발건수 저장 -->
  <activity name="saveLeaseMana" 			class="snis.rbm.business.rbs9240.activity.GreencardInfraSaveList">
    <transition name="success" 				value="end" />
  </activity>
  <transaction-manager id="tx_rbm" commit="true" />
</service>