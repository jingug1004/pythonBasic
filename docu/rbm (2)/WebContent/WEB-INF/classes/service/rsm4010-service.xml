<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm4010" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsRaceInfo" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchRaceDayInfoCRA"         value="searchRaceDayInfoCRA" />
    	<transition name="searchRaceDayInfoMRA"         value="searchRaceDayInfoMRA" />		<transition name="success" 			  value="end" />	</activity>
	
	<!-- 경륜 개최 정보 -->
	<activity name="searchRaceDayInfoCRA" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 		  value="end" />
	  <property name="resultkey" 		  value="dsRaceInfo" />
	  <property name="sqlkey" 		  value="rsm4010_s01" />
	  <property name="param-count" 	  value="3" />
	  <property name="param0" 		  value="STND_YEAR" />
	  <property name="param1" 		  value="MEET_CD" />
	  <property name="param2" 		  value="MEET_CD" />	  
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	
	<!-- 경정 개최 정보 -->
	<activity name="searchRaceDayInfoMRA" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 		  value="end" />
	  <property name="resultkey" 		  value="dsRaceInfo" />
	  <property name="sqlkey" 		  value="rsm4010_s02" />
	  <property name="param-count" 	  value="2" />
	  <property name="param0" 		  value="MEET_CD" />
	  <property name="param1" 		  value="STND_YEAR" />
	  <property name="dao" 			  value="rbmdao" />
	</activity>
  
  <transaction-manager id="tx_rbm" commit="true" /></service>

