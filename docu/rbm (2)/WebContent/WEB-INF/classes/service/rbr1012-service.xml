<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbr1012" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	<property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsHistMana" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchHistMana"  		value="searchHistMana" />  <!-- 연혁 조회  -->
  	<transition name="saveHistMana"   		value="saveHistMana" />    <!-- 연혁 저장  -->
  </activity>
 
  <!-- 연혁관리 조회  -->
  <activity name="searchHistMana" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsHistMana" />

    <property name="sqlkey"				value="rbr1012_s01" />

    <property name="param-count" 		value="1" />
    
    <property name="param0" 			value="BRNC_CD" />   <!-- 지점코드  -->
    
    <property name="dao" 				value="rbmdao" />

  </activity>

  <!-- 연혁관리 저장  -->			   
  <activity name="saveHistMana" 		class="snis.rbm.business.rbr1012.activity.SaveHistMana">
    <transition name="success" 			value="end" />
  </activity>
  	
  <transaction-manager id="tx_rbm" commit="true" />

</service>