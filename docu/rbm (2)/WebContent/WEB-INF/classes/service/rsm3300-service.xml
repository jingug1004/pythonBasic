<?xml version="1.0" encoding="EUC-KR"?>
<service name="rsm3300" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsSales" />	    <transition name="success" 			  value="EventRouter" />
	</activity>
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    	<transition name="searchSales"    		value="searchSales" />		<!-- 지점 단말별 매출액 조회 -->    	<transition name="searchSales1"    		value="searchSales1" />		<!-- 지점 단말별 매출액 조회 -->
		<transition name="success" 			  		value="end" />	</activity>
	<!-- 지점 단말별 매출액 조회 -->
	<activity name="searchSales" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	 value="end" />
	  <property name="resultkey" 	 value="dsSales" />
	  <property name="sqlkey" 		  value="rsm3300_s01" />
	  <property name="param-count" 	  value="16" />
	  <property name="param0" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->
	  <property name="param1" 		  value="RACE_DAY_END" />	<!-- 종료일 -->
	  <property name="param2" 		  value="BRNC_CD" />		<!-- 지점 -->
	  <property name="param3" 		  value="MEET_CD" />		<!-- 구분 -->	  <property name="param4" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->	  <property name="param5" 		  value="RACE_DAY_END" />	<!-- 종료일 -->	  <property name="param6" 		  value="BRNC_CD" />		<!-- 지점 -->	  <property name="param7" 		  value="MEET_CD" />		<!-- 구분 -->	  <property name="param8" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->	  <property name="param9" 		  value="RACE_DAY_END" />	<!-- 종료일 -->	  <property name="param10" 		  value="BRNC_CD" />		<!-- 지점 -->	  <property name="param11" 		  value="MEET_CD" />		<!-- 구분 -->	  <property name="param12" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->	  <property name="param13" 		  value="RACE_DAY_END" />	<!-- 종료일 -->	  <property name="param14" 		  value="BRNC_CD" />		<!-- 지점 -->	  <property name="param15" 		  value="MEET_CD" />		<!-- 구분 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>		<!-- 지점 단말별 매출액 조회 -->	<activity name="searchSales1" class="com.posdata.glue.miplatform.biz.activity.PosSearch">	  <transition name="success" 	 value="end" />	  <property name="resultkey" 	 value="dsSales" />	  <property name="sqlkey" 		  value="rsm3300_s02" />	  <property name="param-count" 	  value="12" />	  <property name="param0" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->	  <property name="param1" 		  value="RACE_DAY_END" />	<!-- 종료일 -->	  <property name="param2" 		  value="MEET_CD" />		<!-- 구분 -->	  <property name="param3" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->	  <property name="param4" 		  value="RACE_DAY_END" />	<!-- 종료일 -->	  <property name="param5" 		  value="MEET_CD" />		<!-- 구분 -->	  <property name="param6" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->	  <property name="param7" 		  value="RACE_DAY_END" />	<!-- 종료일 -->	  <property name="param8" 		  value="MEET_CD" />		<!-- 구분 -->	  <property name="param9" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->	  <property name="param10" 		  value="RACE_DAY_END" />	<!-- 종료일 -->	  <property name="param11" 		  value="MEET_CD" />		<!-- 구분 -->	  <property name="dao" 			  value="rbmdao" />	</activity>	
 
  <transaction-manager id="tx_rbm" commit="true" />		<!-- 경륜 transaction  --></service>



