<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000028" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsRaceYy|dsDetlRaceScheValue|dsCandListValue|dsInduceListValue" />
    <transition name="success" value="EventRouter" />
  </activity> 
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceYY"      	   		value="searchRaceYY" />
    <transition name="searchDetailRace"      	    value="searchDetailRace" />
    <transition name="searchCandList"      	  		value="searchCandList" />
    <transition name="saveInduceDcsn"      	  		value="saveInduceDcsn" />
    <transition name="searchInduceDcsnList"      	value="searchInduceDcsnList" />
    <transition name="success" 						value="end" />
  </activity>
  
    <activity name="searchRaceYY"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRaceYy" />
    <property name="sqlkey" 				value="tbdb_induce_dcsn_fb001" />
    <property name="dao" 					value="candao" />
  </activity>
  
    <!-- 세부시행일정 조회 --> 
  <activity name="searchDetailRace"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchCandList" />
    <property name="resultkey" 				value="dsDetlRaceScheValue" />
    <property name="sqlkey" 				value="tbdb_induce_dcsn_fb002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="Raceyy" />
    <property name="param1" 				value="RaceRndNo" />
    <property name="dao" 					value="candao" />
  </activity>
   <!-- 유도원 조회 -->
    <activity name="searchCandList"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchInduceDcsnList" />
    <property name="resultkey" 				value="dsCandListValue" />
    <property name="sqlkey" 				value="tbdb_induce_dcsn_fb003" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="Raceyy" />
    <property name="param1" 				value="RaceRndNo" />
    <property name="dao" 					value="candao" />
  </activity>
     <!-- 저장된 유도원 조회 -->
    <activity name="searchInduceDcsnList"	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsInduceListValue" />
    <property name="sqlkey" 				value="tbdb_induce_dcsn_fb004" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="Raceyy" />
    <property name="param1" 				value="RaceRndNo" />
    <property name="dao" 					value="candao" />
  </activity>
      <activity name="saveInduceDcsn"			class="snis.can.cyclestd.d02000028.activity.SaveInduceDcsn">
    <transition name="success" 				value="end" />
  </activity>
    <transaction-manager id="tx_can" commit="true" />
</service>