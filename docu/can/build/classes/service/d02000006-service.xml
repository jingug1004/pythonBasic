<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000006" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsExamGbn|dsExam|dsExamChng|dsPerioNo|dsSeltCnt" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchSeltCnt"		value="searchSeltCnt" /> 
    <transition name="searchExamGbn"		value="searchExamGbn" /> 
    <transition name="searchExam"			value="searchExam" />  	
    <transition name="searchExamChng"		value="searchExamChng" />  	
    <transition name="searchPerioNo"        value="searchPerioNo" />
    <transition name="searchExamGbnPerioNoCopy"	 value="searchExamGbnPerioNoCopy" />
    <transition name="searchExamPerioNoCopy"     value="searchExamPerioNoCopy" />
    <transition name="searchExamChngPerioNoCopy" value="searchExamChngPerioNoCopy" />            
    <transition name="saveSeltCnt"			value="saveSeltCnt" />
    <transition name="saveExamGbn"        	value="saveExamGbn" />
    <transition name="saveExam"        		value="saveExam" />
    <transition name="saveExamChng"        	value="saveExamChng" />
    <transition name="success" 				value="end" />
  </activity>
  <!-- 선발인원 1/2차 -->
  <activity name="searchSeltCnt"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsSeltCnt" />
    <property name="sqlkey" 				value="tbdb_selt_bas_fb001" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />         
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 종목구분등록 조회 -->
  <activity name="searchExamGbn"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsExamGbn" />
    <property name="sqlkey" 				value="tbdb_exam_bas_item_gbn_fb001" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="examCd" /> 
    <property name="param2" 				value="itemGbnCd" />           
    <property name="dao" 					value="candao" />    
  </activity>

  <!-- 종목등록 조회 -->
  <activity name="searchExam" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsExam" />
    <property name="sqlkey" 				value="tbdb_exam_bas_item_fb001" />
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="examCd" /> 
    <property name="param2" 				value="itemGbnCd" />
	<property name="param3" 				value="itemCd" />    
    <property name="dao" 					value="candao" />    
  </activity> 

  <!-- 환산기준표 조회 -->
  <activity name="searchExamChng" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsExamChng" />
    <property name="sqlkey" 				value="tbdb_exam_bas_chng_bas_fb001" />
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="examCd" /> 
    <property name="param2" 				value="itemGbnCd" />
	<property name="param3" 				value="itemCd" />  
    <property name="dao" 					value="candao" />    
  </activity> 
  
  
   <!-- 전기수복사 종목구분등록 조회 -->
  <activity name="searchExamGbnPerioNoCopy"	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsExamGbn" />
    <property name="sqlkey" 				value="tbdb_exam_bas_item_gbn_fb002" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />       
    <property name="dao" 					value="candao" />    
  </activity>

  <!--전기수복사 종목등록 조회 -->
  <activity name="searchExamPerioNoCopy" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsExam" />
    <property name="sqlkey" 				value="tbdb_exam_bas_item_fb002" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />  
    <property name="dao" 					value="candao" />    
  </activity> 

  <!--전기수복사 환산기준표 조회 -->
  <activity name="searchExamChngPerioNoCopy" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsExamChng" />
    <property name="sqlkey" 				value="tbdb_exam_bas_chng_bas_fb002" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>  

  <!-- 기수콤보 설정 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdb_PerioNo_fb001" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>
   

  <!-- 종목구분등록 저장 --> 
  <activity name="saveExamGbn"				class="snis.can.cyclestd.d02000006.activity.SaveChoiceDefault">
    <transition name="success"				value="end" />
  </activity> 	

  <!-- 종목등록 저장 --> 
  <activity name="saveExam"					class="snis.can.cyclestd.d02000006.activity.SaveChoiceDefault">
    <transition name="success"				value="end" />
  </activity> 
 
  <!-- 선발인원 저장 --> 
  <activity name="saveSeltCnt"				class="snis.can.cyclestd.d02000006.activity.SaveSeltCnt">
    <transition name="success"				value="end" />
  </activity> 
  <!-- 환산기준표 저장 --> 
  <activity name="saveExamChng"				class="snis.can.cyclestd.d02000006.activity.SaveChoiceDefault">
    <transition name="success"				value="end" />
  </activity> 
 
	<transaction-manager id="tx_can" commit="true" />
</service>