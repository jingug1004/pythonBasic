<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000017" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPerioNo|dsCycAssemBas|dsCycAssem" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				     class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPerioNo"             value="searchPerioNo" /> 
    <transition name="searchCycAssemBas"         value="searchCycAssemBas" />
    <transition name="searchCycAssem"         	 value="searchCycAssem" />
    <transition name="SaveMakeCycle"             value="SaveMakeCycle" />
    <transition name="success" 			         value="end" /> 
   </activity>
   
 
 <!-- 기수 콤보 설정 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdb_cyc_assem_fb001" />
    <property name="dao" 					value="candao" />
  </activity>

  
 <!-- 환산기준표 조회(중간/기말  자전거 분해조립 ) --> 
  <activity name="searchCycAssemBas"	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			    value="searchCycAssem" />
    <property name="resultkey" 			    value="dsCycAssemBas" />
    <property name="sqlkey" 			    value="tbdb_cyc_assem_fb002" />
    <property name="param-count" 			value="2" /> 
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="examGbn" /> 
    <property name="dao" 				    value="candao" />
  </activity>  
  
 
<!-- 자전거분해조립 조회  --> 
  <activity name="searchCycAssem"         	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCycAssem" />
    <property name="sqlkey" 				value="tbdb_cyc_assem_fb003" />
    <property name="param-count" 			value="6" />
    <property name="param0" 				value="examGbn" />
    <property name="param1" 				value="examGbn" />
    <property name="param2" 				value="examGbn" />
    <property name="param3" 				value="examGbn" />
    <property name="param4" 				value="examGbn" />
    <property name="param5" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>    


 <!-- 정기시험 자전거 분해조립 내역 저장 --> 
  <activity name="SaveMakeCycle"	       class="snis.can.cyclestd.d02000017.activity.SaveMakeCycle">
    <transition name="success"	           value="end" />
  </activity>
  
  <transaction-manager id="tx_can" commit="true" />
</service>