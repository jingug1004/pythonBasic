<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000021" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsMatCodeListValue|dsAssignListValue" />
    <transition name="success" value="EventRouter" />
  </activity> 
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
  	<transition name="searchMatCd"       	 value="searchMatCd" />
  	<transition name="searchAssign"       	 value="searchAssign" />
  	<transition name="saveGrdAssign"       	 value="saveGrdAssign" />
  	<transition name="success" 				 value="end" />
  </activity>
 
  <activity name="searchMatCd" 			    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsMatCodeListValue" />
    <property name="sqlkey" 				value="tbdb_grd_assign_fb001" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>
  
   <activity name="searchAssign" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsAssignListValue" />
    <property name="sqlkey" 				value="tbdb_grd_assign_fb002" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="MAT_CD" />
    <property name="dao" 					value="candao" />
   </activity>
  
   <!-- 경주등급별 배정 저장 -->                 
  <activity name="saveGrdAssign" 		    class="snis.can.cyclestd.d02000021.activity.SaveGrdAssign">
    <transition name="success" 	        	value="end" />
  </activity> 
  	 
  	<transaction-manager id="tx_can" commit="true" />
</service>