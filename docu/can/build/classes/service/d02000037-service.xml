<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000037" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
		<property name="staticResultKeyFile" value="resultkey.properties" />
		<property name="resultKeyList" value="dsOutCConfirmRecord" />
		<transition name="success" value="EventRouter" />
	</activity>
	
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
		<transition name="searchCConfirmRecord"   value="searchCConfirmRecord" />
		<transition name="saveCConfirmRecord"     value="saveCConfirmRecord" />
		<transition name="success"             	  value="end" />
	</activity>
	
	<!-- 기록조회 조회 -->
	<activity name="searchCConfirmRecord" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsOutCConfirmRecord" />
		<property name="sqlkey" value="tbec_cert_issu_history_fb001" />
		<property name="param-count" value="5" />
		<property name="dao" value="candao" />
		<property name="param0" value="RACE_YY"/>
		<property name="param1" value="CAND_NM"/>
		<property name="param2" value="CAND_NM"/>
		<property name="param3" value="GRD"/>
		<property name="param4" value="GRD"/>
	</activity>
	
	<!-- 기록조회 저장-->
	<activity name="saveCConfirmRecord" class="snis.can.cyclestd.d02000037.activity.SaveCConfirmRecord">
		<transition name="success" value="searchCConfirmRecord" />
	</activity>
	<transaction-manager id="tx_can" commit="true" />
</service>