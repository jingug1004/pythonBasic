<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000030" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
		<property name="staticResultKeyFile" value="resultkey.properties" />
		<property name="resultKeyList" value="dsOutCUnionGrade|dsRound" />
		<transition name="success" value="EventRouter" />
	</activity>
	
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
		<transition name="searchCUnionGrade"   value="searchCUnionGrade" />
		<transition name="searchRound"		   value="searchRound" />
		<transition name="saveCUnionGrade"     value="saveCUnionGrade" />
		<transition name="success"             value="end" />
	</activity>
	<!-- 회차 조회 -->
	<activity name="searchRound" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsRound" />
		<property name="sqlkey" value="tbdb_racer_grd_scr_fb002" />
		<property name="param-count" value="2" />
		<property name="dao" value="candao" />
		<property name="param0" value="YEAR"/>
		<property name="param1" value="YEAR"/>
	</activity>
	<!-- 선수등급별 고유점수 조회 -->
	<activity name="searchCUnionGrade" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsOutCUnionGrade" />
		<property name="sqlkey" value="tbdb_racer_grd_scr_fb001" />
		<property name="param-count" value="2" />
		<property name="dao" value="candao" />
		<property name="param0" value="STND_YEAR"/>
		<property name="param1" value="RaceRndNo"/>
	</activity>
	<!-- 선수등급별 고유점수 저장-->
	<activity name="saveCUnionGrade" class="snis.can.cyclestd.d02000030.activity.SaveCUnionGrade">
		<transition name="success" value="end" />
	</activity>
	<transaction-manager id="tx_can" commit="true" />
</service>