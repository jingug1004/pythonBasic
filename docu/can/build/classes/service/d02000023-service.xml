<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000023" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property   name="staticResultKeyFile" value="resultkey.properties" />
    <property   name="resultKeyList" value="dsYear1|dsDetlRaceScheValue|dsMatCode|dsMatGrd" />
    <transition name="success" value="EventRouter" />
  </activity> 
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceYY"      	    value="searchRaceYY" />
    <transition name="searchDetailRace"         value="searchDetailRace" />
    <transition name="searchMatCd"      	    value="searchMatCd" />
    <transition name="searchDetailMatGrd"       value="searchDetailMatGrd" />
    <transition name="saveDetlRace"       	    value="saveDetlRace" />
    <transition name="success" 					value="end" />
  </activity>
    <!-- 년도 콤보 설정 --> 
  <activity name="searchRaceYY"	 	       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsYear1" />
    <property name="sqlkey" 				value="tbdb_detl_race_fb001" />
    <property name="dao" 					value="candao" />
  </activity>
  <!-- 세부시행일정 조회 --> 
  <activity name="searchDetailRace"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsDetlRaceScheValue" />
    <property name="sqlkey" 				value="tbdb_detl_race_fb002" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="Raceyy" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 대진표 콤보 설정 --> 
  <activity name="searchMatCd" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsMatCode" />
    <property name="sqlkey" 				value="tbdb_detl_race_fb003" />
    <property name="dao" 					value="candao" />
  </activity>
      
  <!-- 세부시행일정 대진표 등급 조회 -->
   <activity name="searchDetailMatGrd"	 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsMatGrd" />
    <property name="sqlkey" 				value="tbdb_detl_race_fb004" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="Raceyy" />
    <property name="param1" 				value="RaceRndNo" />
    <property name="dao" 					value="candao" />
  </activity>
  <!-- 시행경주등급 저장 -->
  <activity name="saveDetlRace"			class="snis.can.cyclestd.d02000023.activity.SaveDetlRace">
    <transition name="success" 			value="end" />
  </activity>
  
  <transaction-manager id="tx_can" commit="true" />
</service>