<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000042" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
		<property name="staticResultKeyFile" value="resultkey.properties" />
    	<property name="resultKeyList" value="dsPerioNo|dsPerioExam1|dsPerioExam2|dsPerioExam3" />
    	<transition name="success" value="EventRouter" />
	</activity>
  
	<activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchPerioNo"		value="searchPerioNo" />
    	<transition name="searchPerioExam1"   	value="searchPerioExam1" />
    	<transition name="searchPerioExam2"   	value="searchPerioExam2" />
    	<transition name="searchPerioExam3"   	value="searchPerioExam3" />
    	<transition name="saveBodyCheck"      	value="saveBodyCheck" />
    	<transition name="success" 				value="end" />
  	</activity>
  	
  	<!-- 기수콤보 조회 -->
  	<activity name="searchPerioNo"		    	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsPerioNo" />
	    <property name="sqlkey" 				value="tbdb_body_make_mesur_fb001" />
	    <property name="dao" 					value="candao" />    
	</activity>
					
	<!-- 신체조성측정 조회 -->
  	<activity name="searchPerioExam1"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchPerioExam2" />
	    <property name="resultkey" 				value="dsPerioExam1" />
	    <property name="sqlkey" 				value="tbdb_body_make_mesur_fb002" />
	    <property name="param-count" 			value="8" />
	    <property name="param0" 				value="examCd" />
	    <property name="param1" 				value="examCd" />
	    <property name="param2" 				value="examCd" />
	    <property name="param3" 				value="examCd" />
	    <property name="param4" 				value="examCd" />
	    <property name="param5" 				value="examCd" />
	    <property name="param6" 				value="examCd" />
	    <property name="param7" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	
	<!-- 회차별 비교 조회 -->
	<activity name="searchPerioExam2"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchPerioExam3" />
	    <property name="resultkey" 				value="dsPerioExam2" />
	    <property name="sqlkey" 				value="tbdb_body_make_mesur_fb003" />
	    <property name="param-count" 			value="13" />
	    <property name="param0" 				value="bf_examCd" />
	    <property name="param1" 				value="cur_examCd" />
	    <property name="param2" 				value="bf_examCd" />
	    <property name="param3" 				value="cur_examCd" />
	    <property name="param4" 				value="bf_examCd" />
	    <property name="param5" 				value="cur_examCd" />
	    <property name="param6" 				value="bf_examCd" />
	    <property name="param7" 				value="cur_examCd" />
	    <property name="param8" 				value="bf_examCd" />
	    <property name="param9" 				value="cur_examCd" />
	    <property name="param10" 				value="bf_examCd" />
	    <property name="param11" 				value="cur_examCd" />
	    <property name="param12" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	
	<!-- 종합 비교 조회 -->
	<activity name="searchPerioExam3"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsPerioExam3" />
	    <property name="sqlkey" 				value="tbdb_body_make_mesur_fb004" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	
	<!-- 신체조성  저장 --> 
    <activity name="saveBodyCheck"			    class="snis.can.cyclestd.d02000042.activity.SaveBodyCheck">
      <transition name="success"				value="end" />
    </activity>
    	 
	<transaction-manager id="tx_can" commit="true" />
</service>