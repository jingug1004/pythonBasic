<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000034" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
		<property name="staticResultKeyFile" value="resultkey.properties" />
		<property name="resultKeyList" value="dsOutCPersonalRecord" />
		<transition name="success" value="EventRouter" />
	</activity>
	
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
		<transition name="searchCPersonalRecord"   value="searchCPersonalRecord" />
		<transition name="saveCPersonalRecord"     value="saveCPersonalRecord" />
		<transition name="success"             value="end" />
	</activity>
	
	<!-- 개인별기록평가 조회 -->
	<activity name="searchCPersonalRecord" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsOutCPersonalRecord" />
		<property name="sqlkey" value="tbdb_indv_rec_estm_fb001" />
		<property name="param-count" value="4" />
		<property name="dao" value="candao" />
		<property name="param0" value="FROM_DT"/>
		<property name="param1" value="TO_DT"/>
		<property name="param2" value="NM_KOR"/>
		<property name="param3" value="NM_KOR"/>
	</activity>
	
	<!-- 개인별기록평가 저장-->
	<activity name="saveCPersonalRecord" class="snis.can.cyclestd.d02000034.activity.SaveCPersonalRecord">
		<transition name="success" value="searchCPersonalRecord" />
	</activity>
	<transaction-manager id="tx_can" commit="true" />
</service>