<?xml version="1.0" encoding="EUC-KR"?>
<service name="d01000003" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsMenuListValue|dsMenuGrpValue" />
    <transition name="success" 				value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchGrpMenu"		value="searchGrpMenu" />       
    <transition name="searchMenu"			value="searchMenu" />
    <transition name="saveMenu"				value="saveMenu" />           
    <transition name="success" 				value="end" />
  </activity>

  <!-- 메뉴그룹 -->
  <activity name="searchGrpMenu" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchMenu" />
    <property name="resultkey" 				value="dsMenuGrpValue" />
    <property name="sqlkey" 				value="tbda_mn_fa001" />    
    <property name="dao" 					value="candao" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="MN_ID" />      
  </activity>
  
  <!-- 메뉴 List -->
  <activity name="searchMenu" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsMenuListValue" />
    <property name="sqlkey" 				value="tbda_mn_fa002" />    
    <property name="dao" 					value="candao" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="UP_MN_ID" />     
  </activity>

  <!-- 메뉴 Insert -->
  <activity name="saveMenu" 				class="snis.can.system.d01000003.activity.SaveMenu">
    <transition name="success" 				value="end" />
  </activity>      
  
  <transaction-manager id="tx_can" 			commit="true" />
</service>