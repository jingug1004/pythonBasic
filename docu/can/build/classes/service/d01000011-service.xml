<?xml version="1.0" encoding="EUC-KR"?>
<service name="d01000011" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsDeptMove" />
    <transition name="success" 				value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchUser"			value="searchUser" />    	<transition name="saveDeptMove"         value="saveDeptMove" />  <!-- 부서이동권한 저장  --> 
        <transition name="success" 				value="end" />
  </activity>

  <!-- 메뉴그룹 -->    <activity name="searchUser" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success" 				value="end" />    <property name="resultkey" 				value="dsDeptMove" />    <property name="sqlkey" 				value="d01000011_s01" />        <property name="dao" 					value="candao" />    <property name="param-count" 			value="2" />    <property name="param0" 				value="TEAM_CD" />        <property name="param1" 				value="USER_NM" />   </activity>    <!-- 부서이동권한 저장  -->   <activity name="saveDeptMove"             class="snis.can.system.d01000011.activity.SaveDeptMove">    <transition name="success"              value="end" />  </activity>    
  
  <transaction-manager id="tx_can" 			commit="true" />
</service>