<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000045" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
		<property name="staticResultKeyFile" value="resultkey.properties" />
    	<property name="resultKeyList" value="dsPerioExam1|dsPerioExam2|dsPerioExam3|dsPerioExamSum" />
    	<transition name="success" value="EventRouter" />
	</activity>
  
	<activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchPerioExam"   	value="searchPerioExam" />
    	<transition name="savePerioExam"      	value="savePerioExam" />
    	<transition name="success" 				value="end" />
  	</activity>

	<!-- 입소시험  조회 --> 
    <activity name="searchPerioExam"			class="snis.can.cyclestd.d02000045.activity.SearchPerioExam">
      <transition name="success"				value="end" />
    </activity>

	<!-- 입소시험  저장 --> 
    <activity name="savePerioExam"			    class="snis.can.cyclestd.d02000045.activity.SavePerioExam">
      <transition name="success"				value="end" />
    </activity>
    	 
	<transaction-manager id="tx_can" commit="true" />
</service>