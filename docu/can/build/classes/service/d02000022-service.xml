<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000022" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
		<property name="staticResultKeyFile" value="resultkey.properties" />
		<property name="resultKeyList" value="dsOutCStartTimeLength" />
		<transition name="success" value="EventRouter" />
	</activity>
	
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
		<transition name="searchCStartTimeLength"   value="searchCStartTimeLength" />
		<transition name="saveCStartTimeLength"     value="saveCStartTimeLength" />
		<transition name="success"          		value="end" />
	</activity>
	
	<!-- 경주출발시간 및 거리 조회 -->
	<activity name="searchCStartTimeLength" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsOutCStartTimeLength" />
		<property name="sqlkey" value="tbdb_strt_time_dist_fb001" />
		<property name="param-count" value="4" />
		<property name="dao" value="candao" />
		<property name="param0" value="STND_YEAR"/>
		<property name="param1" value="STND_MONTH"/>
		<property name="param2" value="AM_PM_GBN"/>
		<property name="param3" value="AM_PM_GBN"/>
	</activity>
	
	<!-- 경주출발시간 및 거리 저장-->
	<activity name="saveCStartTimeLength" class="snis.can.cyclestd.d02000022.activity.SaveCStartTimeLength">
		<transition name="success" value="searchCStartTimeLength" />
	</activity>
	<transaction-manager id="tx_can" commit="true" />
</service>