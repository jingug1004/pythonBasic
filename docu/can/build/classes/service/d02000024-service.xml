<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000024" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsCfmFg|dsRaceYy|dsDetlRaceScheValue|dsMatGrd" />
    <transition name="success" value="EventRouter" />
  </activity> 
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceYY"      	   		value="searchRaceYY" />
    <transition name="searchDetailRace"      	    value="searchDetailRace" />
    <transition name="searchDetailMatGrd"      	    value="searchDetailMatGrd" /> 
    <transition name="searchCandAssign"      	    value="searchCandAssign" />
    <transition name="saveCandAssign"      	   	 	value="saveCandAssign" />
    <transition name="searchCfmFg"            	value="searchCfmFg" />
    <transition name="success" 					value="end" />
  </activity>
    <activity name="searchRaceYY"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRaceYy" />
    <property name="sqlkey" 				value="tbdb_cand_assign_fb001" />
    <property name="dao" 					value="candao" />
  </activity>
    <!-- 세부시행일정 조회 --> 
  <activity name="searchDetailRace"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchDetailMatGrd" />
    <property name="resultkey" 				value="dsDetlRaceScheValue" />
    <property name="sqlkey" 				value="tbdb_cand_assign_fb002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="Raceyy" />
    <property name="param1" 				value="RaceRndNo" />
    <property name="dao" 					value="candao" />
  </activity>
    <!-- 세부시행일정 대진표 등급 조회 -->
   <activity name="searchDetailMatGrd"	 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsMatGrd" />
    <property name="sqlkey" 				value="tbdb_cand_assign_fb003" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="Raceyy" />
    <property name="param1" 				value="RaceRndNo" />
    <property name="dao" 					value="candao" />
   </activity>
     <!-- 확정플래그 조회 --> 
  <activity name="searchCfmFg"	 	        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCfmFg" />
    <property name="sqlkey" 				value="tbdb_cand_assign_cfm" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="RACE_YY" />
    <property name="param1" 				value="ROUND" />
    <property name="dao" 					value="candao" />
  </activity>
    <!-- 후보생  조회 -->
  <activity name="searchCandAssign"			class="snis.can.cyclestd.d02000024.activity.SearchCandAssign">
    <transition name="success" 				value="end" />
  </activity>
    <activity name="saveCandAssign"			class="snis.can.cyclestd.d02000024.activity.SaveCandAssign">
    <transition name="success" 				value="end" />
  </activity>
    <transaction-manager id="tx_can" commit="true" />
</service>