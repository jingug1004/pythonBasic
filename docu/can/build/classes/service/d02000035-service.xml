<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000035" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
		<property name="staticResultKeyFile" value="resultkey.properties" />
		<property name="resultKeyList" value="dsOutBatch" />
		<transition name="success" value="EventRouter" />
	</activity>
	
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
		<transition name="saveBatch1"     value="saveBatch1" />
		<transition name="saveBatch2"     value="saveBatch2" />
		<transition name="saveBatch3"     value="saveBatch3" />
		<transition name="saveBatch4"     value="saveBatch4" />
		<transition name="saveBatch5"     value="saveBatch5" />
		<transition name="saveBatch6"     value="saveBatch6" />
		<transition name="saveBatch7"     value="saveBatch7" />
		<transition name="saveBatch8"     value="saveBatch8" />
		<transition name="saveBatch9"     value="saveBatch9" />
		<transition name="saveBatch10"     value="saveBatch10" />
		<transition name="success"        value="end" />
	</activity>
	
	
	<!-- 년도별 집계자료를  최초생성-->
	<activity name="saveBatch1" class="snis.can.cyclestd.d02000035.activity.SaveBatch1Record">
		  <transition name="success" value="end" />
	</activity>
	<!--출주횟수 집계자료  생성-->
	<activity name="saveBatch2" class="snis.can.cyclestd.d02000035.activity.SaveBatch2Record">
		  <transition name="success" value="end" />
	</activity>
	
	<!--착순/낙차 집계자료  생성-->
	<activity name="saveBatch3" class="snis.can.cyclestd.d02000035.activity.SaveBatch3Record">
		  <transition name="success" value="end" />
	</activity>
	
	<!--승률/연대율 집계자료  생성-->
	<activity name="saveBatch4" class="snis.can.cyclestd.d02000035.activity.SaveBatch4Record">
		  <transition name="success" value="end" />
	</activity>
	
	<!--총순위득점 집계자료  생성-->
	<activity name="saveBatch5" class="snis.can.cyclestd.d02000035.activity.SaveBatch5Record">
		  <transition name="success" value="end" />
	</activity>
	
	<!--위반행위 집계자료  생성-->
	<activity name="saveBatch6" class="snis.can.cyclestd.d02000035.activity.SaveBatch6Record">
		  <transition name="success" value="end" />
	</activity>
	
	<!--선두유도 횟수 집계자료  생성-->
	<activity name="saveBatch7" class="snis.can.cyclestd.d02000035.activity.SaveBatch7Record">
		  <transition name="success" value="end" />
	</activity>
	
	<!--마지막 기어배수 집계자료  생성-->
	<activity name="saveBatch8" class="snis.can.cyclestd.d02000035.activity.SaveBatch8Record">
		  <transition name="success" value="end" />
	</activity>
	
	<!--전회차 집계자료  생성-->
	<activity name="saveBatch9" class="snis.can.cyclestd.d02000035.activity.SaveBatch9Record">
		  <transition name="success" value="end" />
	</activity>
	<!--종합-->
	<activity name="saveBatch10" class="snis.can.cyclestd.d02000035.activity.SaveBatch10Record">
		  <transition name="success" value="end" />
	</activity>
	<transaction-manager id="tx_can" commit="true" />
</service>