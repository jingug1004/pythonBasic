+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	// 조회 후
	if ( sServiceName == "d02000050-service:searchCValuation" ) {
	    if(dsOutCValuation.GetRowCount() > 0){
			fnc_SetStatus(SNIS_CAN_0003, dsOutCValuation.GetRowCount());
	    }
	}
	// 저장 후
	if ( sServiceName == "d02000050-service:saveCValuation" ) {
		if(dsOutCValuation.GetRowCount() > 0){
			fnc_SetStatus(SNIS_CAN_0003, dsOutCValuation.GetRowCount());
	    }
	}
	
	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

