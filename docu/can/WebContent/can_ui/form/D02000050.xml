//row 변경시 
function dsOutCValuation_OnRowPosChanged(obj,nOldRow,nRow)
{
	LATEST_ROW_POS = nOldRow;
	if (dsOutCValuation.GetRowType(nRow) == "insert")
	{
		cboRaceLen.Enable	= true;
		mskFromTime.Enable 	= true;
		mskToTime.Enable 	= true;
	}
	else
	{
		cboRaceLen.Enable	= false;
		mskFromTime.Enable 	= false;
		mskToTime.Enable 	= false;
	}
	fcSetMask();
}


//DataSet 정상  로드시  (저장후 조회시 저장시점으 Rowposition유지)
function dsOutCValuation_OnLoadCompleted(obj,nErrorCode,strErrorMsg,nReason)
{
	fnc_ProgressClose(); //팝업창이 Close안되었으면 Close시킨다.
	if(LATEST_ROW_POS > 0) dsOutCValuation.Row = LATEST_ROW_POS;
	dsOutCValuation_OnRowPosChanged(obj, LATEST_ROW_POS, LATEST_ROW_POS);
}

function dsOutCValuation_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	fcSetMask();
}
function fcSetMask()
{
	var sRACELEN = dsOutCValuation.GetColumn(dsOutCValuation.currow, "RACELEN");

	switch (sRACELEN)
	{
		case "1000":
			mskToTime.Mask = "#:##:##";
			mskFromTime.Mask = "#:##:##";
			break;
		case "400":
		case "200":
			mskToTime.Mask = "##:##";
			mskFromTime.Mask = "##:##";
			break;
	}
}

