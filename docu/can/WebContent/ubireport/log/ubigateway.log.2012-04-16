                  AND MEET_CD = '001'     
                  AND STND_YEAR = ?   
                  AND TMS   <= ?   
                  AND SELL_CD IN ('02','04')              
                  GROUP BY MEET_CD,SELL_CD                        
                  UNION                                   
                   SELECT    
                   MEET_CD,                                 
                   SELL_CD                               
                   ,0 N_AMT                              
                   ,SUM(NET_AMT) B_AMT                   
                   ,0 L_AMT                              
                  FROM VW_PC_PAYOFFS                    
                  WHERE 1=1
                  AND MEET_CD = '001'     
                  AND STND_YEAR = '2011'  
                  AND TMS   <= ?   
                  AND SELL_CD IN ('02','04')              
                  GROUP BY MEET_CD,SELL_CD                        
                  UNION                                   
                   SELECT   
                   MEET_CD,                                  
                   SELL_CD                               
                   ,0 N_AMT                              
                   ,0 B_AMT                              
                   ,SUM(NET_AMT) L_AMT                   
                  FROM VW_PC_PAYOFFS                    
                  WHERE 1=1                               
                  AND STND_YEAR = (                       
                   SELECT DISTINCT STND_YEAR             
                   FROM VW_SDL_INFO                      
                   WHERE 1=1
                   AND STND_YEAR =?  -1  
                  )
                  AND MEET_CD = '001'         
