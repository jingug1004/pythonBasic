         END TDIV_NO,                                                                              
         SUM(N_DIV_TOTAL) N_DIV_TOTAL,                                                                                                                         
         SUM(L_DIV_TOTAL) L_DIV_TOTAL                                                              
       FROM                                                                                          
       (                                                                                             
        SELECT                                                                                      
         A.MEET_CD,                                                                                
         A.SELL_CD,                                                                                
         A.COMM_NO,                                                                                
         A.DIV_NO,                                                                                 
         A.DIV_TOTAL N_DIV_TOTAL,                                                                                                                                            
         0 L_DIV_TOTAL                                                                             
        FROM VW_SDL_DT_SUM A, (       
         SELECT STND_YEAR, MEET_CD, TMS, DAY_ORD, RACE_YOIL                                        
         FROM VW_SDL_INFO                                                                          
         WHERE 1=1                                                                    
