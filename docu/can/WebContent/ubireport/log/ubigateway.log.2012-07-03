     ,NVL(ROUND( N_DIV_TOTAL / ?  ,0),0)  N_DIV_TOTAL 
     ,NVL(ROUND( L_DIV_TOTAL / (SELECT MAX(TMS) 
                                         FROM VW_SDL_INFO
