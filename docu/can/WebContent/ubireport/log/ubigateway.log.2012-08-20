[I][UbiData][2012-08-20 08:47:29]	- Sql
<
SELECT S.TDIV_NO,   
        C.CD_NM AS BRNCH_NM,                         
        ROUND(S.CURR_AMT/? /1000000,0) AS N_DIV_TOTAL,
        ROUND(S.PREV_AMT/S.LAST_MAX_TMS/1000000,0) AS L_DIV_TOTAL,
        ORD_NO AS NO_ORDER
 FROM  (
 
         SELECT S.TDIV_NO,
                SELL_CD,
                SUM(DECODE(STND_YEAR, ? , NET_AMT_SUM)) AS CURR_AMT,
                SUM(DECODE(STND_YEAR, ?  -1, NET_AMT_SUM)) AS PREV_AMT,
                (SELECT MAX(INF.TMS) FROM VW_SDL_INFO INF WHERE INF.STND_YEAR=?  -1 AND INF.MEET_CD ='001') AS LAST_MAX_TMS
         FROM  (       
                 
                 SELECT STND_YEAR, 
                        '060' AS SELL_CD,
                        CASE WHEN SELL_CD IN ('01','03') AND COMM_NO = '06' THEN '06' 
                             WHEN SELL_CD IN ('01','03') AND COMM_NO < '11' THEN '01'    
                             ELSE COMM_NO
                        END AS TDIV_NO,                                    
                        SUM(NET_AMT)*100 AS NET_AMT_SUM                                            
                 FROM   VW_PC_SATLIT_SUM                                                                                  
                 WHERE 1 = 1                                                                                         
                   AND MEET_CD = '001'           
                   AND STND_YEAR = ?         
                   AND TMS      <= ?         
                 GROUP BY STND_YEAR, SELL_CD, 
                          CASE WHEN SELL_CD IN ('01','03') AND COMM_NO = '06' THEN '06' 
                             WHEN SELL_CD IN ('01','03') AND COMM_NO < '11' THEN '01'    
                             ELSE COMM_NO
                          END  
                 UNION ALL           
                 
                 SELECT STND_YEAR, 
                        '060' AS SELL_CD,
                        CASE WHEN SELL_CD IN ('01','03') AND COMM_NO = '06' THEN '06' 
                             WHEN SELL_CD IN ('01','03') AND COMM_NO < '11' THEN '01'    
                             ELSE COMM_NO
                        END AS TDIV_NO,                                    
                        SUM(NET_AMT)*100 AS NET_AMT_SUM                                            
                 FROM   VW_PC_SATLIT_SUM                                                                                  
                 WHERE 1 = 1                                                                                         
                   AND MEET_CD = '001'           
                   AND STND_YEAR = ?  -1     
                 GROUP BY STND_YEAR, SELL_CD, 
                          CASE WHEN SELL_CD IN ('01','03') AND COMM_NO = '06' THEN '06' 
                             WHEN SELL_CD IN ('01','03') AND COMM_NO < '11' THEN '01'    
                             ELSE COMM_NO
                          END  
                 UNION ALL
                 
                 SELECT STND_YEAR,
                        '063' AS SELL_CD,
                        SELL_CD AS COMM_NO,
                        SUM(NET_AMT) NET_AMT_SUM                   
                 FROM   VW_PC_PAYOFFS                    
                 WHERE  MEET_CD = '001'     
                   AND  STND_YEAR = ?         
                   AND  TMS      <= ?         
                   AND  SELL_CD IN ('02','04')              
                 GROUP BY STND_YEAR, SELL_CD
                 UNION ALL                                   
                 
                 SELECT STND_YEAR,
                        '063' AS SELL_CD,
                        SELL_CD AS COMM_NO,
                        SUM(NET_AMT) NET_AMT_SUM                                 
                 FROM  VW_PC_PAYOFFS                    
                 WHERE MEET_CD = '001'     
                   AND STND_YEAR = ?  -1        
                   AND SELL_CD IN ('02','04')              
                 GROUP BY STND_YEAR, SELL_CD
               ) S
         GROUP BY S.TDIV_NO, SELL_CD    
                           
       ) S, TBRK_SPEC_CD C
 WHERE C.GRP_CD = S.SELL_CD
   AND S.TDIV_NO = C.CD       
 ORDER BY ORD_NO
 #String#32#String#2012#String#2012#String#2012#String#2012#String#32#String#2012#String#2012#String#32#String#2012#
>
