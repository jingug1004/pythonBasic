<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000044" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPerioNo|dsUseGbnItem|dsUseItem|dsSeltChng|dsCycleRec|dsPerioExam2" />
    <transition name="success" value="EventRouter" />
  </activity>
  <activity name="EventRouter" 				     class="com.posdata.glue.biz.activity.PosDefaultRouter">  
    <transition name="searchPerioNo"             value="searchPerioNo" />
    <transition name="searchUseGbnItem"		     value="searchUseGbnItem" />
    <transition name="searchUseItem"			 value="searchUseItem" />
    <transition name="searchSeltChng"			 value="searchSeltChng" />
    <transition name="searchCycleRec"			 value="searchCycleRec" />
    <transition name="savecyclerec"				 value="savecyclerec" />
    <transition name="success" 				     value="end" />
  </activity>
    <!-- 기수 콤보 설정 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdb_cyclerec_f001" />
    <property name="dao" 					value="candao" />
  </activity>
  <!-- 종목구분사용여부-->
  <activity name="searchUseGbnItem"		   class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	<transition name="success" 				value="searchUseItem" />
    <property name="resultkey" 				value="dsUseGbnItem" />
    <property name="sqlkey" 				value="tbdb_cyclerec_f002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="examcd" />
    <property name="dao" 					value="candao" />    
  </activity>
	<!-- 종목사용여부-->
  	<activity name="searchUseItem"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchSeltChng" />
    <property name="resultkey" 				value="dsUseItem" />
    <property name="sqlkey" 				value="tbdb_cyclerec_f003" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="examcd" />
    <property name="dao" 					value="candao" />    
 </activity>
 	<activity name="searchSeltChng"		  	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	<transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsSeltChng" />
    <property name="sqlkey" 				value="tbdb_cyclerec_f005" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="examcd" />
    <property name="dao" 					value="candao" />    
	</activity>
 <activity name="searchCycleRec"		  	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	<transition name="success" 				value="searchCycleStat" />
    <property name="resultkey" 				value="dsCycleRec" />
    <property name="sqlkey" 				value="tbdb_cyclerec_f004" />
    <property name="param-count" 			value="13" />
    <property name="param0" 				value="examcd" />
    <property name="param1" 				value="examcd" />
    <property name="param2" 				value="examcd" />
    <property name="param3" 				value="examcd" />
    <property name="param4" 				value="examcd" />
    <property name="param5" 				value="examcd" />
    <property name="param6" 				value="examcd" />
    <property name="param7" 				value="examcd" />
    <property name="param8" 				value="examcd" />
    <property name="param9" 				value="examcd" />
    <property name="param10" 				value="examcd" />
    <property name="param11" 				value="examcd" />
    <property name="param12" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
   <activity name="searchCycleStat"		  	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	<transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioExam2" />
    <property name="sqlkey" 				value="tbdb_cyclerec_f006" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
  <!-- 1,2차 기록평가  자전거 실기내역 저장 --> 
   <activity name="savecyclerec"	       class="snis.can.cyclestd.d02000044.activity.SaveCycleRec"> 
   <transition name="success"	           value="end" /> 
   </activity> 
	<transaction-manager id="tx_can" commit="true" />
</service>