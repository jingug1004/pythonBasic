<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000008" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsCandNo|dsPerioNo|dsUseGbnItem|dsUseItem|dsSndExamRateBas|dsSeltChng|dsSndSeltExam|dsSndSeltExam1|dsSndSeltExam2|dsSndSeltExam3|dsSndSeltExam4" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				     class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPerioNo"             value="searchPerioNo" />
    <transition name="searchUseGbnItem"		     value="searchUseGbnItem" />
    <transition name="searchUseItem"			 value="searchUseItem" />
    <transition name="searchSeltChng"   		 value="searchSeltChng" />
    <transition name="searchChngRate"   		 value="searchChngRate" />
    <transition name="searchSndSeltExam"         value="searchSndSeltExam" />
    <transition name="searchSndSeltExam1"   	 value="searchSndSeltExam1" />
    <transition name="searchSndSeltExam2"        value="searchSndSeltExam2" />
    <transition name="searchSndSeltExam3"        value="searchSndSeltExam3" />
    <transition name="searchSndSeltExam4"        value="searchSndSeltExam4" />
    <transition name="searchCandCnt"             value="searchCandCnt" />
    <transition name="saveSndSeltExam"           value="saveSndSeltExam" /> 
    <transition name="saveSndyn"      	   	     value="saveSndyn" />
    <transition name="saveCandNo"      	   	     value="saveCandNo" />
    <transition name="success" 				     value="end" />
   </activity>
  <!-- 기수 콤보 설정 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdb_weeks_abs_fb001" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>
  <!-- 종목구분사용여부-->
  	<activity name="searchUseGbnItem"		   class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchUseItem" />
	    <property name="resultkey" 				value="dsUseGbnItem" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb001" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	<!-- 종목사용여부-->
  	<activity name="searchUseItem"		    	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchChngRate" />
	    <property name="resultkey" 				value="dsUseItem" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb002" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	<!-- 환산비율 조회 -->
  	<activity name="searchChngRate"		        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchSeltChng" />
	    <property name="resultkey" 				value="dsSndExamRateBas" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb003" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	<!-- 배점기준 -->
	<activity name="searchSeltChng"		  	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchCandCnt" />
	    <property name="resultkey" 				value="dsSeltChng" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb004" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>	
  <!-- 년도 선수수 조회 --> 
  <activity name="searchCandCnt"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandNo" />
    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb010" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>
  <!-- 2차선발시험 자전거 실기조회  --> 
  <activity name="searchSndSeltExam"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchSndSeltExam1" />
    <property name="resultkey" 				value="dsSndSeltExam" />
    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb005" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />
  </activity> 
  <!-- 2차 선발 체력측정  조회 -->
  	<activity name="searchSndSeltExam1"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchSndSeltExam2" />
	    <property name="resultkey" 				value="dsSndSeltExam1" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb006" />
	    <property name="param-count" 			value="6" />                           
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="param1" 				value="racerPerioNo" />
	    <property name="param2" 				value="racerPerioNo" />
	    <property name="param3" 				value="racerPerioNo" />
	    <property name="param4" 				value="racerPerioNo" />
	    <property name="param5" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
  <!-- 2차선발시험 필기  조회  --> 
  <activity name="searchSndSeltExam2"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchSndSeltExam3" />
    <property name="resultkey" 				value="dsSndSeltExam2" />
    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb007" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 2차선발시험 면접  조회  --> 
  <activity name="searchSndSeltExam3"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchSndSeltExam4" />
    <property name="resultkey" 				value="dsSndSeltExam3" />
    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb008" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>    
     
<!-- 2차선발시험 성적집계  조회  --> 
  <activity name="searchSndSeltExam4"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsSndSeltExam4" />
    <property name="sqlkey" 				value="tbdb_perio_exam_Snd_fb009" />
    <property name="param-count" 			value="5" />    
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="racerPerioNo" />    
    <property name="param2" 				value="racerPerioNo" />
    <property name="param3" 				value="racerPerioNo" />    
    <property name="param4" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>     
 <!-- 2차선발시험  실기내역 저장 --> 
   <activity name="saveSndSeltExam"	       class="snis.can.cyclestd.d02000008.activity.SaveSndSeltExam"> 
   <transition name="success"	           value="end" /> 
   </activity> 
 <!-- 2차선발합격여부 저장 --> 
    <activity name="saveSndyn"					class="snis.can.cyclestd.d02000008.activity.SaveSndYn">
      <transition name="success"				value="end" />
    </activity>	
 <!-- 합격자 신상 저장 --> 
    <activity name="saveCandNo"					class="snis.can.cyclestd.d02000008.activity.SaveCandNo">
      <transition name="success"				value="end" />
    </activity>	      
	<transaction-manager id="tx_can" commit="true" />
</service>
