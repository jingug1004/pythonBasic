<?xml version="1.0" encoding="EUC-KR"?>
<service name="d03000001" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsCandDormAssign|dsCandIdent|dsCandNo|dsPerioNo|dsSearchBuild|dsSearchRoomNo|dsPK" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				    class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchCandDormAssign"		value="searchCandDormAssign" /> 
    <transition name="searchCandIdent"		    value="searchCandIdent" />  	
    <transition name="searchCandIdentComBo"	    value="searchCandIdentComBo" />  	
    <transition name="searchPerioNo"            value="searchPerioNo" />      
    <transition name="searchBuildS"         	value="searchBuildS" />   
    <transition name="searchRoomNoS"       		value="searchRoomNoS" />   
    <transition name="searchPK"      			value="searchPK" />     
    <transition name="saveCandDormAssign"	   	value="saveCandDormAssign" />
    <transition name="success" 				    value="end" />
  </activity>

  <!-- 경륜후보생기숙사배정 조회 -->
  <activity name="searchCandDormAssign"	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandDormAssign" />
    <property name="sqlkey" 				value="tbdc_cand_dorm_assign_fb000" />
    <property name="param-count" 			value="5" />  
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="build" />
   	<property name="param2" 				value="roomNo" />  
   	<property name="param3" 				value="candNo" />  
    <property name="param4" 				value="racerPerioNo" />     	        
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 경륜후보생기숙사배정 조회 -->
  <activity name="searchCandDormAssign1"	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandDormAssign" />
    <property name="sqlkey" 				value="tbdc_cand_dorm_assign_fb001" />
    <property name="param-count" 			value="4" />  
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="build" />
   	<property name="param2" 				value="roomNo" />  
   	<property name="param3" 				value="candNo" />          
    <property name="dao" 					value="candao" />    
  </activity>


  <!-- 경륜호부생신상 조회 -->
  <activity name="searchCandIdent" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandIdent" />
    <property name="sqlkey" 				value="tbdb_CandIdent_fb001" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />    
    <property name="dao" 					value="candao" />    
  </activity> 

  <!-- 경륜호부생신상 콤보 조회 --> 
  <activity name="searchCandIdentComBo"	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandNo" />
    <property name="sqlkey" 				value="tbdb_CandIdent_fb002" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>

    
  <!-- 기수콤보 조회 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdc_cand_dorm_assign_fb005" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 동(검색) 조회 --> 
  <activity name="searchBuildS"	        	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsSearchBuild" />
    <property name="sqlkey" 				value="tbdc_cand_dorm_assign_fb002" />
    <property name="param-count" 			value="0" />>
    <property name="dao" 					value="candao" />
  </activity> 
  

  <!-- 호실(검색) 조회 --> 
  <activity name="searchRoomNoS"        	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsSearchRoomNo" />
    <property name="sqlkey" 				value="tbdc_cand_dorm_assign_fb003" />
    <property name="param-count" 			value="0" />>
    <property name="dao" 					value="candao" />
  </activity>  
    
  <!-- PK 조회 --> 
  <activity name="searchPK"        			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPK" />
    <property name="sqlkey" 				value="tbdc_cand_dorm_assign_fb004" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />    
    <property name="dao" 					value="candao" />
  </activity>

  <!-- PK 조회 --> 
  <activity name="searchBuildRoomNo"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPK" />
    <property name="sqlkey" 				value="tbdc_cand_dorm_assign_fb004" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity> 

  <!-- 경륜반편성 저장 --> 
  <activity name="saveCandDormAssign"	   class="snis.can.cyclestd.d03000001.activity.SaveSetRoom">
    <transition name="success"	value="end" />
  </activity>
	
	<transaction-manager id="tx_can" commit="true" />
</service>