<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02009005" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPerioNo|dsPlaceList|dsOutPlaceList|dsRacerRate|dsReceipt1|dsReceipt2|dsReceipt3|dsReceipt4|dsReceipt5" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPerioNo"		value="searchPerioNo" />
    <transition name="searchReceiptStatus"	value="searchReceiptStatus" /> 
    <transition name="searchPlaceList"		value="searchPlaceList" /> 
    <transition name="searchPlaceList2"		value="searchPlaceList2" /> 
    <transition name="searchPlaceList3"		value="searchPlaceList3" />     
    <transition name="success" 				value="end" />
  </activity>

  <!-- 기수 조회 -->
  <activity name="searchPerioNo"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb001" />
    <property name="dao" 					value="candao" />    
  </activity>
  <!-- 응시원서접수현황 조회 -->
  <activity name="searchReceiptStatus"	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchRacerYN" />
    <property name="resultkey" 				value="dsReceipt1" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb010" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
  <!-- 선수/비선수 출신 분석 조회 -->
  <activity name="searchRacerYN"	  		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchAgeAnalysis" />
    <property name="resultkey" 				value="dsReceipt2" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb011" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
  <!-- 연령별 분석 조회 -->
  <activity name="searchAgeAnalysis"   		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchSchoolAnalysis" />
    <property name="resultkey" 				value="dsReceipt3" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb012" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
  <!-- 학력별 분석 조회 -->
  <activity name="searchSchoolAnalysis"   	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchAreaAnalysis" />
    <property name="resultkey" 				value="dsReceipt4" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb013" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
  <!-- 지역별 분석 조회 -->
  <activity name="searchAreaAnalysis"   	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsReceipt5" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb014" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>     
  <!-- 선발접수자 조회 -->
  <activity name="searchPlaceList"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPlaceList" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb002" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
    <!-- 선발접수자 조회2 -->
  <activity name="searchPlaceList2"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsOutPlaceList" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb003" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
     <!-- 선수 비율  조회 -->
  <activity name="searchPlaceList3"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRacerRate" />
    <property name="sqlkey" 				value="tbdb_plc_list_fb004" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
  <transaction-manager id="tx_can" commit="true" />
</service>