<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000005" initial="SetResultKeys">
  	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile" value="resultkey.properties" />
	    <property name="resultKeyList" value="dsPerioNo|dsSeltReptList|dsSeltReptDesc|dsFamList|dsEduList|dsActList|dsCarList|dsLicList|dsCandSeq|dsUploadFileList" />
	    <transition name="success" value="EventRouter" />
  	</activity>
  
  	<activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
	    <transition name="searchPerioNo"        value="searchPerioNo" />
	    <transition name="searchSeltRept"       value="searchSeltRept" />
	    <transition name="searchSeltReptDesc"	value="searchSeltReptDesc" />
	    <transition name="searchFamList"		value="searchFamList" />
	    <transition name="searchEduList"		value="searchEduList" />
	    <transition name="searchActList"		value="searchActList" />
	    <transition name="searchCarList"		value="searchCarList" />
	    <transition name="searchFileList"		value="searchFileList" />
	    <transition name="searchLicList"		value="searchLicList" />
	    <transition name="saveSeltRept"			value="saveSeltRept" />
	    <transition name="keyCandIdent"			value="keyCandIdent" />
	    <transition name="success" 				value="end" />
  	</activity>

 	<!-- 기수 콤보 설정 --> 
  	<activity name="searchPerioNo"	 	    	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsPerioNo" />
	    <property name="sqlkey" 				value="tbdb_selt_rept_fb001" />
	    <property name="dao" 					value="candao" />
  	</activity>
  
  	<!-- 후보생선발접수  기수별 조회 -->
  	<activity name="searchSeltRept"          	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsSeltReptList" />
	    <property name="sqlkey" 				value="tbdb_selt_rept_fb002" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="RACER_PERIO_NO" />
	    <property name="dao" 					value="candao" />    
  	</activity> 
  
 	<!-- 후보생선발접수  상세 조회 -->
  	<activity name="searchSeltReptDesc"  		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="searchFamList" />
	    <property name="resultkey" 				value="dsSeltReptDesc" />
	    <property name="sqlkey" 				value="tbdb_selt_rept_fb003" />
	    <property name="param-count" 			value="2" />
	    <property name="param0" 				value="RACER_PERIO_NO" />
	    <property name="param1" 				value="REPT_NO" />
	    <property name="dao" 					value="candao" />    
  	</activity>
  
  	<!-- 후보생선발접수  상세 조회 (가족)-->
  	<activity name="searchFamList"  			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="searchEduList" />
	    <property name="resultkey" 				value="dsFamList" />
	    <property name="sqlkey" 				value="tbdb_selt_fmly_fb001" />
	    <property name="param-count" 			value="2" />
	    <property name="param0" 				value="RACER_PERIO_NO" />
	    <property name="param1" 				value="REPT_NO" />
	    <property name="dao" 					value="candao" />    
  	</activity>
  
  	<!-- 후보생선발접수  상세 조회 (학력사항)-->
  	<activity name="searchEduList"  			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="searchActList" />
	    <property name="resultkey" 				value="dsEduList" />
	    <property name="sqlkey" 				value="tbdb_selt_schl_carr_fb001" />
	    <property name="param-count" 			value="2" />
	    <property name="param0" 				value="RACER_PERIO_NO" />
	    <property name="param1" 				value="REPT_NO" />
	    <property name="dao" 					value="candao" />    
  	</activity>
  
  	<!-- 후보생선발접수  상세 조회 (주요활동)-->
  	<activity name="searchActList"  			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="searchCarList" />
	    <property name="resultkey" 				value="dsActList" />
	    <property name="sqlkey" 				value="tbdb_selt_detl_carr_fb001" />
	    <property name="param-count" 			value="2" />
	    <property name="param0" 				value="RACER_PERIO_NO" />
	    <property name="param1" 				value="REPT_NO" />
	    <property name="dao" 					value="candao" />    
  	</activity>
  
   	<!-- 후보생선발접수  상세 조회 (경력사항)-->
  	<activity name="searchCarList"  			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="searchLicList" />
	    <property name="resultkey" 				value="dsCarList" />
	    <property name="sqlkey" 				value="tbdb_selt_carr_fb001" />
	    <property name="param-count" 			value="2" />
	    <property name="param0" 				value="RACER_PERIO_NO" />
	    <property name="param1" 				value="REPT_NO" />
	    <property name="dao" 					value="candao" />    
  	</activity>
  
   	<!-- 후보생선발접수  상세 조회 (자격면허)-->
  	<activity name="searchLicList"  			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="searchFileList" />
	    <property name="resultkey" 				value="dsLicList" />
	    <property name="sqlkey" 				value="tbdb_selt_cert_fb001" />
	    <property name="param-count" 			value="2" />
	    <property name="param0" 				value="RACER_PERIO_NO" />
	    <property name="param1" 				value="REPT_NO" />
	    <property name="dao" 					value="candao" />    
  	</activity>
     <!-- 첨부파일 목록  조회 -->
  <activity name="searchFileList"          class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsUploadFileList" />
    <property name="sqlkey" 				value="d02000005_fa002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="RACER_PERIO_NO" />
    <property name="param1" 				value="REPT_NO" />
    <property name="dao" 					value="candao" />    
  </activity>
	<!-- 후보생선발접수 저장 --> 
  	<activity name="saveSeltRept"            	class="snis.can.cyclestd.d02000005.activity.SaveCadChoice">
    	<transition name="success" 	           	value="end" />
	</activity> 	
	
	<!-- 후보생선발접수 접수번호 조회 -->
  	<activity name="keyCandIdent"  				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsCandSeq" />
	    <property name="sqlkey" 				value="d02000005_fb001" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="RACER_PERIO_NO" />
	    <property name="dao" 					value="candao" />    
  	</activity>

	<transaction-manager id="tx_can" commit="true" />
</service>