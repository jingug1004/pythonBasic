<?xml version="1.0" encoding="EUC-KR"?>
<service name="e01010061" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutProcess|dsInsertBoard" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchProcessProgList"	value="searchProcessProgList" />    
    <transition name="searchProcessProgView"	value="searchProcessProgView" />        
    <transition name="saveProcessProg"			value="saveProcessProg" />    
    <transition name="success" 					value="end" />
  </activity>
  
  <!-- 게시판 seqno가져오기 -->
  <activity name="searchProcessProgList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutProcess" />
    <property name="sqlkey" value="tbea_proc_prog_fa001" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="4" />
    <property name="param0" value="DEL_YN" />
    <property name="param1" value="SBJT" />    
    <property name="param2" value="BORD_DESC" />
    <property name="param3" value="SEQ_NO" />     
  </activity>  
  
  <!-- 게시판 seqno가져오기 -->
  <activity name="searchProcessProgView" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsInsertBoard" />
    <property name="sqlkey" value="tbea_proc_prog_fa001" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="4" />
    <property name="param0" value="DEL_YN" />
    <property name="param1" value="SBJT" />    
    <property name="param2" value="BORD_DESC" />
    <property name="param3" value="SEQ_NO" />     
  </activity>    
               
  <!-- 게시판 INSERT -->
  <activity name="saveProcessProg" class="snis.boa.system.e01010061.activity.SaveProcessProg">
    <transition name="success" value="end" />
  </activity>        
    
  <!-- 게시판 INSERT -->
  <activity name="saveUpdate" class="snis.boa.system.e01010061.activity.SaveProcessProg">
    <transition name="success" value="end" />
  </activity>       
      
  <transaction-manager id="tx_boa" commit="true" />
</service>