<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000050" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
		<property name="staticResultKeyFile" value="resultkey.properties" />
		<property name="resultKeyList" value="dsOutCValuation" />
		<transition name="success" value="EventRouter" />
	</activity>
	
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
		<transition name="searchCValuation"   value="searchCValuation" />
		<transition name="saveCValuation"     value="saveCValuation" />
		<transition name="success"             value="end" />
	</activity>
	
	<!-- 기록평가기준 조회 -->
	<activity name="searchCValuation" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsOutCValuation" />
		<property name="sqlkey" value="tbdb_indv_rec_basis_fb001" />
		<property name="param-count" value="0" />
		<property name="dao" value="candao" />
	</activity>
	
	<!-- 기록평가기준 저장-->
	<activity name="saveCValuation" class="snis.can.cyclestd.d02000050.activity.SaveCValuation">
		<transition name="success" value="searchCValuation" />
	</activity>
	<transaction-manager id="tx_can" commit="true" />
</service>