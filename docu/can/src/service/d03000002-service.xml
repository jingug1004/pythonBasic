<?xml version="1.0" encoding="EUC-KR"?>
<service name="d03000002" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsEquipSet|dsEquipDetl" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				     class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEquip"                value="searchEquip" />
    <transition name="searchEquipDetl1"           value="searchEquipDetl1" />
    <transition name="searchEquipDetl2"           value="searchEquipDetl2" />
    <transition name="saveEquipDetl"              value="saveEquipDetl" />
    <transition name="success" 				      value="end" /> 
   </activity>
   
  
<!-- 장비내역  콤보 셋팅  --> 
  <activity name="searchEquip"              class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsEquipSet" />
    <property name="sqlkey" 				value="tbdc_equip_detl_fc000" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>    
  
 
 <!-- 장비내역  조회1 --> 
  <activity name="searchEquipDetl1"         class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsEquipDetl" />
    <property name="sqlkey" 				value="tbdc_equip_detl_fc001" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="equipCd" />
    <property name="param1" 				value="entDd1" />
    <property name="param2" 				value="entDd2" />
    <property name="dao" 					value="candao" />
  </activity>  
  
  <!-- 장비내역  조회2 --> 
  <activity name="searchEquipDetl2"         class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsEquipDetl" />
    <property name="sqlkey" 				value="tbdc_equip_detl_fc002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="entDd1" />
    <property name="param1" 				value="entDd2" />
    <property name="dao" 					value="candao" />
  </activity>  


 <!-- 장비내역 저장 --> 
  <activity name="saveEquipDetl"	       class="snis.can.cyclestd.d03000002.activity.SaveEquipDetl">
    <transition name="success"	value="end" />
  </activity>
  
  <transaction-manager id="tx_can" commit="true" />
</service>