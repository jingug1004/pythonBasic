<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000052" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
		<property name="staticResultKeyFile" value="resultkey.properties" />
		<property name="resultKeyList" value="dsOutCUpDownLvl|dsOutStndYear|dsOutRound" />
		<transition name="success" value="EventRouter" />
	</activity>
	
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
		<transition name="searchCUpDownLvl"	value="searchCUpDownLvl" />
		<transition name="searchStndYear"	value="searchStndYear" />
		<transition name="searchRound"		value="searchRound" />
		<transition name="success"          value="end" />
	</activity>
	
	<!-- 년도 조회 -->
	<activity name="searchStndYear" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsOutStndYear" />
		<property name="sqlkey" value="tbdb_detl_race_sche_race_yy_fb001" />
		<property name="param-count" value="0" />
		<property name="dao" value="candao" />
	</activity>
	
	<!-- 회차 조회 -->
	<activity name="searchRound" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsOutRound" />
		<property name="sqlkey" value="tbdb_detl_race_sche_round_fb001" />
		<property name="param-count" value="1" />
		<property name="dao" value="candao" />
		<property name="param0" value="RACE_YY"/>
	</activity>
	
	<!-- 등급사정 조회 -->
	<activity name="searchCUpDownLvl" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
		<property name="resultkey" value="dsOutCUpDownLvl" />
		<property name="sqlkey" value="tbdb_grd_estm_bas_scr_fb002" />
		<property name="param-count" value="8" />
		<property name="dao" value="candao" />
		<property name="param0" value="DEFAULT_ROUND"/>
		<property name="param1" value="DEFAULT_ROUND"/>
		<property name="param2" value="DEFAULT_ROUND"/>
		<property name="param3" value="LOWEST_POINT"/>
		<property name="param4" value="DEFAULT_ROUND"/>
		<property name="param5" value="RACE_YY"/>
		<property name="param6" value="FROM_ROUND"/>
		<property name="param7" value="TO_ROUND"/>
	</activity>

	<transaction-manager id="tx_can" commit="true" />
</service>