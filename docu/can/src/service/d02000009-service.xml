<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000009" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPerioNo|dsCandIdentList|dsCandIdent|dsFmly|dsSchlCarr|dsCarr|dsInvwCntnt|dsRacerReln|dsRacerRelnSeq|dsSpecDesc|dsPrizPenalScrSeq|dsPrizPenalScr|dsItemCd|dsPrizPenalCd|dsPrizPenalScore|dsCandSeq" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				     class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPerioNo"             value="searchPerioNo" />
    <transition name="searchCandIdentList"       value="searchCandIdentList" />
    <transition name="searchCandIdent"           value="searchCandIdent" />
    <transition name="searchFmly"                value="searchFmly" />
    <transition name="searchSchlCarr"            value="searchSchlCarr" />
    <transition name="searchCarr"                value="searchCarr" />
    <transition name="searchInvwCntnt"           value="searchInvwCntnt" />
    <transition name="searchRacerReln"           value="searchRacerReln" />
    <transition name="searchRacerRelnSeq"        value="searchRacerRelnSeq" />
    <transition name="searchSpecDesc"            value="searchSpecDesc" />
    <transition name="searchPrizPenalScrSeq"     value="searchPrizPenalScrSeq" />
    <transition name="searchPrizPenalScr"        value="searchPrizPenalScr" />
    <transition name="searchPrizPenalScore"      value="searchPrizPenalScore" />
    <transition name="saveCandIdent"             value="saveCandIdent" />
    <transition name="saveFmly"                  value="saveFmly" />
    <transition name="saveSchlCarr"              value="saveSchlCarr" />
    <transition name="saveCarr"                  value="saveCarr" />
    <transition name="saveRacerReln"             value="saveRacerReln" />
    <transition name="saveInvwCntnt"             value="saveInvwCntnt" />
    <transition name="saveSpecDesc"              value="saveSpecDesc" />
    <transition name="savePrizPenalScr"          value="savePrizPenalScr" />
    <transition name="keyCandIdent"          	 value="keyCandIdent" />
    <transition name="deleteCandIdent"           value="deleteCandIdent" />
    <transition name="success" 				     value="end" />
   </activity>


  <!-- 기수 콤보 설정 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="d02000009_fb001" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>
  
  
  <!-- 후보생 리스트 조회 --> 
  <activity name="searchCandIdentList"	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandIdentList" />
    <property name="sqlkey" 				value="d02000009_fb002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="nmKor" />
    <property name="dao" 					value="candao" />
  </activity>
  
  
  <!-- 후보생 상세 조회 --> 
  <activity name="searchCandIdent"          class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchFmly" />
    <property name="resultkey" 				value="dsCandIdent" />
    <property name="sqlkey" 				value="d02000009_fb003" />
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />
    <property name="param2" 				value="racerPerioNo" />
    <property name="param3" 				value="candNo" />    
    <property name="dao" 					value="candao" />
  </activity>
  
  
  <!-- 후보생 가족사항 조회 --> 
  <activity name="searchFmly"               class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchSchlCarr" />
    <property name="resultkey" 				value="dsFmly" />
    <property name="sqlkey" 				value="d02000009_fb004" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />
    <property name="dao" 					value="candao" />
  </activity>
  
      
  
  <!-- 후보생 학력사항   조회 --> 
  <activity name="searchSchlCarr"           class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchCarr" />
    <property name="resultkey" 				value="dsSchlCarr" />
    <property name="sqlkey" 				value="d02000009_fb005" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />
    <property name="dao" 					value="candao" />
  </activity>
  
  
  <!-- 후보생 경력사항   조회 --> 
  <activity name="searchCarr"               class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchRacerReln" />
    <property name="resultkey" 				value="dsCarr" />
    <property name="sqlkey" 				value="d02000009_fb006" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
  
  
   
  
 <!-- 후보생 경륜선수와 관계  조회 --> 
  <activity name="searchRacerReln"          class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchInvwCntnt" />
    <property name="resultkey" 				value="dsRacerReln" />
    <property name="sqlkey" 				value="d02000009_fb007" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
 
 
 
 <!-- 후보생 면담사항   조회 --> 
  <activity name="searchInvwCntnt"          class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchSpecDesc" />
    <property name="resultkey" 				value="dsInvwCntnt" />
    <property name="sqlkey" 				value="d02000009_fb008" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
  
  <!-- 후보생 특이사항  조회 --> 
  <activity name="searchSpecDesc"           class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchPrizPenalScr" />
    <property name="resultkey" 				value="dsSpecDesc" />
    <property name="sqlkey" 				value="d02000009_fb010" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
  
  
  <!-- 후보생 상벌점사항   조회 --> 
  <activity name="searchPrizPenalScr"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPrizPenalScr" />
    <property name="sqlkey" 				value="d02000009_fb012" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
  
  <!-- 후보생 조항코드    조회 --> 
  <activity name="searchItemCd"             class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsItemCd" />
    <property name="sqlkey" 				value="d02000009_fb013" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
  
  <!-- 후보생 상벌코드    조회 --> 
  <activity name="searchPrizPenalCd"        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPrizPenalCd" />
    <property name="sqlkey" 				value="d02000009_fb014" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="itemCd" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
  
  
  <!-- 후보생 상벌점  조회 --> 
  <activity name="searchPrizPenalScore"     class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPrizPenalScore" />
    <property name="sqlkey" 				value="d02000009_fb015" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="itemCd" />
    <property name="param1" 				value="prizPenalCd" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
  
  <!-- 후보생 입력 번호 조회--> 
  <activity name="keyCandIdent"     		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandSeq" />
    <property name="sqlkey" 				value="d02000009_fb016" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" /> 
  </activity>
  
  
 
 <!-- 후보생 신상 내역 저장 --> 
  <activity name="saveCandIdent"	       class="snis.can.cyclestd.d02000009.activity.SaveCandIdent"> 
   <transition name="success"	           value="saveFmly" /> 
  </activity>
  
  <!-- 후보생 신상 내역삭제 --> 
  <activity name="deleteCandIdent"	       class="snis.can.cyclestd.d02000009.activity.SaveCandIdent"> 
   <transition name="success"	           value="end" /> 
  </activity> 
  
  <!-- 후보생 가족사항 저장 --> 
  <activity name="saveFmly"	               class="snis.can.cyclestd.d02000009.activity.SaveFmly"> 
   <transition name="success"	           value="saveSchlCarr" /> 
  </activity>
  
 
   <!-- 후보생 학력사항 저장 --> 
  <activity name="saveSchlCarr"	           class="snis.can.cyclestd.d02000009.activity.SaveSchlCarr"> 
   <transition name="success"	           value="saveCarr" /> 
  </activity>
  
  
  <!-- 후보생 경력사항 저장 --> 
  <activity name="saveCarr"	               class="snis.can.cyclestd.d02000009.activity.SaveCarr"> 
   <transition name="success"	           value="saveRacerReln" /> 
  </activity>
  
  
  
  <!-- 후보생 경륜선수와 관계 저장 --> 
  <activity name="saveRacerReln"           class="snis.can.cyclestd.d02000009.activity.SaveRacerReln"> 
   <transition name="success"	           value="saveInvwCntnt" />
  </activity>

  
  <!-- 후보생 면담사항 저장 --> 
  <activity name="saveInvwCntnt"	       class="snis.can.cyclestd.d02000009.activity.SaveInvwCntnt"> 
   <transition name="success"	           value="saveSpecDesc" /> 
  </activity>
  
  
  <!-- 후보생 특이사항 저장 --> 
  <activity name="saveSpecDesc"  	       class="snis.can.cyclestd.d02000009.activity.SaveSpecDesc"> 
   <transition name="success"	           value="savePrizPenalScr" /> 
  </activity>
  
  <!-- 후보생 상벌사항 저장 --> 
  <activity name="savePrizPenalScr"	       class="snis.can.cyclestd.d02000009.activity.SavePrizPenalScr"> 
   <transition name="success"	           value="end" /> 
  </activity>
  
  
  <transaction-manager id="tx_can" commit="true" />
</service>