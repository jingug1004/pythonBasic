<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000007" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
		<property name="staticResultKeyFile" value="resultkey.properties" />
    	<property name="resultKeyList" value="dsPerioNo|dsUseItem|dsUseGbnItem|dsSeltChng|dsExamChng|dsFstSeltExam2|dsFstExamRateBas|dsPersonNum|dsPerioExam1|dsPerioExam2|dsPerioExam3|dsScrRate" />
    	<transition name="success" value="EventRouter" />
	</activity>
  
	<activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchPerioNo"		value="searchPerioNo" />
    	<transition name="searchUseGbnItem"		value="searchUseGbnItem" />
    	<transition name="searchUseItem"		value="searchUseItem" />
    	<transition name="searchSeltChng"   	value="searchSeltChng" />
    	<transition name="searchChngRate"   	value="searchChngRate" />
    	<transition name="searchFstSeltExam2"   value="searchFstSeltExam2" />
    	<transition name="searchPerioExam1"   	value="searchPerioExam1" />
    	<transition name="searchPerioExam2"   	value="searchPerioExam2" />
    	<transition name="searchPerioExam3"   	value="searchPerioExam3" />
    	<transition name="savePerioExam"      	value="savePerioExam" />
    	<transition name="saveFstSeltExam"      value="saveFstSeltExam" />
    	<transition name="savefstyn"      	    value="savefstyn" />
    	<transition name="success" 				value="end" />
  	</activity>
  	
  	<!-- 기수콤보 조회 -->
  	<activity name="searchPerioNo"		    	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsPerioNo" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb001" />
	    <property name="dao" 					value="candao" />    
	</activity>
	<!-- 종목구분사용여부-->
  	<activity name="searchUseGbnItem"		   class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchUseItem" />
	    <property name="resultkey" 				value="dsUseGbnItem" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb012" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	<!-- 종목사용여부-->
  	<activity name="searchUseItem"		    	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchChngRate" />
	    <property name="resultkey" 				value="dsUseItem" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb011" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	<!-- 환산비율 조회 -->
  	<activity name="searchChngRate"		        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="searchSeltChng" />
	    <property name="resultkey" 				value="dsFstExamRateBas" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb004" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	<!-- 배점기준 -->
	<activity name="searchSeltChng"		  	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsSeltChng" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb013" />
	    <property name="param-count" 			value="1" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>	
	<!-- 1차 선발 자전거실기  조회 -->
  	<activity name="searchPerioExam1"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsPerioExam1" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb003" />
	    <property name="param-count" 			value="2" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="param1" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	
	<!-- 1차 선발 체력측정  조회 -->
  	<activity name="searchPerioExam2"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsPerioExam2" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb006" />
	    <property name="param-count" 			value="2" />                           
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="param1" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	  <!-- 1차선발시험 필기  조회  --> 
	  <activity name="searchFstSeltExam2"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	    <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsFstSeltExam2" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb009" />
	    <property name="param-count" 			value="2" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="param1" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />
	  </activity>
	<!-- 1차 선발 결과집계 조회 -->
  	<activity name="searchPerioExam3"		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
  	  <transition name="success" 				value="end" />
	    <property name="resultkey" 				value="dsPerioExam3" />
	    <property name="sqlkey" 				value="tbdb_perio_exam_fst_fb008" />
	    <property name="param-count" 			value="4" />
	    <property name="param0" 				value="racerPerioNo" />
	    <property name="param1" 				value="racerPerioNo" />
	    <property name="param2" 				value="racerPerioNo" />
	    <property name="param3" 				value="racerPerioNo" />
	    <property name="dao" 					value="candao" />    
	</activity>
	<!-- 1차선발  저장 --> 
    <activity name="savePerioExam"			    class="snis.can.cyclestd.d02000007.activity.SaveOneTest">
      <transition name="success"				value="end" />
    </activity>
    <!-- 1차선발필기  저장 --> 
    <activity name="saveFstSeltExam"			class="snis.can.cyclestd.d02000007.activity.SaveFstSeltExam">
      <transition name="success"				value="end" />
    </activity>	  
    <!-- 1차선발합격여부 저장 --> 
    <activity name="savefstyn"					class="snis.can.cyclestd.d02000007.activity.SaveFstYN">
      <transition name="success"				value="end" />
    </activity>	 
	<transaction-manager id="tx_can" commit="true" />
</service>