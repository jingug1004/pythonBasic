<?xml version="1.0" encoding="EUC-KR"?>
<service name="d02000004" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsListNum|dsWkSchePrint|dsGisu|dsYyList|dsGudLectr|dsOutEduOutl|dsCurEduStat|dsOutEduAssign|dsOutCurEduTm|dsWkPart|dsWkSche|dsWeek" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchNoYear"         value="searchNoYear" />
    <transition name="searchWK"    			value="searchWK" />
    <transition name="searchYearList"    	value="searchYearList" />
    <transition name="searchUser"    	    value="searchUser" />
    <transition name="searchWkPart"         value="searchWkPart" />
    <transition name="searchWkSche"         value="searchWkSche" />
    <transition name="searchWkScheCopy"     value="searchWkScheCopy" />
    <transition name="searchEduTotal"     	value="searchEduTotal" />
    <transition name="searchEduAssign"     	value="searchEduAssign" />
    <transition name="searchCurEduTime"     value="searchCurEduTime" />
    <transition name="searchListNum"    	value="searchListNum" />
    <transition name="searchPrint"    		value="searchPrint" />
    <transition name="saveEduWeek"    	    value="saveEduWeek" />
    <transition name="success" 			    value="end" />
  </activity>

  <!-- 기수 콤보 설정 --> 
  <activity name="searchNoYear"	 	        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchWK" />
    <property name="resultkey" 				value="dsGisu" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb001" />
    <property name="dao" 					value="candao" />
  </activity>

  <!-- 주차 콤보 설정 -->
  <activity name="searchWK" 		  	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWeek" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb000" /> 
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />     
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 년도 콤보 설정 -->
  <activity name="searchYearVal" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsYyList" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb002" />
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 강사 콤보 설정 -->
  <activity name="searchUser" 		        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsGudLectr" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb003" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>
   <!-- 주차구간 조회 -->
  <activity name="searchWkPart" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWkPart" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb004" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="wKweekPart" /> 
    <property name="dao" 					value="candao" />    
  </activity>
  <!--종합 교육개요 조회 -->
  <activity name="searchEduTotal" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchEduAssign" />
    <property name="resultkey" 				value="dsCurEduStat" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb009" />
    <property name="param-count" 			value="7" />
    <property name="param0" 				value="endDate" />
    <property name="param1" 				value="endDate" />
    <property name="param2" 				value="endDate" />
    <property name="param3" 				value="endDate" />
    <property name="param4" 				value="rPerioNo" />
    <property name="param5" 				value="rPerioNo" />
    <property name="param6" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>
    
  <!--종합 시간배정 조회 -->
  <activity name="searchEduAssign" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchCurEduTime" />
    <property name="resultkey" 				value="dsOutEduAssign" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb006" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>  
  
  <!-- 교육일지 교육시간 조회 -->
  <activity name="searchCurEduTime" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchListNum" />
    <property name="resultkey" 				value="dsOutCurEduTm" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb007" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>
   <!--순서 List -->
  <activity name="searchListNum" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsListNum" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb011" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>   
  <!-- 주간교육일정 조회 -->
  <activity name="searchWkSche" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWkSche" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb008" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="wKweekPart" />
    <property name="dao" 					value="candao" />    
  </activity>
    <!-- 세부교육일정 가져오기 -->
  <activity name="searchWkScheCopy" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWkSche" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb012" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="wKweekPart" />
    <property name="dao" 					value="candao" />    
  </activity>
    <!-- 주간교육일정 조회(print) -->
  <activity name="searchPrint" 		       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWkSchePrint" />
    <property name="sqlkey" 				value="tbdb_wk_sche_fb010" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="wKweekPart" />
    <property name="dao" 					value="candao" />    
  </activity>
  <!-- 주간교육일정 저장 --> 
  <activity name="saveEduWeek" 		        class="snis.can.cyclestd.d02000004.activity.SaveWeekEduSchd">
    <transition name="success" 	            value="end" />
  </activity> 
 
	<transaction-manager id="tx_can" commit="true" />
</service>

