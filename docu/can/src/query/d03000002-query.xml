<?xml version="1.0" encoding="EUC-KR" ?> 
<queryMap desc="tbdc_equip_detl_mngr(장비내역)">
   
  
  
  <query id="tbdc_equip_detl_fc000" desc="장비내역 조회" fetchSize="10">
 <![CDATA[ 
            SELECT 
                 CD_NM,CD
            FROM 
                 TBDA_SPEC_CD             
            WHERE 
                 GRP_CD = 'D10059'
            ORDER BY
                 CD ASC
            
		
  ]]> 
  </query>
  
  
  <query id="tbdc_equip_detl_fc001" desc="장비내역 조회" fetchSize="10">
 <![CDATA[ 
            SELECT 
                 '0' CHK,
                 EQUIP_CD,
                 ENT_DD,
                 SUPR,
                 QTY,
                 STAT,
                 RMK
            FROM 
                 TBDC_EQUIP_DETL             
            WHERE 
                 EQUIP_CD = ?
            AND  
                 ENT_DD BETWEEN NVL(?, '00000000')  AND  NVL(?, '99999999')
            ORDER BY
                 EQUIP_CD ASC,
                 ENT_DD DESC
            
		
  ]]> 
  </query>
  
  <query id="tbdc_equip_detl_fc002" desc="장비내역 조회" fetchSize="10">
 <![CDATA[ 
            SELECT 
                 '0' CHK,
                 EQUIP_CD,
                 ENT_DD,
                 SUPR,
                 QTY,
                 STAT,
                 RMK
            FROM 
                 TBDC_EQUIP_DETL             
            WHERE 
                 ENT_DD BETWEEN ?  AND  ?
            ORDER BY
                 EQUIP_CD ASC,
                 ENT_DD DESC
            
		
  ]]> 
  </query>
  
  
  
  <query id="tbdc_equip_detl_ic001" desc="장비내역입력" fetchSize="10">
 <![CDATA[ 
			INSERT INTO TBDC_EQUIP_DETL (
			      ENT_DD
			    , EQUIP_CD
			    , SUPR
			    , QTY
			    , STAT
			    , RMK			   
			    , INST_ID
			    , INST_DTM
			    , UPDT_ID
			    , UPDT_DTM
			)VALUES (
				 ?
				,?
				,?
				,?
				,?
				,?
				,? 
				,SYSDATE
				,?
				,SYSDATE					
			)    
		

  ]]> 
  </query>
 <query id="tbdc_equip_detl_uc001" desc="장비내역갱신" fetchSize="10">
 <![CDATA[ 
			UPDATE TBDC_EQUIP_DETL
			SET
			      SUPR                = ?
			    , QTY                 = ?
			    , STAT                = ?
			    , RMK                 = ?
			    , UPDT_ID             = ?
			    , UPDT_DTM            = SYSDATE
			WHERE 
				  ENT_DD              = ?
			AND    
			      EQUIP_CD            = ?    
		

  ]]> 
  </query>
 <query id="tbdc_equip_detl_dc001" desc="장비내역 삭제" fetchSize="10">
 <![CDATA[ 
			DELETE 
			FROM   
			      TBDC_EQUIP_DETL
			WHERE
			      ENT_DD         = ?
			AND  			       
		          EQUIP_CD       = ?

  ]]> 
  </query>
  
  
</queryMap>