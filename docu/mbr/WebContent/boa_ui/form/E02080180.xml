function fcSearchCheck() {
    if ( !fnc_IsMandatory(divSearchCond.calStrDt, "시작일") ) return false;
    if ( !fnc_IsMandatory(divSearchCond.calEndDt, "종료일") ) return false;
    
    fcInit();

	return true;
}

function fcGetMaxPerioNo(obj) {
	var sServiceName = "e02080180-service:searchMaxPerioNo";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutMaxPerioNo=dsOutMaxPerioNo";

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

function fcSearch(obj) {
    if ( !fcSearchCheck() ) return;

	var sServiceName = "e02080180-service:searchRacerPrizPerioNo";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRacerPrizPerioNo=dsOutRacerPrizPerioNo dsOutRacerPrizGrdCd=dsOutRacerPrizGrdCd";

	fce_AddParam(Trim("STR_YEAR"), divSearchCond.cboSStndYear.Value);
	fce_AddParam(Trim("END_YEAR"), divSearchCond.cboEStndYear.Value);
	fce_AddParam(Trim("STR_DT  "), divSearchCond.calStrDt.Value    );
	fce_AddParam(Trim("END_DT  "), divSearchCond.calEndDt.Value    );
	fce_AddParam(Trim("MAX_RACER_PERIO_NO"), MAX_RACER_PERIO_NO    );

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


