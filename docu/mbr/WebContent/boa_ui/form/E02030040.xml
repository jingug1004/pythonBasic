/*-------------------------------------------------------------------------------------------------+
>>  Grid Column정보
+-------------------------------------------------------------------------------------------------*/
function fcGetColumn() {
    var sColumn = "";
    sColumn = '					<columns>'
            + '                     	<col width="55" fix="left"/>';
            
    for ( var i = 0; i < dsOutRacer.GetRowCount(); i++ ) {
        sColumn += '                    <col width="40"/>  ';
    }

    sColumn += '					</columns>';
    return sColumn;
}

/*-------------------------------------------------------------------------------------------------+
>>  Grid Head정보
+-------------------------------------------------------------------------------------------------*/
function fcGetHead() {
    var sHead = "";
    sHead       = '     <head>  '
                + '          <cell bkcolor="user20" col="0" color="user21" display="text" font="굴림,9" text="선수명"/>';
    
    for ( var i = 0; i < dsOutRacer.GetRowCount(); i++ ) {
        var sRacerNm = dsOutRacer.GetColumn(i, "NM_KOR");
		sHead	+= '	<cell bkcolor="user20" col="'+ (i + 1) +'" display="text" font="굴림,10" text="' + sRacerNm + '"/>';
    }

    sHead   += '         </head>';
    return sHead;
}

/*-------------------------------------------------------------------------------------------------+
>>  Grid Body정보
+-------------------------------------------------------------------------------------------------*/
function fcGetBody() {
    var sBody       = "";
        sBody       = '     <body>  ' 
                    + '     <cell align="center" col="0"  colid="NM_KOR"       display="text"/>';

    var nCnt = toInteger(meCnt.Value);
    for ( var i = 0; i < dsOutRacer.GetRowCount(); i++ ) {
        var sPerioNo  = dsOutRacer.GetColumn(i, "RACER_NO_" + i);
        var sColumnNm = "RACER_NO_" + i;
        
        var sBkColor  = "expr:iif(dsOutArrangeVerify.GetColumn(row,&apos;" + sColumnNm + "&apos;)&gt;=" + nCnt + ",&apos;#646EFF&apos;,&apos;&apos;)";
        sBody      += '    <cell align="center" col="'+ (i + 1) +'" colid="RACER_NO_'+ i +'" display="number" bkcolor="' + sBkColor + '" />';
    }
    
    sBody   += '</body>';
    
    return sBody;
}
