+-------------------------------------------------------------------------------------------------*/
function fcChoice(obj)
{
	var nChkedIdx = dsSancBasisList.FindRow("CHK", 1);
	if(nChkedIdx==-1) {
        fcd_SetReturnDs1(dsSancBasisList, dsSancBasisList.GetCurrow());
	} else {
        var sRowIdx;
        for(var i=0; i<dsSancBasisList.GetRowCount(); i++) {
            if(dsSancBasisList.GetColumn(i, "CHK")==1) {
                sRowIdx += i + "^";
            }
        }
        sRowIdx = mid(sRowIdx, 0, length(sRowIdx)-1);
        fnc_SetReturnDs1(dsSancBasisList, sRowIdx);
	}
	close(true);
}

