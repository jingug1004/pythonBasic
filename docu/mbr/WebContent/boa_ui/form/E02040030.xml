/*-------------------------------------------------------------------------------------------------+
>>  년도별 최고정번수에 따른 그리드 사이즈 조절
+-------------------------------------------------------------------------------------------------*/
function setGridSize(){
	/*
		IWORK_SFR-008 경정 선수편성 메뉴 개선  정번8 까지 추가 수정.
		회차에 따른 최고(8까지)정번수 별로 1 ~ 정번수 까지 화면에 표시.
	*/
	var nMaxTmsRunCnt = 8;
	var nMaxRaceRegNoCnt = -1;
	
	for(var m=0; m < dsOutSRaceTms.GetRowCount(); m++){
		if(nMaxRaceRegNoCnt < dsOutSRaceTms.GetColumn(m, "MAX_RACE_REG_NO_CNT")){
			nMaxRaceRegNoCnt = dsOutSRaceTms.GetColumn(m, "MAX_RACE_REG_NO_CNT");
		}
	}
	
	if(nMaxRaceRegNoCnt == -1) nMaxRaceRegNoCnt = 6;
	
	//Alert("nMaxRaceRegNoCnt : " + nMaxRaceRegNoCnt);
	
	grdRaceInfo.Redraw = false;
	
	var nCol0W = grdRaceInfo.GetColProp(0, "Width");
	var nCol1W = grdRaceInfo.GetColProp(1, "Width");
	var nCol2W = grdRaceInfo.GetColProp(2, "Width");
	
	//전체적으로 사이즈를 넓힘.
	for ( var i = 0; i < nMaxTmsRunCnt; i++ ) {
        grdRaceInfo.SetColProp(3 + i, "Width", 50);
        grdRaceInfo.SetColProp(11 + i, "Width", 50);
    }
	   
    //nMaxTmsRunCnt 부터 nMaxRaceRegNoCnt까지 사이즈를 줄임.
    for ( var i = nMaxTmsRunCnt; i > nMaxRaceRegNoCnt; i-- ) {
        grdRaceInfo.SetColProp(3 + i-1, "Width", 0);
        grdRaceInfo.SetColProp(11 + i-1, "Width", 0);
    }
    
    grdRaceInfo.SetColProp(0, "Width", nCol0W);
    grdRaceInfo.SetColProp(1, "Width", nCol1W);
    grdRaceInfo.SetColProp(2, "Width", nCol2W);
    
    grdRaceInfo.Redraw = true;
}


