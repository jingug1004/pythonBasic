function fcSearch(obj) {
	fcSearchCheck();
	
	var sServiceName = "e07010050-service:searchRaceRankAmtBac:u";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRaceRankAmtBac=dsOutRaceRankAmtBac";
    fce_AddParam("STND_YEAR", cboStndYear.Value);
    
    fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {

	//변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsOutRaceRankAmtBac))
	{
		//저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");	
		return false;
	}else{
		// 필수항목체크
		oDataSet          = dsOutRaceRankAmtBac;
		sMandatoryColID   = "STND_YEAR|RACE_KINDS_CD|RACE_DGRE_CD";
		sMandatoryColName = "기준년도|경주종류|경주등급";
		oGrid             = grdRaceRankAmtBac;
		if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
			return false;
		}
	}
	return true;
}

//저장 
function fcSave(obj) {
    if ( !fcSaveCheck() ) return;
    
    var sServiceName = "e07010050-service:saveRaceRankAmtBac";
    var sInDataSet   = "dsOutRaceRankAmtBac=dsOutRaceRankAmtBac:u";
    var sOutDataSet  = "dsOutRaceRankAmtBac=dsOutRaceRankAmtBac";
    
    fce_AddParam("STND_YEAR", cboStndYear.Value);
    fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
	
}

