function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;
    
	var sServiceName = "e03040010-service:searchEquipmentPrintSeq";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutPrtSeq=dsOutPrtSeq ";                         
	
	fce_AddParam(Trim("STND_YEAR"), STND_YEAR);
	fce_AddParam(Trim("MBR_CD"), 	MBR_CD);
	fce_AddParam(Trim("TMS"), 		TMS);
	fce_AddParam(Trim("EXCL_YN"),   EXCL_YN);
	//fce_AddParam(Trim("PRT_TYPE"),  sPrtType);
    
 	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}


/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {

	if (dsLotRslt.GetColumn(0, "PRT_SEQ_001") < 0) return false;
	if (dsLotRslt.GetColumn(0, "PRT_SEQ_002") < 0) return false;
	if (dsLotRslt.GetColumn(0, "PRT_SEQ_003") < 0) return false;
	return true;
}

function fcSave(obj) {
    if ( !fcSaveCheck(obj) ) return;

	var sServiceName = "e03040010-service:saveEquipmentArrangeRslt";
    var sInDataSet   = "dsLotRslt=dsLotRslt ";
    var sOutDataSet  = "";
Trace(dsLotRslt.savexml());
	fce_AddParam(Trim("STND_YEAR"), STND_YEAR);
	fce_AddParam(Trim("MBR_CD"), 	MBR_CD);
	fce_AddParam(Trim("TMS"), 		TMS);
	fce_AddParam(Trim("EXCL_YN"),   EXCL_YN);
	
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");

}

