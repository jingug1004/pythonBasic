	if (sInpt == "F") {
		btnSendMail.Enable = false;
		divBtnRow.Visible = false;
	}
	
	//초기 화면 BK Color설정 
	fnc_SetBKColor(this);

    //년도조회
    fce_SetStndYear(dsOutSStndYear, toInteger(fce_GetSysdate("YYYY")));
