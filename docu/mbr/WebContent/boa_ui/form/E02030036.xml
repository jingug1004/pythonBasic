	var sServiceName = "e02030036-service:saveMailNoti";
    var sInDataSet   = "dsOutMailNotiList=dsOutMailNotiList ";
    var sOutDataSet  = "dsOutMailNoti=dsOutMailNoti ";

	fce_AddParam("STND_YEAR",			dsOutMailNoti.GetColumn(dsOutMailNoti.rowposition, 'STND_YEAR'));
	fce_AddParam("TITLE",				edTitle.Text);
	fce_AddParam("SENDER",				edSender.Text);
	fce_AddParam("MAIL_SEND_REQ_DAY",	calSendReqDt.Value);
	fce_AddParam("NOTICE",				taNotice.Text);

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
