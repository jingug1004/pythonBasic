		http.Sync = false;
		if (dsRacerList.rowcount == 0) {
			fnc_message("보험에 가입되지 않는 선수입니다.");
			return;
		}
		dsList.SetColumn(dsList.row, "RACER_NO", sResponse[0]);
		dsList.SetColumn(dsList.row, "RACER_NM", sResponse[1]);
	}
}

function fcCheckInsuExist(sRacerNo)
{
	if (fnc_IsNull(sRacerNo)) return;
	
	var sServiceName = "";
	var sInDataSet   = "";
	var sOutDataSet  = "";    
	
	sServiceName = "e07050030-service:searchInsuExist";
	sInDataSet   = "";	
	sOutDataSet  = "dsRacerList=dsRacerList";	
	
	
	fce_AddParam(Trim("RACER_NO"), sRacerNo);	
	
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}]]></Script>
