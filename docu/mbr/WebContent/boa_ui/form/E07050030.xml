function fcSaveCheck(obj) {   	
	
	
	//변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsList)) {
		//저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");	
		return false;
	}
	
	// 필수항목체크
	oDataSet          = dsList;
	sMandatoryColID   = "RACER_NO|CHG_RSN|REG_DT";
	sMandatoryColName = "선수번호|변경사유|등록일";
	oGrid             = grdList;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
	    return false;
	}		
	

	return true;
}

function fcSave(obj) {
    if ( !fcSaveCheck() ) return;    
    
    var confirmMsg = "수정은 불가능합니다. (+)버튼을 누를 후 추가만 가능합니다. 저장하시겠습니까?";
    if(fnc_Message(L_COM_CNF_0000, "Y", confirmMsg)) {           
		var sServiceName = "";
		var sInDataSet   = "";
		var sOutDataSet  = "";    
		
		sServiceName = "e07050030-service:saveInsHstList";
		sInDataSet   = "dsList=dsList:u ";
		
		sOutDataSet  = "";	
		
		fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
    }
}

