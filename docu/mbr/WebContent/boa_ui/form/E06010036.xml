        //dsOutRacerStat.UnFilter();
        //var sFilter = "CD_NM!='신인'";
        //dsOutRacerStat.Filter(sFilter);

        fcSearch();
    }

	// 조회 후
	if ( ServiceName == "e06010035-service:searchRacerPropeller" ) {
	    if(dsOutRacerPropeller.GetRowCount()>0){
			fnc_SetStatus(SNIS_BOA_0003,  dsOutRacerPropeller.GetRowCount());
	    }
        
	}

	// 저장 후
	if ( ServiceName == "e06010035-service:saveRacerPropeller" ) {
		
	}
	
	//오류가 없이 정상 처리되었다면...
	fcPostProceS(ServiceName);
}

