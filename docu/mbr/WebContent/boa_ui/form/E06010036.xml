function fcCallBack(ServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	// 공통코드 조회시
	if ( ServiceName == "common-service:searchCode" ) {
	    // 등급에 '전체' 항목 추가
		fce_AddCodeRow(dsOutRacerGrd, "전체", "", cboSRacerGrd);	
	    // 선수구분에 '전체' 항목 추가
