//조회 
function fcSearch(obj) {
	fcSearchCheck();
	
    var ServiceName = "e06010036-service:searchRacerPropeller";
    var sInDataSet  = "";
    var sOutDataSet = "dsOutRacerPropeller=dsOutRacerPropeller";
    
    fce_AddParam("RACER_NM", Trim(edRacerNm.Value));
    fce_AddParam("RACER_GRD_CD", cboSRacerGrd.Value);
	fce_AddParam("RACER_STAT_CD", cboSRacerStat.Value);
	
	fce_SendTransaction(ServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}


/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {

	//변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck( dsOutRacerPropeller )  )
	{
		//저장 할 자료가 없음
		fnc_MeSage(SNIS_COM_1006, "N");	
		return false;
	}

	// 필수항목체크
	oDataSet          = dsOutRacerPropeller;
	sMandatoryColID   = "PROPELLER1";
	sMandatoryColName = "프로펠러번호";
	oGrid             = "grdRacerPropeller";
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
	    return false;
	}
	
	return true;
}

function fcSave(obj) {
    if ( !fcSaveCheck() ) return;
    
	var ServiceName = "e06010036-service:saveRacerPropeller";
    var sInDataSet   = "dsOutRacerPropeller=dsOutRacerPropeller:u";
    var sOutDataSet  = "dsOutRacerPropeller=dsOutRacerPropeller";
    
	fce_SendTransaction(ServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

