+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_Result(ErrorCode, ErrorMSG) ) return;
	
	// 공통코드 조회 완료 후
	if ( sServiceName == "common-service:searchCode" ) {
        cboSMbr.Index = 0;
        cboSMbr.Value = fce_GetUserInfo("MBR_CD");

        fce_GetRaceTms(cboSStndYear.value, cboSMbr.value, "", "dsOutRaceTms");
	}
	
	// 회차 리스트 조회시
	if ( sServiceName == "common_race-service:searchRaceTms" ) {
        cboSRaceTms.Index = 0;
        if ( cboSStndYear.Value == fce_GetCurRaceInfo("STND_YEAR") ) {
            var nIndexRow = fnc_GetExistValue(dsOutRaceTms, "TMS", fce_GetCurRaceInfo("TMS"));
            if ( nIndexRow > -1 ) cboSRaceTms.Index = nIndexRow;
        }

        cboSRaceTms_OnChanged();
        //fce_GetRaceDayOrd(cboSStndYear.Value, cboSMbr.Value, cboSRaceTms.Value, "dsOutDayOrd");
		return;
	}

	// 회차 리스트 조회시
	if ( sServiceName == "common_race-service:searchRaceDayOrd" ) {
        if ( dsOutDayOrd.GetRowCount() > 0 ) {
            dsOutDayOrd.InsertRow(0);
            dsOutDayOrd.SetColumn(0, "DAY_ORD", "0");
    
            cboSDayOrd.Index = 0;
        }
        
		return;
	}

	// 결과조회 완료후
	if ( sServiceName == "e02060010-service:searchRaceResult" ) {
        return;
	}
	
	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 Post Process!
+-------------------------------------------------------------------------------------------------*/
function fcPostProcess(sServiceName) {

}

/*-------------------------------------------------------------------------------------------------+
>>  년도, 분기 변경시
+-------------------------------------------------------------------------------------------------*/
function cboStndYear_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	fce_GetRaceTms(cboSStndYear.Value, cboSMbr.value, "", "dsOutRaceTms");
}

/*-------------------------------------------------------------------------------------------------+
>>  회차변경시
+-------------------------------------------------------------------------------------------------*/
function cboSRaceTms_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
    fce_GetRaceDayOrd(cboSStndYear.Value, cboSMbr.Value, cboSRaceTms.Value, "dsOutDayOrd");

    fcInit();

    var nRow = fnc_GetExistValue(dsOutRaceTms, "TMS", cboSRaceTms.Value);
    var nDayOrd = toInteger(dsOutRaceTms.GetColumn(nRow, "DAY_ORD"));

    for ( var i = 0; i < nDayOrd; i++ ) {
        grdRacerDayRecd.SetColProp(3 + (i * 2), "Width", 90);
        grdRacerDayRecd.SetColProp(4 + (i * 2), "Width", 90);
    }
    for ( var i = nDayOrd; i < 7; i++ ) {
        grdRacerDayRecd.SetColProp(3 + (i * 2), "Width", 0);
        grdRacerDayRecd.SetColProp(4 + (i * 2), "Width", 0);
    }
}

/*-------------------------------------------------------------------------------------------------+
>>  일차변경시
+-------------------------------------------------------------------------------------------------*/
function cboSDayOrd_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
    fcInit();
}
