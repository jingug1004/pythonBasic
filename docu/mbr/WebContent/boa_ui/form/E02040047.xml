/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 Load時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormLoadSetting(obj) {
	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";
	
	//화면 정보 Display 및 Button 권한 설정
	//fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
	
	//초기 화면 BK Color설정 
	fnc_SetBKColor(this);

	// 조회
	//fcSearch();
	for ( var i = 0; i < toInteger(Arg_sMaxRegNo); i++ ) {
        dsOutColumn.AddRow();
        dsOutColumn.SetColumn(i, "COLUMN", i + 1);
	}
	
	for ( var i = 0; i < toInteger(Arg_sRaceCnt); i++ ) {
        dsOutRow.AddRow();
        dsOutRow.SetColumn(i, "ROW", i + 1);
	}
	
	cboStrColumn.Index = 0;
	cboEndColumn.Index = dsOutColumn.GetRowCount() - 1;
	cboStrRow.Index    = 0;
	cboEndRow.Index    = dsOutRow.GetRowCount() - 1;
	
	//공통코드 조회
	fce_GetComboDs(dsInComCd);
}

