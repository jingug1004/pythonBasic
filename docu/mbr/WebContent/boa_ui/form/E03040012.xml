	argsValues    += "#" + dsRePrint.GetColumn(0, "PRT_SEQ");
	argsValues    += "#" + logImgUrl;
	argsValues    += "#" + sPrtType;

	fnc_SQLRPTPrint("e03040012", rptFileName, 0, argsKeys, argsValues);
}


function fcDsOnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if (obj.id == "dsRePrint") {
		if (strColumnID == "PRT_TYPE") {
			dsRePrint.SetColumn(0, "PRT_NUM", "");
			dsOutPrtSeq.Filter("PRT_TYPE='"+dsRePrint.GetColumn(0, "PRT_TYPE")+"'");
		} else if (strColumnID == "PRT_NUM") {
			var sPrtType = dsRePrint.GetColumn(0, "PRT_TYPE");
			var sPrtNum = dsRePrint.GetColumn(0, "PRT_NUM");
			if (sPrtType != "") {
				var cRow = -1;
				cRow = dsOutPrtSeq.SearchRow("PRT_TYPE='"+sPrtType+ "' && PRT_NUM='" + sPrtNum + "'");
				dsRePrint.SetColumn(0, "PRT_DTM", dsOutPrtSeq.GetColumn(cRow, "PRT_DTM"));
				dsRePrint.SetColumn(0, "PRT_SEQ", dsOutPrtSeq.GetColumn(cRow, "PRT_SEQ"));
			}
		}
	}
}

function Tab0_tab1_grdPrtList_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	fcGridOnLButtonUp(obj,nX,nY,nRow,nCell,nPivotIndex);
}
