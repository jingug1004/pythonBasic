function PrintReport()
{
	//SQL ¹æ½ÄÀ¸·Î È£Ãâ
	var logImgUrl = GBL_REPORT_URL+"log.jpg";
	var sPrtType = "";
	var rptFileName = "e03049010_racer_equip_";
	
	sPrtType       = dsPrtSeq.GetColumn(0, "PRT_TYPE");
	rptFileName   += sPrtType;
	
	var argsKeys = "STND_YEAR#MBR_CD#TMS#PRT_SEQ#IMG_URL#PRT_TYPE";
	var argsValues = "";
	argsValues     = dsPrtSeq.GetColumn(0, "STND_YEAR");
	argsValues    += "#" + "001";
	argsValues    += "#" + dsPrtSeq.GetColumn(0, "TMS");
	argsValues    += "#" + dsPrtSeq.GetColumn(0, "PRT_SEQ");
	argsValues    += "#" + logImgUrl;
	argsValues    += "#" + sPrtType;
	
	fnc_SQLRPTPrint("e03040012", rptFileName, 0, argsKeys, argsValues);

}


