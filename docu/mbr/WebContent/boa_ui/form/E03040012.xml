}

/*-------------------------------------------------------------------------------------------------+
>>  추첨랜덤출력 버튼 클릭 時 랜덤값 저장
+-------------------------------------------------------------------------------------------------*/
function fcRandomSave(sPrtType) 
{
	var sServiceName = "e03040010-service:SaveEquipmentRandom";
    //var sInDataSet   = "dsOutRacerArrange_rpt=dsOutRacerArrange_rpt ";
    var sOutDataSet  = "dsPrtSeq=dsPrtSeq ";
    var sInDataSet   = "";
	
	fce_AddParam(Trim("STND_YEAR"), cboSStndYear.Value);
	fce_AddParam(Trim("MBR_CD "), 	cboSMbr.Value);
	fce_AddParam(Trim("TMS"), 		cboSRaceTms.Value);
	fce_AddParam(Trim("PRT_TYPE"),  sPrtType);
	fce_AddParam(Trim("EXCL_YN"),   dsOutRacerArrange_rpt.GetColumn(0, "EXCL_YN"));
	
	//http.sync = true;
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
	//http.sync = false;
}

/*-------------------------------------------------------------------------------------------------+
>>  추첨랜덤저장 결과를 출력 처리
+-------------------------------------------------------------------------------------------------*/
function PrintReport()
{
	//SQL 방식으로 호출
	var logImgUrl = GBL_REPORT_URL+"log.jpg";
	var sPrtType = "";
	var rptFileName = "e03049010_racer_equip_";
	
	sPrtType       = dsPrtSeq.GetColumn(0, "PRT_TYPE");
	rptFileName   += sPrtType;
	
	var argsKeys = "STND_YEAR#MBR_CD#TMS#PRT_SEQ#IMG_URL#PRT_TYPE";
	var argsValues = "";
	argsValues     = dsPrtSeq.GetColumn(0, "STND_YEAR");
	argsValues    += "#" + "001";
	argsValues    += "#" + dsPrtSeq.GetColumn(0, "TMS");
	argsValues    += "#" + dsPrtSeq.GetColumn(0, "PRT_SEQ");
	argsValues    += "#" + logImgUrl;
	argsValues    += "#" + sPrtType;
	
	fnc_SQLRPTPrint("e03040012", rptFileName, 0, argsKeys, argsValues);

}


/*-------------------------------------------------------------------------------------------------+
>>  추첨랜덤저장 결과를 출력 처리
+-------------------------------------------------------------------------------------------------*/
function fcRePrint()
{

	//SQL 방식으로 호출
	var logImgUrl = GBL_REPORT_URL+"log.jpg";
	var sPrtType = "";
	var rptFileName = "e03049010_racer_equip_";
	
	sPrtType       = dsRePrint.GetColumn(0, "PRT_TYPE");
	rptFileName   += sPrtType;
	
	var argsKeys = "STND_YEAR#MBR_CD#TMS#PRT_SEQ#IMG_URL#PRT_TYPE";
	var argsValues = "";
	argsValues     = STND_YEAR;
	argsValues    += "#" + MBR_CD;
	argsValues    += "#" + TMS;
	argsValues    += "#" + dsRePrint.GetColumn(0, "PRT_SEQ");
	argsValues    += "#" + logImgUrl;
	argsValues    += "#" + sPrtType;

	fnc_SQLRPTPrint("e03040012", rptFileName, 0, argsKeys, argsValues);
}


function fcDsOnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if (obj.id == "dsRePrint") {
		if (strColumnID == "PRT_TYPE") {
			dsRePrint.SetColumn(0, "PRT_NUM", "");
			dsOutPrtSeq.Filter("PRT_TYPE='"+dsRePrint.GetColumn(0, "PRT_TYPE")+"'");
		} else if (strColumnID == "PRT_NUM") {
			var sPrtType = dsRePrint.GetColumn(0, "PRT_TYPE");
			var sPrtNum = dsRePrint.GetColumn(0, "PRT_NUM");
			if (sPrtType != "") {
				var cRow = -1;
				cRow = dsOutPrtSeq.SearchRow("PRT_TYPE='"+sPrtType+ "' && PRT_NUM='" + sPrtNum + "'");
				dsRePrint.SetColumn(0, "PRT_DTM", dsOutPrtSeq.GetColumn(cRow, "PRT_DTM"));
				dsRePrint.SetColumn(0, "PRT_SEQ", dsOutPrtSeq.GetColumn(cRow, "PRT_SEQ"));
			}
		}
	}
}

function Tab0_tab1_grdPrtList_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	fcGridOnLButtonUp(obj,nX,nY,nRow,nCell,nPivotIndex);
}
