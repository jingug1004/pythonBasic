function fcInit(obj) {
    dsOutRacerDegree.ClearData();
    dsOutRacerRankDetail.ClearData();
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
    if( !fnc_IsMandatory(calStrDt, "시작일") ) return false;
    if( !fnc_IsMandatory(calEndDt, "종료일") ) return false;
    
    fcInit();
    return true;
}

function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "e02020030-service:searchRacerUpDown";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRacerDegree=dsOutRacerDegree ";
    
    if ( chkAllYn.Value == "1" ) {
        sServiceName = "e02020030-service:searchRacerUpDownAll";
    }
    
    fce_AddParam("STND_YEAR", cboEStndYear.value);
    fce_AddParam("MBR_CD"   , cboEMbr.value);
    fce_AddParam("TMS"      , cboERaceTms.value);
    fce_AddParam("STR_DT"   , calStrDt.value);
    fce_AddParam("END_DT"   , calEndDt.value);
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {

    return true;
}

function fcSave(obj) {
    if ( !fcSaveCheck(obj) ) return;
    
	var sServiceName = "e02020020-service:saveRacerDegree ";
    var sInDataSet   = "dsOutRacerDegree=dsOutRacerDegree:u ";
    var sOutDataSet  = "";
    
    fce_AddParam("CHG_DT"  , calAplyDt.value);
    fce_AddParam("RMK"     , "특별승강급대상선수");
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

