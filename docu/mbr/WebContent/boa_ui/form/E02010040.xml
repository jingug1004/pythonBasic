+-------------------------------------------------------------------------------------------------*/
function fcInit(obj) {
    dsOutRecdStndTerm.ClearData();
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
	fcInit(obj);
	
	return true;
}

function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "e02010040-service:searchRecdStndTerm";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRecdStndTerm=dsOutRecdStndTerm ";
    
	fce_AddParam(Trim("STND_YEAR"), cboSStndYear.Value);

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {

	//변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsOutRecdStndTerm) )
	{
		//저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");
		return false;
	}

	// 필수항목체크
	oDataSet          = dsOutRecdStndTerm;
	sMandatoryColID   = "STND_YEAR|QURT_CD|STR_DT|END_DT|RACER_GRD_STR_DT|RACER_GRD_END_DT";
	sMandatoryColName = "기준년도|분기|시작일자|종료일자|등급시작일자|등급종료일자";
	oGrid             = grdRecdStndTerm;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
	    return false;
	}
	
	return true;
}

function fcSave(obj) {

    if ( !fcSaveCheck(obj) ) return;

	var sServiceName = "e02010040-service:saveRecdStndTerm";
    var sInDataSet   = "dsOutRecdStndTerm=dsOutRecdStndTerm:u ";
    var sOutDataSet  = "dsOutRecdStndTerm=dsOutRecdStndTerm ";
    
	fce_AddParam(Trim("STND_YEAR"), cboSStndYear.Value);

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

