function fcSearch(obj) {
	if(!fcSearchCheck()){
		return;
	}
	
	fce_AddParam("STND_YEAR", cboStndYear.Value);	
    fce_AddParam("PAY_MONTH", cboStndMonth.Value);   
           
    var sServiceName = "e07020010-service:searchRankAmtList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutAmtlList=dsOutAmtlList dsOutRunList=dsOutRunList dsOutEventList=dsOutEventList dsOutStrList=dsOutStrList dsOutSafyList=dsOutSafyList";
    fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);  
    
}

