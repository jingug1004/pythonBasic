         }
		return;
	}

	// º±ºˆ∫∞ø‰æ‡¡§∫∏¡∂»∏
	if ( sServiceName == "e09030001-service:searchEduCont" ) {
    }
    
	// ¿˙¿Â »ƒ
	if ( sServiceName == "e09030001-service:saveEduCont" ) {
         fcSearch();
