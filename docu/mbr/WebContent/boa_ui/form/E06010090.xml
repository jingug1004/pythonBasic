/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	// 공통코드 조회 완료 후
	if ( sServiceName == "common-service:searchCode" ) {
        cboMbrCd.Index = 0;
        cboMbrCd.Value = fce_GetUserInfo("MBR_CD");

        fce_GetRaceTms(cboStndYear.value, cboMbrCd.value, "", "dsOutTms");
	}

	// 회차 리스트 조회시
	if ( sServiceName == "common_race-service:searchRaceTms" ) {
		dsOutMaxTms.Filter("STND_YEAR='"+cboStndYear.Value+"'&&MBR_CD='"+cboMbrCd.Value+"'");
		var maxTms = dsOutMaxTms.GetColumn(0,"MAX_TMS");
		dsOutTms.Filter("TMS<=" + maxTms);
	
        cboTms.Index = 0;
        if ( cboStndYear.Value == fce_GetCurRaceInfo("STND_YEAR") ) {
            var nIndexRow = fnc_GetExistValue(dsOutTms, "TMS", fce_GetCurRaceInfo("TMS"));
            if ( nIndexRow > -1 ) cboTms.Index = nIndexRow;
        }

	    fcSearch();

		return;
	}
	// 조회 후
	if ( sServiceName == "e06010090-service:searchEquipFormer" ) {
	    if(dsOutEquipFormer.GetRowCount()>0){
			fnc_SetStatus(SNIS_BOA_0003,  dsOutEquipFormer.GetRowCount());
	    }else{
			fnc_SetStatus(SNIS_BOA_0003,  0);
	    }
        
	}

	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

