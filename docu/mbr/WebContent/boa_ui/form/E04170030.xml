	<Script><![CDATA[/***************************************************************************************************
*   작성일 : 2015. 8. 6
*   작성자 : 정리운 
*   화면명 : 경주 위반행위 현황자료
***************************************************************************************************/
/***************************************************************************************************
*                                         공통 Script Include                                      *
***************************************************************************************************/
#include "lib::boa_common_script.js";
#include "lib::boa_app_script.js";
/***************************************************************************************************
*                                          화면 변수 선언부                                        *
***************************************************************************************************/
/*
clear,저장,삭제,엑셀,인쇄,닫기
*/
var sFormId       = 'E04170030';
var sBUTTONLIST   = 'FFFFFF';                          //공통기능정의(버튼순으로 T or F)
var sUSERAUTH     = '';                                //사용자별 프로그램 사용 권한
var sFORMCAPTION  = '';                                //Form의 Title 정보
var sFORMLOCATION = '';                                //Form의 Location 정보
var sUSERLAVEL    = '';                                //Form의 사용  Level
var vREVOBJ       = Array(9);                          //결과값 처리를 위한 오브젝트 
var sGRDTIT       = "";                                //그리드 제목
var sCUROBJ       ;                                    //현재 LOST FOCUS 오브젝트

var oLastDs       = null;
var oLastDsRow    = 0;

/***************************************************************************************************
*                                         공통 Event 처리 부분                                     *
***************************************************************************************************/

// 최초 화면 Load時 처리 할 사항
function fcFormLoadSetting(obj) {

	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";
	
	grdSangSum.Visible = false;
	staTitle2.Visible  = false;

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
	
	//초기 화면 BK Color설정 
	fnc_SetBKColor(this);

    //년도조회
    fce_SetStndYear(dsOutSStndYear, toInteger(fce_GetSysdate("YYYY")));
    cboSStndYear.Index = 0;

	//공통코드 조회
	fce_GetComboDs(dsInComCd);
	
	http.sync= false; 
}


// 최초 화면 Load時 처리 할 사항
function fcFormUnloadProcess(obj) {
	//자료 변경 여부를 체크하고자 하는  DataSet Object를 넘긴다.
	var sDataset = "";
	if ( !fnc_FormUnloadCheck(sDataset , sFormId) ) return false;
}


/***************************************************************************************************
*                                      사용자 정의  처리 부분                                      *
***************************************************************************************************/


// 년도콤보 변경시 이벤트
function cboSStndYear_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	fcGetRaceTms(cboSStndYear.Value, cboSMbr.Value, "dsOutSRaceTms");
}


//  엑셀 버튼 클릭 時 처리
function fcToExcel(obj) {
	if(grdSangSum.Visible == true) {
		fnc_OpenExcelExport("grdSangSum"); 
	} else if(grdSangResult.Visible == true) {
		fnc_OpenExcelExport("grdSangResult");   
	}
	
}


// 조회버튼 이벤트
function fcSearch(obj) {
    grdSangSum.Visible 	  = false;
	grdSangResult.Visible = true;
	staTitle1.Visible	  = true;
	staTitle2.Visible     = false;
    
    var sServiceName = ""; 
    var sInDataSet   = "";
    var sOutDataSet  = "";
    
    
    if ( !fcSearchCheck(obj) ) return;
    
	sServiceName = "e04170010-service:searchSangResult";
	sInDataSet   = "";
	sOutDataSet  = "dsSangResult=dsSangResult";

    fce_AddParam(Trim("STND_YEAR"), 			cboSStndYear.Value		); //년도
	fce_AddParam(Trim("MBR_CD"), 				cboSMbr.Value     		); //경정장코드
	fce_AddParam(Trim("TMS"), 					cboSRaceTms.Value 		); //회차
      
    fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


// 조회버튼 클릭시 체크사항 처리
function fcSearchCheck(obj) {
    if(fnc_isNull(cboSMbr.Value)) {
            fnc_Message(SNIS_COM_1034, "N", "경정장");
            cboSMbr.SetFocus();
            return false;
    } 

	if(fnc_isNull(cboSStndYear.Value)) {
            fnc_Message(SNIS_COM_1034, "N", "년도");
            cboSStndYear.SetFocus();
            return false;
    }

    if(fnc_isNull(cboSRaceTms.Text)) {
            fnc_Message(SNIS_COM_1034, "N", "회차");
            cboSRaceTms.SetFocus();
            return false;
    }

    return true;
}

//회차조회 처리
function fcGetRaceTms(sStndYear, sMbrCd, soutDS){
	var sServiceName = "e04170010-service:searchRaceTms";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutSRaceTms=dsOutSRaceTms";
    
	fce_AddParam(Trim("STND_YEAR"), sStndYear);
	fce_AddParam(Trim("MBR_CD"   ), sMbrCd   );

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}


// CallBack함수
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
	if ( !fnc_Result(ErrorCode, ErrorMSG) ) return;
	
	// 공통코드 조회 후
	if ( sServiceName == "common-service:searchCode" ) {
        cboSMbr.Index = 0;
        cboSMbr.Value = fce_GetUserInfo("MBR_CD");

        fcGetRaceTms(cboSStndYear.Value, cboSMbr.Value, "dsOutSRaceTms");
		return;
	}
	
	// 회차 조회 후 
	if ( sServiceName == "e04170010-service:searchRaceTms" ) {
        cboSRaceTms.Index = 0;
		return;
	}
	
	// 위반행위 현황자료 조회 후
	if ( sServiceName == "e04170010-service:searchSangResult" ) {
        fnc_SetStatus(SNIS_BOA_0003, dsSangResult.GetRowCount());
         return;
	}

	//  회차별 위반행위 집계현황 조회 후
	if ( sServiceName == "e04170010-service:searchSangSum" ) {
        fnc_SetStatus(SNIS_BOA_0003, dsSangSum.GetRowCount());
         return;
	}
}



//경주 위반행위 회차별 집계현황 조회
function btnTmsSangSum_OnClick(obj)
{
	grdSangSum.Visible    = true;
	grdSangResult.Visible = false;
	staTitle1.Visible	  = false;
	staTitle2.Visible	  = true;
	cboSRaceTms.Index	  = 0;	
	
    var sServiceName = ""; 
    var sInDataSet   = "";
    var sOutDataSet  = "";
    
    
    if ( !fcSearchCheck(obj) ) return;
    
	sServiceName = "e04170010-service:searchSangSum";
	sInDataSet   = "";
	sOutDataSet  = "dsSangSum=dsSangSum";

    fce_AddParam(Trim("STND_YEAR"), 			cboSStndYear.Value		); //년도
	fce_AddParam(Trim("MBR_CD"), 				cboSMbr.Value     		); //경정장코드
      
    fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
	
}



// 화면닫기 버튼
function fcEnd(obj) {
	Close();	
}]]></Script>
