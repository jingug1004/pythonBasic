}

function fcSearch(obj) {

    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "e07050010-service:searchList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsList=dsList ";
	
	
	fce_AddParam(Trim("NM_KOR       "), edRacerNm.Value);
		
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/

function fcSaveCheck(obj) {   	
	
	
	//변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsList)) {
		//저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");	
		return false;
	}
	
	
	// 필수항목체크
	oDataSet          = dsList;
	sMandatoryColID   = "RACER_NO|RACER_NM|ENGM_NM|ENGM_RES_NO|INSU_CD|INSU_AMT|REG_DT|ENGM_YN";
	sMandatoryColName = "선수번호|선수명|계약자명|주민등록번호|보험사|보험료|등록일|가입상태";
	oGrid             = grdList;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
	    return false;
	}		
	

	return true;
}

function fcSave(obj) {
    if ( !fcSaveCheck() ) return;    
    
    var confirmMsg = "저장하시겠습니까?";
    if(fnc_Message(L_COM_CNF_0000, "Y", confirmMsg)) {           
		var sServiceName = "";
		var sInDataSet   = "";
		var sOutDataSet  = "";    
		
