function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	// 조회 후
	if ( sServiceName == "e07010020-service:searchComInfo" ) {
	    if(dsOutComInfo.GetRowCount()>0){
			fnc_SetStatus(SNIS_BOA_0003,  dsOutComInfo.GetRowCount());
	    }else{
			fnc_SetStatus(SNIS_BOA_0003,  0);
	    }
        
	}
	
	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

