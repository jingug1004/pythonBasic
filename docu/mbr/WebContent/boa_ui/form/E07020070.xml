function fcSearchCheck() {
   if(!fnc_IsMandatory(cboStndYear, "기준년도")){
		return false;
   }else if(!fnc_IsMandatory(cboPayMonth, "기준월")){
		return false;
   }
   return true;
   
}

//조회 
function fcSearch(obj) {
	if(!fcSearchCheck()){
		return;
	}
    
   var sServiceName = "e07020070-service:searchRacePrizList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRacePrizList=dsOutRacePrizList";
    
    fce_AddParam("STND_YEAR", cboStndYear.Value);
	fce_AddParam("RACER_NM", edRacerNm.Value);
    if (stPayMonth.text = "회차")   fce_AddParam("TMS", 		cboPayMonth.Value);
    else     						fce_AddParam("PAY_MONTH", 	cboPayMonth.Value);
	
	
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}


