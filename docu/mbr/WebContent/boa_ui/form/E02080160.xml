                    + '     <cell align="center" col="0"  colid="STND_YEAR"       display="text"/>';

    var sSumCnt        = "";
    var sSumPercentage = "";
    
    for ( var i = 0; i < dsOutRacerPerioNo.GetRowCount(); i++ ) {
        var sPerioNo = dsOutRacerPerioNo.GetColumn(i, "RACER_PERIO_NO");
        sBody      += '    <cell align="center" col="'+ (1 + (i * 2)) +'" colid="CNT_'       + sPerioNo +'" display="number" />';
        sBody      += '    <cell align="center" col="'+ (2 + (i * 2)) +'" colid="PERCENTAGE_'+ sPerioNo +'" display="number" />';
        
        if ( i != 0 ) {
            sSumCnt        += "+";
            sSumPercentage += "+";
        }

        sSumCnt        += "dsOutPerioList.GetColumn(row,&apos;CNT_"        + sPerioNo + "&apos;)";
        sSumPercentage += "dsOutPerioList.GetColumn(row,&apos;PERCENTAGE_" + sPerioNo + "&apos;)";
    }
    sBody      += '    <cell align="center" col="'+ (1 + (dsOutRacerPerioNo.GetRowCount() * 2)) +'" expr="' + sSumCnt        + '" display="number" />';
    sBody      += '    <cell align="center" col="'+ (2 + (dsOutRacerPerioNo.GetRowCount() * 2)) +'" expr="' + sSumPercentage + '" display="number" />';
    
    sBody   += '</body>';
    
    return sBody;
}
