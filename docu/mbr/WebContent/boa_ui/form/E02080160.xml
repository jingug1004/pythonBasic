/*-------------------------------------------------------------------------------------------------+
>>  Grid Column 생성
+-------------------------------------------------------------------------------------------------*/
function fcGetColumn() {
    var sColumn = "";
    sColumn = '					<columns>'
            + '                     	<col width="55"/>';
            
    for ( var i = 0; i <= dsOutRacerPerioNo.GetRowCount(); i++ ) {
        sColumn += '                    <col width="40"/>  ';
        sColumn += '                    <col width="40"/>  ';
    }

            
    sColumn += '					</columns>';
    return sColumn;
}

/*-------------------------------------------------------------------------------------------------+
>>  Grid Head 생성
+-------------------------------------------------------------------------------------------------*/
function fcGetHead() {
    var sHead = "";
    sHead       = '     <head>  '
                + '          <cell bkcolor="user20" col="0" color="user21" display="text" font="굴림,9" rowspan="2" text="년도"/>';
    
    for ( var i = 0; i < dsOutRacerPerioNo.GetRowCount(); i++ ) {
        var sPerioNo = dsOutRacerPerioNo.GetColumn(i, "RACER_PERIO_NO");
		sHead   += '	<cell bkcolor="user20" col="'+ (1 + (i * 2)) +'" color="user21" colspan="2" display="text" font="굴림,9" text="' + sPerioNo + '기"/>';
		sHead	+= '	<cell bkcolor="user20" col="'+ (1 + (i * 2)) +'" display="text" font="굴림,10" row="1" text="건수"/>';
		sHead	+= '	<cell bkcolor="user20" col="'+ (2 + (i * 2)) +'" display="text" font="굴림,10" row="1" text="백분률"/>';
    }
    sHead   += '	<cell bkcolor="user20" col="'+ (1 + (i * 2)) +'" color="user21" colspan="2" display="text" font="굴림,9" text="합계"/>';
    sHead	+= '	<cell bkcolor="user20" col="'+ (1 + (i * 2)) +'" display="text" font="굴림,10" row="1" text="건수"/>';
    sHead	+= '	<cell bkcolor="user20" col="'+ (2 + (i * 2)) +'" display="text" font="굴림,10" row="1" text="백분률"/>';

    sHead   += '         </head>';
    return sHead;
}

