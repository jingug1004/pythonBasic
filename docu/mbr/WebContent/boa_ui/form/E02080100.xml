function setGridSize(maxRaceNo){

	var nMaxRaceCnt = 8*2;
	var nMaxRaceNo = 0;
	
	if(fnc_isNull(maxRaceNo)){
		nMaxRaceNo = nMaxRaceCnt;
	}else{
		nMaxRaceNo = maxRaceNo;
	}
	
	nMaxRaceNo = toInteger(nMaxRaceNo)*2;
	
	//Alert("nMaxRaceNo : " + nMaxRaceNo);
	
	grdCourseList.Redraw = false;
	
	var nCol0W = grdCourseList.GetColProp(0, "Width");
	
	//ÀüÃ¼ÀûÀ¸·Î »çÀÌÁî¸¦ ³ÐÈû.
	for ( var i = 0; i < nMaxRaceCnt; i++ ) {
        grdCourseList.SetColProp(1 + i, "Width", 55);
    }
	   
    //nMaxRaceCnt ºÎÅÍ nMaxRaceNo±îÁö »çÀÌÁî¸¦ ÁÙÀÓ.
    for ( var i = nMaxRaceCnt; i > nMaxRaceNo; i-- ) {
        grdCourseList.SetColProp(i, "Width", 0);
    }
    
    grdCourseList.SetColProp(0, "Width", nCol0W);
    
    grdCourseList.Redraw = true;
