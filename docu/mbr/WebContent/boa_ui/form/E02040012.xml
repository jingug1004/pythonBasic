>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_Result(ErrorCode, ErrorMSG) ) return;
	
	// 공통코드 조회시
	if ( sServiceName == "common-service:searchCode" ) {
        return;
	}

	// 조회 후
	if ( sServiceName == "e02040012-service:searchMot" ) {
        fnc_SetStatus(SNIS_BOA_0003, dsOutEquip.GetRowCount());
        taRsn.Text = Arg_sEquipRsn;
        return;
	}

	// 조회 후
	if ( sServiceName == "e02040012-service:searchMotList" ) {
        fnc_SetStatus(SNIS_BOA_0003, dsOutEquipList.GetRowCount());
        return;
	}

	// 조회 후
	if ( sServiceName == "e02040012-service:searchBoat" ) {
        fnc_SetStatus(SNIS_BOA_0003, dsOutEquip.GetRowCount());
        taRsn.Text = Arg_sEquipRsn;
        return;
	}

	// 조회 후
	if ( sServiceName == "e02040012-service:searchBoatList" ) {
        fnc_SetStatus(SNIS_BOA_0003, dsOutEquipList.GetRowCount());
        return;
	}

	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

