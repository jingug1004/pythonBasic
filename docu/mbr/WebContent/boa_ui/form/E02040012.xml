function fcSearchEquipCheck(obj) {
    //if ( fnc_IsNull(Arg_sRacerNo) ) return false;
    
	return true;
}

function fcSearchEquip(obj) {
    if ( !fcSearchEquipCheck(obj) ) return;

	var sServiceName = "";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutEquip=dsOutEquip ";
    
	fce_AddParam(Trim("STND_YEAR  "), Arg_sStndYear );
	fce_AddParam(Trim("MBR_CD     "), Arg_sMbrCd    );
	fce_AddParam(Trim("TMS        "), Arg_sTms      );
	fce_AddParam(Trim("RACER_NO   "), Arg_sRacerNo  );
	fce_AddParam(Trim("NM_KOR     "), Arg_sNmKor    );
	// fce_AddParam(Trim("DAY_ORD    "), Arg_sDayOrd   );
	// fce_AddParam(Trim("RACE_NO    "), Arg_sRaceNo   );
	// fce_AddParam(Trim("RACE_REG_NO"), Arg_sRaceRegNo);
	
	if ( IsExistVar("Arg_sMotNo") ) {
        sServiceName = "e02040012-service:searchMot";
        fce_AddParam(Trim("MOT_NO"), Arg_sMotNo);
        stFormTitle.Value = "모터변경";
        E02040012.Value = "모터변경";
	}

	if ( IsExistVar("Arg_sBoatNo") ) {
        sServiceName = "e02040012-service:searchBoat";
        fce_AddParam(Trim("BOAT_NO"), Arg_sBoatNo);
        stFormTitle.Value = "보트변경";
        E02040012.Value = "보트변경";
	}

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


