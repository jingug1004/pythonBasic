function grdEquipList_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	var sEquipNo = dsOutEquipList.GetColumn(dsOutEquipList.currow, "EQUIP_NO");
	if ( fnc_IsNull(sEquipNo) ) {
        return;
	}
	
	if ( dsOutEquipList.GetColumn(dsOutEquipList.currow, "TRAN_USE_YN") == "Y" ) {
        fnc_message("이미 사용된 장비 입니다.", "N");
        return;
	}

	var asReturn = Array(2);
	asReturn[0] = sEquipNo;
	asReturn[1] = taRsn.Text;
	
	close(asReturn);
}

/*-------------------------------------------------------------------------------------------------+
>>  닫기버튼 Click시
+-------------------------------------------------------------------------------------------------*/
function btnClose_OnClick(obj)
{
	close(false);
}
