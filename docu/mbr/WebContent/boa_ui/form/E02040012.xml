function fcSearchCheck(obj) {

	return true;
}

function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutEquipList=dsOutEquipList ";
    
	fce_AddParam(Trim("STND_YEAR   "), Arg_sStndYear );
	fce_AddParam(Trim("MBR_CD      "), Arg_sMbrCd    );
	fce_AddParam(Trim("TMS         "), Arg_sTms      );
	fce_AddParam(Trim("AVG_RANK_SCR"), dsOutEquip.GetColumn(0, "AVG_RANK_SCR"));

	if ( IsExistVar("Arg_sMotNo") ) {
        sServiceName = "e02040012-service:searchMotList";
        fce_AddParam(Trim("MOT_NO"), edEquipNo.Text);
	}

	if ( IsExistVar("Arg_sBoatNo") ) {
        sServiceName = "e02040012-service:searchBoatList";
        fce_AddParam(Trim("BOAT_NO"), edEquipNo.Text);
	}

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


