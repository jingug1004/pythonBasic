function btnConfirm_OnClick(obj)
{
	if ( fnc_GetExistValue(dsOutTrngAskRacerList, "chk", "1") < 0 ) {
        fnc_message(L_COM_ALT_0027, "N");
        return;
	}
	
	var asReturn = Array();
	var nCount   = 0;
	for ( var i = 0; i < dsOutTrngAskRacerList.GetRowCount(); i++ ) {
        if ( dsOutTrngAskRacerList.GetColumn(i, "chk") == "1" ) {
            var sReturn = "";
            var sAplit  = "|";
            sReturn += dsOutTrngAskRacerList.GetColumn(i, "TRNG_EXPT_SEQ") + sAplit;
            sReturn += dsOutTrngAskRacerList.GetColumn(i, "RACER_NO"     ) + sAplit;
            sReturn += dsOutTrngAskRacerList.GetColumn(i, "NM_KOR"       ) + sAplit; 
            sReturn += dsOutTrngAskRacerList.GetColumn(i, "RACER_GRD_CD" ) + sAplit;
            sReturn += dsOutTrngAskRacerList.GetColumn(i, "TRNG_RSN_CD"  ) + sAplit;
            sReturn += dsOutTrngAskRacerList.GetColumn(i, "REST_DD"      ) + sAplit;

            asReturn[nCount++] = sReturn;
        }
	}
	close(asReturn);
}
