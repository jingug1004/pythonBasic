<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
선수사업자 정보/계좌 등록 
 -->
<service name="e07010010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerComInfo" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerComInfo"      value="searchRacerComInfo" />
    <transition name="saveRacerComInfo"    value="saveRacerComInfo" />
    <transition name="success" value="end" />
  </activity>

  <!-- 선수사업자 정보/계좌 조회   -->
  <activity name="searchRacerComInfo" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerComInfo" />
    <property name="sqlkey" value="tbeg_racer_com_info_ff001" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="RACER_NO"/>
    <property name="param1" value="RACER_NM"/>
  </activity>
  
 
  <!-- 선수사업자 정보/계좌 저장-->
  <activity name="saveRacerComInfo" class="snis.boa.financial.e07010010.activity.SaveRacerComInfo">
    <transition name="success" value="searchRacerComInfo" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

