<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02080120" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerList" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
	<transition name="searchIntroRunTm"  			value="searchIntroRunTm" /> 
	<transition name="searchStarTm"  			    value="searchStarTm" /> 
  </activity>

  <!-- 1위자(소개항주) 입상률 리스트 -->
  <activity name="searchIntroRunTm" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerList" />
    <property name="sqlkey" value="tbeb_organ_fb021" />
    <property name="param-count" value="8" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="ST_MTHD_CD" />
    <property name="param3" value="ST_MTHD_CD" />
    <property name="param4" value="STR_DT" />
    <property name="param5" value="END_DT" />
    <property name="param6" value="ST_MTHD_CD" />
    <property name="param7" value="ST_MTHD_CD" />
    <property name="dao" value="boadao" />
  </activity>

  <!-- 1위자(스타트타임) 입상률 리스트 -->
  <activity name="searchStarTm" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerList" />
    <property name="sqlkey" value="tbeb_organ_fb020" />
    <property name="param-count" value="8" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="ST_MTHD_CD" />
    <property name="param3" value="ST_MTHD_CD" />
    <property name="param4" value="STR_DT" />
    <property name="param5" value="END_DT" />
    <property name="param6" value="ST_MTHD_CD" />
    <property name="param7" value="ST_MTHD_CD" />
    <property name="dao" value="boadao" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

