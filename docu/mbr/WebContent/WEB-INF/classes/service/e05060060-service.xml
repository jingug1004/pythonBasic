<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05060060" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutPrivitWork|dsOutUploadFile" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPrivitWork"   value="searchPrivitWork" />
    <transition name="searchUploadFile"   value="searchUploadFile" />
    <transition name="savePrivitWork"     value="savePrivitWork" />
    <transition name="success"            value="end" />
  </activity>
  
  <!-- 사설경정활동 조회 -->
  <activity name="searchPrivitWork" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPrivitWork" />
    <property name="sqlkey" value="tbee_investigate_work_fe001" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="FROM_DATE"/>
    <property name="param1" value="TO_DATE"/>
    <property name="param2" value="ACT_DESC"/>
  </activity>
  
  <!-- 첨부파일 조회 -->
  <activity name="searchUploadFile" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutUploadFile" />
    <property name="sqlkey" value="tbee_investigate_work_fe002" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="ACT_DT"/>
    <property name="param1" value="ACT_NM"/>
  </activity>
  
  <!-- 사설경정활동 저장-->
  <activity name="savePrivitWork" class="snis.boa.fairness.e05060060.activity.SavePrivitWork">
    <transition name="success" value="searchPrivitWork" />
  </activity>
  <transaction-manager id="tx_boa" commit="true" />
</service>