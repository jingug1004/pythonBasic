<?xml version="1.0" encoding="EUC-KR"?>
<service name="e01010030" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsAuthListValue|dsUserListValue" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchUser"		    value="searchUser" />                     
    <transition name="searchUserMenu"		value="searchUserMenu" />                     
    <transition name="saveMenuAuth"			value="saveMenuAuth" />
    <transition name="success" 				value="end" />
  </activity>

  <!-- searchUserMenu -->
  <activity name="searchUserMenu" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsAuthListValue" />
    <property name="sqlkey" 				value="tbea_mn_auth_fa001" />    
    <property name="dao" 					value="boadao" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="USER_ID" />
    <property name="param1" 				value="USER_ID" />
  </activity>  
  
  <!-- searchTeamNm -->
  <activity name="searchUser" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsUserListValue" />
    <property name="sqlkey" 				value="tbea_user_fa005" />
    <property name="dao" 					value="boadao" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="TEAM_CD" />
    <property name="param1" 				value="USER_NM" />
  </activity>  
  
  <!-- saveMenuAuth -->
  <activity name="saveMenuAuth" class="snis.boa.system.e01010040.activity.SaveMenuAuth">
    <transition name="success" 				value="searchUserMenu" />    
  </activity>  
    
  <transaction-manager id="tx_boa" commit="true" />
</service>