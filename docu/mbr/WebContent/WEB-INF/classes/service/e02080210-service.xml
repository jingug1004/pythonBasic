<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02080210" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutPrizRace" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPrizRace"          value="searchPrizRace" />
    <transition name="success" value="end" />
  </activity>

  <!-- 대상경주 조회 -->
  <activity name="searchPrizRace" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPrizRace" />
    <property name="sqlkey" value="tbeb_race_tms_fb103" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="GRP_CD" />
    <property name="param1" value="CD" />
    <property name="param2" value="RACE_FROM_DT" />
    <property name="param3" value="RACE_TO_DT" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

