<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05040070" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutInvRacer" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchInvRacer"   value="searchInvRacer" />
    <transition name="saveInvRacer"     value="saveInvRacer" />
    <transition name="success"            value="end" />
  </activity>
  
 <!-- 조사중인 선수 등록 조회 -->
 <activity name="searchInvRacer" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutInvRacer" />
    <property name="sqlkey" value="tbee_investigate_racer_fe001" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
  </activity>
  
   <!-- 조사중인 선수 등록 저장-->
  <activity name="saveInvRacer" class="snis.boa.fairness.e05040070.activity.SaveInvRacer">
    <transition name="success" value="searchInvRacer" />
  </activity>
  <transaction-manager id="tx_boa" commit="true" />
</service>