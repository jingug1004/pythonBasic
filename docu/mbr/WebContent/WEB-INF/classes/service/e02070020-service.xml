<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02070020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutTrngExptRacer|dsOutRacerSanc|dsOutRacerIjry" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerSanc"        	value="searchRacerSanc" />
    <transition name="searchTrngExptRacer"    	value="searchTrngExptRacer" />
    <transition name="saveTrngExptRacer" 		value="saveTrngExptRacer" />
    <transition name="success" value="end" />
  </activity>

  <!-- 제재내용 조회 -->
  <activity name="searchRacerSanc" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerSanc" />
    <property name="sqlkey" value="tbee_racer_sanc_fb001" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="1" />
    <property name="param0" value="SANC_ISSUE_NO" />
  </activity>

  <!-- 훈련예정선수 조회 -->
  <activity name="searchTrngExptRacer" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutTrngExptRacer" />
    <property name="sqlkey" value="tbeb_trng_expt_racer_fb012" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="5" />
    <property name="param0" value="TRNG_RSN_CD" />
    <property name="param1" value="NM_KOR" />
    <property name="param2" value="TRNG_EXPT_STAT_CD" />
    <property name="param3" value="STR_DT" />
    <property name="param4" value="END_DT" />
  </activity>

  <!-- 훈련예정선수 저장 -->
  <activity name="saveTrngExptRacer" class="snis.boa.organization.e02070020.activity.SaveTrngExptRacer">
    <transition name="success" value="searchTrngExptRacer" />
  </activity>
  
  
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

