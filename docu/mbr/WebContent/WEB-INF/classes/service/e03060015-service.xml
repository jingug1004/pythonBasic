<?xml version="1.0" encoding="EUC-KR"?>
<service name="e03060015" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerArrangeEsc" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerArrangeEsc"      value="searchRacerArrangeEsc" />
    <transition name="searchRacerArrangeEscPrint" value="searchRacerArrangeEscPrint" />
    <transition name="saveRacerArrangeEsc"        value="saveRacerArrangeEsc" />
    <transition name="success" value="end" />
  </activity>
  
  <!-- 주선제외선수정보  조회 -->
  <activity name="searchRacerArrangeEsc" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerArrangeEsc" />
    <property name="sqlkey" value="tbec_arrange_esc_fc001" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="NM_KOR" />
  </activity>

  <!-- 주선제외선수정보 저장 -->
  <activity name="saveRacerArrangeEsc" class="snis.boa.racer.e03060015.activity.SaveRacerArrangeEsc">
    <transition name="success" value="searchRacerArrangeEsc" />
  </activity>

  <!-- 주선제외선수정보인쇄 조회 -->
  <activity name="searchRacerArrangeEscPrint" class="snis.boa.racer.e03060015.activity.SearchRacerArrangeEscPrint">
    <transition name="success" value="end" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

