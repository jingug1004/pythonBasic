<?xml version="1.0" encoding="EUC-KR"?>
<service name="e04110020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutStatTime1|dsOutStatTime2|dsOutStatTime3" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchStatTime1"     	value="searchStatTime1" />
    <transition name="searchStatTime2"     	value="searchStatTime2" />
    <transition name="searchStatTime3"     	value="searchStatTime3" />
    <transition name="success"        	    value="end" />
  </activity>
  
  <!-- 평균 스타트타임 -->
  <activity name="searchStatTime1" class="snis.boa.referere.e04110020.activity.SearchStatTime1">
    <transition name="success"     value="end" />
  </activity>
  
  <!-- 스타트타임 초별 분포도  -->
  <activity name="searchStatTime2" class="snis.boa.referere.e04110020.activity.SearchStatTime2">
    <transition name="success"     value="end" />
  </activity>
  
  <!-- 진입코스별 스타트 타임현황  -->
  <activity name="searchStatTime3" class="snis.boa.referere.e04110020.activity.SearchStatTime3">
    <transition name="success"     value="end" />
  </activity>
  
   <transaction-manager id="tx_boa" commit="true" />
</service>