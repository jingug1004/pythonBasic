<?xml version="1.0" encoding="EUC-KR"?>
<service name="e06050020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList"       value="dsVender" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchSuppVendr"  	    value="searchSuppVendr" />    
    <transition name="saveSuppVendr"   		value="saveSuppVendr" />    
  </activity>

  <!-- 공급처  조회 -->
  <activity name="searchSuppVendr" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsVender" />
    <property name="sqlkey" 				value="e06050020_s01" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="VENDR_NM" />  
    <property name="dao" 					value="boadao" />    
  </activity>
  
  <!-- 코드저장 -->
  <activity name="saveSuppVendr" 			class="snis.boa.equipment.e06050020.activity.SaveSuppVendr">
    <transition name="success" 	           	value="searchSuppVendr" />
  </activity>
  
	<transaction-manager id="tx_boa" commit="true" />
</service>

