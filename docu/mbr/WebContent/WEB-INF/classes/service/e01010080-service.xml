<?xml version="1.0" encoding="EUC-KR"?>
<service name="e01010080" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRefe" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRefe"			value="searchRefe" />
    <transition name="saveRefe"				value="saveRefe" />
    <transition name="success" 				value="end" />
  </activity>
  
  <!-- 심판 담당자  리스트 -->
  <activity name="searchRefe" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 	value="end" />
    <property name="resultkey" 	value="dsOutRefe" />
    <property name="sqlkey" 	value="tbed_refe_fd001" />
    <property name="dao" value="boadao" />
  </activity>  
      
  <!-- 심판 담당자  리스트 -->
  <activity name="saveRefe" class="snis.boa.system.e01010080.activity.SaveRefe">
    <transition name="success" value="end" />
  </activity>  
  
  <transaction-manager id="tx_boa" commit="true" />
</service>