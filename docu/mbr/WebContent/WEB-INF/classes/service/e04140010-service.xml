<?xml version="1.0" encoding="EUC-KR"?>
<service name="e04140010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutStatTime1|dsOutStatTime2|dsOutStatTime3|dsOutStatTime4" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchStatTime1"     	value="searchStatTime1" />
    <transition name="searchStatTime2"     	value="searchStatTime2" />
    <transition name="searchStatTime3"     	value="searchStatTime3" />
    <transition name="searchStatTime4"     	value="searchStatTime4" />
    <transition name="success"        	    value="end" />
  </activity>
  
  <!-- 평균 경주 주회타임 -->
  <activity name="searchStatTime1" class="snis.boa.referere.e04140010.activity.SearchStatTime1">
    <transition name="success"     value="end" />
  </activity>
  
  <!--월별 선두정 주회타임 비교  -->
  <activity name="searchStatTime2" class="snis.boa.referere.e04140010.activity.SearchStatTime2">
    <transition name="success"     value="end" />
  </activity>
  
   <!--년도별 BEST10 주회타임  -->
  <activity name="searchStatTime3" class="snis.boa.referere.e04140010.activity.SearchStatTime3">
    <transition name="success"     value="end" />
  </activity>
  
   <!--시간차가 < 0.00.099  -->
  <activity name="searchStatTime4" class="snis.boa.referere.e04140010.activity.SearchStatTime4">
    <transition name="success"     value="end" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>