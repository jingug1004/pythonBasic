<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
성적/수당 지급제한기준 등록 
 -->
<service name="e07010070" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutPrizDepsBac" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPrizDepsBac"      value="searchPrizDepsBac" />
    <transition name="savePrizDepsBac"    value="savePrizDepsBac" />
    <transition name="success" value="end" />
  </activity>
  
  
  <!-- 성적/수당 지급제한기준 조회   -->
  <activity name="searchPrizDepsBac" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPrizDepsBac" />
    <property name="sqlkey" value="tbeg_priz_deps_bac_ff001" />
    <property name="param-count" value="1" />
    <property name="param0" value="STND_YEAR"/>
    <property name="dao" value="boadao" />
  </activity>
  
 
  <!-- 성적/수당 지급제한기준 저장-->
  <activity name="savePrizDepsBac" class="snis.boa.financial.e07010070.activity.SavePrizDepsBac">
    <transition name="success" value="searchPrizDepsBac" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

