<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02010040" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRecdStndTerm" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRecdStndTerm"  value="searchRecdStndTerm" />
    <transition name="saveRecdStndTerm"    value="saveRecdStndTerm" />
    <transition name="success" value="end" />
  </activity>

  <!-- 성적반영분기 조회 -->
  <activity name="searchRecdStndTerm" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRecdStndTerm" />
    <property name="sqlkey" value="tbeb_recd_stnd_term_fb001" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
  </activity>
  
  <!-- 성적반영분기 저장 -->
  <activity name="saveRecdStndTerm" class="snis.boa.organization.e02010040.activity.SaveRecdStndTerm">
    <transition name="success" value="searchRecdStndTerm" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

