<?xml version="1.0" encoding="EUC-KR"?>
<service name="e09030001" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutEduCont" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEduCont"	value="searchEduCont" />
    <transition name="saveEduCont"		value="saveEduCont" />
    <transition name="success" 			value="end" />
  </activity>

  <!-- searchEduCont -->
  <activity name="searchEduCont" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="end" />
    <property name="resultkey" 		value="dsOutEduCont" />
    <property name="sqlkey" 		value="tbed_race_voil_edu_s04" />    
    <property name="dao" 			value="boadao" />
    <property name="param-count" 	value="6" />
    <property name="param0" 		value="STND_YEAR" />
    <property name="param1" 		value="MBR_CD" />
    <property name="param2" 		value="STMS" />
    <property name="param3" 		value="ETMS" />
    <property name="param4" 		value="DAY_ORD" />
    <property name="param5" 		value="DAY_ORD" />
  </activity>  
  
  <!-- saveEduCont -->
  <activity name="saveEduCont" class="snis.boa.referere.e09030001.activity.SaveEduCont">
    <transition name="success" 	 value="end" />    
  </activity>
    
  <transaction-manager id="tx_boa" commit="true" />
</service>