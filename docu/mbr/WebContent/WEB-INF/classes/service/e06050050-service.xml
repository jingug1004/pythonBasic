<?xml version="1.0" encoding="EUC-KR"?>
<service name="e06050050" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutList|dsOutSuppList|dsSuppList" />
    <transition name="success" value="EventRouter" />
  </activity>  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchOutList"  		value="searchOutList" />    
    <transition name="searchSuppList"  		value="searchSuppList" />    
    <transition name="searchOutSuppList"  	value="searchOutSuppList" />
    <transition name="saveOutList"			value="saveOutList" />    
    <transition name="success" 				value="end" />
  </activity>


  <!-- 출고리스트  조회      -->
  <activity name="searchOutList" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="searchSuppList" />
    <property name="resultkey" 		value="dsOutList" />
    <property name="sqlkey" 		value="e06050050_s01" />
    <property name="param-count" 	value="2" />
    <property name="dao" 			value="boadao" />
    <property name="param0" 		value="OUT_STR_DT"/>
    <property name="param1" 		value="OUT_END_DT"/>
  </activity>
  

  <!-- 현재고 소모품 조회      -->
  <activity name="searchSuppList" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="end" />
    <property name="resultkey" 		value="dsSuppList" />
    <property name="sqlkey" 		value="e06050050_s03" />
    <property name="param-count" 	value="1" />
    <property name="dao" 			value="boadao" />
    <property name="param0" 		value="SUPPLY_NM"/>
  </activity>
  

  <!-- 입고내역내역  조회      -->
  <activity name="searchOutSuppList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="end" />
    <property name="resultkey" 		value="dsOutSuppList" />
    <property name="sqlkey" 	 	value="e06050050_s02" />
    <property name="param-count" 	value="1" />
    <property name="dao" 			value="boadao" />
    <property name="param0" 		value="OUT_DT"/>
  </activity>

  <!-- 입고내역 저장 -->
  <activity name="saveOutList" 		class="snis.boa.equipment.e06050050.activity.SaveOutList">
    <transition name="success" 		value="searchOutList" />
  </activity>    <transaction-manager id="tx_boa" commit="true" />
</service>

