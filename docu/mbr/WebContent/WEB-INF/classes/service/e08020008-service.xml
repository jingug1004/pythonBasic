<?xml version="1.0" encoding="EUC-KR"?>
<service name="e08020008" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsIsoLog|dsLog" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchIsoLog"			value="searchIsoLog" />
    <transition name="saveIsoLog"  			value="saveIsoLog"   />
    <transition name="success" 					value="end" />
  </activity>
   <!-- ISOLOG List -->
  <activity name="searchIsoLog" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsIsoLog" />
    <property name="sqlkey" 				value="tbtq_isolog_fa001" />
    <property name="dao" 					value="boadao" />
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="ST_YEAR" />  
    <property name="param1" 				value="ED_YEAR" />    
    <property name="param2" 				value="USER_NM" />    
    <property name="param3" 				value="TEAM_CD" />       
  </activity>
    <!-- ISOLOGÀúÀå -->
  <activity name="saveIsoLog" 				class="snis.boa.tqc.e08020008.activity.SaveIsoLog">
    <transition name="success" 	value="end" />
  </activity>
    <transaction-manager id="tx_boa" commit="true" />
</service>