<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05070020" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
	    <property name="staticResultKeyFile" 	value="resultkey.properties" />
	    <property name="resultKeyList" 			value="dsOutRaceAnalysis|dsOutRaceResult" />
	    <transition name="success" 				value="EventRouter" />
	</activity>
  
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
	    <transition name="searchRaceAnalysis"   value="searchRaceAnalysis" />
	    <transition name="searchRaceResult"     value="searchRaceResult" />
	    <transition name="saveRaceAnalysis"		value="saveRaceAnalysis" />
	    <transition name="success"         		value="end" />
 	</activity>

 	<!-- 경주분석결과 조회 -->
	<activity name="searchRaceAnalysis" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
	    <property name="resultkey" value="dsOutRaceAnalysis" />
	    <property name="sqlkey" value="tbee_race_analysis_fe001" />
	    <property name="param-count" value="8" />
	    <property name="dao" value="boadao" />
	    <property name="param0" value="STND_YEAR"/>
	    <property name="param1" value="TMS"/>
	    <property name="param2" value="DAY_ORD"/>
	    <property name="param3" value="RACE_NO"/>
	    <property name="param4" value="STND_YEAR"/>
	    <property name="param5" value="TMS"/>
	    <property name="param6" value="DAY_ORD"/>
	    <property name="param7" value="RACE_NO"/>
	</activity>

	<!-- 경주결과 조회 -->
	<activity name="searchRaceResult" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
	    <property name="resultkey" value="dsOutRaceResult" />
	    <property name="sqlkey" value="tbee_race_analysis_fe002" />
	    <property name="param-count" value="4" />
	    <property name="dao" value="boadao" />
	    <property name="param0" value="STND_YEAR"/>
	    <property name="param1" value="TMS"/>
	    <property name="param2" value="DAY_ORD"/>
	    <property name="param3" value="RACE_NO"/>
	</activity>

	<!-- 경주분석결과 저장-->
	<activity name="saveRaceAnalysis" class="snis.boa.fairness.e05070020.activity.SaveRaceAnalysis">
    	<transition name="success" value="searchRaceAnalysis" />
	</activity>

	<transaction-manager id="tx_boa" commit="true" />
</service>