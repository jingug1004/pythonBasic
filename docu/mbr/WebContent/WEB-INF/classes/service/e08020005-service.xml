<?xml version="1.0" encoding="EUC-KR"?>
<service name="e08020005" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutIsoBord|dsOutIsoBordCount" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchCheckBoardList"			value="searchCheckBoardList" />
    <transition name="searchCheckCount"  			value="searchCheckCount" />
    <transition name="success" 					value="end" />
  </activity>
 <!-- ISO 업무내용 List -->
  <activity name="searchCheckBoardList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchCheckCount" />
    <property name="resultkey" value="dsOutIsoBord" />
    <property name="sqlkey" value="tbtq_checkbord_fa001" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="9" />
    <property name="param0" value="DEL_YN" />  
    <property name="param1" value="SBJT" />    
    <property name="param2" value="BORD_DESC" />    
    <property name="param3" value="MAKE_MAN" />  
    <property name="param4" value="SEQ_NO" />     
    <property name="param5" value="ROWNUM1" />    
    <property name="param6" value="ROWNUM2" />   
    <property name="param7" value="ROWNUM3" />    
    <property name="param8" value="ROWNUM4" />         
  </activity>
  <!-- ISO 업무내용 COUNT -->
  <activity name="searchCheckCount" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutIsoBordCount" />
    <property name="sqlkey" value="tbtq_checkbord_fa002" />
    <property name="dao" value="boadao" />    
    <property name="param-count" value="4" />
    <property name="param0" value="DEL_YN" />
    <property name="param1" value="SBJT" />    
    <property name="param2" value="BORD_DESC" />    
    <property name="param3" value="MAKE_MAN" />        
  </activity> 
          
  <transaction-manager id="tx_boa" commit="true" />
</service> 