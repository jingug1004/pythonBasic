<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
기수별 연금보조금 등록 
 -->
<service name="e07010040" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutPenSub" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPenSub"      value="searchPenSub" />
    <transition name="savePenSub"    value="savePenSub" />
    <transition name="success" value="end" />
  </activity>

  <!-- 기수별 연금보조금 조회   -->
  <activity name="searchPenSub" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPenSub" />
    <property name="sqlkey" value="tbeg_pen_sub_ff001" />
    <property name="param-count" value="1" />
    <property name="param0" value="STND_YEAR"/>
    <property name="dao" value="boadao" />
  </activity>
  
 
  <!-- 기수별 연금보조금 저장-->
  <activity name="savePenSub" class="snis.boa.financial.e07010040.activity.SavePenSub">
    <transition name="success" value="searchPenSub" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

