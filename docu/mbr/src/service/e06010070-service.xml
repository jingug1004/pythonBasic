<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
모터 보트 추첨 대상 등록  
 -->
<service name="e06010070" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutMotLot|dsOutBoatLot|dsOutPropellerLot|dsOutEquipDrwlt" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEquipLot"      value="searchEquipLot" />
    <transition name="saveEquipLot"    value="saveEquipLot" />
    <transition name="searchEquiDrwlt"    value="searchEquiDrwlt" />
    <transition name="saveEquipDrwlt"    value="saveEquipDrwlt" />
    <transition name="success" value="end" />
  </activity>

  <!-- 모터 보트 추점 대상 조회   -->  <activity name="searchEquipLot" class="snis.boa.equipment.e06010070.activity.SearchEquipLot">    <transition name="success" value="end" />  </activity>
  <!-- 모터 보트 추점 대상 저장-->  <activity name="saveEquipLot" class="snis.boa.equipment.e06010070.activity.SaveEquipLot">    <transition name="success" value="searchEquipLot" />  </activity>
  <transaction-manager id="tx_boa" commit="true" /></service>

