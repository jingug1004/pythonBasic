<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02070060" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsTrngYear|dsTrngSche" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchTrngYear"        	value="searchTrngYear" />
    <transition name="searchTrngScheList"       value="searchTrngScheList" />
    <transition name="saveTrngSche"             value="saveTrngSche" />
    <transition name="success" value="end" />
  </activity>

  <!-- 선수훈련년도 조회 -->
  <activity name="searchTrngYear" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsTrngYear" />
    <property name="sqlkey" value="tbeb_trng_sche_fb001" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="0" />
  </activity>


  <!-- 선수훈련일정 조회 -->
  <activity name="searchTrngScheList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsTrngSche" />
    <property name="sqlkey" value="tbeb_trng_sche_fb002" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="1" />
    <property name="param0" value="TRNG_YEAR" />
  </activity>


  <!-- 선수훈련일정 생성 및 저장 -->
  <activity name="saveTrngSche" class="snis.boa.organization.e02070060.activity.SaveTrngSche">
    <transition name="success" value="end" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

