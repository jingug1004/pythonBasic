<?xml version="1.0" encoding="EUC-KR"?>
<service name="e06050030" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsVender|dsOutOrderList|dsOutOrderSuppList" />
    <transition name="success" value="EventRouter" />
  </activity>  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchVenderList"     value="searchVenderList" />
    <transition name="searchOrder"  		value="searchOrder" />    
    <transition name="searchOrderDetail"  	value="searchOrderDetail" />
    <transition name="saveOrderSupp"		value="saveOrderSupp" />    
    <transition name="success" 				value="end" />
  </activity>

  <!-- 공급처 리스트 조회 -->
  <activity name="searchVenderList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsVender" />
    <property name="sqlkey" value="e06050030_s01" />
    <property name="param-count" value="0" />
    <property name="dao" value="boadao" />
  </activity>
  

  <!-- 주문서  조회      -->
  <activity name="searchOrder" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutOrderList" />
    <property name="sqlkey" value="e06050030_s02" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="ORDER_STR_DT"/>
    <property name="param1" value="ORDER_END_DT"/>
  </activity>
  

  <!-- 주문 상세내역  조회      -->
  <activity name="searchOrderDetail" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutOrderSuppList" />
    <property name="sqlkey" value="e06050030_s03" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="JUMUN_NO"/>
  </activity>
  
  <!-- 주문서저장 -->
  <activity name="saveOrderSupp" class="snis.boa.equipment.e06050030.activity.SaveOrderSupp">
    <transition name="success" 	value="searchOrder" />
  </activity>    <transaction-manager id="tx_boa" commit="true" />
</service>

