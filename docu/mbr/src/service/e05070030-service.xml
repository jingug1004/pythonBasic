<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05070030" initial="SetResultKeys">
	<activity name="SetResultKeys"				class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
	    <property name="staticResultKeyFile" 	value="resultkey.properties" />
	    <property name="resultKeyList" 			value="dsOutBrchRtrn|dsOutRacerRank" />
	    <transition name="success" 				value="EventRouter" />
	</activity>
  
	<activity name="EventRouter"				class="com.posdata.glue.biz.activity.PosDefaultRouter">
	    <transition name="searchBrchRtrn"		value="searchBrchRtrn" />
	    <transition name="searchRacerRank"		value="searchRacerRank" />
	    <transition name="saveBrchRtrnDel"		value="saveBrchRtrnDel" />
	    <transition name="success"				value="end" />
 	</activity>

	<!-- 지점고액환급정보  조회 -->
	<activity name="searchBrchRtrn"		class="snis.boa.fairness.e05070030.activity.SearchBrchRtrn">
    	<transition name="success"		value="end" />
	</activity>

	<!-- 해당경주 순위선수조회 -->
	<activity name="searchRacerRank"	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success"		value="end" />
		<property name="resultkey"		value="dsOutRacerRank" />
		<property name="sqlkey"			value="tbee_brch_rtrn_fe002" />
		<property name="param-count"	value="4" />
	    <property name="dao"			value="boadao" />
		<property name="param0" 		value="STND_YEAR" />  
		<property name="param1" 		value="TMS" />  
		<property name="param2" 		value="DAY_ORD" />  
		<property name="param3" 		value="RACE_NO" />  
	</activity>

	<!-- 지점고액환급정보저장 -->
	<activity name="saveBrchRtrnDel"	class="snis.boa.fairness.e05070030.activity.SaveBrchRtrnDel">
    	<transition name="success"		value="end" />
	</activity>

	<transaction-manager id="tx_boa" commit="true" />
</service>
