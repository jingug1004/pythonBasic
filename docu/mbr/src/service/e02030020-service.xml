<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02030020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutEscRacer" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRecdArrangeEscStnd"      value="searchRecdArrangeEscStnd" />
    <transition name="searchEscRacer"                value="searchEscRacer" />
    <transition name="saveRecdArrangeEscStnd"        value="saveRecdArrangeEscStnd" />
    <transition name="success" value="end" />
  </activity>

  <!-- 성적별 주선제외 기준 조회 -->
  <activity name="searchRecdArrangeEscStnd" class="snis.boa.organization.e02030020.activity.SearchRecdArrangeEscStnd">
    <transition name="success" value="end" />
  </activity>
  
  <!-- 성적별 주선제외 선수 조회 -->
  <activity name="searchEscRacer" class="snis.boa.organization.e02030020.activity.SearchEscRacer">
    <transition name="success" value="end" />
  </activity>
  
  <!-- 성적별 주선제외 선수 저장 -->
  <activity name="saveRecdArrangeEscStnd" class="snis.boa.organization.e02030020.activity.SaveRecdArrangeEscStnd">
    <transition name="success" value="searchRecdArrangeEscStnd" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

