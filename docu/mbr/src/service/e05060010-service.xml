<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05060010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutAcctTrans|dsOutAcctTransDetail" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchAcctTrans"   		value="searchAcctTrans" />
    <transition name="searchAcctTransDetail"   	value="searchAcctTransDetail" />
    <transition name="saveAcctTrans"     		value="saveAcctTrans" />
    <transition name="saveAcctTransDetail"		value="saveAcctTransDetail" />
    <transition name="success"           		value="end" />
  </activity>
  
 <!-- 계좌 거래내역 관리 조회 -->
  <activity name="searchAcctTrans" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchAcctTransDetail" />
    <property name="resultkey" value="dsOutAcctTrans" />
    <property name="sqlkey" value="tbee_main_account_fe001" />
    <property name="param-count" value="8" />
    <property name="dao" value="boadao" />
    <property name="param0" value="FROM_DATE"/>
    <property name="param1" value="TO_DATE"/>
    <property name="param2" value="FROM_DATE"/>
    <property name="param3" value="TO_DATE"/>
    <property name="param4" value="MJR_NM"/>
    <property name="param5" value="MJR_NM"/>
    <property name="param6" value="MJR_BK_ACT"/>
    <property name="param7" value="MJR_BK_ACT"/>
  </activity>
  
  <!-- 계좌 거래내역 관리 상세조회 -->
  <activity name="searchAcctTransDetail" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutAcctTransDetail" />
    <property name="sqlkey" value="tbee_money_account_fe001" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="FROM_DATE"/>
    <property name="param1" value="TO_DATE"/>
  </activity>
  
   <!-- 계좌 거래내역 관리 저장-->
  <activity name="saveAcctTrans" class="snis.boa.fairness.e05060010.activity.SaveAcctTrans">
    <transition name="success" value="searchAcctTrans" />
  </activity>
  
  <!-- 계좌 거래내역 관리상세  저장-->
  <activity name="saveAcctTransDetail" class="snis.boa.fairness.e05060010.activity.SaveAcctTransDetail">
    <transition name="success" value="searchAcctTransDetail" />
  </activity>
  <transaction-manager id="tx_boa" commit="true" />
</service>