<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02080200" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutPrizeRace|dsOutMapRaceInfo" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPrizGrp"          value="searchPrizGrp" />
    <transition name="searchMapRaceInfo"          value="searchMapRaceInfo" />
    <transition name="saveMapping"          value="saveMapping" />
    <transition name="success" value="end" />
  </activity>

  <!-- 그룹정보 조회 -->
  <activity name="searchPrizGrp" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPrizeRace"/>
    <property name="sqlkey" value="tbeb_priz_race_map_fb001" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="GRP_CD" />
  </activity>
  
  
  <!-- 매핑정보 조회 -->
  <activity name="searchMapRaceInfo" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutMapRaceInfo"/>
    <property name="sqlkey" value="tbeb_priz_race_map_fb002" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="GRP_CD" />
    <property name="param1" value="CD" />
  </activity>  
  
  <!-- 매핑저장 -->
  <activity name="saveMapping" class="snis.boa.organization.e02080200.activity.SaveMapping">
    <transition name="success" 	value="end" />
  </activity>  

  <transaction-manager id="tx_boa" commit="true" />
</service>

