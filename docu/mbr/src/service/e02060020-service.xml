<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02060020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRaceWinner|dsOutMaxRaceNo" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceWinner"      value="searchRaceWinner" />
    <transition name="searchRaceWinner2"      value="searchRaceWinner2" />
    <transition name="success" value="end" />
  </activity>

  <!-- 입상선수현황 조회 -->
  <activity name="searchRaceWinner" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchRaceWinner2" />
    <property name="resultkey" value="dsOutRaceWinner" />
    <property name="sqlkey" value="tbeb_race_winner_fb001" />
    <property name="param-count" value="2" />
    <property name="param0"  value="STR_DT" />
	<property name="param1"  value="END_DT" />
    <property name="dao" value="boadao" />
  </activity>
  
    <!-- 최대 경주수 조회 -->
  <activity name="searchRaceWinner2" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutMaxRaceNo" />
    <property name="sqlkey" value="tbeb_race_winner_fb002" />
    <property name="param-count" value="2" />
    <property name="param0"  value="STR_DT" />
	<property name="param1"  value="END_DT" />
    <property name="dao" value="boadao" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

