<?xml version="1.0" encoding="EUC-KR"?>
<!-- 소모성부품교체 관리  -->
<service name="e06010130" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutExpendableChg|dsOutExpendableChgDetail" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchExpendableChg"			value="searchExpendableChg" />
    <transition name="searchExpendableChgDetail"	value="searchExpendableChgDetail" />
    <transition name="saveExpendableChg"			value="saveExpendableChg" />
    <transition name="success"						value="end" />
  </activity>

  <!-- 소모성부품교체 조회  -->
  <activity name="searchExpendableChg" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchExpendableChgDetail" />
    <property name="resultkey" value="dsOutExpendableChg" />
    <property name="sqlkey" value="tbef_expendable_chg_ff001" />
    <property name="param-count" value="15" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="TMS"/>
    <property name="param3" value="STND_YEAR"/>
    <property name="param4" value="MBR_CD"/>
    <property name="param5" value="STND_YEAR"/>
    <property name="param6" value="MBR_CD"/>
    <property name="param7" value="STND_YEAR"/>
    <property name="param8" value="MBR_CD"/>
    <property name="param9" value="STND_YEAR"/>
    <property name="param10" value="MBR_CD"/>
    <property name="param11" value="STND_YEAR"/>
    <property name="param12" value="MBR_CD"/>
    <property name="param13" value="STND_YEAR"/>
    <property name="param14" value="MBR_CD"/>
  </activity>
  
  <!-- 소모성부품교체(모터 회차별) 조회  -->
  <activity name="searchExpendableChgDetail" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutExpendableChgDetail" />
    <property name="sqlkey" value="tbef_expendable_chg_ff002" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="MOT_NO"/>
  </activity>
  
  <!-- 소모성부품교체 저장-->
  <activity name="saveExpendableChg" class="snis.boa.equipment.e06010130.activity.SaveExpendableChg">
    <transition name="success" value="searchExpendableChg" />
  </activity>
  <transaction-manager id="tx_boa" commit="true" />
</service>

