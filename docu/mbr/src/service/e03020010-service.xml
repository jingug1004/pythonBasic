<?xml version="1.0" encoding="EUC-KR"?>
<service name="e03020010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerIjry|dsOutRacerSanc" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerIjry" value="searchRacerIjry" />
    <transition name="SaveRacerIjry" value="SaveRacerIjry" />
    <transition name="searchRacerSanc" value="searchRacerSanc" />
    <transition name="success" value="end" />
  </activity>
  
  <!-- 부상선수정보  조회 -->
  <activity name="searchRacerIjry" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchRacerSanc" />
    <property name="resultkey" value="dsOutRacerIjry" />
    <property name="sqlkey" value="tbec_racer_injury_fc002" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="NM_KOR" />
  </activity>

  <!-- 부상선수정보 저장 -->
  <activity name="SaveRacerIjry" class="snis.boa.racer.e03020010.activity.SaveRacerIjry">
    <transition name="success" value="searchRacerIjry" />
  </activity>


  <!-- 제재선수정보  조회 -->
  <activity name="searchRacerSanc" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerSanc" />
    <property name="sqlkey" value="tbee_racer_sanc_fc002" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="NM_KOR" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

