<?xml version="1.0" encoding="EUC-KR"?>
<service name="e01010210" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutProcess" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchProcess" value="searchProcess" />
    <transition name="saveProcess"   value="saveProcess" />
    <transition name="success" value="end" />
  </activity>

  <!-- 조회 -->
  <activity name="searchProcess" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutProcess" />
    <property name="sqlkey" value="tbea_duty_prs_hst_fa001" />
    <property name="param-count" value="9" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="OPTION" />
    <property name="param4" value="DT" />
    <property name="param5" value="DT" />
    <property name="param6" value="OPTION" />
    <property name="param7" value="MONTH" />
    <property name="param8" value="OPTION" />
    <property name="dao" value="boadao" />
  </activity>

  <!-- 착순점/사고점 저장 -->
  <activity name="saveProcess" class="snis.boa.system.e01010220.activity.SaveProcess">
    <transition name="success" value="end" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

