<?xml version="1.0" encoding="EUC-KR"?>
<service name="e06050040" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutOrderList|dsEntList" />
    <transition name="success" value="EventRouter" />
  </activity>  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchOrder"  		value="searchOrder" />    
    <transition name="searchEntList"  		value="searchEntList" />
    <transition name="saveEntList"			value="saveEntList" />    
    <transition name="success" 				value="end" />
  </activity>


  <!-- 주문서  조회      -->
  <activity name="searchOrder" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutOrderList" />
    <property name="sqlkey" value="e06050040_s01" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="ORDER_STR_DT"/>
    <property name="param1" value="ORDER_END_DT"/>
  </activity>
  

  <!-- 입고내역내역  조회      -->
  <activity name="searchEntList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="end" />
    <property name="resultkey" 		value="dsEntList" />
    <property name="sqlkey" 	 	value="e06050040_s02" />
    <property name="param-count" 	value="3" />
    <property name="dao" 			value="boadao" />
    <property name="param0" 		value="IO_DT"/>
    <property name="param1" 		value="IO_DT"/>
    <property name="param2" 		value="JUMUN_NO"/>
  </activity>

  <!-- 입고내역 저장 -->
  <activity name="saveEntList" class="snis.boa.equipment.e06050040.activity.SaveEntList">
    <transition name="success" 	value="searchOrder" />
  </activity>    <transaction-manager id="tx_boa" commit="true" />
</service>

