<?xml version="1.0" encoding="EUC-KR"?>
<service name="e08010001" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPartCodeListValue|dsSpecCodeListValue|dsDeptListValue" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchbranchcode"  	value="searchbranchcode" />
    <transition name="searchPartCode"    	value="searchPartCode" />
    <transition name="searchSpecCode"  	 	value="searchSpecCode" />
    <transition name="saveTqcCode"   		value="saveTqcCode" />    
    <transition name="success" 				value="end" />
  </activity> 
  <!-- 부서  조회(combo) -->
  <activity name="searchbranchcode" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsDeptListValue" />
    <property name="sqlkey" 				value="tbtq_branch_common" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="boadao" />    
  </activity>  
  <!-- 그룹코드  조회 -->
  <activity name="searchPartCode" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchSpecCode" />
    <property name="resultkey" 				value="dsPartCodeListValue" />
    <property name="sqlkey" 				value="tbtq_part_cd_fa001" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="boadao" />    
  </activity>
  
  <!-- 상세코드  조회 -->
  <activity name="searchSpecCode" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsSpecCodeListValue" />
    <property name="sqlkey" 				value="tbtq_spec_cd_fa001" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="boadao" />    
  </activity>
  
  <!-- 코드저장 -->
  <activity name="saveTqcCode" 				class="snis.boa.tqc.e08010001.activity.SaveTqcCode">
    <transition name="success" 	value="end" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>