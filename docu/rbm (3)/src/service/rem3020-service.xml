<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem3020" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsStayStatByRace" />
    <transition name="success"              value="EventRouter" />

  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">

    <transition name="searchStayStatByRace"       value="searchStayStatByRace" />         <!-- 체류인원  경주별 현황 조회 -->

    <transition name="success"              value="end" />

  </activity>


  <!--  체류인원  경주별 현황 조회  -->
  
  <activity name="searchStayStatByRace"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsStayStatByRace" />
    <property name="sqlkey"                 value="rem3020_s01" />    
    <property name="param-count"            value="6" />    
    <property name="param0"                 value="RACE_STR_DT" />      <!--  경주일자(시작) -->
    <property name="param1"                 value="RACE_END_DT" />      <!--  경주일자(종료) -->
    <property name="param2"                 value="MEET_CD" />          <!--  경륜장구분  -->
    <property name="param3"                 value="BRNC_CD" />          <!--  지점  -->
    <property name="param4"                 value="BRNC_CD" />          <!--  지점  -->
    <property name="param5"                 value="BRNC_CD" />          <!--  지점  -->
   <property name="dao"                    value="rbmdao" />       

  </activity>  

  <transaction-manager id="tx_rbm" commit="true" />

</service>



