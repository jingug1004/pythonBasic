<?xml version="1.0" encoding="EUC-KR"?>
<service name="rbs8020" initial="SetResultKeys">
  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsApplyList|dsWinnerList" />
    <transition name="success"              value="EventRouter" />
  </activity>  

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchApplyList"      value="searchApplyList" />  	<!-- 휴양소 신청내역  조회  -->
    <transition name="searchWinnerList"     value="searchWinnerList" />   	<!-- 휴양소별 전체 정보  조회 -->
    <transition name="saveDrawRslt"  		value="saveDrawRslt" /> 		<!-- 휴양소 정보 저장 -->
    <transition name="success"              value="end" />
  </activity>

  <!-- 휴양소 신청자  조회 -->
  <activity name="searchApplyList"       	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsApplyList" />
    <property name="sqlkey"                 value="rbs8020_s01" />    
    <property name="param-count"            value="2" />        
    <property name="param0" 				value="RSV_YEAR" /> 
    <property name="param1" 				value="RSV_TMS" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
    
  <!-- 휴양소 당첨자  조회  --> 
  <activity name="searchWinnerList"       	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsWinnerList" />
    <property name="sqlkey"                 value="rbs8020_s02" />    
    <property name="param-count"            value="2" />        
    <property name="param0" 				value="RSV_YEAR" /> 
    <property name="param1" 				value="RSV_TMS" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
  
  <!-- 휴양소 추첨결과 저장  --> 
  <activity name="saveDrawRslt"            class="snis.rbm.business.rbs8020.activity.SaveDrawRslt">
    <transition name="success"              value="searchApplyList" />
  </activity>    

  <transaction-manager id="tx_rbm" commit="true" />
</service>
