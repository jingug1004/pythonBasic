<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm3200" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsSellst|dsSellstRefineWin|dsSellstRefineGrnZn|dsTmsList" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 			class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchDivSales"	  	value="searchDivSales" />		<!-- 투표소별 발매(회차별,전체지점) -->    	
    	<transition name="searchDivSalesRefineWin"	value="searchDivSalesRefineWin" />		<!-- 투표소별 발매(회차별,전체지점) -->    	
		<transition name="searchDivSalesRefineGrnZn"	value="searchDivSalesRefineGrnZn" />		<!-- 투표소별 발매(회차별,전체지점) -->    	
		<transition name="searchTmsList"		value="searchTmsList" />		<!-- 회차리스트 조회 -->    	
		<transition name="success" 			  	value="end" />
	</activity>

	 <activity name="searchDivSales"         class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	   <transition name="success"              value="end" />
	   <property name="resultkey"              value="dsSellst" />
	   <property name="sqlkey"                 value="rsm3200_s01" />
	   <property name="param-count"            value="12" />
	   <property name="param0"                 value="CHK_REFUND" />      <!-- 시행처 구분 -->
	   <property name="param1"                 value="MEET_CD_001" />      <!-- 시행처 구분 -->
	   <property name="param2"                 value="MEET_CD_002" />      <!-- 시행처 구분 -->
	   <property name="param3"                 value="MEET_CD_003" />      <!-- 시행처 구분 -->
	   <property name="param4"                 value="MEET_CD_004" />      <!-- 시행처 구분 -->
	   <property name="param5"                 value="RACE_DAY_ST" />  <!-- 검색 시작일 -->    
	   <property name="param6"                 value="RACE_DAY_END" /> <!-- 검색종료일 -->
	   <property name="param7"                 value="CHK_GREEN" />      <!-- 그린카드 포함 여부 -->
	   <property name="param8"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param9"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param10"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param11"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="dao"                    value="rbmdao" />
	 </activity>  
	 
	 <activity name="searchDivSalesRefineWin"         class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	   <transition name="success"              value="end" />
	   <property name="resultkey"              value="dsSellstRefineWin" />
	   <property name="sqlkey"                 value="rsm3200_s03" />
	   <property name="param-count"            value="11" />
	   <property name="param0"                 value="MEET_CD_001" />      <!-- 시행처 구분 -->
	   <property name="param1"                 value="MEET_CD_002" />      <!-- 시행처 구분 -->
	   <property name="param2"                 value="MEET_CD_003" />      <!-- 시행처 구분 -->
	   <property name="param3"                 value="MEET_CD_004" />      <!-- 시행처 구분 -->
	   <property name="param4"                 value="RACE_DAY_ST" />  <!-- 검색 시작일 -->    
	   <property name="param5"                 value="RACE_DAY_END" /> <!-- 검색종료일 -->
	   <property name="param6"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param7"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param8"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param9"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param10"                 value="CHK_GREEN" />      <!-- 그린카드 포함 여부 -->
	   <property name="dao"                    value="rbmdao" />
	 </activity>  
	 
	 <activity name="searchDivSalesRefineGrnZn"         class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	   <transition name="success"              value="end" />
	   <property name="resultkey"              value="dsSellstRefineGrnZn" />
	   <property name="sqlkey"                 value="rsm3200_s04" />
	   <property name="param-count"            value="16" />
	   <property name="param0"                 value="MEET_CD_001" />      <!-- 시행처 구분 -->
	   <property name="param1"                 value="MEET_CD_002" />      <!-- 시행처 구분 -->
	   <property name="param2"                 value="MEET_CD_003" />      <!-- 시행처 구분 -->
	   <property name="param3"                 value="MEET_CD_004" />      <!-- 시행처 구분 -->
	   <property name="param4"                 value="TMS" /> 			 <!-- 회차 -->    
	   <property name="param5"                 value="RACE_DAY_ST" />  <!-- 검색 시작일 -->    
	   <property name="param6"                 value="RACE_DAY_END" /> <!-- 검색종료일 -->
	   <property name="param7"                 value="TMS" /> 			 <!-- 회차 -->    
	   <property name="param8"                 value="MEET_CD" /> 			 <!-- 회차 -->    
	   <property name="param9"                 value="STND_YEAR" /> 			 <!-- 회차 -->    
	   <property name="param10"                 value="TMS" /> 			 <!-- 회차 -->    
	   <property name="param11"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param12"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param13"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param14"                 value="COMM_NO" />      <!-- 지점 코드 -->
	   <property name="param15"                 value="CHK_GREEN" />      <!-- 그린카드 포함 여부 -->
	   <property name="dao"                    value="rbmdao" />
	 </activity>  
	 
	 <activity name="searchTmsList"         	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	   <transition name="success"              value="end" />
	   <property name="resultkey"              value="dsTmsList" />
	   <property name="sqlkey"                 value="rsm3200_s05" />
	   <property name="param-count"            value="2" />
	   <property name="param0"                 value="MEET_CD" />      	<!-- 경륜/경정 구분 -->
	   <property name="param1"                 value="STND_YEAR" />     <!-- 경주년도 -->
	   <property name="dao"                    value="rbmdao" />
	 </activity>  
	 
	<transaction-manager id="tx_rbm" commit="true" />
  
</service>



