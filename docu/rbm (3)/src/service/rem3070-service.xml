<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem3070" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsStayEndMana" />
    <transition name="success"              value="EventRouter" />

  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">

    <transition name="searchStayEndMana"       value="searchStayEndMana" />         <!--  마감취소    조회 -->
    <transition name="saveStayEnd"         value="saveStayEnd" />           <!--  마감취소  저장  -->

    <transition name="success"              value="end" />

  </activity>


  <!--  마감취소  조회 -->
  
  <activity name="searchStayEndMana"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsStayEndMana" />
    <property name="sqlkey"                 value="rem3070_s01" />
    
    <property name="param-count"            value="5" />
    
    <property name="param0"                 value="BRNC_CD" />      <!--  지점 -->
    <property name="param1"                 value="MEET_CD" />      <!--  구분 -->
    <property name="param2"                 value="RACE_STR_DT" />  <!--  경주일자  -->
    <property name="param3"                 value="RACE_END_DT" />  <!--  경주일자  -->
    <property name="param4"                 value="END_YN" />       <!--  마감여부  -->
   
   <property name="dao"                    value="rbmdao" />       

  </activity>  
  
  <!-- 마감취소  저장 -->

  <activity name="saveStayEnd"             class="snis.rbm.business.rem3070.activity.SaveStayEnd">
    <transition name="success"              value="end" />
  </activity>



  <transaction-manager id="tx_rbm" commit="true" />

</service>



