<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbs5010" initial="SetResultKeys">

	<activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
		<property name="staticResultKeyFile"    value="resultkey.properties" />
		<property name="resultKeyList"          value="dsPcManaList" />
		<transition name="success"              value="searchPcMana" />
	</activity>

   <!-- PC정보  조회  -->
	<activity name="searchPcMana" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" 			value="end" />
		<property name="resultkey" 			value="dsPcManaList" />
		<property name="sqlkey"				value="rbs5010_s01" />
		<property name="param-count" 		value="2" />    
		<property name="param0" 			value="USE_DEPT_CD" />   	<!-- 사용부서코드  -->
		<property name="param1" 			value="USER_NM" /> 			<!-- 사용자이름     -->    
		<property name="dao" 				value="rbmdao" />
	</activity>

	<transaction-manager id="tx_rbm" commit="true" />

</service>



