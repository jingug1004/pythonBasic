<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm3140" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsPool" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchPool"         value="searchPool" /> 	<!-- 승식별 배당률 년도별 -->
    	<transition name="searchPoolRaceDay"  value="searchPoolRaceDay" /> 	<!-- 승식별 배당률 기간별-->
		<transition name="success" 			  value="end" />
	
	</activity>
	
	
	<!-- 승식별 배당률 년도별-->
	<activity name="searchPool" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	  value="end" />
	  <property name="resultkey" 	  value="dsPool" />
	  <property name="sqlkey" 		  value="rsm3140_s01" />
	  <property name="param-count" 	  value="4" />
	  <property name="param0" 		  value="MEET_CD" />	<!-- 개최지 코드    -->
	  <property name="param1" 		  value="STND_YEAR" />	<!-- 기준년도 -->
	  <property name="param2" 		  value="MEET_CD" />	<!-- 개최지 코드    -->
	  <property name="param3" 		  value="STND_YEAR" />	<!-- 기준년도 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	
	<!-- 승식별 배당률 기간별 -->
	<activity name="searchPoolRaceDay" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 		  value="end" />
	  <property name="resultkey" 		  value="dsPool" />
	  <property name="sqlkey" 		  value="rsm3140_s02" />
	  <property name="param-count" 	  value="6" />
	  <property name="param0" 		  value="MEET_CD" />	<!-- 개최지 코드    -->
	  <property name="param1" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->
	  <property name="param2" 		  value="RACE_DAY_END" />	<!-- 종료일 -->
	  <property name="param3" 		  value="MEET_CD" />	<!-- 개최지 코드    -->
	  <property name="param4" 		  value="RACE_DAY_ST" />	<!-- 시작일 -->
	  <property name="param5" 		  value="RACE_DAY_END" />	<!-- 종료일 -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	
 	
  <transaction-manager id="tx_rbm" commit="true" />

</service>



