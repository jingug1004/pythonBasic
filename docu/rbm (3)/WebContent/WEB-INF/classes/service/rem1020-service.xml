<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem1020" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsEntRefuReport" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEntRefuReport"	value="searchEntRefuReport" /> <!-- 입장거부자 현황조회  -->
  </activity>
  

  <!-- 입장거부자 현황 조회  -->
   <activity name="searchEntRefuReport"	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 			value="end" />
    <property name="resultkey" 			value="dsEntRefuReport" />

    <property name="sqlkey"				value="rem1020_s01" />

    <property name="param-count" 		value="3" />
    
    <property name="param0" 			value="BRNC_CD" />			<!-- 지점  -->
    <property name="param1" 			value="RACE_DT_FROM" />		<!-- 경주일자From -->
    <property name="param2" 			value="RACE_DT_TO" />		<!-- 경주일자To -->
    
    <property name="dao" 				value="rbmdao" />
  </activity>   
  
  <transaction-manager id="tx_rbm" commit="true" />
</service>