		<!-- PC파일 업로드 내역 조회 -->
	<activity name="searchPcUpList" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  <transition name="success" 				value="end" />
	  <property name="resultkey" 				value="dsPcUpList" />
	  <property name="sqlkey" 				value="rsm1020_s01" />
	  <property name="param-count" 			value="3" />
	  <property name="param0" 				value="STND_YEAR" />	<!-- 기준년도 -->
	  <property name="param1" 				value="STND_YEAR" />	<!-- 기준년도 -->
	  <property name="param2" 				value="STND_YEAR" />	<!-- 기준년도 -->
	  <property name="dao" 					value="rbmdao" />    
	
	</activity>
	
	<!-- PC파일 업로드 내역 조회 -->
	<activity name="searchPcUpList(X)" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  <transition name="success" 				value="end" />
	  <property name="resultkey" 				value="dsPcUpList" />
	  <property name="sqlkey" 				value="rsm1020_s01" />
