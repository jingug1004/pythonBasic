<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm1020" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsPcUpList|dsGita|dsKtax|dsPayoffs|dsResults" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchPcUpList"	value="searchPcUpList" />	<!-- PC파일 업로드 내역 조회 -->
