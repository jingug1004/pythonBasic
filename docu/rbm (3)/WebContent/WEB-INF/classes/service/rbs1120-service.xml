<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs1120" initial="SetResultKeys">
    <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsWebNotice|dsHnrRefreReqWeb|dsHnrRefreReq" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 			class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchWebNotice"     	value="searchWebNotice" />	
	    <transition name="searchWebRecv"  		value="searchWebRecv" />	
	    <transition name="convHnrRefreWebRecv"  value="convHnrRefreWebRecv" />
	    <transition name="searchHnrRefreRecv"  	value="searchHnrRefreRecv" />	
	    <transition name="saveHnrRefreReq"  	value="saveHnrRefreReq" />
		<transition name="success" 			  	value="end" />	</activity> 		
  	<!-- 명예심판 홈페이지 공지사항 조회 -->
  	<activity name="searchWebNotice" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsWebNotice" />
    	<property name="sqlkey" 		  value="rbs1120_s01" />
    	<property name="param-count" 	  value="2" />
    	<property name="param0" 		  value="MEET_CD" />    
    	<property name="param1" 		  value="STND_YEAR" />    
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
  	<!-- 명예심판 홈페이지 신청내역 조회 -->
  	<activity name="searchWebRecv" 	  	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsHnrRefreReqWeb" />
    	<property name="sqlkey" 		  value="rbs1120_s02" />
    	<property name="param-count" 	  value="1" />
    	<property name="param0" 		  value="WEB_PARENT_SEQ_ID" />    
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
  	<!-- 명예심판 대상자 등록내역 조회 -->
  	<activity name="searchHnrRefreRecv"   class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success" 		  value="end" />
    	<property name="resultkey" 		  value="dsHnrRefreReq" />
    	<property name="sqlkey" 		  value="rbs1120_s03" />
    	<property name="param-count" 	  value="2" />
    	<property name="param0" 		  value="MEET_CD" />    
    	<property name="param1" 		  value="STND_YEAR" />    
    	<property name="dao" 			  value="rbmdao" />
  	</activity>
	
	<!-- 명예심판 홈페이지 신청내역 가져오기 -->
  	<activity name="convHnrRefreWebRecv" 	  class="snis.rbm.business.rbs1120.activity.ConvHnrRefreWebRecv">
    	<transition name="success" 	  	  value="end" />
  	</activity>
  	
	<!-- 명예심판 직접 입력자료 저장 -->
  	<activity name="saveHnrRefreReq" 	  class="snis.rbm.business.rbs1120.activity.SaveHnrRefreReq">
    	<transition name="success" 	  	  value="end" />
  	</activity>
  	  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

