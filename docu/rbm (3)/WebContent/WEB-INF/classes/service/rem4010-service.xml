<?xml version="1.0" encoding="EUC-KR"?>

<service name="rem4010" initial="SetResultKeys">

  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsStayStatByDay" />
    <transition name="success"              value="EventRouter" />

  </activity>

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchStayStatByDay"  value="searchStayStatByDay" />         <!--  일별 지점별 입장현황 조회 -->
    <transition name="success"              value="end" />
  </activity>


  <!--  일별 지점별 입장현황 조회 -->  
  <activity name="searchStayStatByDay"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsStayStatByDay" />
    <property name="sqlkey"                 value="rem4010_s01" />    
    <property name="param-count"            value="6" />    
    <property name="param0"                 value="T_ONLY" />	    <!--  광명본장 입장권 발매기 제외 여부  -->
    <property name="param1"                 value="MEET_CD" />      <!--  경륜/경정 -->
    <property name="param2"                 value="STR_DT" />  		<!--  시작일자  -->
    <property name="param3"                 value="END_DT" />  		<!--  종료일자  -->   
    <property name="param4"                 value="BRNC_CD" />      <!--  지점 -->
    <property name="param5"                 value="EXCEPT_KM" />	<!--  광명본장 제외 여부  -->
    <property name="dao"                    value="rbmdao" />
  </activity>  
  
  <transaction-manager id="tx_rbm" commit="true" />

</service>



