<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm1150" initial="SetResultKeys">
    <activity name="SetResultKeys"   				class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  		value="resultkey.properties" />	    <property name="resultKeyList" 		  		value="dsList" />	    <transition name="success" 			  		value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchBuyList"  			value="searchBuyList" />		<!--  기간별 응모현황  -->
		<transition name="success" 		  			value="end" />	</activity> 	
 	
 	<!-- 개인별 구매내역 -->
	<activity name="searchBuyList" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">	
	  <transition name="success" 		value="end" />
	  <property name="resultkey" 		value="dsList" />
	  <property name="sqlkey" 			value="rsm1150_s01" />
	  <property name="param-count" 		value="6" />
	  <property name="param0" 			value="BUY_DT" />	<!-- 구매일자  -->
	  <property name="param1" 			value="CARD_NO" />	<!-- 카드번호 -->
	  <property name="param2" 			value="BUY_DT" />	<!-- 구매일자  -->
	  <property name="param3" 			value="BUY_DT" />	<!-- 구매일자  -->
	  <property name="param4" 			value="BUY_DT" />	<!-- 구매일자  -->
	  <property name="param5" 			value="CARD_NO" />	<!-- 카드번호 -->
	  <property name="dao" 				value="rbmdao" />
	</activity>
	
	<transaction-manager id="tx_rbm" commit="true" />
  </service>