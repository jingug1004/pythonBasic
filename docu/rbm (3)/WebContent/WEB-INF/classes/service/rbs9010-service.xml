<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbs9010" initial="SetResultKeys">
  <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile"    value="resultkey.properties" />
    <property name="resultKeyList"          value="dsNetPlan|dsNetRslt|dsNetRsltComp|dsTotPlan|dsTotRslt|dsTotRsltComp" />
    <transition name="success"              value="EventRouter" />
  </activity>  

  <activity name="EventRouter"              class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchNetPlan"      	value="searchNetPlan" />  		<!-- 연도별 총량관리계획  조회  -->
    <transition name="searchNetRslt"        value="searchNetRslt" />   	    <!-- 연도별 총량 실적  조회 -->
    <transition name="searchNetRsltComp"    value="searchNetRsltComp" />   	<!-- 분기별별 총량관리 실적  조회 -->
    <transition name="saveNetPlan"  		value="saveNetPlan" /> 		<!-- 연도별 총량관리 계획 저장 -->
    <transition name="searchTotPlan"      	value="searchTotPlan" />  		<!-- 연도별 총량관리계획  조회  -->
    <transition name="searchTotRslt"        value="searchTotRslt" />   	    <!-- 연도별 총량 실적  조회 -->
    <transition name="searchTotRsltComp"    value="searchTotRsltComp" />   	<!-- 분기별별 총량관리 실적  조회 -->
    <transition name="saveTotPlan"  		value="saveTotPlan" /> 		<!-- 연도별 총량관리 계획 저장 -->
    <transition name="success"              value="end" />
  </activity>

  <!-- 연도별 총량관리 계획  조회 -->
  <activity name="searchNetPlan"       		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="searchNetRslt" />
    <property name="resultkey"              value="dsNetPlan" />
    <property name="sqlkey"                 value="rbs9010_s01" />    
    <property name="param-count"            value="2" />        
    <property name="param0" 				value="MEET_CD" /> 
    <property name="param1" 				value="STND_YEAR" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
    
  <!-- 월별 총량관리별  실적 조회  --> 
  <activity name="searchNetRslt"       	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="searchNetRsltComp" />
    <property name="resultkey"              value="dsNetRslt" />
    <property name="sqlkey"                 value="rbs9010_s02" />    
    <property name="param-count"            value="8" />        
    <property name="param0" 				value="MEET_CD" /> 
    <property name="param1" 				value="STND_YEAR" /> 
    <property name="param2" 				value="MEET_CD" /> 
    <property name="param3" 				value="STND_YEAR" /> 
    <property name="param4" 				value="MEET_CD" /> 
    <property name="param5" 				value="STND_YEAR" /> 
    <property name="param6" 				value="MEET_CD" /> 
    <property name="param7" 				value="STND_YEAR" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
  
  <!-- 분기별 총량관리 실적  조회 --> 
  <activity name="searchNetRsltComp"       		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsNetRsltComp" />
    <property name="sqlkey"                 value="rbs9010_s03" />    
    <property name="param-count"            value="10" />        
    <property name="param0" 				value="MEET_CD" /> 
    <property name="param1" 				value="STND_YEAR" /> 
    <property name="param2" 				value="MEET_CD" /> 
    <property name="param3" 				value="STND_YEAR" /> 
    <property name="param4" 				value="MEET_CD" /> 
    <property name="param5" 				value="STND_YEAR" /> 
    <property name="param6" 				value="MEET_CD" /> 
    <property name="param7" 				value="STND_YEAR" /> 
    <property name="param8" 				value="MEET_CD" /> 
    <property name="param9" 				value="STND_YEAR" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
  
  <!-- 연도별 총량관리 계획 저장  --> 
  <activity name="saveNetPlan"            class="snis.rbm.business.rbs9010.activity.SaveSalesPlan">
    <transition name="success"              value="searchNetPlan" />
  </activity>    

  <!-- 연도별 총량관리 계획  조회 -->
  <activity name="searchTotPlan"       		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="searchTotRslt" />
    <property name="resultkey"              value="dsTotPlan" />
    <property name="sqlkey"                 value="rbs9010_s11" />    
    <property name="param-count"            value="2" />        
    <property name="param0" 				value="MEET_CD" /> 
    <property name="param1" 				value="STND_YEAR" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
    
  <!-- 월별 총량관리별  실적 조회  --> 
  <activity name="searchTotRslt"       	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="searchTotRsltComp" />
    <property name="resultkey"              value="dsTotRslt" />
    <property name="sqlkey"                 value="rbs9010_s12" />    
    <property name="param-count"            value="4" />        
    <property name="param0" 				value="MEET_CD" /> 
    <property name="param1" 				value="STND_YEAR" /> 
    <property name="param2" 				value="MEET_CD" /> 
    <property name="param3" 				value="STND_YEAR" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
  
  <!-- 분기별 총량관리 실적  조회 --> 
  <activity name="searchTotRsltComp"       		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"              value="end" />
    <property name="resultkey"              value="dsTotRsltComp" />
    <property name="sqlkey"                 value="rbs9010_s13" />    
    <property name="param-count"            value="6" />        
    <property name="param0" 				value="MEET_CD" /> 
    <property name="param1" 				value="STND_YEAR" /> 
    <property name="param2" 				value="MEET_CD" /> 
    <property name="param3" 				value="STND_YEAR" /> 
    <property name="param4" 				value="MEET_CD" /> 
    <property name="param5" 				value="STND_YEAR" /> 
    <property name="dao"                    value="rbmdao" />    
  </activity>
  
  <!-- 연도별 총량관리 계획 저장  --> 
  <activity name="saveTotPlan"            class="snis.rbm.business.rbs9010.activity.SaveSalesPlan">
    <transition name="success"              value="searchTotPlan" />
  </activity>    

  <transaction-manager id="tx_rbm" commit="true" />
</service>
