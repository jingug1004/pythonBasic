<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbo4090" initial="SetResultKeys">

    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsList|dsListDetail" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchList"         value="searchList" />
	    <transition name="searchListDetail"   value="searchListDetail" />
		<transition name="SaveBranchUse"   	  value="SaveBranchUse" />	
		<transition name="success" 			  value="end" />
	</activity>
	
	<!-- 지점부품사용 조회 -->
  <activity name="searchList"		  class="com.posdata.glue.miplatform.biz.activity.PosSearch">  
	
	<transition name="success" 		  value="end" />
	
	<property name="resultkey" 		  value="dsList" />
	<property name="sqlkey" 		  value="rbo4090_s01" />
	<property name="param-count" 	  value="4" />
	<property name="param0" 		  value="BRNC_CD" /> 
	<property name="param1" 		  value="PARTS_CD" /> 
	<property name="param2" 		  value="PARTS_LCD" />
	<property name="param3" 		  value="PARTS_MCD" />
	<property name="dao" 			  value="rbmdao" />
     
  </activity>
  
  	<!-- 지점부품 세부사용 조회 -->
  <activity name="searchListDetail"		  class="com.posdata.glue.miplatform.biz.activity.PosSearch">  
	
	<transition name="success" 		  value="end" />
	
	<property name="resultkey" 		  value="dsListDetail" />
	<property name="sqlkey" 		  value="rbo4090_s02" />
	<property name="param-count" 	  value="4" />	
	<property name="param0" 		  value="USE_DT_FROM" />
	<property name="param1" 		  value="USE_DT_TO" />
	<property name="param2" 		  value="PARTS_CD" />
	<property name="param3" 		  value="BRNC_CD" />	
	<property name="dao" 			  value="rbmdao" />

  </activity>
  
    <!-- 지점부품사용등록 저장 -->
  <activity name="SaveBranchUse" 	  class="snis.rbm.business.rbo4090.activity.SaveBranchUse">
    	<transition name="success" 	  value="end" />
  </activity>
  
  <transaction-manager id="tx_rbm" commit="true" />

</service>