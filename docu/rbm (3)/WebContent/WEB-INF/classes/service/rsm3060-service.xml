<?xml version="1.0" encoding="EUC-KR"?>

<service name="rsm3060" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsDIVTMSCNT|dsDivTmsManCnt" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchDIVTMSCNT01"         value="searchDIVTMSCNT01" />	<!-- 지점별 매출건수(전체) -->    	
    	<transition name="searchDivTmsManCnt"        value="searchDivTmsManCnt" />	<!-- 지점별 매출건수(전체) -->    	
		<transition name="success" 			  value="end" />
	</activity>
 	
 	<!-- 지점별 매출건수  -->
	<activity name="searchDIVTMSCNT01" 	  class="snis.rbm.business.rsm3060.activity.SearchDIVTMSCNT">
	  <transition name="success" 		  value="searchDivTmsManCnt" />
	</activity>
	
 	
 	<!-- 지점별 매출건수  -->
	<activity name="searchDivTmsManCnt" 	  class="snis.rbm.business.rsm3060.activity.SearchDivTmsManCnt">
	  <transition name="success" 		  value="end" />
	</activity>
	
  <transaction-manager id="tx_rbm" commit="true" />

</service>



