<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rsm2120" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsDivGoalSalesRslt|dsTmsProcessRatio" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">
    	<transition name="searchDivGoalSalesRslt" value="searchDivGoalSalesRslt" />					<!-- 사업장별 매출목표 및 실적 검색 -->
    	<transition name="searchTmsProcessRatio"  value="searchTmsProcessRatio" />					<!-- 경주일 진행률 -->
		<transition name="success" 			  	  value="end" />
	</activity>
 	
 	<!-- 사업장별 매출목표 및 실적 검색 -->
	<activity name="searchDivGoalSalesRslt" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	  value="searchTmsProcessRatio" />
	  <property name="resultkey" 	  value="dsDivGoalSalesRslt" />
	  <property name="sqlkey" 		  value="rsm2120_s01" />
	  <property name="param-count" 	  value="5" />
	  <property name="param0" 		  value="RACE_DAY" />	<!-- 경주일 yyyymmdd  -->
	  <property name="param1" 		  value="RACE_DAY" />	<!-- 경주일 yyyymmdd  -->
	  <property name="param2" 		  value="RACE_DAY" />	<!-- 경주일 yyyymmdd  -->
	  <property name="param3" 		  value="RACE_DAY" />	<!-- 경주일 yyyymmdd  -->
	  <property name="param4" 		  value="RACE_DAY" />	<!-- 경주일 yyyymmdd  -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
	
	<!-- 경주일 진행률 -->
	<activity name="searchTmsProcessRatio" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
	  <transition name="success" 	  value="end" />
	  <property name="resultkey" 	  value="dsTmsProcessRatio" />
	  <property name="sqlkey" 		  value="rsm2120_s02" />
	  <property name="param-count" 	  value="1" />
	  <property name="param0" 		  value="RACE_DAY" />	<!-- 경주일 yyyymmdd  -->
	  <property name="dao" 			  value="rbmdao" />
	</activity>
		
  <transaction-manager id="tx_rbm" commit="true" />
</service>



