<?xml version="1.0" encoding="EUC-KR"?>

<service name="rbo3010" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  

    <property name="staticResultKeyFile" 	value="resultkey.properties" />

    <property name="resultKeyList" 			value="dsRollMana|dsStrYear" />

    <transition name="success" 				value="EventRouter" />

  </activity>

  

  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">

    <transition name="searchRollMana"  		value="searchRollMana" />

    <transition name="saveRollMana"   		value="saveRollMana" />
    
    <transition name="searchStndYear"  		value="searchStndYear" />    

    <transition name="success" 				value="end" />

  </activity>


  

  <!--  용지재고  조회 -->
  
  <activity name="searchRollMana" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success" 				value="end" />

    <property name="resultkey" 				value="dsRollMana" />

    <property name="sqlkey"					value="rbo3010_s01" />

    <property name="param-count" 			value="15" />
    
  
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="TMS" /> 
    
    <property name="param2"                 value="STND_YEAR" />    
    <property name="param3"                 value="TMS" />     
    
    <property name="param4"                 value="TMS" />    
    <property name="param5"                 value="STND_YEAR" />  
    
    <property name="param6"                 value="TMS" />    
    <property name="param7"                 value="STND_YEAR" />    
    
    <property name="param8"                 value="STND_YEAR" />  
    
    <property name="param9"                 value="STND_YEAR" />    
    <property name="param10"                 value="TMS" />    
    <property name="param11"                 value="BRNC_CD" />    
    <property name="param12"                 value="ROLL_GBN" />  
    
    <property name="param13" 				value="ROLL_GBN" />    
    <property name="param14" 				value="BRNC_CD" />
     
  

    <property name="dao" 					value="rbmdao" />

  </activity>  
  


  <!-- 용지재고  저장 -->

  <activity name="saveRollMana" 				class="snis.rbm.business.rbo3010.activity.SaveRollMana">
    <transition name="success" 					value="end" />
  </activity>


  <!-- 용지재고  기준년도 조회  -->
 <activity name="searchStndYear" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">

    <transition name="success" 				value="end" />

    <property name="resultkey" 				value="dsStrYear" />

    <property name="sqlkey"					value="rbo3010_s02" />

    <property name="param-count" 			value="0" />
    
    <property name="dao" 					value="rbmdao" />

  </activity>   
  
  
  
  <transaction-manager id="tx_rbm" commit="true" />

</service>



