<?xml version="1.0" encoding="EUC-KR"?>

<service name="rev3050" initial="SetResultKeys">

    <activity name="SetResultKeys"            class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
	    <property name="staticResultKeyFile"  value="resultkey.properties" />
	    <property name="resultKeyList" 		  value="dsEvAprv" />
	    <transition name="success" 			  value="EventRouter" />
	</activity>
  
    <activity name="EventRouter" 	 		  class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchEvAprv"       value="searchEvAprv" />	 <!-- 부서별 승인현황 조회-->	
		<transition name="success" 			  value="end" />
	</activity>

  <!-- 부서별 승인현황 조회-->
  <activity name="searchEvAprv"		  class="com.posdata.glue.miplatform.biz.activity.PosSearch">  
	<transition name="success" 		  value="end" />
	<property name="resultkey" 		  value="dsEvAprv" />
	<property name="sqlkey" 		  value="rev3050_s01" />
	<property name="param-count" 	  value="8" />
	<property name="param0" 		  value="ESTM_YEAR" />    
	<property name="param1" 		  value="ESTM_NUM" />
	<property name="param2" 		  value="APRV_DEPT" />
	<property name="param3" 		  value="APRV_DEPT" />
	<property name="param4" 		  value="APRV_DEPT" />
	<property name="param5" 		  value="APRV_STAS" />
	<property name="param6" 		  value="APRV_STAS" />
	<property name="param7" 		  value="APRV_STAS" />
	<property name="dao" 			  value="rbmdao" />
  </activity>

  <transaction-manager id="tx_rbm" commit="true" />
</service>