<?xml version="1.0" encoding="EUC-KR"?> 
<service name="rbs2030" initial="SetResultKeys">
    <activity name="SetResultKeys"   class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  	    <property name="staticResultKeyFile"  value="resultkey.properties" />	    <property name="resultKeyList" 		  value="dsList|dsOutTeamNm" />	    <transition name="success" 			  value="EventRouter" />	</activity>  
    <activity name="EventRouter" 	 class="com.posdata.glue.biz.activity.PosDefaultRouter">    		
	    <transition name="searchList"              value="searchList" />
	    <transition name="searchHealBusiTeam"      value="searchHealBusiTeam" />		<transition name="success" 			       value="end" />	</activity> 		
  <!-- 건전레저 모니터링 조회 -->  <activity name="searchList" 	  class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		  value="end" />
    <property name="resultkey" 		  value="dsList" />
    <property name="sqlkey" 		  value="rbs2030_s_list" />
    <property name="param-count" 	  value="3" />
    <property name="param0" 		  value="EXEC_YEAR" />   
    <property name="param1" 		  value="DPRT_CD" />  
    <property name="param2" 		  value="EXEC_YEAR" /> 
    <property name="dao" 			  value="rbmdao" />
  </activity>
    <!-- 건전레저 팀정보  조회 -->
  <activity name="searchHealBusiTeam"     class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"        value="end" />
    <property name="resultkey"        value="dsOutTeamNm" />
    <property name="sqlkey"           value="rbs2030_s02" />
    <property name="param-count"      value="1" />
    <property name="param0"           value="HEAL_LEISU_YEAR" />
    <property name="dao"              value="rbmdao" />
  </activity>	
  	
  <transaction-manager id="tx_rbm" commit="true" /></service>

