+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {

	if (Length(edGreenCardNo.text) != 10) {
		fnc_message("그린카드 번호가 10자리가 아닙니다.", "N");
		return;
	}

	var sServiceName = "rsm1090-service:searchGreenCdPwd";
	var sInDataSet   = "";
	var sOutDataSet  = "dsGreenCdPwd=dsGreenCdPwd ";	

	fcd_AddParam("GREEN_CARD_NO", edGreenCardNo.Text);
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}



