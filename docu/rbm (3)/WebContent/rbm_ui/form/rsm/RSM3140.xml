/*-------------------------------------------------------------------------------------------------+
>>  combobox, radio 바뀔 때 이벤트
+-------------------------------------------------------------------------------------------------*/
function fcOnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	switch ( obj.ID ) {		
		// 년도 선택
		case "cboStndYear" :	
			fcSearch();
		break;

		// 조회기준 선택
		case "cboSelCond" :	
			trace("##### case strCode:" + strCode);
			if(strCode == "001") { //년도별 조회
				calRaceDaySt.Enable = "False";
				calRaceDayEnd.Enable = "False";
				cboStndYear.Enable = "True";
			}
			if(strCode == "002") {	//기간별 조회
				calRaceDaySt.Enable = "True";
				calRaceDayEnd.Enable = "True";
				cboStndYear.Enable = "False";
			}
		break;
	
	}
}
