>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

    if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
    
    if ( sServiceName == "rsm2050-service:searchTms" ) { // TMS 값 검색
		if(dsTms.GetRowCount()==0)
		{		
			dsTms.SetColumn(0,"TMS","");
			dsTms.SetColumn(0,"TMS_DESC","없음");
			cboTms.Index=0;
		}
		else
		{
			cboTms.Value = dsTms.GetColumn(0,"MAX_TMS");		
			fcSearch();
		}
	} else if ( sServiceName == "rsm2090-service:searchSalesTms") {
		if (dsSalesTms.rowcount() > 0) {
			/* 화요일 */
			if (dsSalesTms.GetColumn(5, "TUE_AMT") > 0) fcColResize(1, 100);
			else										fcColResize(1, 0);
			
			/* 수요일 */
			if (dsSalesTms.GetColumn(5, "WED_AMT") > 0) fcColResize(3, 100);
			else										fcColResize(3, 0);
			
			/* 목요일 */
			if (dsSalesTms.GetColumn(5, "THU_AMT") > 0) fcColResize(5, 100);
			else										fcColResize(5, 0);
			
			/* 금요일 */
			if (dsSalesTms.GetColumn(5, "FRI_AMT") > 0) fcColResize(7, 100);
			else										fcColResize(7, 0);
			
			/* 토요일 */
			if (dsSalesTms.GetColumn(5, "SAT_AMT") > 0) fcColResize(9, 100);
			else										fcColResize(9, 0);
			
			/* 일요일 */
			if (dsSalesTms.GetColumn(5, "SUN_AMT") > 0) fcColResize(11, 100);
			else										fcColResize(11, 0);
			
		}
	}
    
    
}

function fcColResize(nCol, nWidth) 
{

	grdSalesTms.SetColProp(nCol  ,"Width", nWidth);
	grdSalesTms.SetColProp(nCol+1,"Width", nWidth);
}

function fcOnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	switch ( obj.ID ) {		
		// 경주일자에서 ENTER_KEY 눌렀을 경우
		case "rdoMeetCd" :	
			fcSearchTms();
			break;
	}
}

