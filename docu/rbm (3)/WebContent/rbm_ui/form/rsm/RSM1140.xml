    //조회조건 체크및 trim처리

	fcInit(obj);    //기존 데이터셋 초기화
	
	return true;
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/

