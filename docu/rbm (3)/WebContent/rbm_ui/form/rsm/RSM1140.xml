/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

    if ( !fnc_result(ErrorCode, ErrorMSG) ) return;    
    
	if ( sServiceName == "rsm1140-service:searchEventHitList" ) {		
		
		if (dsList.rowcount() > 0) {
			stInfo.Visible = false;
			
			var sMinRaceNo = dsList.GetColumn(0, "MIN_RACE_NO");
			for (var i=7;i<=13;i++) {
				grdList.SetCellProp("head", i, "text", toString(ToNumber(sMinRaceNo) + i - 7) + "경주");
			}
		} else {
		    stInfo.Visible = true;
		}
		
		
		
		return;
	} else if ( sServiceName == "rsm1140-service:searchEvenHitDetl" ) {		
		fcSetPrintInfo();
		return;
	}
}


