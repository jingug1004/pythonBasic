}

function dsList_OnLoadCompleted(obj,nErrorCode,strErrorMsg,nReason)
{
	dsList.row = -1;
}

function fcSetPrintInfo()
{
	dsListDetl_copy.copy(dsListDetl);
	dsListDetl_copy.row = -1;
	var nPrtCnt     = dsListDetl.GetColumn(0, "PRT_CNT");
	var sSlipStatus = dsListDetl.GetColumn(0, "SLIP_STATUS");
	if (nPrtCnt == 1) {
		stPrtCnt.Text  = "응모표 발행";
		stPrtCnt.Color = "default";
	} else if (nPrtCnt > 1) {
		stPrtCnt.Text  = "재발행 "+toString(nPrtCnt)+"회";
		stPrtCnt.Color = "red";
	} else {
		stPrtCnt.Text  = "";
		stPrtCnt.Color = "default";
	}
	if (sSlipStatus == "005") {
		btnConfirm.Enable = false;
	} else {
		btnConfirm.Enable = true;
	}
