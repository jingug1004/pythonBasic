function fcSearchHitDetl(nRow)
{
	// 당첨처리 : 그린카드 
 	var sServiceName = "rsm1140-service:searchEvenHitDetl"; 	// 이벤트 세부적중 현황
		
    var sInDataSet   = "";
    var sOutDataSet  = "dsListDetl=dsListDetl";		
        
    fcd_AddParam("EVENT_DT", 	 dsList.GetColumn(nRow, "EVENT_DT"));			// 응모일자
