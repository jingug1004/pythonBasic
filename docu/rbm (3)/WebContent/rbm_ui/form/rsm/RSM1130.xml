function fcInit(obj) {
	if (tab0.TabIndex == 0)    		dsListEx.ClearData();
	else if (tab0.TabIndex == 1)    dsList.ClearData();
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 체크 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
    //조회조건 체크및 trim처리

	fcInit(obj);    //기존 데이터셋 초기화
	
	return true;
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/

function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;
    
	var sServiceName = "rsm1130-service:searchEventHitExStat"; 	// 이벤트 적중현황		
    var sInDataSet   = "";
    var sOutDataSet  = "dsListEx=dsListEx dsList=dsList";	
   
		
    fcd_AddParam("STR_DT", calStrDt.Value);				// 시작일자
	fcd_AddParam("END_DT", calEndDt.Value );			// 종료일자
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}


function fcSearchDetail(sEventDt) {

    if (length(sEventDt) != 8) return;
    
	var sServiceName = "rsm1130-service:searchEventHitList"; 	// 이벤트 적중현황		
    var sInDataSet   = "";
    var sOutDataSet  = "dsListDetl=dsListDetl";		    
		
    fcd_AddParam("EVENT_DT", sEventDt);				// 이벤트 날짜
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
	
    stEventDt.text = "(응모일자 : "+ mid(sEventDt,0,4)+"-"+mid(sEventDt,4,2)+"-"+mid(sEventDt,6,2) + ")";
}

