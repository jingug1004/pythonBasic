function fcdsOnRowPosChanged(obj,nOldRow,nRow)
{
	if (obj.id == "dsList") {
		fcSearchDetail(dsList.GetColumn(nRow, "EVENT_DT"));
	} else if (obj.id == "dsListEx") {
		fcSearchDetail(dsListEx.GetColumn(nRow, "EVENT_DT"));
	}
}

function fccalOnKeyDown(obj,nChar,bShift,bCtrl,bAlt,nLLParam,nHLParam)
{
	switch ( obj.ID ) {		
		// 시작일자에서 ENTER_KEY 눌렀을 경우
		case "calStrDt" :	
			if ( nChar == ENTER_KEY ) {
				calEndDt.value = calStrDt.value;
				calEndDt.SetFocus();
				return;
			}
		break;
		// 종료일자에서 ENTER_KEY 눌렀을 경우
		case "calEndDt" :	
			if ( nChar == ENTER_KEY ) {
				fcSearch();	
				return;
			}
		break;
	}
}
