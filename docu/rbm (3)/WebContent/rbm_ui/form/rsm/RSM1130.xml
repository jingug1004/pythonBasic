function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

    if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
    
	if ( sServiceName == "rsm1130-service:searchEventHitExStat" ) {		
		dsListEx.row = -1;
		dsList.row = -1;
		return;
		
	} else if ( sServicename == "rsm1130-service:searchEventHitList") {
		if (dsListDetl.rowcount() > 0) stInfo.Visible = false;
		else                           stInfo.Visible = true;
	}
	
	
}


