				cboFloor   .Enable = false;
				cboFacCd   .Enable = false;
				cboFacGbnCd.Enable = false;
			}
			
			if( dsFacStat.GetColumn(nRow, "FAC_GBN").length == 0 ) {
				var sFilter = "CD_TERM1 = " + quote("");
				dsFacCd.Filter(sFilter);
			}
			break;
	}
}

