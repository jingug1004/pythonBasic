function fcFormLoadSetting(obj) {  
    sBUTTONLIST   = 'FTFFFFT';
  
 	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";
	
	calStartDate.Value = substr(getDate(), 0, 6) + "01";	// 현재달의 첫날(매월 1일)
	calEndDate.Value   = fcd_GetSysdate("YYYYMMDD");	
	

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
	
	//초기 화면 BK Color설정 
	fnc_SetBKColor(this);
	
	//공통코드 조회
	fcd_GetComboDs(dsInComCd);
	
	//전체 추가
	dsPartsLcd.insertRow(0);
	dsPartsLcd.setColumn(0,"CD","");
	dsPartsLcd.setColumn(0,"CD_NM","전체");
	cboSchPartsLcd.Index = 0;
	
	dsPartsMcd.insertRow(0);
	dsPartsMcd.setColumn(0,"CD","");
	dsPartsMcd.setColumn(0,"CD_NM","전체");
	cboSchPartsMcd.Index = 0;
	
	// 부품목록조회	
	http.sync = true;
	getComboschPartsNm();
	http.sync = false;

	fcSearch();
}


