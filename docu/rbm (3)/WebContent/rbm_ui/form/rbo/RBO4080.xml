    rptViewer.setJrfFileDir(GBL_REPORT_URL);
    rptViewer.setJrfFileName("rbo/rbo4080.jrf");
    rptViewer.ServletURL2="";
    rptViewer.SetDataset("dsList", "dsList");
    rptViewer.Retrieve(); 
