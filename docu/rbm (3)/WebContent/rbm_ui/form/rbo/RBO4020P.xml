function fcFormUnloadProcess(obj) {
	//자료 변경 여부를 체크하고자 하는  DataSet Object를 넘긴다.
	var sDataset = "dsList";
	if (!fnc_FormUnloadCheck(sDataset  , sFormId)) return false;
}


/*-------------------------------------------------------------------------------------------------+
>>  단축키 처리(Form KeyDown)
+-------------------------------------------------------------------------------------------------*/
function fcHotKey(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

	// Return Key Press時 Next Component focus
	if ( nChar == ENTER_KEY ) {
		var oFocusComponent = GetNextComponent(true);
		oFocusComponent.setfocus();
		return;
	}
}


/***************************************************************************************************
*                                      공통 버튼 Event 처리 부분                                   *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  취소 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInit(obj) {
    dsList.ClearData();  
}


/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
    
	
}


function fcSearch(obj) {
    
	var sServiceName = "rbo4021-service:searchList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsList=dsList ";
    
    // RBO4020 화면에서 넘겨 받은 폼변수
    // 해당 발매기 > 해당 부품분류 > 해당 부품명 > 해당 구분
    fcd_AddParam("PARTS_LCD", pv_partsLcd	);	// 발매기
    fcd_AddParam("PARTS_MCD", pv_partsMcd	);	// 부품분류
    fcd_AddParam("PARTS_CD"	, pv_partsCd	);	// 부품코드
    fcd_AddParam("IN_DT"	, pv_inDt		);	// 입고일자
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}


/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {  
 	
 	// 변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsList) ) {
	
		// 저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");
		return false;
	}

	// 필수항목체크
	oDataSet          = dsList;
	sMandatoryColID   = "PARTS_LCD|PARTS_MCD|PARTS_NM|MADE_GBN|IN_CNT|INPUT_DT|UPDT_RSN";
	sMandatoryColName = "발매기|부품모델분류|부품명|구분|입고수량|입력일자|수정사유";
	oGrid             = grdList2;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
	
		return false;
	}
	
	return true;
}


function fcSave(obj) {

	if ( !fcSaveCheck() ) return;
		
	var sServiceName = "rbo4021-service:SavePartsInUpdt";
	var sInDataSet   = "dsList=dsList:u ";
	var sOutDataSet  = "dsList=dsList ";
	
	var confirmMsg = "저장하시겠습니까?";
    if(fnc_Message(SNIS_CRA_B999, "Y", confirmMsg)) {           
	
		fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
	}
}

