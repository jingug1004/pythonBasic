>>  Transaction 후 Post Process!
+-------------------------------------------------------------------------------------------------*/
function fcPostProcess(sServiceName) {
	
}


/*-------------------------------------------------------------------------------------------------+
>>  행추가 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcAddRow(obj) {

	if ( pv_inDt != substr(getDate(), 0, 8) ) {
		// 금일데이터만 수정 가능합니다.
		fnc_Message(SNIS_RBM_B009, "N", "수정");
		return false;
	
	} else {

		var nRow = dsList.AddRow(); // Record Add
		
		dsList.SetColumn(nRow, "PARTS_LCD"		, pv_partsLcd			);	// 발매기
		dsList.SetColumn(nRow, "PARTS_MCD"		, pv_partsMcd			);	// 부품분류
		dsList.SetColumn(nRow, "PARTS_CD"		, pv_partsCd			);	// 부품코드
		dsList.SetColumn(nRow, "PARTS_NM"		, pv_partsNm			);	// 부품명
		dsList.SetColumn(nRow, "MADE_GBN"		, pv_madeGbn			);	// 원산지구분
		dsList.SetColumn(nRow, "IN_DT"			, pv_inDt				);	// 입고일자
		dsList.SetColumn(nRow, "INPUT_DT"		, substr(getDate(), 0, 8));	// 입력일자	
		dsList.SetColumn(nRow, "DEL_YN"			, "N"					);	// 삭제 " Y "	아니면, " N " (이력관리)
		dsList.SetColumn(nRow, "TODT"			, substr(getDate(), 0, 8));	// TODAY	
	
	
		grdList2.SetFocus(nRow);		// 포커스준 Row의 첫번째 Cell에 포지션을 둔다.
		grdList2.SetCellPos(0);
	}
}


/*-------------------------------------------------------------------------------------------------+
>>  행삽입 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInsRow(obj) {

}


/*-------------------------------------------------------------------------------------------------+
>>  행삭제 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcDelRow(obj) {
		
	//선택된 Row Count
	var iSelectRowCount = fnc_GetColumnValueCount(dsList, "CHK", "1");
	
	if ( iSelectRowCount <= 0 ) {
		// 삭제할 자료가 없습니다.
		fnc_Message(SNIS_COM_1003, "N");
		return false;
	}
	
	var nRow = dsList.row;
	var strInDt = dsList.GetColumn(nRow, "IN_DT");
	
	if ( strInDt < substr(getDate(), 0, 8)) {
		// 금일데이터만 삭제 가능합니다.
		fnc_Message(SNIS_RBM_B009, "N", " 삭제 ");
		return;
		
	} 
	
	// 선택된 자료 [ @ ]건를 삭제 하시겠습니까?
	var bResult = fnc_Message(SNIS_COM_1010, "Y", iSelectRowCount);
	if ( bResult ) {
		fnc_DeleteRow(dsList, "CHK", "1");
	}
		
	return bResult;
}
