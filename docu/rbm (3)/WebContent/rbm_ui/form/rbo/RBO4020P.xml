function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
	
	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	//조회 완료 후
	if ( sServiceName == "rbo4021-service:searchList" ) {
	
	}
	
	//저장 완료 후
	if ( sServiceName == "rbo4021-service:SavePartsInUpdt" ) {
	
		fcSearch();		
	}	
}


