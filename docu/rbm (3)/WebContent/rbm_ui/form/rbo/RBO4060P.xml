function fcChoice(obj) {

	var arrReturn    = array(6);               //결과 반환 할 Array
	var sTmpUserId   = "";
	var nChkCnt   	 = 0 ;
	

	for(var i = 0; i <= dsInMngList.GetRowCount();i++){
		if(dsInMngList.GetColumn(i, "CHK") == "1"){
			sTmpUserId = dsInMngList.GetColumn(i, "USER_ID");
			
			nChkCnt++;
		}
	
	}

	if(nChkCnt > 1){
		fnc_Message(L_COM_ALT_0007, "N","목록에서 한 건의 사용자 정보  ");
		return;
	}

	arrReturn[0] = sTmpUserId;   //사용자 ID 반환

    Close(arrReturn); 

}

