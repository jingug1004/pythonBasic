function fcFormUnloadProcess(obj) {
	// 자료 변경 여부를 체크하고자 하는 DataSet Object를 넘긴다.
	var sDataset = "dsAppoSeatCard";
	if (   !fnc_FormUnloadCheck(sDataset  , sFormId) ) return false;
}

/*-------------------------------------------------------------------------------------------------+
>>  단축키 처리(Form KeyDown)
+-------------------------------------------------------------------------------------------------*/
function fcHotKey(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

	// Return Key Press時 Next Component focus
	if ( nChar == 13 ) {
		var oFocusComponent = GetNextComponent(true);
		oFocusComponent.setfocus();
		return;
	}


}

/***************************************************************************************************
*                                      공통 버튼 Event 처리 부분                                   *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  취소 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInit(obj) {
	dsAppoSeatCard.ClearData();
	
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 필수조건 체크 및 그리드 초기화
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
	fcInit(obj);

	return true;
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {
	if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "rbo2060-service:searchAppoSeatCard";
	var sInDataSet   = "";
	var sOutDataSet  = "dsAppoSeatCard=dsAppoSeatCard ";
		//sOutDataSet += "dsIpMana=dsIpMana ";

	fcd_AddParam("GRN_CRD_NO",  edSearchGrnCrdNo.Text);	
	fcd_AddParam("CUST_CRD_CD", cboCustGrdCd.Value);	
	fcd_AddParam("BRNC_CD", cboSearchBrncCd.Value);


	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}



/*-------------------------------------------------------------------------------------------------+
>>  저장 하기전에 필수항목 체크
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {
	// 변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsIpMana) ) {
		// 저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");
		return false;
	}

	// 필수항목체크
	oDataSet          = dsAppoSeatCard;
	sMandatoryColID   = "GRN_CRD_NO|CUST_GRD_CD";
	sMandatoryColName = "카드번호|고객등급";
	oGrid             = grdAppoSeatCard;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
		return false;
	}

	for(var i = 0 ;i<dsAppoSeatCard.GetRowCount();i++){
		if( Length(dsAppoSeatCard.GetColumn(i,"GRN_CRD_NO")) <> 5) {
			 	fnc_Message(SNIS_RBM_B007,"N","그린카드번호","5");
			 	
			 	grdAppoSeatCard.SetCellPos(1);
			 	grdAppoSeatCard.SetFocus();
			 	return false; 

		}    
	}
	
	
	return true;
}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSave(obj) {    
	if ( !fcSaveCheck() ) return;




	var sServiceName = "";
	var sInDataSet   = "";
	var sOutDataSet  = "";

	sServiceName = "rbo2060-service:saveAppoSeatCard";
	sInDataSet   = "dsAppoSeatCard=dsAppoSeatCard:u ";


	sOutDataSet  = "dsAppoSeatCard=dsAppoSeatCard ";


	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

/*-------------------------------------------------------------------------------------------------+
>>  행추가 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcAddRow(obj) {
	// DataSet의 Header 정보가 있을 때  Add 처리
	switch ( obj.GetForm().id ) {
		case "grpBtnRow" :

			var nRow = dsAppoSeatCard.AddRow(); // Record Add
			
			dsAppoSeatCard.SetColumn(nRow,"BRNC_CD","27");
			
			grdAppoSeatCard.SetCellPos(1);	//고객카드번호 
			grdAppoSeatCard.SetFocus(); // 입력 첫 항목으로 Focus

			break;


	}
}


