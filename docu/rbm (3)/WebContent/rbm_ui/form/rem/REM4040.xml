/*-------------------------------------------------------------------------------------------------+
>>  행추가 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcAddRow(obj) {

	switch( obj.GetForm().id ) {
		case "BtnRow" :
			var nRow = dsPeriod.AddRow();
			dsPeriod.SetColumn(nRow, "TRAN_DT", AddDate(today(), 1));	// 전송일자
			dsPeriod.SetColumn(nRow, "TERM",    60*60);			//기본간격: 1시간
			dsPeriod.SetColumn(nRow, "START_TM", "1000");					// 시작시간
			dsPeriod.SetColumn(nRow, "END_TM",   "1700");					// 종료시간
			break;
	}
}

/*-------------------------------------------------------------------------------------------------+
>>  행삭제 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcDelRow(obj) {
	switch( obj.GetForm().id ) {
		case "BtnRow" :
			if ( !fnc_DeleteCheck(dsPeriod, "CHK", "1") ) return;
			for (var icnt=dsPeriod.rowcount();icnt>=0;icnt--) {
				if (dsPeriod.GetColumn(icnt, "CHK") == "1") {
					dsPeriod.DeleteRow(icnt);
				}
			}
			break;
	}	
}

