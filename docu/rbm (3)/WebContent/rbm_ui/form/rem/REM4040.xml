/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 Load時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormLoadSetting(obj) {  

    sBUTTONLIST   = 'TTTFTFT';
  
 	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
	
	calStrDt.Value = Mid(today(),0,4)+"0101";
	calEndDt.Value = AddDate(today(), 7);

    fcSearch();
}

/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 UNLoad時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormUnloadProcess(obj) {
	// 자료 변경 여부를 체크하고자 하는 DataSet Object를 넘긴다.
	var sDataset = "dsPeriod";
	if (   !fnc_FormUnloadCheck(sDataset  , sFormId) ) return false;
}

/*-------------------------------------------------------------------------------------------------+
>>  단축키 처리(Form KeyDown)
+-------------------------------------------------------------------------------------------------*/
function fcHotKey(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

	// Return Key Press時 Next Component focus
	if ( nChar == 13 ) {
		var oFocusComponent = GetNextComponent(true);
		oFocusComponent.setfocus();
		return;
	}


}

/***************************************************************************************************
*                                      공통 버튼 Event 처리 부분                                   *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  취소 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInit(obj) {

	dsStayTax.ClearData();
	
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 필수조건 체크 및 그리드 초기화
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {

	fcInit(obj);

	return true;
}



/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {

	var sServiceName = "";
	var sInDataSet   = "";
	var sOutDataSet  = "";
	if ( !fcSearchCheck(obj) ) return;
	

	 sServiceName = "rem4040-service:searchPeriod";
	 sInDataSet   = "";
	 sOutDataSet  = "dsPeriod=dsPeriod ";

	 //조회조건 : 
	 fcd_AddParam("STR_DT", calStrDt.Value);
	 fcd_AddParam("END_DT", calEndDt.Value);
    
	 fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
}


/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSave(obj) {

	var sServiceName = "";
	var sInDataSet   = "";
	var sOutDataSet  = "";
	if (obj.id == "btnSave") {
		if ( !fcSaveCheck() ) return;
		
		sServiceName = "rem4040-service:savePeriod";
		sInDataSet   = "dsPeriod=dsPeriod:u ";
		sOutDataSet  = "dsPeriod=dsPeriod ";
	
		fcd_AddParam("IMPORT_YN", "N");
		fcd_AddParam("STR_DT", calStrDt.Value);
		fcd_AddParam("END_DT", calEndDt.Value);
	
		fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
	} else if(obj.id == "btnImport") {
		sServiceName = "rem4040-service:ImportPeriod";
		sInDataSet   = "";
		sOutDataSet  = "";
	
		fcd_AddParam("IMPORT_YN", "Y");
		
		fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
	}
	
}


/*-------------------------------------------------------------------------------------------------+
>>  엑셀 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcToExcel(obj) {
		var sExcelList = "grdPeriod:전송주기 관리";
		
		fcd_OpenExcelListExport(sExcelList);
}


