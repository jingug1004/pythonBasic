/*-------------------------------------------------------------------------------------------------+
>>  대회분류 팝업 열기  
+-------------------------------------------------------------------------------------------------*/
function fcContestTypeDialog(obj)
{
	var sGameCd = "";
	if (dsPayAdjScrRule.rowcount() == 0) {
		fnc_message("한줄을 추가후 작업하세요.", "N");
		return;
	}
	switch (tab0.TabIndex) { 
		case 0:
			sGameCd = "001";
			break;
		case 1:
			sGameCd = "002";
			break;
		case 2:
			sGameCd = "003";
			break;
		case 3:
			sGameCd = "004";
			break;
		case 4:
			sGameCd = "005";
			break;
		case 5:
			sGameCd = "005";
			break;
		default :
			return;
	}
	var args  = "pGameCd=" + quote(sGameCd) + " MULTY=" + quote("N");	
	var bRtn = Dialog("frm_rbs::RBS5020P.xml", args, 334, 443, "");

	if(bRtn) {
		var nRow = dsPayAdjScrRule.Row;
		dsPayAdjScrRule.SetColumn(nRow, "GAME_CD", 			G_ds_return.GetColumn(0, "GAME_CD"));		
		dsPayAdjScrRule.SetColumn(nRow, "GAME_NM", 			G_ds_return.GetColumn(0, "GAME_NM"));		
		dsPayAdjScrRule.SetColumn(nRow, "CONTEST_TYPE_SEQ", 	G_ds_return.GetColumn(0, "CONTEST_TYPE_SEQ"));		
		dsPayAdjScrRule.SetColumn(nRow, "CONTEST_TYPE_NM", 	G_ds_return.GetColumn(0, "CONTEST_TYPE_NM"));		
		dsPayAdjScrRule.SetColumn(nRow, "INTL_TYPE", 		G_ds_return.GetColumn(0, "INTL_TYPE"));		
	}
}


