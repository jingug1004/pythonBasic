function fctabOnChanged(obj,nOldIndex,nNewindex)
{
	fcRankfilter(nNewindex);
	fcSearch(obj);
}

/*-------------------------------------------------------------------------------------------------+
>>  팀별로 관리하는 종목만 조회되게 권한 관리
+-------------------------------------------------------------------------------------------------*/
function fcGamefilter(obj)
{
	if (IsExistVar("sApro", true)) {
		if (sApro != "T") {
			tab0.tabindex = toNumber(sMnAuthRmk) -1;
			
			return false;
		}
    }
    return true;
}


function fcRankfilter(nRaceCd)
{		
	var filterStr = "";
	if (nRaceCd != 4) {
		filterStr = "CD_TERM1 != " + quote("WK");
	}
	dsRank.Filter(filterStr);
}


function fctabOnChanging(obj,nOldIndex,nNewIndex)
{
	if (IsExistVar("sApro", true)) {
		if (sApro != "T" && sMnAuthRmk != "") {
			if (nNewindex != toNumber(sMnAuthRmk) -1) {
				return false;
			}
		}
	}	
}
