function fcDelRow(obj) {

	if (obj.GetForm().id == "specBtnRow") {
		var dCnt = 0;
		for(var i = dsContestList.GetRowCount() -1; i >=0 ; i--) {
			if(dsContestList.GetColumn(i, "chk") == 1)
				dsContestList.DeleteRow(i);
		}		
	}
}


