function fcInit(obj) {
    dsContestList.ClearData();  
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {
   
	if (!fcSearchCheck(obj)) return;// 필수조건 체크 및 그리드 초기화
			
	var sServiceName = "rbs5030-service:searchContestList";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsContestList=dsContestList";     
	
	fcd_AddParam("GAME_CD"      , cboGameCd.Value);        // 종목코드  	
	fcd_AddParam("CONTEST_NM"   , edContestNm.Text);       // 대회명
	
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);

}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSave(obj) {
	if ( !fcSaveCheck() ) return;
		
	var sServiceName = "rbs5030-service:saveContestList";
	var sInDataSet   = "dsContestList=dsContestList:u";
	var sOutDataSet  = "dsContestList=dsContestList";
	
	fcd_AddParam("GAME_CD"     , cboGameCd.Value);        // 종목코드  	
	fcd_AddParam("CONTEST_NM"   , edContestNm.Text);       // 대회명
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

