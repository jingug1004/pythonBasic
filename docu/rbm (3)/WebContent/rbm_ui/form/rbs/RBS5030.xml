function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
	
	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	// 공통코드 조회 완료 후
	if ( sServiceName == "common-service:searchCode" ) {
		dsGameCdOrg.Copy(dsGameCd);
		if (fcGamefilter() == true) {		
			var nRow = dsGameCd.InsertRow(0);
			dsGameCd.SetColumn(nRow, "CD", "%");
			dsGameCd.SetColumn(nRow, "CD_NM", "전체");
			cboGameCd.Value = GetReg("GAME_CD");
		}
	}			
	
	if ( sServiceName == "rbs5030-service:searchContestList" ) {
	}			
	
	// 저장 완료 후
	if ( sServiceName == "rbs5030-service:saveContestList" ) {
		//fcSearch();  // 다시 조회한다.
	}			
	
	
}


