>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {
   
	if (!fcSearchCheck(obj)) return;// 필수조건 체크 및 그리드 초기화
			
	var sServiceName = "rbs1210-service:searchList";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsYesCom=dsYesCom ";     	
				
	fcd_AddParam("MEET_CD"    , rdoMeetCd.value);        // 예상지 업체명	
	fcd_AddParam("YES_COM_NM"    , edYesComNm.text);        // 예상지 업체명	
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
	
}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
