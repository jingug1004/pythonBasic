+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {
	
	// 변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsYesCom) ) {
		// 저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");
		return false;
	}

	// 필수항목체크
	oDataSet          = dsYesCom;
	sMandatoryColID   = "YES_COM_CD|YES_COM_NM";
	sMandatoryColName = "접속계정|업체명";
	oGrid             = grdList;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
		return false;
	}

	return true;
}


