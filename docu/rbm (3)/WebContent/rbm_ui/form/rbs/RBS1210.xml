	if ( !fcSaveCheck() ) return;
		
	var sServiceName = "rbs1210-service:saveYesCom";
	var sInDataSet   = "dsYesCom=dsYesCom:u ";
	var sOutDataSet  = "dsYesCom=dsYesCom ";

	fcd_AddParam("MEET_CD"    , rdoMeetCd.value);        // 예상지 업체명	
	fcd_AddParam("YES_COM_NM" , edYesComNm.text);        // 예상지 업체명	
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

