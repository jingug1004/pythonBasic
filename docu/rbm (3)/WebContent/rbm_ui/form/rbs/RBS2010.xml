+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {
	// 변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsHeal) ) {
		// 저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");
		return false;
	}

	// 필수항목체크
	oDataSet          = dsHeal;
	sMandatoryColID   = "HEAL_LEISU_CD|BUSI_NM|DPRT_CD|MNG_ID|BUSI_YM_STR";
	sMandatoryColName = "건전레저코드|사업명|부서코드|담당자ID|사업시작년월";
	oGrid             = grdHeal;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
		return false;
	}

	return true;
}


