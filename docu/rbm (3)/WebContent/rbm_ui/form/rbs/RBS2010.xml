>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {
   
	if (!fcSearchCheck(obj)) return;// 필수조건 체크 및 그리드 초기화
			
	var sServiceName = "rbs2010-service:searchHeal";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsHeal=dsHeal ";     
		
	fcd_AddParam("BUSI_STAT_CD"     , cboBusiStatus.Value);  // 사업상태 
		
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);

	
}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSave(obj) {
   // if ( !fcInputDataCheck() ) return;  // 입력한 시작년월, 종료년월 체크
	if ( !fcSaveCheck() ) return;
	
	
	var sServiceName = "rbs2010-service:saveHeal";
	var sInDataSet   = "dsHeal=dsHeal:u ";
	var sOutDataSet  = "dsHeal=dsHeal ";

	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

