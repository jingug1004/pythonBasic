function fcGamefilter(obj)
{
	if (IsExistVar("sApro", true)) {
		if (sApro != "T") {
			tab0.tabindex = toNumber(sMnAuthRmk) -1;
			
			return false;
		}
    }
    return true;
}

function fctabonChanging(obj,nOldIndex,nNewIndex)
{
	if (IsExistVar("sApro", true)) {
		if (sApro != "T" && sMnAuthRmk != "") {
			if (nNewindex != toNumber(sMnAuthRmk) -1) {
				return false;
			}
		}
	}
}
