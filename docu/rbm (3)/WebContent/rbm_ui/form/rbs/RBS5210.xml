+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {
   
	if (!fcSearchCheck(obj)) return;// 필수조건 체크 및 그리드 초기화
			
	var sServiceName = "rbs5210-service:searchPayAdj";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsPayAdj=dsPayAdj";     
	
	fcd_AddParam("GAME_CD",   cboGameCd.Value);      // 종목코드  
	fcd_AddParam("CNTR_YEAR", cboYear.Value);        // 조회년도
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);

}


function fcSearchPayAdjScrList(obj) {
   
	var sServiceName = "rbs5210-service:searchPayAdjScrList";  // Grid 조회
