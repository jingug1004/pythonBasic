function fcFormLoadSetting(obj) {  
    //sBUTTONLIST   = 'FT'+sInpt+'FTFT';     //초기화,조회,저장,삭제,엑셀,인쇄,닫기
    sBUTTONLIST   = 'FT'+sInpt+'FTFT'; 

 	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);

	// 초기화
    fcInit(obj);
    
    //공통코드 조회
	fcd_GetComboDs(dsInComCd);
			
	dsYear.ClearData();
	var nRow = "";
	for (var icnt=2012;icnt<=toNumber(Mid(today(),0,4));icnt++) {
		nRow = dsYear.AddRow();
		dsYear.SetColumn(nRow, "CD", toString(icnt));
		dsYear.SetColumn(nRow, "CD_NM", toString(icnt)+"년");		
	}
	cboYear.value = Mid(today(),0,4);

	fcSearch(obj);	

}

