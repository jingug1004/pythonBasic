/*-------------------------------------------------------------------------------------------------+
>>  저장 하기전에 필수항목 체크
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {
	
	// 변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsPayAdj)) {
		// 저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");
		return false;
	}

	// 필수항목체크
	oDataSet          = dsPayAdj;	
	sMandatoryColID   = "GAME_CD|RACER_ID";
	sMandatoryColName = "종목|선수";	
	oGrid             = grdPayAdj;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
		// 필수항목 누락		
		return false;
	}

	return true;
}


