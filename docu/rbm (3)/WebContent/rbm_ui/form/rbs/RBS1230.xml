/***************************************************************************************************
*                          CALL BACK 처리 부분                                                     *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {
	
	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	if (sServicename == "common-service:searchCode") {	// 공통코드 조회후
		if (dsMeetCd.rowcount() > 0) rdoMeetCd.Value = "001";
				
	} else if ( sServiceName == "rsm2020-service:searchTms" ) { // TMS 값 검색
		
		if(dsTms.GetRowCount()==0) {		
			dsTms.SetColumn(0,"TMS","");
			dsTms.SetColumn(0,"TMS_DESC","없음");
			cboTms.Index=0;
		} else {
			cboTms.Value = dsTms.GetColumn(0,"MAX_TMS");		
		}
		fcSearchDayOrd();
		
	} else if ( sServiceName == "rsm2020-service:searchDayOrd" ) {	//  일차 
		fcSearch();
		
	} else if ( sServiceName == "rbs1230-service:searchList" ) {	// 날짜별 리스트
		fcSearchYesSangRslt();
		
	} else if ( sServiceName == "rbs1230-service:searchYesSangRslt" ) {	// 예상내역  조회 완료 후
		dsYesSangRslt.row = -1;
	}	else if ( sServiceName == "rbs1230-service:saveYesSangRslt" ) { // 저장 완료 후
		dsRaceNoList.row = nLastRow;
		fcSearchYesSangRslt();
	}		
}


