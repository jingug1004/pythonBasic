	if ( !fcSaveCheck() ) return;
	nLastRow = dsRaceNoList.row;
		
	var sServiceName = "rbs1230-service:saveYesSangRslt";
	var sInDataSet   = "dsYesSangRslt=dsYesSangRslt:u ";
	var sOutDataSet  = "dsRaceNoList=dsRaceNoList ";

	fcd_AddParam("MEET_CD"  , rdoMeetCd.value);        // 경륜/경정 구분
	fcd_AddParam("STND_YEAR", cboStndYear.value);      //  기준년도
	fcd_AddParam("TMS"      , cboTms.value);      		//  회차
	fcd_AddParam("DAY_ORD"  , cboDayOrd.value);      	//  일차
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

