function fcInit(obj) {
    dsRaceNoList.ClearData();  
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch(obj) {   		
   		
	var sServiceName = "rbs1230-service:searchList";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsRaceNoList=dsRaceNoList ";     	
				
	fcd_AddParam("MEET_CD"  , rdoMeetCd.value);        	// 경륜/경정 구분
	fcd_AddParam("STND_YEAR", cboStndYear.value);      	//  기준년도
	fcd_AddParam("TMS"      , cboTms.value);      		//  회차
	fcd_AddParam("DAY_ORD"  , cboDayOrd.value);      	//  일차
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
	
}

function fcSearchYesSangRslt(obj) {
   		
	var sServiceName = "rbs1230-service:searchYesSangRslt";  // Grid 조회
	var sInDataSet   = "";
	var sOutDataSet  = "dsRaceRslt=dsRaceRslt dsYesSangRslt=dsYesSangRslt ";     	
				
	fcd_AddParam("MEET_CD"  , rdoMeetCd.value);        // 경륜/경정 구분
	fcd_AddParam("STND_YEAR", cboStndYear.value);      //  기준년도
	fcd_AddParam("TMS"      , cboTms.value);      		//  회차
	fcd_AddParam("DAY_ORD"  , cboDayOrd.value);      	//  일차
	fcd_AddParam("RACE_NO"  , dsRaceNoList.GetColumn(dsRaceNoList.row, "RACE_NO"));      	//  경주번호
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
	
}
/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
