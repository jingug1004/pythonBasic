function fcrdoOnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if (dsYesCom.RowCount() > 0) dsYesCom.Filter("MEET_CD='"+rdoMeetCd.Value+"'");	
}

function fcCboOnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	switch ( obj.ID ) {				
		case "cboStndYear" : // 기준년도 변경
			fcSearchTms();
			break;
		
		case "cboTms" :		// 회차 변경
			fcSearchDayOrd();	
			break;
			
		case "cboDayOrd" :	// 일차 변경
			fcSearch();
			break;
	}
}
