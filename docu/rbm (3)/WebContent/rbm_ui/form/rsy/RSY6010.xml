    //공통코드 조회
    fcd_GetComboDs(dsInComCd);
    
    //조회 조건의 날짜(today~ today)
    calToDt  .Value = sCurDate;
    calFromDt.Value = AddDate(sCurDate, -1);
    
    //조회 조건의 배치코드에는 '전체'를 추가한다.
    if( dsBtcCdSearch.rowcount > 0 ) {
		dsBtcCdSearch.InsertRow(0);
		dsBtcCdSearch.SetColumn(0, "CD_NM", "전체");
		cboBtcCdSearch.index = 0;
	}
	
	//조회 조건의 로그구분에는 '전체'를 추가한다.
    if( dsLogCdSearch.rowcount > 0 ) {
		dsLogCdSearch.InsertRow(0);
		dsLogCdSearch.SetColumn(0, "CD_NM", "전체");
		cboLogCdSearch.index = 0;
	}
	
	fcSearch();	//시작시 조회

}

