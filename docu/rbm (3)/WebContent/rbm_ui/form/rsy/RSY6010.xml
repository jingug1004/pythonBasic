    if ( !fcSearchCheck(obj) ) return;
    
	var sServiceName = "rsy6010-service:searchBtcLog";
    var sInDataSet   = "";
    var sOutDataSet  = "dsBtcLog=dsBtcLog ";     
    
    //조회조건
