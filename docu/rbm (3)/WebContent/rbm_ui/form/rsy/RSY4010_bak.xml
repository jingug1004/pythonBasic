function fcSaveCheck(obj) {  
 	
	//변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsList) && sPreYearDataYn == "N") {
		//저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");	
		return false;
	}
	
	// 필수항목체크
	oDataSet          = dsList;
	sMandatoryColID   = "RACER_NO|PRIZ_PENA_CD|PRIZ_PENA_DT|PRIZ_PENA_SCR";
	sMandatoryColName = "선수번호|상벌구분|상벌일자|상벌점";
	oGrid             = grdList;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
	    return false;
	}

	 	
	return true;
}

function fcSave(obj) {

    if ( !fcSaveCheck() ) return;
       
    var confirmMsg = "저장하시겠습니까?";
    if(fnc_Message(SNIS_CRA_B999, "Y", confirmMsg)) {           
		var sServiceName = "";
		var sInDataSet   = "";
		var sOutDataSet  = "";    
		
		sServiceName = "jpl0013-service:saveList";
		
		sInDataSet   = "dsList=dsList:u ";
		sOutDataSet  = "";		
	
		fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", true);
    }
}

