<?xml version="1.0" encoding="EUC-KR"?>
<service name="d07000004" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsRacer|dsDiaryMst|dsDiaryAcdnt|dsDiaryDetl|dsDiaryAbs" />
    <transition name="success" 				value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacer"      	value="searchRacer" />
    <transition name="searchDiaryMst"      	value="searchDiaryMst" />
    <transition name="searchDiaryAcdnt"    	value="searchDiaryAcdnt" />      
    <transition name="searchDiaryDetl"     	value="searchDiaryDetl" />  
    <transition name="searchDiaryAbs"      	value="searchDiaryAbs" />          
    <transition name="saveDiaryMst"    		value="saveDiaryMst" />
    <transition name="saveDiaryAcdnt"   	value="saveDiaryAcdnt" />    
    <transition name="saveDiaryDetl"    	value="saveDiaryDetl" />
    <transition name="saveDiaryAbs"    		value="saveDiaryAbs" />        
    <transition name="success" 				value="end"  />
  </activity>
  
  <!-- 선수콤보 조회 -->
  <activity name="searchRacer" 		    	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRacer" />
    <property name="sqlkey" 				value="racer_code_d07000004_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="DT" />
    <property name="param1" 				value="TRNG_ASK_SEQ" />
    <property name="dao" 					value="candao" />
  </activity> 

  <!-- 선수훈련일지마스터 조회 -->
  <activity name="searchDiaryMst" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchDiaryAcdnt" />
    <property name="resultkey" 				value="dsDiaryMst" />
    <property name="sqlkey" 				value="tbdo_racer_trng_mst_fb001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="DT" />
    <property name="param1" 				value="TRNG_ASK_SEQ" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 선수훈련일지사고 조회 -->
  <activity name="searchDiaryAcdnt" 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchDiaryDetl" />
    <property name="resultkey" 				value="dsDiaryAcdnt" />
    <property name="sqlkey" 				value="tbdo_racer_trng_acdnt_fb001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="DT" />
    <property name="param1" 				value="TRNG_ASK_SEQ" />
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 선수훈련일지상세 조회 -->
  <activity name="searchDiaryDetl" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchDiaryAbs" />
    <property name="resultkey" 				value="dsDiaryDetl" />
    <property name="sqlkey" 				value="tbdo_racer_trng_detl_fb001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="DT" />
    <property name="param1" 				value="TRNG_ASK_SEQ" />
    <property name="dao" 					value="candao" />    
  </activity>   
  
  <!-- 선수훈련일지결원 조회 -->
  <activity name="searchDiaryAbs" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsDiaryAbs" />
    <property name="sqlkey" 				value="tbdo_racer_trng_abs_fb001" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="DT" />
    <property name="dao" 					value="candao" />
  </activity>
 
  <!-- 선수훈련일지마스터 저장 --> 
  <activity name="saveDiaryMst" 			class="snis.can_boa.boatstd.d07000004.activity.SavePlayerTrain">
    <transition name="success" 	        	value="end" />
  </activity> 	
  
  <!-- 선수훈련일지사고 저장 --> 
  <activity name="saveDiaryAcdnt" 			class="snis.can_boa.boatstd.d07000004.activity.SavePlayerTrain">
    <transition name="success" 	        	value="end" />
  </activity> 	
  
  <!-- 선수훈련일지상세 저장 --> 
  <activity name="saveDiaryDetl" 			class="snis.can_boa.boatstd.d07000004.activity.SavePlayerTrain">
    <transition name="success" 	        	value="end" />
  </activity> 	    
  
  <!-- 선수훈련일지결원 저장 --> 
  <activity name="saveDiaryAbs" 			class="snis.can_boa.boatstd.d07000004.activity.SavePlayerTrain">
    <transition name="success" 	        	value="end" />
  </activity> 	  

  <transaction-manager id="tx_can" 			commit="true" />
</service>