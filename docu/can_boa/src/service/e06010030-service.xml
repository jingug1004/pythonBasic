<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
비정기 출주 관리 
 -->
<service name="e06010030" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutEquipUnfixRun" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEquipUnfixRun"      value="searchEquipUnfixRun" />
    <transition name="saveEquipUnfixRun"    value="saveEquipUnfixRun" />
    <transition name="success" value="end" />
  </activity>

  <!-- 비정기 출주 조회  -->
  <activity name="searchEquipUnfixRun" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutEquipUnfixRun" />
    <property name="sqlkey" value="tbef_equip_unfix_run_ff001" />
    <property name="param-count" value="6" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="EQUIP_TPE_CD"/>
    <property name="param3" value="EQUIP_NO"/>
    <property name="param4" value="EQUIP_TPE_CD"/>
    <property name="param5" value="EQUIP_NO"/>
  </activity>
  
 

  <!-- 비정기 출주 저장-->
  <activity name="saveEquipUnfixRun" class="snis.boa.equipment.e06010030.activity.SaveEquipUnfixRun">
    <transition name="success" value="searchEquipUnfixRun" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

