<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06010010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutCandRace" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 			   class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchCandRace"	   value="searchCandRace" />
    <transition name="saveCandRace"		   value="saveCandRace" />
    <transition name="success" 			   value="end" />
  </activity>
  
  <!-- 경정후보생신상 조회 -->
  <activity name="searchCandRace" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsOutCandRace" />
    <property name="sqlkey" 				value="tbdn_cand_race_fn001" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="RACER_PERIO_NO" />
    <property name="param1" 				value="DAY_NO" />
    <property name="param2" 				value="DT" />
    <property name="dao" 					value="candao" />    
  </activity>
 
  
  <!-- 출주 저장-->
  <activity name="saveCandRace" class="snis.can_boa.boatstd.d06010010.activity.SaveCandRace">
    <transition name="success" value="searchCandRace" />
  </activity> 
  
  <transaction-manager id="tx_can" commit="true" />
</service>