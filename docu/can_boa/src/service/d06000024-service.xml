<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000024" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsStartTime|dsVirtualRace|dsRideTime|dsViolTime" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchStartTime"    	value="searchStartTime" />     
    <transition name="searchVirtualRace"  	value="searchVirtualRace" />   
    <transition name="searchRideTime"    	value="searchRideTime" />  
    <transition name="searchViolTime"    	value="searchViolTime" />
	<transition name="success" 				value="end" />
  </activity>

  <!-- 기수별 결과 집계  조회 -->
  <activity name="searchStartTime"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchVirtualRace" />
    <property name="resultkey" 				value="dsStartTime" />
    <property name="sqlkey" 				value="tbdn_cand_race_recd_fn001" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />  
    <property name="dao" 					value="candao" />    
  </activity>
  
  <activity name="searchVirtualRace"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchRideTime" />
    <property name="resultkey" 				value="dsVirtualRace" />
    <property name="sqlkey" 				value="tbdn_cand_race_organ_fn002" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />  
    <property name="dao" 					value="candao" />    
  </activity>
  
  <activity name="searchRideTime"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchViolTime" />
    <property name="resultkey" 				value="dsRideTime" />
    <property name="sqlkey" 				value="tbdn_cand_race_organ_fn003" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />  
    <property name="dao" 					value="candao" />    
  </activity>

  <activity name="searchViolTime"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsViolTime" />
    <property name="sqlkey" 				value="tbdn_cand_race_organ_fn004" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />  
    <property name="param1" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>  
  
  
  <transaction-manager id="tx_can" commit="true" />
</service>