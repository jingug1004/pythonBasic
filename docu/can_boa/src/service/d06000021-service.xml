<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000021" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPerioNo|dsEstmItemTot|dsCfm" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPerioNo"        value="searchPerioNo" />    
    <transition name="searchEstmItemTot"   	value="searchEstmItemTot" /> 
    <transition name="saveEstmItemTot"   	value="saveEstmItemTot" />
    <transition name="searchCfm"   	        value="searchCfm" />
    <transition name="saveCfm"   	        value="saveCfm" />
    <transition name="success" 				value="end" />
  </activity> 
  
  <!-- 기수콤보 설정 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdn_total_d06000021_fn002" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>  
  
  <!-- 총괄평가표 조회 -->
  <activity name="searchEstmItemTot"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchCfm" />
    <property name="resultkey" 				value="dsEstmItemTot" />
    <property name="sqlkey" 				value="tbdn_total_d06000021_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="round" /> 
    <property name="dao" 					value="candao" />    
  </activity> 

  <!-- 총괄평가표 저장 --> 
  <activity name="saveEstmItemTot"		class="snis.can_boa.boatstd.d06000021.activity.SaveEstmItemTot">
    <transition name="success" 	        value="end" />
  </activity>
  
    <!-- 확정 조회 --> 
  <activity name="searchCfm"	 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCfm" />
    <property name="sqlkey" 				value="tbdn_total_d06000021_fn003" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="round" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 확정 저장 --> 
  <activity name="saveCfm" 		  		    class="snis.can_boa.boatstd.d06000021.activity.SaveCfm">
    <transition name="success" 	            value="end" />
  </activity>
  
  <transaction-manager id="tx_can" commit="true" />
</service>