<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
안전재고미달현황
 -->
<service name="e06030060" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutParts" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPartsLack"      value="searchPartsLack" />
    <transition name="searchPartsLackNum"      value="searchPartsLackNum" />
    <transition name="success" value="end" />
  </activity>

  <!-- 재고현황  조회      -->
  <activity name="searchPartsLack" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutParts" />
    <property name="sqlkey" value="tbef_parts_lack_ff001" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="PARTS_CD"/>
    <property name="param3" value="PARTS_ITEM_CD_NM"/>
    <property name="param4" value="PARTS_GBN"/>    
  </activity>
   
  <!-- 미달품  조회      -->
  <activity name="searchPartsLackNum" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutParts" />
    <property name="sqlkey" value="tbef_parts_lack_ff002" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="PARTS_CD"/>
    <property name="param3" value="PARTS_ITEM_CD_NM"/>
    <property name="param4" value="PARTS_GBN"/>
  </activity>
</service>

