<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000004" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsGisu|dsYyList|dsGudLectr|dsOutEduOutl|dsOutEduAssign|dsOutCurEduTm|dsWkPart|dsWkSche|dsWkScheImport|dsWeek|dsCurEduStat|dsListNum|dsWkSchePrint|dsMMSche" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchNoYear"         value="searchNoYear" />
    <transition name="searchWK"    			value="searchWK" />
    <transition name="searchYearList"    	value="searchYearList" />
    <transition name="searchGudLectr"    	value="searchGudLectr" />
    <transition name="searchWkPart"         value="searchWkPart" />
    <transition name="searchWkSche"         value="searchWkSche" />
    <transition name="searchMMSche"         value="searchMMSche" />
    <transition name="searchEduTotal"     	value="searchEduTotal" />
    <transition name="searchEduAssign"     	value="searchEduAssign" />
    <transition name="searchCurEduTime"     value="searchCurEduTime" />
    <transition name="saveEduWeek"    	    value="saveEduWeek" />
    <transition name="importWkSche"         value="importWkSche" />
    <transition name="searchPrint"    		value="searchPrint" />    
    <transition name="success" 			    value="end" />
  </activity>

  <!-- 기수 콤보 설정 --> 
  <activity name="searchNoYear"	 	        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsGisu" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn001" />
    <property name="dao" 					value="candao" />
  </activity>

  <!-- 주차 콤보 설정 -->
  <activity name="searchWK" 		  	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchGudLectr" />
    <property name="resultkey" 				value="dsWeek" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn000" />  
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity>
 
  <!-- 년도 콤보 설정 -->
  <activity name="searchYearVal" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchGudLectr" />
    <property name="resultkey" 				value="dsYyList" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn002" />
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 강사 콤보 설정 -->
  <activity name="searchGudLectr" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsGudLectr" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn003" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 주차구간 조회 -->
  <activity name="searchWkPart" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWkPart" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn004" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="perioNo" />
    <property name="param1" 				value="weekPart" />  	
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!--종합 교육개요 조회 -->
  <activity name="searchEduTotal" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchEduAssign" />
    <property name="resultkey" 				value="dsCurEduStat" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn010" />
    <property name="param-count" 			value="5" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="rPerioNo" />
    <property name="param2" 				value="strDate" />
    <property name="param3" 				value="rPerioNo" />
    <property name="param4" 				value="wKweekPart" />
    <property name="dao" 					value="candao" />
  </activity>
    
  <!--종합 시간배정 조회 -->
  <activity name="searchEduAssign" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchCurEduTime" />
    <property name="resultkey" 				value="dsOutEduAssign" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn006" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>  
  
  <!-- 교육일지 교육시간 조회 -->
  <activity name="searchCurEduTime" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchListNum" />
    <property name="resultkey" 				value="dsOutCurEduTm" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn007" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>

   <!--순서 List -->
  <activity name="searchListNum" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsListNum" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn011" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="rPerioNo" />
    <property name="dao" 					value="candao" />
  </activity>   
    
  <!-- 주간교육일정 조회 -->
  <activity name="searchWkSche" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWkSche" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn008" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="wKweekPart" />
    <property name="dao" 					value="candao" />    
  </activity>
  
    <!-- 월간교육일정 조회 -->
  <activity name="searchMMSche" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsMMSche" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn012" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="rMM" />
    <property name="dao" 					value="candao" />    
  </activity>
    
  <!-- 세부교육일정 가져오기 -->
  <activity name="importWkSche" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWkScheImport" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fn009" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="wKweekPart" />
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 주간교육일정 조회(print) -->
  <activity name="searchPrint" 		       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWkSchePrint" />
    <property name="sqlkey" 				value="tbdn_wk_sche_fb010" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="rPerioNo" />
    <property name="param1" 				value="wKweekPart" />
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 주간교육일정 저장 --> 
  <activity name="saveEduWeek" 		        class="snis.can_boa.boatstd.d06000004.activity.SaveWeekEduSchd">
    <transition name="success" 	            value="end" />
  </activity> 
 
	<transaction-manager id="tx_can" commit="true" />
</service>

