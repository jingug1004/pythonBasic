<?xml version="1.0" encoding="EUC-KR"?>
<service name="d07000007" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsRacerTrngMentor|dsRacerMentoring|dsMentee|dsRacer|dsMenteeList" />
    <transition name="success" 				value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 					class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchTrngAskMentor"  	value="searchTrngAskMentor" />
    <transition name="searchMentoring" 			value="searchMentoring" />
    <transition name="searchTrngAskMentee" 		value="searchTrngAskMentee" />
    <transition name="searchTrngAskRacer" 		value="searchTrngAskRacer" />
    <transition name="saveRacerTrngMentoring"	value="saveRacerTrngMentoring" />
    <transition name="searchTrngMenteeList"  	value="searchTrngMenteeList" />
    <transition name="success" 			   		value="end" />
  </activity>
  
  <!-- 조회 -->
  <activity name="searchTrngAskMentor" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchTrngAskMentee" />
    <property name="resultkey" 				value="dsRacerTrngMentor" />
    <property name="sqlkey" 				value="d07000007_s01" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="STR_DT" />
    <property name="param1" 				value="END_DT" />
    <property name="dao" 					value="candao" />
  </activity> 
  
  <!-- 조회 -->
  <activity name="searchMentoring" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRacerMentoring" />
    <property name="sqlkey" 				value="d07000007_s02" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="TRNG_ASK_SEQ" />
    <property name="param1" 				value="MENTO_GRP_NO" />
    <property name="dao" 					value="candao" />
  </activity> 
  
  
  <!-- 조회 -->
  <activity name="searchTrngAskMentee" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchTrngAskRacer" />
    <property name="resultkey" 				value="dsMentee" />
    <property name="sqlkey" 				value="d07000007_s03" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="STR_DT" />
    <property name="param1" 				value="END_DT" />
    <property name="dao" 					value="candao" />
  </activity> 
  
  
  <!-- 조회 -->
  <activity name="searchTrngAskRacer" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRacer" />
    <property name="sqlkey" 				value="d07000007_s04" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="STR_DT" />
    <property name="param1" 				value="END_DT" />
    <property name="dao" 					value="candao" />
  </activity> 
  
  <!-- 조회 -->
  <activity name="searchTrngMenteeList" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsMenteeList" />
    <property name="sqlkey" 				value="d07000007_s05" />
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="TRNG_ASK_SEQ" />
    <property name="param1" 				value="MENTO_GRP_NO" />
    <property name="param2" 				value="STR_DT" />
    <property name="param3" 				value="END_DT" />
    <property name="dao" 					value="candao" />
  </activity> 
  
  
  <!-- 저장 -->
  <activity name="saveRacerTrngMentoring" class="snis.can_boa.boatstd.d07000007.activity.SaveRacerTrngMentoring">
    <transition name="success" value="end" />
  </activity>

  <transaction-manager id="tx_can" commit="true" />
</service>