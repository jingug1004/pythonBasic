<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000003" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsDetlEduAssign|dsPerioNo|dsGbn|dsCrs|dsObj|dsUser|dsYear|dsCfm" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEduDetail"      value="searchEduDetail" />  
    <transition name="searchPerioNo"        value="searchPerioNo" />
    <transition name="searchUser"    	    value="searchUser" />
    <transition name="searchYear"    	    value="searchYear" />    
    <transition name="saveEduDetail"    	value="saveEduDetail" />
    <transition name="searchCfm"  		    value="searchCfm" />
    <transition name="saveCfm"      		value="saveCfm" />
    <transition name="success" 				value="end" />
  </activity>
  
  <!-- 세부교육일정 조회 -->
  <activity name="searchEduDetail" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsDetlEduAssign" />
    <property name="sqlkey" 				value="tbdn_detl_edu_assign_fn001" />
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="racerPerioNo" />    
    <property name="param2" 				value="yy" />
    <property name="param3" 				value="mm" />    
    <property name="dao" 					value="candao" />    
  </activity> 
    
  <!-- 기수콤보 설정 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdn_detl_edu_assign_fn002" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>
   
  <!-- 강사콤보 설정 -->
  <activity name="searchUser" 		        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsUser" />
    <property name="sqlkey" 				value="tbdn_detl_edu_assign_fn003" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />    
  </activity>
          
  <!-- 년도 설정 -->
  <activity name="searchYear" 		        class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsYear" />
    <property name="sqlkey" 				value="tbdn_detl_edu_assign_fn004" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />    
  </activity>          
       
  <!-- 확정 --> 
  <activity name="searchCfm"	 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCfm" />
    <property name="sqlkey" 				value="tbdn_detl_edu_assign_fn005" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="mn_id" />
    <property name="dao" 					value="candao" />
  </activity>        
          
  <!-- 세부교육일정 저장 --> 
  <activity name="saveEduDetail" 		class="snis.can_boa.boatstd.d06000003.activity.SaveDetailEduSchd">
    <transition name="success" 	        value="end" />
  </activity> 	
 
    <!-- 확정 --> 
  <activity name="saveCfm" 		  		class="snis.can_boa.boatstd.d06000001.activity.SaveCfm">
    <transition name="success" 	        value="end" />
  </activity>

  <transaction-manager id="tx_can" commit="true" />
</service>