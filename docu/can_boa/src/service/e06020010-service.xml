<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
부품 등록  
 -->
<service name="e06020010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutParts" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchParts"      value="searchParts" />
    <transition name="searchPartsByOrder"      value="searchPartsByOrder" />
    <transition name="searchPartsByDelv"      value="searchPartsByDelv" />
    <transition name="saveParts"    value="saveParts" />
    <transition name="success" value="end" />
  </activity>

  <!-- 부품  조회(부품코드기준)     -->
  <activity name="searchParts" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutParts" />
    <property name="sqlkey" value="tbef_parts_master_ff001" />
    <property name="param-count" value="7" />
    <property name="dao" value="boadao" />
    <property name="param0" value="PARTS_CD"/>
    <property name="param1" value="PARTS_ITEM_CD_NM"/>
    <property name="param2" value="PARTS_COM_CD"/>
    <property name="param3" value="USE_YN"/>
    <property name="param4" value="USE_YN"/>
    <property name="param5" value="PARTS_TPE"/>
    <property name="param6" value="PARTS_GBN"/>
    
  </activity>
   
  
  
  <!-- 부품 조회(최종단가  기준 조회/주문 대상 품목 조회)     -->
  <activity name="searchPartsByOrder" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutParts" />
    <property name="sqlkey" value="tbef_parts_master_ff101" />
    <property name="param-count" value="9" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    
    <property name="param2" value="PARTS_CD"/>
    <property name="param3" value="PARTS_ITEM_CD_NM"/>
    <property name="param4" value="PARTS_COM_CD"/>
    <property name="param5" value="USE_YN"/>
    <property name="param6" value="USE_YN"/>
    
    <property name="param7" value="STND_YEAR"/>
    <property name="param8" value="MBR_CD"/>
  </activity>
  
  
  <!-- 부품 조회(현재고 기준 조회/출고 대상 품목 조회)      -->
  <activity name="searchPartsByDelv" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutParts" />
    <property name="sqlkey" value="tbef_parts_master_ff102" />
    <property name="param-count" value="10" />
    <property name="dao" value="boadao" />
    
    <property name="param0" value="PARTS_CD"/>
    <property name="param1" value="PARTS_ITEM_CD_NM"/>
    <property name="param2" value="PARTS_COM_CD"/>
    <property name="param3" value="USE_YN"/>
    <property name="param4" value="USE_YN"/>
    <property name="param5" value="PARTS_TPE"/>
    <property name="param6" value="PARTS_GBN"/>
    
    <property name="param7" value="STND_YEAR"/>
    <property name="param8" value="MBR_CD"/>
    <property name="param9" value="CHK_NOW"/>
    
  </activity>
  
 
 
  <!-- 부품  저장 -->
  <activity name="saveParts" class="snis.boa.equipment.e06020010.activity.SaveParts">
    <transition name="success" value="searchParts" />
  </activity>
  
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

