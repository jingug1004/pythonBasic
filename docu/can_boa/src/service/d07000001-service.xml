<?xml version="1.0" encoding="EUC-KR"?>
<service name="d07000001" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutTrngAsk|dsOutTrngAskRacer|dsOutTrngAskRacerList|dsOutRacerDetail|dsOutTrngRest" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				  	class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchTrngAsk"          	value="searchTrngAsk" />
    <transition name="searchTrngAskRacer"     	value="searchTrngAskRacer" />
    <transition name="searchTrngAskRacerList" 	value="searchTrngAskRacerList" />
    <transition name="searchTrngAskRacerDetail" value="searchTrngAskRacerDetail" />
    <transition name="searchTrngRest" 			value="searchTrngRest" />
    <transition name="AceptTrngAsk"        		value="AceptTrngAsk" />
    <transition name="EndTrngAsk"        		value="EndTrngAsk" />
    <transition name="SaveTrngAskRacer"        	value="SaveTrngAskRacer" />
    <transition name="success" 					value="end" />
  </activity>

  <!-- 훈련요청 조회 -->
  <activity name="searchTrngAsk" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 					value="end" />
    <property name="resultkey" 					value="dsOutTrngAsk" />
    <property name="sqlkey" 					value="tbdo_trng_ask_fb002" />
    <property name="param-count" 				value="5" />
    <property name="param0" 					value="STR_DT" />
    <property name="param1" 					value="END_DT" />
    <property name="param2" 					value="TRNG_ASK_STAT_CD" />
    <property name="param3" 					value="NM_KOR" />
    <property name="param4" 					value="TRNG_ASK_SEQ" />
    <property name="dao" 						value="boadao" />
  </activity>

  <!-- 선수명단  조회 -->
  <activity name="searchTrngAskRacer" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 					value="end" />
    <property name="resultkey" 					value="dsOutTrngAskRacer" />
    <property name="sqlkey" 					value="tbdo_trng_ask_racer_fb001" />
    <property name="param-count" 				value="1" />
    <property name="param0" 					value="TRNG_ASK_SEQ" />
    <property name="dao" 						value="boadao" />
  </activity>

  <!-- 상세성적 조회 -->
  <activity name="searchTrngAskRacerList" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 					value="end" />
    <property name="resultkey" 					value="dsOutTrngAskRacerList" />
    <property name="sqlkey" 					value="tbdo_trng_expt_racer_fb005" />
    <property name="param-count" 				value="10" />
    <property name="param0" 					value="RECD_STR_DT" />
    <property name="param1" 					value="RECD_END_DT" />
    <property name="param2" 					value="RECD_STR_DT" />
    <property name="param3" 					value="RECD_END_DT" />
    <property name="param4" 					value="RECD_STR_DT" />
    <property name="param5" 					value="RECD_END_DT" />
    <property name="param6" 					value="STR_DT" />
    <property name="param7" 					value="STR_DT" />
    <property name="param8" 					value="STR_DT" />
    <property name="param9" 					value="STR_DT" />
    <property name="dao" 						value="boadao" />
  </activity>
  
  <!-- 상세성적 조회 -->
  <activity name="searchTrngAskRacerDetail" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 					value="end" />
    <property name="resultkey" 					value="dsOutRacerDetail" />
    <property name="sqlkey" 					value="tbdo_trng_ask_racer_fb003" />
    <property name="param-count" 				value="2" />
    <property name="param0" 					value="TRNG_STR_DT" />
    <property name="param1" 					value="RACER_NO" />
    <property name="dao" 						value="boadao" />
  </activity>

  <!-- 개인별잔여훈련집계 조회 -->
  <activity name="searchTrngRest" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutTrngRest" />
    <property name="sqlkey" value="tbeb_trng_expt_racer_fb004" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="6" />
    <property name="param0" value="NM_KOR" />
    <property name="param1" value="STR_DT" />
    <property name="param2" value="END_DT" />
    <property name="param3" value="NM_KOR" />
    <property name="param4" value="STR_DT" />
    <property name="param5" value="END_DT" />
  </activity>
  
 
  <!-- 훈련요청상태 변경 -->
  <activity name="AceptTrngAsk" class="snis.can_boa.boatstd.d07000001.activity.AceptTrngAsk">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutTrngAsk" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="2" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
  </activity>
  
  <!-- 훈련요청상태(완료) 변경 -->
  <activity name="EndTrngAsk" class="snis.can_boa.boatstd.d07000001.activity.AceptTrngAsk">
    <transition name="success" 		value="searchTrngAsk" />
    <property name="resultkey" 		value="dsOutTrngAsk" />
    <property name="param-count" 	value="5" />
    <property name="param0" 		value="STR_DT" />
    <property name="param1" 		value="END_DT" />
    <property name="param2" 		value="TRNG_ASK_STAT_CD" />
    <property name="param3" 		value="NM_KOR" />
    <property name="param4" 		value="TRNG_ASK_SEQ" />
    <property name="dao" 			value="boadao" />
  </activity>
  
  
  <!-- 교육비 납부 상태 변경 변경 -->
  <activity name="SaveTrngAskRacer" class="snis.can_boa.boatstd.d07000001.activity.AceptTrngAsk">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutTrngAsk" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="2" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

