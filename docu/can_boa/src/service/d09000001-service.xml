<?xml version="1.0" encoding="EUC-KR"?>
<service name="d09000001" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPerioNo|dsCandIdent|dsHealth|dsWound1|dsWound2|dsExtExam" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchHealth"			value="searchHealth" /> 	 	
    <transition name="searchPerioNo"        value="searchPerioNo" />
    <transition name="searchCandIdent" 		value="searchCandIdent" />
    <transition name="searchWoundCheck1" 	value="searchWoundCheck1" />  
    <transition name="searchWoundList2" 	value="searchWoundList2" />    
    <transition name="searchWoundCheck1_2" 	value="searchWoundCheck1_2" />  
    <transition name="searchWoundList2_2" 	value="searchWoundList2_2" />       
    <transition name="searchExtExam" 		value="searchExtExam" />          
    <transition name="saveHealth"	    	value="saveHealth" />
    <transition name="saveWound1"	    	value="saveWound1" />
    <transition name="saveWound2"	    	value="saveWound2" />
    <transition name="saveExtExam"	    	value="saveExtExam" />
    <transition name="success" 				value="end" />
  </activity>

  <!-- 개인건강상태 조회 -->
  <activity name="searchHealth"				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsHealth" />
    <property name="sqlkey" 				value="tbdq_health_fb001" />
    <property name="param-count" 			value="2" />  
    <property name="param0" 				value="racerPerioNo" />   
    <property name="param1" 				value="candNo" />        	        
    <property name="dao" 					value="candao" />    
  </activity>

  <!-- 기수 조회 -->
  <activity name="searchPerioNo" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdq_health_fb002" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />    
  </activity> 

  <!-- 경륜호부생신상 조회 --> 
  <activity name="searchCandIdent"	   		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandIdent" />
    <property name="sqlkey" 				value="tbdq_health_fb003" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="racerPerioNo" />
   	<property name="param1" 				value="nmKor" />
   	<property name="param2" 				value="nmKor" />	
    <property name="dao" 					value="candao" />
  </activity>

  <!-- 신체부위 조회1_1 -->
  <activity name="searchWoundCheck1"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWound1" />
    <property name="sqlkey" 				value="tbdq_wound_fb001_1" />
    <property name="param-count" 			value="4" />  
    <property name="param0" 				value="racerPerioNo" />   
    <property name="param1" 				value="candNo" />  
    <property name="param2" 				value="racerPerioNo" />
    <property name="param3" 				value="candNo" />     
    <property name="dao" 					value="candao" />    
  </activity>  
  
  <!-- 신체부위 조회2_1 -->
  <activity name="searchWoundList2"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWound2" />
    <property name="sqlkey" 				value="tbdq_wound_fb002_1" />
    <property name="param-count" 			value="4" />  
    <property name="param0" 				value="racerPerioNo" />   
    <property name="param1" 				value="candNo" />  
    <property name="param2" 				value="racerPerioNo" />
    <property name="param3" 				value="candNo" />   
    <property name="dao" 					value="candao" />       
  </activity>
  
  <!-- 신체부위 조회1_2 -->
  <activity name="searchWoundCheck1_2"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWound1" />
    <property name="sqlkey" 				value="tbdq_wound_fb001_2" />
    <property name="param-count" 			value="4" />  
    <property name="param0" 				value="racerPerioNo" />   
    <property name="param1" 				value="candNo" />  
    <property name="param2" 				value="dt" />  
    <property name="param3" 				value="dt" />     
    <property name="dao" 					value="candao" />    
  </activity>
  
  <!-- 신체부위 조회2_2 -->
  <activity name="searchWoundList2_2"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWound2" />
    <property name="sqlkey" 				value="tbdq_wound_fb002_2" />
    <property name="param-count" 			value="4" />  
    <property name="param0" 				value="racerPerioNo" />   
    <property name="param1" 				value="candNo" />  
    <property name="param2" 				value="dt" />  
    <property name="param3" 				value="dt" />          
    <property name="dao" 					value="candao" />       
  </activity>  
     
  <!-- 외진기록(부상내역) 조회 -->
  <activity name="searchExtExam"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsExtExam" />
    <property name="sqlkey" 				value="tbdq_ext_exam_fb001" />
    <property name="param-count" 			value="2" />  
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="candNo" />           	        
    <property name="dao" 					value="candao" />    
  </activity>  

  <!-- 개인상태 저장 --> 
  <activity name="saveHealth"	class="snis.can_boa.boatstd.d09000001.activity.SaveHealth">
    <transition name="success"	value="end" />
  </activity>
  
  <!-- 신체부위정보1 저장 --> 
  <activity name="saveWound1"	class="snis.can_boa.boatstd.d09000001.activity.SaveHealth">
    <transition name="success"	value="end" />
  </activity>  
  
  <!-- 신체부위정보2 저장 --> 
  <activity name="saveWound2"	class="snis.can_boa.boatstd.d09000001.activity.SaveHealth">
    <transition name="success"	value="end" />
  </activity>  
  
  <!-- 외진기록 저장 --> 
  <activity name="saveExtExam"	class="snis.can_boa.boatstd.d09000001.activity.SaveHealth">
    <transition name="success"	value="end" />
  </activity>     
	
  <transaction-manager id="tx_can" commit="true" />
</service>