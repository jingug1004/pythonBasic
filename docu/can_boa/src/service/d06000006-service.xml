<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000006" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPerioNo|dsBasItem|dsBodyInsp|dsStrtMesur|dsIdentInq|dsWrtn|dsHmntrInsp|dsInvw|dsEndRslt" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPerioNo"		value="searchPerioNo" />
    <transition name="searchBasItem"		value="searchBasItem" />  
    <transition name="searchBodyInsp"		value="searchBodyInsp" />  
    <transition name="searchStrtMesur"      value="searchStrtMesur" />  
    <transition name="searchIdentInq"		value="searchIdentInq" />  
    <transition name="searchWrtn"			value="searchWrtn" />  
    <transition name="searchHmntrInsp"      value="searchHmntrInsp" />  
    <transition name="searchInvw"			value="searchInvw" />  
    <transition name="searchEndRslt"		value="searchEndRslt" />  
    <transition name="saveSelt"	    		value="saveSelt" />	
    <transition name="success" 				value="end" />
  </activity>
  
  <!-- 기수콤보 설정 --> 
  <activity name="searchPerioNo"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsPerioNo" />
    <property name="sqlkey" 				value="tbdn_selt_bas_item_fn002" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="candao" />
  </activity>
     
  <!-- 선발기준종목 조회 -->
  <activity name="searchBasItem" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsBasItem" />
    <property name="sqlkey" 				value="tbdn_selt_bas_item_fn001" />
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="dao" 					value="candao" />    
  </activity> 

  
  <!-- 신체검사 조회 -->
  <activity name="searchBodyInsp" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsBodyInsp" />
    <property name="sqlkey" 				value="tbdn_selt_body_insp_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="reptNo" />
    <property name="dao" 					value="candao" />    
  </activity> 
    
  <!-- 기초체력측정 조회 -->
  <activity name="searchStrtMesur" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsStrtMesur" />
    <property name="sqlkey" 				value="tbdn_selt_strt_mesur_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="reptNo" />
    <property name="dao" 					value="candao" />    
  </activity>   
  
  <!-- 신원조회 조회 -->
  <activity name="searchIdentInq" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsIdentInq" />
    <property name="sqlkey" 				value="tbdn_selt_ident_inq_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="reptNo" />
    <property name="dao" 					value="candao" />    
  </activity>    
  
  <!-- 필기 조회 -->
  <activity name="searchWrtn" 		    	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsWrtn" />
    <property name="sqlkey" 				value="tbdn_selt_wrtn_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="reptNo" />
    <property name="dao" 					value="candao" />    
  </activity>  
  
  <!-- 인성검사 조회 -->
  <activity name="searchHmntrInsp" 	    	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsHmntrInsp" />
    <property name="sqlkey" 				value="tbdn_selt_hmntr_insp_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="reptNo" />
    <property name="dao" 					value="candao" />    
  </activity>   
     
  <!-- 면접 조회 -->
  <activity name="searchInvw" 	    		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsInvw" />
    <property name="sqlkey" 				value="tbdn_selt_invw_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="reptNo" />
    <property name="dao" 					value="candao" />    
  </activity>      
  
  <!-- 최종결과 조회 -->
  <activity name="searchEndRslt" 	   		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsEndRslt" />
    <property name="sqlkey" 				value="tbdn_selt_end_rslt_fn001" />
    <property name="param-count" 			value="5" />
    <property name="param0" 				value="racerPerioNo" />
    <property name="param1" 				value="racerPerioNo" />
    <property name="param2" 				value="racerPerioNo" />
    <property name="param3" 				value="racerPerioNo" />
    <property name="param4" 				value="reptNo" />    
    <property name="dao" 					value="candao" />    
  </activity>     
                    
  <!-- 선발측정 저장 --> 
  <activity name="saveSelt" 			class="snis.can_boa.boatstd.d06000006.activity.SaveChoiceTest">
    <transition name="success" 	        value="end" />
  </activity>
	
	<transaction-manager id="tx_can" commit="true" />
</service>