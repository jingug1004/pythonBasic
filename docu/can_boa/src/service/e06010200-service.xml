<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
공구자산 관리  
 -->
<service name="e06010200" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutTools" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchTools"      value="searchTools" />
    <transition name="saveTools"    value="saveTools" />
    <transition name="success" value="end" />
  </activity>

  <!-- 공구자산  조회     -->
  <activity name="searchTools" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutTools" />
    <property name="sqlkey" value="e06010200_s01" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="TOOL_CD"/>
    <property name="param1" value="TOOL_NM"/>
    <property name="param2" value="USE_YN"/>    
  </activity>  
  
 
  <!-- 공구자산  저장 -->
  <activity name="saveTools" class="snis.boa.equipment.e06010200.activity.SaveTools">
    <transition name="success" value="searchTools" />
  </activity>
  
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

