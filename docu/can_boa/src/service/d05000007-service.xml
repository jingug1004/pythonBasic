<?xml version="1.0" encoding="EUC-KR"?>
<service name="d05000007" initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsUserListValue|dsAuthListValue|dsAuthUser|dsUser" />
    <transition name="success" 				value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchMenu"			value="searchMenu" />		<!-- 메뉴 조회   -->
    <transition name="searchUserList"		value="searchUserList" />	<!-- 사용자 조회   -->
    <transition name="saveUserMenu"			value="saveUserMenu" />		<!-- 사용자 저장   -->
    <transition name="success" 				value="end" />
  </activity>

  <!-- searchUserMenu -->
  <activity name="searchMenu" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsAuthListValue" />
    <property name="sqlkey" 				value="d05000007_s01" />    
    <property name="param-count" 			value="1" />
    <property name="param0" 				value="MN_GRP" />     
  	<property name="dao" 					value="candao" />
  </activity>  
  
  <!-- searchUserList -->
  <activity name="searchUserList" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsUserListValue" />
    <property name="sqlkey" 				value="d05000007_s02" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="MN_ID" />
    <property name="param1" 				value="TEAM_NM" />
    <property name="param2" 				value="USER_NM" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- saveUserMenu -->
  <activity name="saveUserMenu" 			class="snis.can_boa.system.d05000007.activity.SaveMenuAuth">
    <transition name="success" 				value="searchMenu" />    
  </activity>  
    
  <transaction-manager id="tx_can" commit="true" />
</service>