<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
부품 이월  
 -->
<service name="e06020060" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutParts|dsLastStndYear" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchParts"      value="searchParts" />
    <transition name="savePartsCrfw"    value="savePartsCrfw" />
    <transition name="searchLastCarryYear"    value="searchLastCarryYear" />
    <transition name="success" value="end" />
  </activity>

  <!-- 부품  조회      -->
  <activity name="searchParts" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutParts" />
    <property name="sqlkey" value="tbef_parts_crfw_ff001" />
    <property name="param-count" value="9" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>    
    <property name="param2" value="PARTS_GBN"/>    
    <property name="param3" value="STND_YEAR"/>
    <property name="param4" value="MBR_CD"/>
    <property name="param5" value="PARTS_GBN"/>
    <property name="param6" value="STND_YEAR"/>
    <property name="param7" value="MBR_CD"/>
    <property name="param8" value="PARTS_GBN"/>
  </activity>
  
 
  <!-- 부품  이월 -->
  <activity name="savePartsCrfw" class="snis.boa.equipment.e06020060.activity.SavePartsCrfw">
    <transition name="success" value="searchLastCarryYear" />
  </activity>
  
  <!-- 최종이월년도 조회      -->
  <activity name="searchLastCarryYear" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsLastStndYear" />
    <property name="sqlkey" value="tbef_parts_sf001" />
    <property name="param-count" value="0" />
    <property name="dao" value="boadao" />
  </activity>
  
 
   <transaction-manager id="tx_boa" commit="true" />
</service>

