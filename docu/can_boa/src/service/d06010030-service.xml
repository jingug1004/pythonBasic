<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000023" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsCandList|dsCandSum" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchCandList"	value="searchCandList" />
    <transition name="searchCandSum"	value="searchCandSum" />
    <transition name="success" 			value="end" />
  </activity>
  
  <!-- 모의훈련 조회 -->
  <activity name="searchCandList" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchCandSum" />
    <property name="resultkey" 				value="dsCandList" />
    <property name="sqlkey" 				value="tbdn_cand_race_fn301" />
    <property name="param-count" 			value="11" />
    <property name="param0" 				value="RACER_PERIO_NO" />
    <property name="param1" 				value="GBN" />
    <property name="param2" 				value="RACER_PERIO_NO" />
    <property name="param3" 				value="GBN" />
    <property name="param4" 				value="RACER_PERIO_NO" />
    <property name="param5" 				value="GBN" />
    <property name="param6" 				value="GBN" />
    <property name="param7" 				value="RACER_PERIO_NO" />
    <property name="param8" 				value="DAY_NO" />
    <property name="param9" 				value="ROUND" />
    <property name="param10" 				value="RACER_PERIO_NO" />
    <property name="dao" 					value="candao" />    
  </activity> 
  
  <!-- 모의훈련 sum 조회 -->
  <activity name="searchCandSum" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCandSum" />
    <property name="sqlkey" 				value="tbdn_cand_race_fn302" />
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="GBN" />
    <property name="param1" 				value="RACER_PERIO_NO" />
    <property name="param2" 				value="DAY_NO" />
    <property name="param3" 				value="ROUND" />
    <property name="dao" 					value="candao" />    
  </activity> 

  <transaction-manager id="tx_can" commit="true" />
</service>