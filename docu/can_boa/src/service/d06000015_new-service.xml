<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000015" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsPerioNo|dsDateList|dsWrtExam_Detail|dsWrtExam_CandList|dsWrtExam_Date|dsWrtExam_Round|dsWrtExam_Total" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				   class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchWrtExam" 		   value="searchWrtExam" />
    <transition name="searchWrtExamDetail" 	   value="searchWrtExamDetail" />
    <transition name="searchWrtExamCanList"    value="searchWrtExamCanList" />
    <transition name="searchWrtExamDate"       value="searchWrtExamDate" />
    <transition name="searchWrtExamRound"      value="searchWrtExamRound" />
    <transition name="saveWrtnEstmDetail" 	   value="saveWrtnEstmDetail" />
    <transition name="saveWrtnEstmTotal" 	   value="saveWrtnEstmTotal" />
    <transition name="success" 				   value="end" />
  </activity>
    
  <!-- 학과일자 조회 -->
  <activity name="searchWrtExam" 		    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchWrtExamCanList" />
    <property   name="resultkey" 			value="dsDateList" />
    <property   name="sqlkey" 				value="tbdn_wrt_exam_se001" />
    <property   name="param-count" 			value="1" />
    <property   name="param0" 				value="RACER_PERIO_NO" />
    <property   name="dao" 					value="candao" />    
  </activity>   

  <!-- 일자별 학과집계 조회 -->
  <activity name="searchWrtExamCanList"     class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchWrtExamRound" />
    <property   name="resultkey" 			value="dsWrtExam_CandList" />
    <property   name="sqlkey" 				value="tbdn_wrt_exam_canlist_se001" />
    <property   name="param-count" 			value="1" />
    <property   name="param0" 				value="RACER_PERIO_NO" />
    <property   name="dao" 					value="candao" />    
  </activity>   

  <!-- 차수별 학과집계 조회 -->
  <activity name="searchWrtExamRound"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="saveWrtnEstmTotal" />
    <property   name="resultkey" 			value="dsWrtExam_Round" />
    <property   name="sqlkey" 				value="tbdn_wrt_exam_round_se001" />
    <property   name="param-count" 			value="1" />
    <property   name="param0" 				value="RACER_PERIO_NO" />
    <property   name="dao" 					value="candao" />    
  </activity>
  
  <!-- 학과총계 조회 -->
  <activity name="saveWrtnEstmTotal"       class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property   name="resultkey" 			value="dsWrtExam_Total" />
    <property   name="sqlkey" 				value="tbdn_wrt_exam_total_se001" />
    <property   name="param-count" 			value="1" />
    <property   name="param0" 				value="RACER_PERIO_NO" />
    <property   name="dao" 					value="candao" />    
  </activity>    

  <!-- 학과일자별 세부학과 평가조회 -->
  <activity name="searchWrtExamDetail" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property   name="resultkey" 			value="dsWrtExam_Detail" />
    <property   name="sqlkey" 				value="tbdn_wrt_exam_detail_se001" />
    <property   name="param-count" 			value="8" />
    <property   name="param0" 				value="RACER_PERIO_NO" />
    <property   name="param1" 				value="ROUND" />
    <property   name="param2" 				value="DT" />
    <property   name="param3" 				value="RACER_PERIO_NO" />
    <property   name="param4" 				value="ROUND" />
    <property   name="param5" 				value="DT" />
    <property   name="param6" 				value="RACER_PERIO_NO" />
    <property   name="param7" 				value="DT" />
    <property   name="dao" 					value="candao" />    
  </activity>
  
  <!-- 후보생 일자별기록 조회 -->
  <activity name="searchWrtExamDate" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property   name="resultkey" 			value="dsWrtExam_Date" />
    <property   name="sqlkey" 				value="tbdn_wrt_exam_date_se001" />
    <property   name="param-count" 			value="2" />
    <property   name="param0" 				value="RACER_PERIO_NO" />
    <property   name="param1" 				value="CAND_NO" />
    <property   name="dao" 					value="candao" />    
  </activity>
  
  <!-- 학과일자별 세부학과 평가 저장 --> 
  <activity name="saveWrtnEstmDetail"	class="snis.can_boa.boatstd.d06000015.activity.saveWrtnEstm">
    <transition name="success" 	        value="end" />
  </activity>
	
	<transaction-manager id="tx_can" commit="true" />
</service>