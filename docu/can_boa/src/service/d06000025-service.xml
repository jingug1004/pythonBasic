<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000025" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsMesurItem" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchMesurItem"		value="searchMesurItem" />
    <transition name="saveMesurItem"		value="saveMesurItem" />
    <transition name="success" 				value="end" />
  </activity>
  
  <!-- 평가기준종목 설정 --> 
  <activity name="searchMesurItem"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsMesurItem" />
    <property name="sqlkey" 				value="tbdn_mesur_item_fn001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="round" />
    <property name="dao" 					value="candao" />
  </activity>
     
  <!-- 평가기준종목 저장 --> 
  <activity name="saveMesurItem" 			class="snis.can_boa.boatstd.d06000025.activity.SaveMesurItem">
    <transition name="success" 	        value="end" />
  </activity>
  	
  <transaction-manager id="tx_can" commit="true" />
</service>