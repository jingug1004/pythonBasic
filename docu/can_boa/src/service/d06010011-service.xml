<?xml version="1.0" encoding="EUC-KR"?>
<service name="d0601011" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutCandRaceMst|dsRacePlan|dsRacePlanTotal|dsRacePlanDetail" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				   class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchCandRaceDetl"	   value="searchCandRaceDetl" />
    <transition name="searchRacePlan"		   value="searchRacePlan" />
    <transition name="searchRacePlanDetail"	   value="searchRacePlanDetail" />
    <transition name="searchRacePlanTotal"	   value="searchRacePlanTotal" />
    <transition name="saveCandRaceDetl"		   value="saveCandRaceDetl" />
    <transition name="saveCfm"		           value="saveCfm" />
    <transition name="success" 				   value="end" />
  </activity>
  
  <!-- 출주 조회 -->
  <activity name="searchCandRaceDetl" class="snis.can_boa.boatstd.d06010011.activity.SearchCandRaceDetl">
    <transition name="success" value="searchRacePlan" />
  </activity> 
  
  <!-- 경정후보생 출주표조회 -->
  <activity name="searchRacePlan" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRacePlan" />
    <property name="sqlkey" 				value="tbdn_race_plan_se001" />
    <property name="param-count" 			value="5" />
    <property name="param0" 				value="DAY_NO" />
    <property name="param1" 				value="DT" />
    <property name="param2" 				value="DT" />
    <property name="param3" 				value="RACER_PERIO_NO" />
    <property name="param4" 				value="DAY_NO" />
    <property name="dao" 					value="candao" />    
  </activity> 
  
  <!-- 경정후보생 출주표 세부조회 -->
  <activity name="searchRacePlanDetail" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchRacePlanTotal" />
    <property name="resultkey" 				value="dsRacePlanDetail" />
    <property name="sqlkey" 				value="tbdn_race_plan_detail_se001" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="RACER_PERIO_NO" />
    <property name="param1" 				value="FROM_DT" />
    <property name="param2" 				value="TO_DT" />
    <property name="dao" 					value="candao" />        
  </activity> 
  
    <!-- 경정후보생 출주표 총괄조회 -->
  <activity name="searchRacePlanTotal" 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRacePlanTotal" />
    <property name="sqlkey" 				value="tbdn_race_plan_total_se001" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="RACER_PERIO_NO" />
    <property name="param1" 				value="FROM_DT" />
    <property name="param2" 				value="TO_DT" />
    <property name="dao" 					value="candao" />    
  </activity> 
  
  <!-- 출주 저장 -->
  <activity name="saveCandRaceDetl" class="snis.can_boa.boatstd.d06010011.activity.SaveCandRaceDetl">
    <transition name="success" value="searchCandRaceDetl" />
  </activity> 
  
  <!-- 확정처리 -->
  <activity name="saveCfm" class="snis.can_boa.boatstd.d06010011.activity.SaveCfm">
    <transition name="success" value="end" />
  </activity> 
  
  <transaction-manager id="tx_can" commit="true" />
</service>