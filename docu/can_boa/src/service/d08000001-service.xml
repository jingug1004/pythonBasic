<?xml version="1.0" encoding="EUC-KR"?>
<service name="d08000001" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsEquipList|dsRepairList|dsOilEntList|dsOilOutList|dsOutSum|dsBoaHstry|dsMotHstry|dsOilEasyOut" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEquipList"    	value="searchEquipList" />
    <transition name="searchRepairList"    	value="searchRepairList" />
    <transition name="searchEntOilList"    	value="searchEntOilList" />
    <transition name="searchOutOilList"    	value="searchOutOilList" />
    <transition name="searchOutSum"    		value="searchOutSum" />
    <transition name="saveOutOil"   		value="saveOutOil" />
    <transition name="saveEasyOutOil"   	value="saveEasyOutOil" />
    <transition name="saveEquip"    		value="saveEquip" />    
    <transition name="saveRepair"   		value="saveRepair" />
    <transition name="saveEntOil"   		value="saveEntOil" />
    <transition name="searchBoaHstry"		value="searchBoaHstry" />
    <transition name="searchMotHstry"		value="searchMotHstry" />
    <transition name="searchEasyOutOilList"	value="searchEasyOutOilList" />
	<transition name="success" 				value="end" />
  </activity>

  <!-- 장비내역  조회 -->
  <activity name="searchEquipList"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsEquipList" />
    <property name="sqlkey" 				value="d08000001_fb001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="EQUIP_GBN" />
    <property name="param1" 				value="EQUIP_NO" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 정비내역  조회 -->
  <activity name="searchRepairList"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRepairList" />
    <property name="sqlkey" 				value="d08000001_fb002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="EQUIP_GBN" />
    <property name="param1" 				value="EQUIP_NO" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 유류입고내역  조회 -->
  <activity name="searchEntOilList"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsOilEntList" />
    <property name="sqlkey" 				value="d08000001_fb003" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="OIL_GBN" />
    <property name="param1" 				value="ENT_YEAR" />
    <property name="param2" 				value="ENT_MONTH" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 유류출고내역  조회 -->
  <activity name="searchOutOilList"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsOilOutList" />
    <property name="sqlkey" 				value="d08000001_fb004" />
    <property name="param-count" 			value="3" />
    <property name="param0" 				value="OIL_GBN" />
    <property name="param1" 				value="OUT_YEAR" />
    <property name="param2" 				value="OUT_MONTH" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 유류간편출고내역  조회 -->
  <activity name="searchEasyOutOilList"			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsOilEasyOut" />
    <property name="sqlkey" 				value="d08000001_fb005" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="schYY" />
    <property name="param1" 				value="schMM" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 출고현황  조회 -->
  <activity name="searchOutSum"				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsOutSum" />
    <property name="sqlkey" 				value="d08000001_out_sum" />
    <property name="param-count" 			value="24" />
    <property name="param0" 				value="schYY" />
    <property name="param1" 				value="schMM" />
    <property name="param2" 				value="schYY" />
    <property name="param3" 				value="schMM" />
    <property name="param4" 				value="schYY" />
    <property name="param5" 				value="schMM" />
    <property name="param6" 				value="schYY" />
    <property name="param7" 				value="schMM" />
    <property name="param8" 				value="schYY" />
    <property name="param9" 				value="schMM" />
    <property name="param10" 				value="schYY" />
    <property name="param11" 				value="schMM" />
    <property name="param12" 				value="schYY" />
    <property name="param13" 				value="schMM" />
    <property name="param14" 				value="schYY" />
    <property name="param15" 				value="schMM" />
    <property name="param16" 				value="schYY" />
    <property name="param17" 				value="schMM" />
    <property name="param18" 				value="schYY" />
    <property name="param19" 				value="schMM" />
    <property name="param20" 				value="schYY" />
    <property name="param21" 				value="schMM" />
    <property name="param22" 				value="schYY" />
    <property name="param23" 				value="schMM" />
    <property name="dao" 					value="candao" />
  </activity>
  
  <!-- 장비내역  저장 --> 
  <activity name="saveEquip" 			class="snis.can_boa.boatstd.d08000001.activity.SaveEquip">
    <transition name="success" 	        value="saveRepair" />
  </activity>
  
  <!-- 정비내역  저장 --> 
  <activity name="saveRepair" 			class="snis.can_boa.boatstd.d08000001.activity.SaveRepair">
    <transition name="success" 	        value="end" />
  </activity>
  
  <!-- 유류입고내역  저장 --> 
  <activity name="saveEntOil" 			class="snis.can_boa.boatstd.d08000001.activity.SaveEntOil">
    <transition name="success" 	        value="end" />
  </activity>
  
  <!-- 유류출고내역  저장 --> 
  <activity name="saveOutOil" 			class="snis.can_boa.boatstd.d08000001.activity.SaveOutOil">
    <transition name="success" 	        value="end" />
  </activity>

  <!-- 유류출고내역 간편 저장 --> 
  <activity name="saveEasyOutOil" 		class="snis.can_boa.boatstd.d08000001.activity.SaveEasyOutOil">
    <transition name="success" 	        value="end" />
  </activity>

  <transaction-manager id="tx_can" commit="true" />
</service>