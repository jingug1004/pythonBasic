<?xml version="1.0" encoding="EUC-KR"?>
<service name="d06000026" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsBodyInsp|dsBodyInspDt" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchBodyInsp"		value="searchBodyInsp" />
    <transition name="searchBodyInspDt"		value="searchBodyInspDt" />
    <transition name="saveBodyInsp"			value="saveBodyInsp" />
    <transition name="saveBodyInspDt"		value="saveBodyInspDt" />
    <transition name="success" 				value="end" />
  </activity>
  
  <!-- 평가기준종목 설정 --> 
  <activity name="searchBodyInsp"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsBodyInsp" />
    <property name="sqlkey" 				value="tbdn_mesur_body_insp_fn001" />
    <property name="param-count" 			value="4" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="round" />
    <property name="param2" 				value="racerPerioNo" /> 
    <property name="param3" 				value="round" />    
    <property name="dao" 					value="candao" />
  </activity>

  <!-- 평가기준종목 설정 --> 
  <activity name="searchBodyInspDt"	 	    class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsBodyInspDt" />
    <property name="sqlkey" 				value="tbdn_mesur_body_insp_fn002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="racerPerioNo" /> 
    <property name="param1" 				value="round" />
    <property name="dao" 					value="candao" />
  </activity>  
     
  <!-- 평가기준종목 저장 --> 
  <activity name="saveBodyInsp" 			class="snis.can_boa.boatstd.d06000026.activity.SaveBodyInsp">
    <transition name="success" 	        value="end" />
  </activity>
  
    <!-- 평가기준종목 저장 --> 
  <activity name="saveBodyInspDt" 			class="snis.can_boa.boatstd.d06000026.activity.SaveBodyInsp">
    <transition name="success" 	        value="end" />
  </activity>
  	
  <transaction-manager id="tx_can" commit="true" />
</service>