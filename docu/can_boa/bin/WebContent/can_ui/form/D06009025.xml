function fcSearch() {

	if(!fcSearchCheck()){
		return;
	}

	var sServiceName = "d06009025-service:searchViolResult";
    var sInDataSet   = "";
    var sOutDataSet  = "dsCandList=dsCandList dsCandListResult=dsCandListResult";
    var pRacerName	 = "";
    
    fcd_AddParam("pRacerPerioNo", Trim(cbPerioNo.Value));    
	
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
	
}


