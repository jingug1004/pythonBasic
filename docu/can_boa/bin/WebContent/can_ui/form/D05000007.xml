>>  취소 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInit() {
    dsUserListValue.ClearData();
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck() {
	fcInit();
	
	return true;
}

/*-------------------------------------------------------------------------------------------------+
>>  메뉴 조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch() 
{    
    if ( !fcSearchCheck() ) return;
    
    var sServiceName = "d05000007-service:searchMenu";
    var sInDataSet   = "";
    var sOutDataSet  = "dsAuthListValue=dsAuthListValue";
    
    fcd_AddParam("MN_GRP", cbMenuGrp.Value);
    
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);     
}

/*-------------------------------------------------------------------------------------------------+
>>  사용자 조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchAuthUserList(nRow)
{
	if ( !fcSearchCheck() ) return;
	
	if(nRow != null && nRow > -1){
		dsAuthListValue.row = nRow;
	}
	
	var sServiceName = "d05000007-service:searchUserList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsUserListValue=dsUserListValue";
    
    fcd_AddParam("MN_ID", dsAuthListValue.GetColumn(dsAuthListValue.row,"MN_ID"));
    fcd_AddParam("TEAM_NM", edDeptNm.text);
    fcd_AddParam("USER_NM", edSearchTxt.text);
        
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);       
}


/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {
	//변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsUserListValue) ) {
		//저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");	
		return false;
	}
	
	for ( i = 0; i < dsUserListValue.GetRowCount(); i++ ) {
		if ( dsUserListValue.GetRowType(i) != "normal" ) {
			if(dsUserListValue.GetColumn(i, "PERSONAL_YN") == 1) {
				oDataSet          = dsUserListValue;
				sMandatoryColID   = "PERSONAL_MN_IP";
				sMandatoryColName = "허용IP";
				oGrid             = grdUserList;
				if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
					return false;
				}
			}
		}
	}	
	
	return true;
}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSave(obj) {
    if ( !fcSaveCheck(obj) ) return;
    
    var sServiceName = "";
    var sInDataSet   = "";
    var sOutDataSet  = "";  
    
    sServiceName = "d05000007-service:saveUserMenu";
    sInDataSet   = "dsAuthListValue=dsAuthListValue:u ";
    sInDataSet  += "dsUserListValue=dsUserListValue:u ";
    
    dsAuthListValue.SetColumn(dsAuthListValue.row,"CHK","1");
    
    sOutDataSet  = "";
        
    //fcd_AddParam("USER_ID", dsUserListValue.getColumn(grdUserList.clickedRow, "USER_ID"));
    
    for ( var i = 0; i < dsUserListValue.rowcount; i++ ) {        
        if( dsUserListValue.getColumn(i, "PERSONAL_YN") == 1) {
            if(dsUserListValue.getColumn(i, "PERSONAL_YN") != 1) dsUserListValue.setColumn(i, "PERSONAL_YN", 0);
        }
    }
    
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}


