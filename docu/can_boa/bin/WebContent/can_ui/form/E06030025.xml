/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck() {
   if(!fnc_IsMandatory(cboStndYear, "출납년도")){
		return false;
   }else if(!fnc_IsMandatory(cboPartsMonth, "출납월")){
		return false;
   }
   return true;
}

//조회 
function fcSearch(obj) {
	fcSearchCheck();
    //부품 조회 
    fcSearchParts();
    
}

//부품  조회 
function fcSearchParts(){
	//fcd_AddParam("STND_YEAR", CURR_STNT_YEAR);
	fcd_AddParam("DT", cboStndYear.Value+cboPartsMonth.Value);    
    fcd_AddParam('MBR_CD', GBL_MBRCD);
    fcd_AddParam('PARTS_CD', edPartsCd.Text);
    fcd_AddParam('PARTS_NM', edItemCdNm.Text);
    fcd_AddParam("PARTS_GBN",  fcgetPartGbn());	// 장비구분    

    var sServiceName = "e06030025-service:searchOutMonthPartsDesc";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutParts=dsOutParts";
    fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
    
}
