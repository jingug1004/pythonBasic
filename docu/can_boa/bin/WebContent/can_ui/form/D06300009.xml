***************************************************************************************************/
#include "lib::can_common_script.js";
#include "lib::can_app_script.js";

/***************************************************************************************************
*                                          화면 변수 선언부                                        *
***************************************************************************************************/
var sFormId       = 'D06300009';
var sBUTTONLIST   = 'TTFTFT';                          //공통기능정의(버튼순으로 T or F)
var sUSERAUTH     = '';                                //사용자별 프로그램 사용 권한
var sFORMCAPTION  = '';                                //Form의 Title 정보
var sFORMLOCATION = '';                                //Form의 Location 정보
var sUSERLAVEL    = '';                                //Form의 사용  Level
var vREVOBJ       = Array(9);                          //결과값 처리를 위한 오브젝트 
var sGRDTIT       = "";                                //그리드 제목
var sCUROBJ       ;                                    //현재 LOST FOCUS 오브젝트
/***************************************************************************************************
*                                         공통 Event 처리 부분                                     *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 Load時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormLoadSetting(obj) {  
	
	sBUTTONLIST   = 'T'+sInpt+'FTFT';
    
 	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
	
	//초기 화면 BK Color설정 
	fnc_SetBKColor(this);
	
	// 화면 초기화
	fcInit();

	//공통코드 조회
	fcd_GetComboDs(dsInComCd);
	
	calEndDt.Value = Today();
	calStrDt.Value = AddMonth(calEndDt.Value, -1);
	
	divBtnRow.btnAddRow.visible = "false";
}

/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 UNLoad時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/

function fcFormUnloadProcess(obj) {
	//자료 변경 여부를 체크하고자 하는  DataSet Object를 넘긴다.
	var sDataset = "";
	if ( !fnc_FormUnloadCheck(sDataset , sFormId) ) return false;
}

/*-------------------------------------------------------------------------------------------------+
>>  단축키 처리(Form KeyDown)
+-------------------------------------------------------------------------------------------------*/
function fcHotKey(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

	//Return Key Press時 Next Component focus
	if ( nChar == 13 ) {
		var oFocusComponent = GetNextComponent(true);
		oFocusComponent.setfocus();
		return;
	}
	//Control Key가 눌려져 있다면 단축키 처리
	//if (bControl) fnc_HotKey(this, nChar);
}

/***************************************************************************************************
*                                      공통 버튼 Event 처리 부분                                   *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  취소 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInit(obj) {
    dsDiaryMstList.ClearData();
}

function fcToExcel(obj) {
	var sExcelList = "";
	
	fcd_OpenExcelListExport(sExcelList);
}  
  
/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
	fcInit(obj);	

	if ( !fnc_IsMandatory(calStrDt, "시작일") ) return false;
	if ( !fnc_IsMandatory(calEndDt, "종료일") ) return false;
			
	return true;
}


//조회버튼
function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "d06000014-service:searchDiaryMstList";
    var sInDataSet   = "";
    var sOutDataSet  = " dsDiaryMstList=dsDiaryMstList ";
	
	fcd_AddParam("STR_DT", Trim(calStrDt.Value));
	fcd_AddParam("END_DT", Trim(calEndDt.Value));

    fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack",false);
}

/*-------------------------------------------------------------------------------------------------+
>>  삭제 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {
	
	return true;
}

//저장버튼클릭
function fcSave(obj) {
    if ( !fcSaveCheck(obj) ) return;

	var sServiceName = "d06000014-service:saveDiaryMstList";
    var sInDataSet   = "dsDiaryMstList=dsDiaryMstList:u ";
    var sOutDataSet  = "dsDiaryMstList=dsDiaryMstList ";

    fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack",false);
}


/*-------------------------------------------------------------------------------------------------+
>>  화면 종료(닫기) 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcEnd(obj) {
	//화면 종료
	Close();	
}

/*-------------------------------------------------------------------------------------------------+
>>  행추가 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcAddRow(obj) {
    var sArg = " Arg_sDt=" + quote("")
             + " Arg_sPerio=" + quote("");
    Dialog("form::D06000014.xml", sArg, 834, 450, "");
    fcSearch(divSearch.btnSearch);
}

/*-------------------------------------------------------------------------------------------------+
>>  행삭제 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcDelRow(obj) {
    if ( !fnc_DeleteCheck(dsDiaryMstList, "CHK", "1") ) return;
}

/***************************************************************************************************
*                                      사용자 정의  처리 부분                                      *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/

function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

    if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
    
	// 공통코드 조회시
	if ( sServiceName == "common-service:searchCode" ) {
        return;
	}
    
	// 조회
	if ( sServiceName == "d06000014-service:searchDiaryMstList" ) {
        dsDiaryMstList.AddColumn("CHK");
        return;
	}
	 
	// 저장
	if ( sServiceName == "d06000014-service:saveDiaryMstList" ) {	
        dsDiaryMstList.AddColumn("CHK");
		fcSearch();
        return;
	}
	 
    //오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 Post Process!
+-------------------------------------------------------------------------------------------------*/
function fcPostProcess(sServiceName) {
}

function grdRacerTrngMst_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
    var sDt    = dsDiaryMstList.GetColumn(nRow, "DT");
    var sPerio = dsDiaryMstList.GetColumn(nRow, "RACER_PERIO_NO");
    if ( fnc_IsNull(sDt   ) ) return;
    if ( fnc_IsNull(sPerio) ) return;

    var sArg = " Arg_sDt=" + quote(sDt)
             + " Arg_sPerio=" + quote(sPerio);
    Dialog("form::D06000014.xml", sArg, 834, 450, "");
    fcSearch(divSearch.btnSearch);
}
