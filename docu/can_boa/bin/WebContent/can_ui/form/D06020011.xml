***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 Load時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormLoadSetting(obj) {  

 	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
	
	//초기 화면 BK Color설정 
	fnc_SetBKColor(this);
	//공통코드 조회
	fcFirstSerach();	
}


/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 UNLoad時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/

function fcFormUnloadProcess(obj) {
	//자료 변경 여부를 체크하고자 하는  DataSet Object를 넘긴다.
	var sDataset = "";
	if ( !fnc_FormUnloadCheck(sDataset , sFormId) ) return false;
}


/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
//화면로드시 기수 조회
function fcFirstSerach(obj) {
	var sServiceName = "common-service:searchPerioNo";
    var sInDataSet   = "";
    var sOutDataSet  = "dsPerioNo=dsPerioNo ";
    
    fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

//조회버튼
function fcSearch(obj) {
    var sServiceName = "d06010011-service:searchRacePlanDetail";
    var sInDataSet   = "";
    var sOutDataSet  = "dsRacePlanTotal=dsRacePlanTotal "
                     + "dsRacePlanDetail=dsRacePlanDetail ";
    
    fcd_AddParam("RACER_PERIO_NO", cbPerioNo  .Value);
    fcd_AddParam("FROM_DT"       , calFromDate.Value);
    fcd_AddParam("TO_DT"         , calToDate.Value);
    
    fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/

function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

    if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
    
	// 공통코드 조회시
	if ( sServiceName == "common-service:searchPerioNo" ) {
        fcSearch();
        return;
	}
}


/*-------------------------------------------------------------------------------------------------+
>>  엑셀 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcToExcel(obj) {
	var sExcelList = "grdRacePlanT:후보생출주자료총계|grdRacePlanD:후보생출주세부자료";
    fcd_OpenExcelListExport(sExcelList);
}


/*-------------------------------------------------------------------------------------------------+
>>  화면 종료(닫기) 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcEnd(obj) {
	//화면 종료
	Close();	
