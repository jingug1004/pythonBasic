//조회버튼
function fcSearch(obj) {

    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "d07000008-service:searchUsage";
    var sInDataSet   = "";
    var sOutDataSet  = "dsUsage=dsUsage dsUsageSum=dsUsageSum ";
    
	fcd_AddParam("STND_YEAR",    Mid(calUseDt.Value,0,4));
	fcd_AddParam("MBR_CD",       "001");
	fcd_AddParam("EQUIP_TPE_CD", fcGetEquipTypeCd());
	fcd_AddParam("USE_DT",       calUseDt.Value);

	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

