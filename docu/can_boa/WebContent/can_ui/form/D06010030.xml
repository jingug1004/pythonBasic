function fcSearchCheck(obj) {
    if ( !fnc_IsMandatory(cboPerioNo, "기수"    ) ) return false;
    if ( !fnc_IsMandatory(cboDayNo  , "출주번호") ) return false;
    if ( !fnc_IsMandatory(cboRound  , "라운드"  ) ) return false;
    
    fcInit(obj);
	return true;
}

function fcSearch(obj) {

	if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "d06010030-service:searchCandList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsCandList=dsCandList "
                     + "dsCandSum=dsCandSum ";
    
	fcd_AddParam(Trim("RACER_PERIO_NO"), cboPerioNo.Value);
	fcd_AddParam(Trim("DAY_NO        "), cboDayNo  .Value);
	fcd_AddParam(Trim("ROUND         "), cboRound  .Value);
	fcd_AddParam(Trim("GBN           "), '006'           );

	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

