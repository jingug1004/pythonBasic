/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

    if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
    
	// 공통코드 조회시
	if ( sServiceName == "common-service:searchCode" ) {
        cboPerioNo.Index = 0;
        
        fcd_GetDayNo(cboPerioNo.Value, "dsDayNo");
        return;
	}
    
	// 경주일차 조회시
	if ( sServiceName == "common_06500000-service:searchDayNo" ) {
        cboDayNo.Index = 0;
        
        fcd_GetRound(cboPerioNo.Value, cboDayNo.Value, "dsRound");
        return;
	}
    
	// 라운드 조회시
	if ( sServiceName == "common_06500000-service:searchRound" ) {
        cboRound.Index = 0;
        
        dsRound.UnFilter();
        dsRound.Filter("GBN='006'");
        return;
	}
    
    // 조회 후
	if( sServiceName == "d06010030-service:searchCandList" ) {
        for ( var i = 1; i <= 4; i++ ) {
            grdCandList.SetCellProp('summary', ((i - 1) * 2) + 2 + 5, 'text', dsCandSum.GetColumn(0, "RACE_TIME_" + i + "_" + 7));
        }
    }
}

