/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 Load時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormLoadSetting(obj) {
  sBUTTONLIST   = 'FFFTFT';
    
 	//화면환경 설정
	sFORMCAPTION  = obj.Title;
	sFORMLOCATION = "";

	//화면 정보 Display 및 Button 권한 설정
	fnc_FormLoadSetting(obj, sFORMCAPTION, sFORMLOCATION, sBUTTONLIST, sUSERAUTH);
	
	//초기 화면 BK Color설정 
	fnc_SetBKColor(this);
    
    fcInit();
    
    calFromDt.Value = AddDate(today(), -7); 
	calToDt.Value   = today();
	    
    // 메뉴 조회
    fcSearch();
    
}

/*-------------------------------------------------------------------------------------------------+
>>  최초 화면 UNLoad時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormUnloadProcess(obj) {
	//자료 변경 여부를 체크하고자 하는  DataSet Object를 넘긴다.
	var sDataset = "dsAccessList";
	if ( !fnc_FormUnloadCheck(sDataset , sFormId) ) return false;
}

/*-------------------------------------------------------------------------------------------------+
>>  단축키 처리(Form KeyDown)
+-------------------------------------------------------------------------------------------------*/
function fcHotKey(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {

	if ( nChar == 13 ) {
		var oFocusComponent = GetNextComponent(true);
		oFocusComponent.setfocus();
		return;
	}
}

/***************************************************************************************************
*                                      공통 버튼 Event 처리 부분                                   *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  취소 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInit() {
    dsAccessList.ClearData();
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck() {
	fcInit();
	
	return true;
}

/*-------------------------------------------------------------------------------------------------+
>>  메뉴 조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearch() 
{    
    if ( !fcSearchCheck() ) return;
    
    var sServiceName = "d05000010-service:searchAccessList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsAccessList=dsAccessList";
        
    fcd_AddParam("STR_DT",  calFromDt.Value);
	fcd_AddParam("END_DT",  calToDt.Value);
	fcd_AddParam("USER_ID", edSearchTxt.Text);
    
	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);     
}

