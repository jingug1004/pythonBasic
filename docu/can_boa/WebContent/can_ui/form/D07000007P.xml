function fcInit(obj) {
    dsRacerMentoring.ClearData();
}

function fcToExcel(obj) {
	var sExcelList = "";
	
	fcd_OpenExcelListExport(sExcelList);
}  
  
/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
	fcInit(obj);	

	if ( !fnc_IsMandatory(calStrDt, "시작일") ) return false;
	if ( !fnc_IsMandatory(calEndDt, "종료일") ) return false;
			
	return true;
}


//조회버튼
function fcSearch(obj) {

    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "d07000007-service:searchTrngMenteeList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsMenteeList=dsMenteeList";
    
	fcd_AddParam(Trim("STR_DT"), calStrDt.Value);
	fcd_AddParam(Trim("END_DT"), calEndDt.Value);
	fcd_AddParam(Trim("MENTO_GRP_NO"), MENTO_GRP_NO);
	fcd_AddParam(Trim("TRNG_ASK_SEQ"), TRNG_ASK_SEQ);

	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
	
}	    

/*-------------------------------------------------------------------------------------------------+
>>  화면 종료(닫기) 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcClose(obj) {
	//화면 종료
	close(false);	
}

/*-------------------------------------------------------------------------------------------------+
>>  선택  버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcChoice(obj)
{
	var nChkedIdx = dsMenteeList.FindRow("CHK", 1);
	if(nChkedIdx==-1) {
        fcd_SetReturnDs(dsMenteeList, dsMenteeList.GetCurrow());
	} else {
        var sRowIdx;
        for(var i=0; i<dsMenteeList.GetRowCount(); i++) {
            if(dsMenteeList.GetColumn(i, "CHK")==1) {
                sRowIdx += i + "^";
            }
        }
        sRowIdx = mid(sRowIdx, 0, length(sRowIdx)-1);
        fcd_SetReturnDs(dsMenteeList, sRowIdx);
	}
	close(true);
}

