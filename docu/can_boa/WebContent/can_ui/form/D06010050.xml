function fcSearch(obj) {

	if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "d06010050-service:searchCandList";
    var sInDataSet   = "";
    var sOutDataSet  = "dsCandList=dsCandList "
                     + "dsCandSum=dsCandSum ";
    
	fcd_AddParam(Trim("RACER_PERIO_NO"), cboPerioNo.Value);
	// fcd_AddParam(Trim("DAY_NO        "), cboDayNo  .Value);
	// fcd_AddParam(Trim("ROUND         "), cboRound  .Value);
	fcd_AddParam(Trim("GBN           "), cdGdu     .Value);

	fcd_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
