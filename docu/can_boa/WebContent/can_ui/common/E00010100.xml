function fcSearchRaceTms(obj) {
    if ( !fcSearchCheck() ) return;

	var sServiceName = "e00010010-service:searchRaceTms";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutSRaceTms=dsOutRaceTms";
    
	fce_AddParam(Trim("STND_YEAR  "), cmbSStndYear.Value);
	fce_AddParam(Trim("TMS        "), null              );
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

function fcSearchRaceDayOrd(obj) {
    if ( !fcSearchCheck() ) return;

	cmbSRaceDayOrd.Index = -1;
	var sServiceName = "e00010010-service:searchRaceDayOrd";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutSRaceDayOrd=dsOutRaceDayOrd";
    
	fce_AddParam(Trim("STND_YEAR"), cmbSStndYear.Value);
	fce_AddParam(Trim("TMS      "), cmbSRaceTms.Value );
	fce_AddParam(Trim("DAY_ORD  "), null              );
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

function fcSearchRace(obj) {
    if ( !fcSearchCheck() ) return;

	cmbSRace.Index = -1;
	var sServiceName = "e00010010-service:searchRace";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutSRace=dsOutRace";
    
	fce_AddParam(Trim("STND_YEAR"), cmbSStndYear.Value  );
	fce_AddParam(Trim("TMS      "), cmbSRaceTms.Value   );
	fce_AddParam(Trim("DAY_ORD  "), cmbSRaceDayOrd.Value);
	fce_AddParam(Trim("RACE_NO  "), null                );
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}

