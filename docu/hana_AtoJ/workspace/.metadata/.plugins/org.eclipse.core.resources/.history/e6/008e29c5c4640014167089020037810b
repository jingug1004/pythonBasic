						         #{as_siljukyul_byung_su,jdbcType=VARCHAR} AS as_siljukyul_byung_su
						    FROM sale.salepart_batch2
						   WHERE SUBSTR (date_time, 1, 6) = #{as_date_time,jdbcType=VARCHAR}
