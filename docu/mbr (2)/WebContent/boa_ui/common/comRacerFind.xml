	<Script><![CDATA[/***************************************************************************************************
*   Form Common Script                                                                             *
*     현재 총 100 컬럼임 되도록 100 컬럼 안으로 코딩을 하세요                                      *
123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 123456789 1234567890
***************************************************************************************************/
/***************************************************************************************************
*                                         공통 Event 처리 부분                                     *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  01. 최초 화면 Load時 처리 할 사항
+-------------------------------------------------------------------------------------------------*/
function fcFormLoadSetting(obj) {
	edRacerNm.SetFocus();
}

/*-------------------------------------------------------------------------------------------------+
>>  02. Key Down (ESC)
+-------------------------------------------------------------------------------------------------*/
function fcFormOnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam){
	//ESC키 누르면 화면 종료
	if (nChar==27) Close(false);
	if (nChar==13) fcFind(obj);
}

/*-------------------------------------------------------------------------------------------------+
>>  05.확인 버튼 클릭
+-------------------------------------------------------------------------------------------------*/
function fcFind(obj){
	var sReturn = Trim(edRacerNm.Text);
    if ( fnc_IsNull(sReturn) ) Close(false);
	close(sReturn);
}

/*-------------------------------------------------------------------------------------------------+
>>  06.취소 버튼 클릭
+-------------------------------------------------------------------------------------------------*/
function fcClose(obj){
	Close(false);
}]]></Script>
