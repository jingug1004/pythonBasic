	dsOutFoodBasis.AddRow();
	
	var stnd_year =  cboSStndYear.Value;
    var mbr_cd = cboSMbr.Value;
    	
    dsOutFoodBasis.SetColumn(dsOutFoodBasis.rowposition,"STND_YEAR",stnd_year);
    dsOutFoodBasis.SetColumn(dsOutFoodBasis.rowposition,"MBR_CD",mbr_cd);
}

