/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_Result(ErrorCode, ErrorMSG) ) return;
	
	// 공통코드 조회시
	if ( sServiceName == "common-service:searchCode" ) {
        
        // 경정장코드에 '전체' 항목 추가
		fce_AddCodeRow(dsOutMbr, "전체", "", "");
        cboSMbr.Index = 0;
        cboSMbr.Value = fce_GetUserInfo("MBR_CD");
        		
        //조회
        fcSearch();
		return;
	}

	// 조회 후
	if ( sServiceName == "e03090010-service:searchFoodBasis" ) {       
        fnc_SetStatus(SNIS_BOA_0003, dsOutFoodBasis.GetRowCount());
	    if ( dsOutFoodBasis.GetColumn(dsOutFoodBasis.currow, "STND_YEAR") == 0 ) {
            return;
	    }
        
		return;
	}

	// 저장 후
	if ( sServiceName == "e03090010-service:SaveFoodBasis" ) {
        fcSearch();
		return;
	}
	
	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

