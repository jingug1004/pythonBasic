}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck() {
    if ( !fnc_IsMandatory(divSearchCond.calStrDt, "시작일") ) return false;
    if ( !fnc_IsMandatory(divSearchCond.calEndDt, "종료일") ) return false;
    
    fcInit();

	return true;
}

function fcSearch(obj) {
    if ( !fcSearchCheck() ) return;

	var sServiceName = "e02080120-service:searchIntroRunTm";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRacerList=dsOutRacerList";

    if ( cboType.Value == "002" ) {
        sServiceName = "e02080120-service:searchStarTm";
    }
    
	fce_AddParam(Trim("STR_DT"), divSearchCond.calStrDt.Value);
	fce_AddParam(Trim("END_DT"), divSearchCond.calEndDt.Value);
	fce_AddParam(Trim("ST_MTHD_CD"),cboStMthdCd.value 	); //온라인스타트여부

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


