function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "e02040035-service:searchRacerRunCount";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRacerRunCount=dsOutRacerRunCount ";
    
	fce_AddParam(Trim("STND_YEAR"), cboSStndYear.Value);
	fce_AddParam(Trim("MBR_CD"   ), cboSMbr.Value     );
	fce_AddParam(Trim("STMS"      ), cboSRaceTms.Value );
