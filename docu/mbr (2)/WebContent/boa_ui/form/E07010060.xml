>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	// 조회 후
	if ( sServiceName == "e07010060-service:searchAllowBac" ) {
	    fnc_SetStatus(SNIS_BOA_0003,  dsOutAllowBac.GetRowCount());
	    dsSpecCodeListValue.AddColumn("CHK");	//  이게 들어가야 checkbox에 체크됨. why??
	}

	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

