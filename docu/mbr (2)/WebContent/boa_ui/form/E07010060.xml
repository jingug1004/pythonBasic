/*-------------------------------------------------------------------------------------------------+
>>  연승상금-코드값 입력하면 코드명(연승)값이 자동으로 셀 입력
+-------------------------------------------------------------------------------------------------*/
function dsSpecCodeListValue_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if(strColumnID == "CD") {
		dsSpecCodeListValue.fireEvent = false; //반드시 필요
		var cd_nm = toNumber(dsSpecCodeListValue.GetColumn(nRow, "CD")) + "연승";
		dsSpecCodeListValue.SetColumn(nRow, "CD_NM", cd_nm);
		dsSpecCodeListValue.fireEvent = true; //반드시 필요
	}
}
