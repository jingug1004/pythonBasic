function fcInit(obj) {	
	if(sInpt == "F"){
		this.divBtnLst.btnSave.Visible = false;
	}
	dsOutAllowBac.ClearData();
    dsSpecCodeListValue.ClearData();
}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck() {
   
}

function fcSearch(obj) {
	fcSearchCheck();
	
	var sServiceName = "e07010060-service:searchAllowBac";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutAllowBac=dsOutAllowBac ";
        sOutDataSet += "dsSpecCodeListValue=dsSpecCodeListValue ";    
	
    fce_AddParam("STND_YEAR", cboStndYear.Value);
    fce_AddParam("GRP_CD", "E00138");
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {

	//변경된 자료가 있는지 체크
	if ( !fnc_DatasetChangeCheck(dsOutAllowBac)
	  && !fnc_DatasetChangeCheck(dsSpecCodeListValue)) {
		//저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");	
		return false;
	}
	return true;
}

//저장 
function fcSave(obj) {

	if ( !fcSaveCheck() ) return;
	
    var sServiceName = "e07010060-service:saveAllowBac";
    var sInDataSet   = "dsOutAllowBac=dsOutAllowBac:u ";
		sInDataSet  += "dsSpecCodeListValue=dsSpecCodeListValue:u ";
    var sOutDataSet  = "dsOutAllowBac=dsOutAllowBac ";
		sOutDataSet += "dsSpecCodeListValue=dsSpecCodeListValue ";
    
    fce_AddParam("STND_YEAR", cboStndYear.Value);
    fce_AddParam("GRP_CD", "E00138");
    fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
	
}

