            + '           <cell col="0"  colid="VIEW_MOT_NO"       align="center" display="text"/>'
            + '           <cell col="1"  colid="AVG_RANK_SCR"      align="center" display="text"/>'
            + '           <cell col="2"  colid="HIGH_RANK_RATIO"   align="center" display="text"/>'
            + '           <cell col="3"  colid="AVG_ITRDT_RUN_TM"  align="center" display="text"/>';
            
    for ( var i = 0; i < toInteger(cboSRaceTms.Value); i++ ) {
        sBody += '           <cell col="' + (i + 4) + '"  colid="TMS' + (i + 1) + '"      align="center"      display="text"/>';
    }

    sBody  += '         </body>';
    return sBody;
}

