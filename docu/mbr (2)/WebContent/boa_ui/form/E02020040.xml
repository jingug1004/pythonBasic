function fcSearch() {
    fcCreateGrid();

	var sDt = cboSStndYear.Value;
	var eDt = cboEStndYear.Value;
	if(    !fnc_isNull(sDt) 
        && !fnc_isNull(eDt)
        && toInteger(sDt) >= toInteger(eDt) ) {
        fnc_message(L_COM_ALT_0024, "N", "시작년도가 종료년도보다 이후일 수");
        cboSStndYear.Value = "";
        cboEStndYear.Value = "";
        return false;
	}	
	
	var sServiceName = "e02020040-service:searchRacerGrdStat";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRacerGrdStat=dsOutRacerGrdStat ";
    
	fce_AddParam("SSTND_YEAR", cboSStndYear.Value);
	fce_AddParam("ESTND_YEAR", cboEStndYear.Value);
	fce_AddParam("NM_KOR"    , edSearchText.Text );

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

