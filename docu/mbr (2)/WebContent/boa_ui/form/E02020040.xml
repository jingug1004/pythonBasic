function fcGetColumn() {
    var sColumn = "";
    sColumn = '					<columns>'
            + '                     <col width="100" fix="left" />'
            + '                     <col width="90"  fix="left" />';
    sColumn = sColumn + fcAddColumn();
            
    sColumn = sColumn
            + '					</columns>';
    return sColumn;
}

/*-------------------------------------------------------------------------------------------------+
>>  주선 Grid의 Column정보 생성
+-------------------------------------------------------------------------------------------------*/
function fcAddColumn() {
    var sColumn = "";
    for ( var i = cboSStndYear.Value; i <= cboEStndYear.Value; i++ ) {        
        sColumn += ' <col width="100" />';
        sColumn += ' <col width="100" />';
    }
    
    return sColumn;
}

/*-------------------------------------------------------------------------------------------------+
>>  주선 Grid의 Head정보 생성
+-------------------------------------------------------------------------------------------------*/
function fcGetHead() {
    var sHead = "";
    sHead   = '         <head>'
            + '           <cell bkcolor="user20" col="0" color="user21" display="text" font="굴림,9" rowspan="2" text="등록번호"/>'
            + '           <cell bkcolor="user20" col="1" color="user21" display="text" font="굴림,9" rowspan="2" text="선수명"/>';
    sHead   = sHead + fcAddHead();

    sHead   += '         </head>';
    return sHead;
}

/*-------------------------------------------------------------------------------------------------+
>>  주선 Grid의 Head정보 생성
+-------------------------------------------------------------------------------------------------*/
function fcAddHead() {
    
    var sHead   = "";
    var nCol    = 2;
    
    var sYear   = "";
    var sQuat   = "";
    var sYearCount = 0;
    for ( var i = cboSStndYear.Value; i <= cboEStndYear.Value; i++) {
        sHead += '  <cell bkcolor="user20" col="' + nCol + '" color="user21" colspan="2" display="text" font="굴림,9" text="' + i + '"/>  ';
        sYearCount++;
        nCol += 2;
    }
    
    nCol = 2;
    for ( var i = 0; i <= sYearCount; i++ ) {
        sHead += '  <cell bkcolor="user20" col="' + nCol++ + '" color="user24" display="text" font="굴림,9" row="1" text="전반기"/>';
        sHead += '  <cell bkcolor="user20" col="' + nCol++ + '" color="user24" display="text" font="굴림,9" row="1" text="후반기"/>';
    }

    return sHead;
}

/*-------------------------------------------------------------------------------------------------+
>>  주선 Grid의 Body정보 생성
+-------------------------------------------------------------------------------------------------*/
function fcGetBody() {
    var sBody = "";
    sBody   = '<body>';
    sBody   += '     <cell col="0" colid="RACER_NO" display="text" align="CENTER" />';
    sBody   += '     <cell col="1" colid="NM_KOR"   display="text" align="CENTER" />';
    sBody   += fcAddBody();
    sBody   += '</body>';
    
    return sBody;
}

