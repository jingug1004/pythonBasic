function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "e03040010-service:searchRacerChangeList";
    var sInDataSet   = "dsOutRacerArrange=dsOutRacerArrange ";
    var sOutDataSet  = "dsOutRacerList=dsOutRacerList ";
    
    if (Arg_sExclYn == "Y") {
		sInDataSet   = "dsOutRacerArrange=dsOutRacerArrange_0 ";
    }    
    
	fce_AddParam(Trim("STND_YEAR   "), Arg_sStndYear);
	fce_AddParam(Trim("MBR_CD      "), Arg_sMbrCd   );
	fce_AddParam(Trim("TMS         "), Arg_sTms     );
	fce_AddParam(Trim("EXCL_YN     "), Arg_sExclYn  );

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


