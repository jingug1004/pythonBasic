	if ( fnc_IsNull(sRacerNo) ) {
        return;
	}
	
	var asReturn = Array(2);
	asReturn[0] = sRacerNo;
	asReturn[1] = dsOutRacerList.GetColumn(dsOutRacerList.currow, "NM_KOR");
	
	close(asReturn);
}

