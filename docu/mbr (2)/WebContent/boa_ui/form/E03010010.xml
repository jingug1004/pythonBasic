>>  선수 선택시
+-------------------------------------------------------------------------------------------------*/
function grdMaster_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	var sRacerNo = dsOutSRacer.GetColumn(nRow, "RACER_NO");
	
	if ( fnc_IsNull(sRacerNo) ) return;
	
    // Content로 넘겨지는 arguements
	var sArg = "Arg_sRacerNo=" + quote(sRacerNo);
	
    if ( Dialog("form::E03010011.xml", sArg, 834, 450, "") ) fcSearch(divSearch.btnSearch);
}
