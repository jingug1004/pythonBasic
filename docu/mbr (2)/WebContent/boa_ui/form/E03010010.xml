/*-------------------------------------------------------------------------------------------------+
>>  Transaction 후 처리 해야 할 내용!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;

	// 공통코드 조회시
	if ( sServiceName == "common-service:searchCode" ) {
	    // 등급에 '전체' 항목 추가
		fce_AddCodeRow(dsOutRacerGrd, "전체", "", cboSRacerGrd);	
	    // 선수구분에 '전체' 항목 추가
		fce_AddCodeRow(dsOutRacerStat, "전체", "", cboSRacerStat);
        dsOutRacerStat.UnFilter();
        var sFilter = "CD_NM!='신인'";
        dsOutRacerStat.Filter(sFilter);

        fcSearch();
    }
    	
	// 조회 후
	if ( sServiceName == "e03010010-service:searchRacer" ) {
        fnc_SetStatus(SNIS_BOA_0003, dsOutSRacer.GetRowCount());
	}

	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

