/*-------------------------------------------------------------------------------------------------+
>>  Transaction »ƒ √≥∏Æ «ÿæﬂ «“ ≥ªøÎ!
+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_Result(ErrorCode, ErrorMSG) ) return;
	
	// ∞¯≈Îƒ⁄µÂ ¡∂»∏Ω√
	if ( sServiceName == "common-service:searchCode" ) {
        fce_SetStndYear(dsOutSStndYear, toInteger(fce_GetSysdate("YYYY")));
        cboSStndYear.Value = Arg_sStndYear;
        cboSMbr.Value      = Arg_sMbrCd;

        fce_GetRaceTms(Arg_sStndYear, Arg_sMbrCd, "", "dsOutSRaceTms");
        return;
	}

	// »∏¬˜ ∏ÆΩ∫∆Æ ¡∂»∏Ω√
	if ( sServiceName == "common_race-service:searchRaceTms" ) {
        cboSRaceTms.Value = Arg_sTms;
        
        fce_GetRaceDayOrd(Arg_sStndYear, Arg_sMbrCd, Arg_sTms, "dsOutSDayOrd");
		return;
	}

	// ¿œ¬˜ ∏ÆΩ∫∆Æ ¡∂»∏Ω√
	if ( sServiceName == "common_race-service:searchRaceDayOrd" ) {
        cboSDayOrd.Value = toInteger(Arg_sDayOrd) - 1;
        nRow = fnc_GetExistValue(dsOutSDayOrd, "DAY_ORD", cboSDayOrd.Value);
        
        fce_GetRace(Arg_sStndYear, Arg_sMbrCd, Arg_sTms, toInteger(Arg_sDayOrd) - 1, "dsOutSRace");
        fce_GetRace(Arg_sStndYear, Arg_sMbrCd, Arg_sTms, Arg_sDayOrd, "dsOutRace");

		return;
	}

	// ¿œ¬˜ ∏ÆΩ∫∆Æ ¡∂»∏Ω√
	if ( sServiceName == "common_race-service:searchRace" ) {
        cboSStrRaceNo.Index = 0;
        cboSEndRaceNo.Index = dsOutSRace.GetRowCount() - 1;
        cboRaceNo.Index     = dsOutSRace.GetRowCount() - 2;
		return;
	}

	// ¡∂»∏ »ƒ
	if ( sServiceName == "e02040045-service:searchRacer" ) {
        dsOutRacerList.AddColumn("chk");
        
        for ( var i = 0; i < dsOutRacerList.GetRowCount(); i++ ) {
            if ( i > 5 ) break;
            dsOutRacerList.SetColumn(i, "chk", 1);
        }
        
        return;
	}

	//ø¿∑˘∞° æ¯¿Ã ¡§ªÛ √≥∏Æµ«æ˙¥Ÿ∏È...
	fcPostProcess(sServiceName);
}

