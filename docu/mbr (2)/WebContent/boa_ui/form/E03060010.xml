}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {

	fcInit(obj);
	
	return true;
}

function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "e03060010-service:SearchRacerPrz";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRacerPrz=dsOutRacerPrz  ";
    
	fce_AddParam(Trim("STND_YEAR"), cboSStndYear.Value);
	fce_AddParam(Trim("GBN"), cboSPrzGbn.Value);
	fce_AddParam(Trim("RMK"), edPrzNm.Value);
	fce_AddParam(Trim("NM_KOR"), edRacerNm.Value);	

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}


