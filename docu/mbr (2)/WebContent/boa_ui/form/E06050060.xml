+-------------------------------------------------------------------------------------------------*/
function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_result(ErrorCode, ErrorMSG) ) return;
	
	if ( sServiceName == "common-service:searchCode" ) {
	    
    }else if( sServiceName == "e06050060-service:searchBFYearList" ) {
	    fcSearch();
	}else if( sServiceName == "e06050060-service:searchSuppBalance" ) {
		var iRow = dsBFYear.FindRow("BF_YEAR", ToNumber(cboBfYear.value) + 1);
		if (iRow > -1) {
			btnProcBringForward.Enable = false;
			btnCnclProcBF.Enable = true;
		} else {
			btnProcBringForward.Enable = true;
			btnCnclProcBF.Enable = false;
		}
	}else if( sServiceName == "e06050060-service:procBringForward" ) {
		fcSearch();
	}
	
	//오류가 없이 정상 처리되었다면...
	fcPostProcess(sServiceName);
}

