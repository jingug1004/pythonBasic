	//저장 할 자료가 없음
		fnc_Message(SNIS_COM_1006, "N");	
		return false;
	}
	
	if ( !fcCheckData() ) return false;
   
	return true;
}

function fcSave(obj) {
	
	if ( !fcSaveCheck() ) return;
	
	var ServiceName = "e09010001-service:saveAcctEduRacer";
    var sInDataSet   = "dsOutRacer=dsOutRacer:u";
    var sOutDataSet  = "dsOutRacer=dsOutRacer";
 
 	fce_SendTransaction(ServiceName, sInDataSet, sOutDataSet, "fcCallBack"); 
