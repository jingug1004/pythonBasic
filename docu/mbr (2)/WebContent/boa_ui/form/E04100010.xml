}

/*-------------------------------------------------------------------------------------------------+
>>  조회 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSearchCheck(obj) {
	
	if( fnc_isNull(cboStndYear.Value) ) {
        fnc_message(L_COM_ALT_0024, "N", "년도가 ");
        cboStndYear.Value = "";
        return false;
	}	

    fcInit(obj);
 	return true;
}

function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;

	var sServiceName = "e04100010-service:searchSyncRace";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutSyncRace=dsOutSyncRace ";
    
    fce_AddParam(Trim("STND_YEAR"), 			cboStndYear.Value		);
	fce_AddParam(Trim("MBR_CD"), 				cboMbr.Value     		);

	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


