function dsOutRacerComInfo_OnLoadCompleted(obj,nErrorCode,strErrorMsg,nReason)
{
	fnc_ProgressClose(); //팝업창이 Close안되었으면 Close시킨다.
	if(LATEST_ROW_POS>0 )dsOutRacerComInfo.Row =LATEST_ROW_POS;
}

function dsOutRacerComInfo_CanRowPosChange(obj,nOldRow,nRow)
{
	LATEST_ROW_POS = nOldRow;
}

function dsOutRacerComInfo_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if(strColumnID=='BK_CD' || strColumnID=='BK_ACCUNT'){
		var bk_cd =  dsOutRacerComInfo.getColumn(dsOutRacerComInfo.rowposition,'BK_CD');
		var org_bk_cd =  dsOutRacerComInfo.getColumn(dsOutRacerComInfo.rowposition,'ORG_BK_CD');
		var bk_accunt =  dsOutRacerComInfo.getColumn(dsOutRacerComInfo.rowposition,'BK_ACCUNT');
		var org_bk_accunt =  dsOutRacerComInfo.getColumn(dsOutRacerComInfo.rowposition,'ORG_BK_ACCUNT');
		var org_fnc_chk_yn =  dsOutRacerComInfo.getColumn(dsOutRacerComInfo.rowposition,'ORG_FNC_CHK_YN');
		if(bk_cd !=org_bk_cd || bk_accunt != org_bk_accunt ){
			dsOutRacerComInfo.SetColumn(dsOutRacerComInfo.rowposition, 'FNC_CHK_YN', 0);
		}else{
			dsOutRacerComInfo.SetColumn(dsOutRacerComInfo.rowposition, 'FNC_CHK_YN', org_fnc_chk_yn );
		}
	}
}
