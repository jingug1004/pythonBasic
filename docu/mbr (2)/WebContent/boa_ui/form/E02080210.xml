}

function fcSearch(obj) {
    if ( !fcSearchCheck(obj) ) return;
    
    //경주기준정보조회
	var sServiceName = "e02080210-service:searchPrizRace";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutPrizRace=dsOutPrizRace";
    
    if(chkPrizeGrp.Value){			// 대상경주그룹
		fce_AddParam(Trim("GRP_CD"), "E00139");
    }else if(chkEventGrp.Value){	// 이벤트경주그룹
		fce_AddParam(Trim("GRP_CD"), "E00142");
    }
    
    fce_AddParam(Trim("CD"), cboPrizRaceGrpCd.Value);
    
    if(chkSearch.Value){
		fce_AddParam(Trim("RACE_FROM_DT"), calRaceFromDt.Value	);
		fce_AddParam(Trim("RACE_TO_DT"), calRaceToDt.Value 		);
	}else{
		fce_AddParam(Trim("RACE_FROM_DT"), "");
		fce_AddParam(Trim("RACE_TO_DT"), "");
	}
	
    fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");    
}



