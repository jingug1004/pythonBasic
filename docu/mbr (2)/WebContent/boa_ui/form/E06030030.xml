function fcSearchCheck() {
   if(!fnc_IsMandatory(cboMbrCd, "경정장")){
		return false;
   }else if(!fnc_IsMandatory(calIjrySdt, "출납시작일자")){
		return false;
   }else if(!fnc_IsMandatory(calIjryEdt, "출납종료일자")){
		return false;
   }
   return true;
}

//조회 
function fcSearch(obj) {
	fcSearchCheck();
    //부품 조회 
    fcSearchParts();
    
}

