/***************************************************************************************************
*                                      공통 버튼 Event 처리 부분                                   *
***************************************************************************************************/
/*-------------------------------------------------------------------------------------------------+
>>  취소 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcInit(obj) {
    dsOutRace.ClearData();
    dsOutRace.AddColumn("RACE_NO");
    
    var sRaceNo = "";
    for ( var i = 1; i <= dsOutSRace.GetRowCount(); i++ ) {
        dsOutRace.AddRow();
        dsOutRaceGap.AddRow();
        
        sRaceNo = i;
        if ( i < 10 ) sRaceNo = "0" + sRaceNo;
        dsOutRace.SetColumn(i - 1, "RACE_NO", sRaceNo);
    }
    
    // 경주간격
    dsOutRaceGap.Copy(dsOutSRaceGap);
    
    if(dsOutSRace.GetRowCount() > dsOutRaceGap.GetRowCount()){		
		var sRaceNo = "";
		for ( var i = dsOutRaceGap.GetRowCount() + 1; i <= dsOutSRace.GetRowCount(); i++ ) {
			dsOutRaceGap.AddRow();
            sRaceNo = i;
            if ( i < 10 ) sRaceNo = "0" + sRaceNo;
            dsOutRaceGap.SetColumn(i - 1, "RACE_NO", sRaceNo);
		}
    }
}

