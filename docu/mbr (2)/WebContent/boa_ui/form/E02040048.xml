	var sServiceName = "e02040048-service:searchRaceGap";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutRaceGap=dsOutRaceGap ";
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}


/*-------------------------------------------------------------------------------------------------+
>>  저장 버튼 클릭 時 처리
+-------------------------------------------------------------------------------------------------*/
function fcSaveCheck(obj) {
	// 필수항목체크
	oDataSet          = dsOutRaceGap;
	sMandatoryColID   = "RACE_NO|STR_RACE_NO|END_RACE_NO";
	sMandatoryColName = "경주번호|시작경주번호|종료경주번호";
	oGrid             = grdRaceGap;
	if ( fnc_GetMandatoryViolateRow(oDataSet, sMandatoryColID, sMandatoryColName, oGrid) > -1 ) {
	    return false;
	}

	return true;
}

function fcSave(obj) {
    if ( !fcSaveCheck(obj) ) return;

	var sServiceName = "e02040048-service:saveRaceGap";
    var sInDataSet   = "dsOutRaceGap=dsOutRaceGap ";
    var sOutDataSet  = "dsOutRaceGap=dsOutRaceGap ";
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
}

