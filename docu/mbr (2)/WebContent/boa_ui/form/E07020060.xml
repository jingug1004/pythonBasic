function fcSearchCheck() {
   if(!fnc_IsMandatory(cboStndYear, "기준년도")){
		return false;
   }else if(!fnc_IsMandatory(cboPayMonth, "기준월")){
		return false;
   }
   return true;
   
}

//조회 
function fcSearch(obj) {
	if(!fcSearchCheck()){
		return;
	}
    
    var sServiceName = "e07020060-service:searchMonthPrizStat";
    var sInDataSet   = "";
    var sOutDataSet  = "dsOutMonthPrizPerio=dsOutMonthPrizPerio dsOutMonthPrizGrd=dsOutMonthPrizGrd dsOutMonthPrizAmt=dsOutMonthPrizAmt";
    
    fce_AddParam("STND_YEAR", cboStndYear.Value);
    fce_AddParam("PAY_MONTH", cboPayMonth.Value);
    fce_AddParam("INCLUDE_ALL_REG_RACER", chkAllRegRacer.Value);
    fce_AddParam("INCLUDE_ALL_STAT_RACER", chkAllStatRacer.Value);
    
    fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack", false);
}


