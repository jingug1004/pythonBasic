        dsOutPrizRaceGrpCd.InsertRow(0); //∫Û∑ŒøÏ ª˝º∫
        cboPrizRaceGrpCd.Index = 0;
        dsOutPrizRaceGrpCd.SetColumn(0, "CD_NM", "¿¸√º");
        
        dsOutEventRaceGrpCd.InsertRow(0); //∫Û∑ŒøÏ ª˝º∫
        dsOutEventRaceGrpCd.SetColumn(0, "CD_NM", "¿¸√º");

		return;
	}
	
	if(sServiceName == "e02080230-service:searchRacerStts"){
		var maxRaceRegNo = dsMaxRaceRegNo.GetColumn(0, "MAX_RACE_REG_NO");
		//alert(maxRaceRegNo);
		setGridSize1(maxRaceRegNo);
		setGridSize2(maxRaceRegNo);
	}
	
	//ø¿∑˘∞° æ¯¿Ã ¡§ªÛ √≥∏Æµ«æ˙¥Ÿ∏È...
	fcPostProcess(sServiceName);
}

