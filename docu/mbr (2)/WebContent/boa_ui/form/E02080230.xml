function setGridSize1(maxRaceRegNo){

	var nMaxTmsRunCnt = 8;
	var nMaxRaceRegNoCnt = maxRaceRegNo;
	
	//Alert("nMaxRaceRegNoCnt : " + nMaxRaceRegNoCnt);
	
	grdRacer.Redraw = false;
	
	//¿¸√º¿˚¿∏∑Œ ªÁ¿Ã¡Ó∏¶ ≥–»˚.
	for ( var i = 0; i < nMaxTmsRunCnt; i++ ) {
        grdRacer.SetColProp(7 + i, "Width", 60);
    }
	   
    //nMaxTmsRunCnt ∫Œ≈Õ nMaxRaceRegNoCnt±Ó¡ˆ ªÁ¿Ã¡Ó∏¶ ¡Ÿ¿”.
    for ( var i = nMaxTmsRunCnt; i > nMaxRaceRegNoCnt; i-- ) {
        grdRacer.SetColProp(7 + i-1, "Width", 0);
    }
    
    for(var k=0; k<7; k++){
		grdRacer.SetColProp(k, "Width", 60);
	}
	
    grdRacer.Redraw = true;
}

function setGridSize2(maxRaceRegNo){

	var nMaxTmsRunCnt = 8;
	var nMaxRaceRegNoCnt = maxRaceRegNo;
	
	//Alert("nMaxRaceRegNoCnt : " + nMaxRaceRegNoCnt);
	
	grdPerioNo.Redraw = false;
	
	//¿¸√º¿˚¿∏∑Œ ªÁ¿Ã¡Ó∏¶ ≥–»˚.
	for ( var i = 0; i < nMaxTmsRunCnt; i++ ) {
        grdPerioNo.SetColProp(6 + i, "Width", 60);
    }
	   
    //nMaxTmsRunCnt ∫Œ≈Õ nMaxRaceRegNoCnt±Ó¡ˆ ªÁ¿Ã¡Ó∏¶ ¡Ÿ¿”.
    for ( var i = nMaxTmsRunCnt; i > nMaxRaceRegNoCnt; i-- ) {
        grdPerioNo.SetColProp(6 + i-1, "Width", 0);
    }
    
    for(var k=0; k<6; k++){
		grdPerioNo.SetColProp(k, "Width", 60);
	}
	
    grdPerioNo.Redraw = true;
}

