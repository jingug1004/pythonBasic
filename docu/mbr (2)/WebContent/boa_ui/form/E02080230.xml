    }else if(chkEventGrp.Value){	// ¿Ã∫•∆Æ∞Ê¡÷±◊∑Ï
		fce_AddParam(Trim("GRP_CD"), "E00142");
    }
    
    fce_AddParam(Trim("CD"), cboPrizRaceGrpCd.Value);
    
    if(chkSearch.Value){
