function fcSave(obj) {
    if ( !fcSaveCheck(obj) ) return;

	var sServiceName = "e02080090-service:saveFL";
    var sInDataSet   = "dsOutFLList=dsOutFLList:u ";
    var sOutDataSet  = " ";
    
	fce_SendTransaction(sServiceName, sInDataSet, sOutDataSet, "fcCallBack");
