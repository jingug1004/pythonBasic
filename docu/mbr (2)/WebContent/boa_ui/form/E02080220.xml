function setGridSize(maxRaceRegNo, grdObj){

	var nMaxTmsRunCnt = 8;
	var nMaxRaceRegNoCnt = maxRaceRegNo;
	
	//Alert("nMaxRaceRegNoCnt : " + nMaxRaceRegNoCnt);
	
	grdObj.Redraw = false;
	
	//¿¸√º¿˚¿∏∑Œ ªÁ¿Ã¡Ó∏¶ ≥–»˚.
	for ( var i = 0; i < nMaxTmsRunCnt; i++ ) {
        grdObj.SetColProp(1 + i, "Width", 60);
    }
	   
    //nMaxTmsRunCnt ∫Œ≈Õ nMaxRaceRegNoCnt±Ó¡ˆ ªÁ¿Ã¡Ó∏¶ ¡Ÿ¿”.
    for ( var i = nMaxTmsRunCnt; i > nMaxRaceRegNoCnt; i-- ) {
        grdObj.SetColProp(1 + i-1, "Width", 0);
    }
    
    grdObj.SetColProp(0, "Width", 60);
    
    grdObj.Redraw = true;
}


