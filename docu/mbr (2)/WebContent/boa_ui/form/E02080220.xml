function fcCallBack(sServiceName, ErrorCode, ErrorMSG) {

	if ( !fnc_Result(ErrorCode, ErrorMSG) ) return;
	// ∞¯≈Îƒ⁄µÂ ¡∂»∏Ω√
	if ( sServiceName == "common-service:searchCode" ) {
        dsOutPrizRaceGrpCd.InsertRow(0); //∫Û∑ŒøÏ ª˝º∫
        cboPrizRaceGrpCd.Index = 0;
        dsOutPrizRaceGrpCd.SetColumn(0, "CD_NM", "¿¸√º");
        
        dsOutEventRaceGrpCd.InsertRow(0); //∫Û∑ŒøÏ ª˝º∫
        dsOutEventRaceGrpCd.SetColumn(0, "CD_NM", "¿¸√º");

		return;
	}
	
	if(sServiceName == "e02080220-service:searchPrizRaceRaceRegNoWin"){
		var maxRaceRegNo = dsMaxRaceRegNo.GetColumn(0, "MAX_RACE_REG_NO");
		
		setGridSize(maxRaceRegNo, grdRaceRegNoWin);
		setGridSize(maxRaceRegNo, grdCourseWin);
		setGridSize(maxRaceRegNo, grdRaceRegNo999);
		setGridSize(maxRaceRegNo, grdRaceRegNo140);
		setGridSize(maxRaceRegNo, grdRaceRegNo110);
		setGridSize(maxRaceRegNo, grdRaceRegNo120);
		setGridSize(maxRaceRegNo, grdRaceRegNoWin2);
		setGridSize(maxRaceRegNo, grdRaceRegNoWin3);
		
	}
	
	//ø¿∑˘∞° æ¯¿Ã ¡§ªÛ √≥∏Æµ«æ˙¥Ÿ∏È...
	fcPostProcess(sServiceName);
}

