<!-- 경주 위반행위 정보 가져오기 -->               
  <activity name="searchSangResult" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsSangResult" />
    <property name="sqlkey" value="tbea_sang_data_rio001" />
    <property name="param-count" value="6" />
