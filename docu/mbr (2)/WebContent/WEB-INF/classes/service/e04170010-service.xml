<?xml version="1.0" encoding="EUC-KR"?>
<service name="e04170010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsWeatherList|dsOutSRaceTms|dsRaceResult|dsSangResult|dsSangSum" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchWeather"		value="searchWeather" />    
    <transition name="searchRaceTms"		value="searchRaceTms" />
    <transition name="searchRaceResult"		value="searchRaceResult" />
    <transition name="searchSangResult"		value="searchSangResult" />
    <transition name="searchSangSum"		value="searchSangSum" />
    <transition name="success" 		    	value="end" />
  </activity>

  
  <!-- 경정 기상정보자료 가져오기 -->
  <activity name="searchWeather" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsWeatherList" />
    <property name="sqlkey" value="tbea_race_weather_rio001" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="TMS"/>
  </activity>
               
<!-- 회차정보 가져오기 -->               
  <activity name="searchRaceTms" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutSRaceTms" />
    <property name="sqlkey" value="tbea_race_tms_rio001" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="STND_YEAR"/>
  </activity>
  
<!-- 경주결과(매출포함)정보 가져오기 -->               
  <activity name="searchRaceResult" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsRaceResult" />
    <property name="sqlkey" value="tbea_race_result_rio001" />
    <property name="param-count" value="32" />
    <property name="dao" value="boadao" />
    <property name="param0"  value="STND_YEAR"/>
    <property name="param1"  value="MBR_CD"/>
    <property name="param2"  value="TMS"/>
    <property name="param3"  value="STND_YEAR"/>
    <property name="param4"  value="MBR_CD"/>
    <property name="param5"  value="TMS"/>
    <property name="param6"  value="STND_YEAR"/>
    <property name="param7"  value="TMS"/>
    <property name="param8"  value="STND_YEAR"/>
    <property name="param9"  value="MBR_CD"/>
    <property name="param10" value="TMS"/>  
    <property name="param11" value="STND_YEAR"/>
    <property name="param12" value="TMS"/>  
    <property name="param13" value="STND_YEAR"/>
    <property name="param14" value="MBR_CD"/>
    <property name="param15" value="TMS"/>
    <property name="param16"  value="STND_YEAR"/>
    <property name="param17"  value="MBR_CD"/>
