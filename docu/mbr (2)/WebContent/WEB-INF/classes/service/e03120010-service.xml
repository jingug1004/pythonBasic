<?xml version="1.0" encoding="EUC-KR"?>
<service name="e03120010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerList|dsOutRacerList_Date|dsOutRacerCheckup" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerlist" value="searchRacerlist" />
    <transition name="searchRacerlist_Date" value="searchRacerlist_Date" />
    <transition name="searchRacerCheckup" value="searchRacerCheckup" />
    <transition name="success" value="end" />
  </activity>
  
  <!-- 출주선수명단  조회 -->
  <activity name="searchRacerlist" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerList" />
    <property name="sqlkey" value="tbec_racer_print_fc001" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="DAY_ORD" />
    <property name="param4" value="RACE_NO" />
  </activity>

  <!-- 출주선수명단-당일공지용  조회 -->
  <activity name="searchRacerlist_Date" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerList_Date" />
    <property name="sqlkey" value="tbec_racer_print_fc002" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="DAY_ORD" />
  </activity>   
  
  <!-- 출주선수명단(신체검사용)  조회 -->
  <activity name="searchRacerCheckup" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerCheckup" />
    <property name="sqlkey" value="tbec_racer_print_fc003" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

