<?xml version="1.0" encoding="EUC-KR"?>
<service name="e06050100" 					initial="SetResultKeys">
  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsInList|dsInSuppList" />
    <transition name="success" 				value="EventRouter" />
  </activity>  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchInList"     	value="searchInList" />
    <transition name="searchInDetail"  		value="searchInDetail" />   
    <transition name="success" 				value="end" />
  </activity>
  <!-- 입고 목록  조회      -->
  <activity name="searchInList" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="end" />
    <property name="resultkey" 		value="dsInList" />
    <property name="sqlkey" 		value="e06050100_s01" />
    <property name="param-count" 	value="2" />
    <property name="param0" 		value="STR_DT"/>
    <property name="param1" 		value="END_DT"/>
    <property name="dao" 			value="boadao" />
  </activity>
  
  <!-- 입고 상세내역  조회      -->
  <activity name="searchInDetail" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="end" />
    <property name="resultkey" 		value="dsInSuppList" />
    <property name="sqlkey" 		value="e06050100_s02" />
    <property name="param-count" 	value="1" />
    <property name="param0" 		value="IN_DT"/>
    <property name="dao" 			value="boadao" />
  </activity>
      <transaction-manager id="tx_boa" commit="true" />
</service>

