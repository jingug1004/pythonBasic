<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02020010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerPerioNo|dsOutRegTerm|dsOutCancelRacer" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerPerioNo"      value="searchRacerPerioNo" />
    <transition name="searchRegTerm"           value="searchRegTerm" />
    <transition name="saveRacerPerioNo"        value="saveRacerPerioNo" />
    <transition name="success" value="end" />
  </activity>

  <!-- 기수 조회 -->
  <activity name="searchRacerPerioNo" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerPerioNo" />
    <property name="sqlkey" value="tbeb_racer_perio_no_reg_term_fb001" />
    <property name="param-count" value="0" />
    <property name="dao" value="boadao" />
  </activity>

  <!-- 기수별등록기간 조회 -->
  <activity name="searchRegTerm" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchCancelRacer" />
    <property name="resultkey" value="dsOutRegTerm" />
    <property name="sqlkey" value="tbeb_racer_perio_no_reg_term_fb002" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="RACER_PERIO_NO" />
  </activity>
  
  <!-- 취소자 조회 -->
  <activity name="searchCancelRacer" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutCancelRacer" />
    <property name="sqlkey" value="tbec_racer_master_fb001" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="RACER_PERIO_NO" />
  </activity>
  
  <!-- 기수별등록기간 저장 -->
  <activity name="saveRacerPerioNo" class="snis.boa.organization.e02020010.activity.SaveRacerPerioNo">
    <transition name="success" value="searchRacerPerioNo" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

