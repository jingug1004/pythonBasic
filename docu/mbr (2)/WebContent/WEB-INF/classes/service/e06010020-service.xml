<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
장비 출주 현황
 -->
<service name="e06010020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutTms|dsOutEquipRunStat" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchMinMaxTms"      value="searchMinMaxTms" />
    <transition name="searchEquipRunStat"    value="searchEquipRunStat" />
    <transition name="success" value="end" />
  </activity>

  <!-- 최대, 최소 회차 조회   -->
  <activity name="searchMinMaxTms" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutTms" />
    <property name="sqlkey" value="tbef_race_tms_ff001" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
  </activity>
  
  <!-- 장비 출주 현황 조회  -->
  <activity name="searchEquipRunStat" class="snis.boa.equipment.e06010020.activity.SearchEquipRunStat">
    <transition name="success" value="end" />
  </activity>

 

  <transaction-manager id="tx_boa" commit="true" />
</service>

