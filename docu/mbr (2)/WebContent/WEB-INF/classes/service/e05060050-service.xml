<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05060050" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutAcctSumry" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchAcctSumry"   value="searchAcctSumry" />
    <transition name="success"          value="end" />
  </activity>
  
 <!-- 계좌 거래내역현황 조회 -->
 <activity name="searchAcctSumry" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutAcctSumry" />
    <property name="sqlkey" value="tbee_money_account_fe004" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="FROM_DATE"/>
    <property name="param1" value="TO_DATE"/>
    <property name="param2" value="ADAY"/>
    <property name="param3" value="ADAY"/>
    <property name="param4" value="ADAY"/>
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>