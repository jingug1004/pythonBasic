<?xml version="1.0" encoding="EUC-KR"?>
<service name="post" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="ds_post" />
    <transition name="success" value="EventRouter" />
  </activity>

  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="search"  value="SearchPost" />
    <transition name="searchDoro"  value="searchDoro" />
    <transition name="success" value="end" />
  </activity>
  
  <activity name="SearchPost" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="ds_post" />
    <property name="sqlkey" value="tbea_post_f0001" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="DONG" />
  </activity>
  
  <activity name="searchDoro" class="snis.boa.common.util.activity.SearchPost">
    <transition name="success" value="end" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

