<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05070010" initial="SetResultKeys">
	<activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
	    <property name="staticResultKeyFile" value="resultkey.properties" />
	    <property name="resultKeyList" value="dsOutDivRate|dsOutOddSeq" />
	    <transition name="success" value="EventRouter" />
	</activity>
  
	<activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
	    <transition name="searchDivRate"   	value="searchDivRate" />
	    <transition name="searchOddSeq"   	value="searchOddSeq" />
	    <transition name="success"         	value="end" />
 	</activity>

 	<!-- 배당률 조회 -->
	<activity name="searchDivRate" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
	    <property name="resultkey" value="dsOutDivRate" />
	    <property name="sqlkey" value="tbes_sdl_pb_fe003" />
	    <property name="param-count" value="8" />
	    <property name="dao" value="boadao" />
	    <property name="param0" value="ODDS_SEQ"/>
	    <property name="param1" value="STND_YEAR"/>
	    <property name="param2" value="MBR_CD"/>
	    <property name="param3" value="TMS"/>
	    <property name="param4" value="DAY_ORD"/>
	    <property name="param5" value="RACE_NO"/>
	    <property name="param6" value="ODDS_SEQ"/>
	    <property name="param7" value="ODDS_SEQ"/>
	</activity>

	<!-- 수신률순서 조회 -->
	<activity name="searchOddSeq" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success" value="end" />
	    <property name="resultkey" value="dsOutOddSeq" />
	    <property name="sqlkey" value="tbes_sdl_pb_fe004" />
	    <property name="param-count" value="5" />
	    <property name="dao" value="boadao" />
	    <property name="param0" value="STND_YEAR"/>
	    <property name="param1" value="MBR_CD"/>
	    <property name="param2" value="TMS"/>
	    <property name="param3" value="DAY_ORD"/>
	    <property name="param4" value="RACE_NO"/>
	</activity>
	
	<transaction-manager id="tx_boa" commit="true" />
</service>