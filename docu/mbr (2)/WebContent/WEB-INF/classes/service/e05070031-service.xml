<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05070031" initial="SetResultKeys">
	<activity name="SetResultKeys"				class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
	    <property name="staticResultKeyFile" 	value="resultkey.properties" />
	    <property name="resultKeyList" 			value="dsOutBrchRtrn|dsOutRankRacer|dsOutSdlRate" />
	    <transition name="success" 				value="EventRouter" />
	</activity>
  
	<activity name="EventRouter"				class="com.posdata.glue.biz.activity.PosDefaultRouter">
	    <transition name="searchBrchRtrn"		value="searchBrchRtrn" />
	    <transition name="searchSdlRate"		value="searchSdlRate" />
	    <transition name="saveBrchRtrn"			value="saveBrchRtrn" />
	    <transition name="success"				value="end" />
 	</activity>

	<!-- 지점고액환급정보  조회 -->
	<activity name="searchBrchRtrn"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success"		value="end" />
		<property name="resultkey"		value="dsOutBrchRtrn" />
		<property name="sqlkey"			value="tbee_brch_rtrn_fe003" />
		<property name="param-count"	value="1" />
	    <property name="dao"			value="boadao" />
		<property name="param0" 		value="SR_NO" /> 
	</activity>

	<!-- 조회 -->
	<activity name="searchSdlRate"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success"		value="end" />
		<property name="resultkey"		value="dsOutSdlRate" />
		<property name="sqlkey"			value="tbee_brch_rtrn_fe004" />
		<property name="param-count"	value="4" />
	    <property name="dao"			value="boadao" />
		<property name="param0" 		value="STND_YEAR" />  
		<property name="param1" 		value="TMS" />  
		<property name="param2" 		value="DAY_ORD" />  
		<property name="param3" 		value="RACE_NO" />  
	</activity>

	<!-- 지점고액환급정보저장 -->
	<activity name="saveBrchRtrn"		class="snis.boa.fairness.e05070030.activity.SaveBrchRtrn">
    	<transition name="success"		value="end" />
	</activity>

	<transaction-manager id="tx_boa" commit="true" />
</service>