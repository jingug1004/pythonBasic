<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05070040" initial="SetResultKeys">
	<activity name="SetResultKeys"				class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
	    <property name="staticResultKeyFile" 	value="resultkey.properties" />
	    <property name="resultKeyList" 			value="dsOutSdl|dsOutSdlDtl|dsOutCsv" />
	    <transition name="success" 				value="EventRouter" />
	</activity>
  
	<activity name="EventRouter"				class="com.posdata.glue.biz.activity.PosDefaultRouter">
	    <transition name="searchSdlAnalysis"	value="searchSdlAnalysis" />
	    <transition name="searchSdlDtl"			value="searchSdlDtl" />
	    <transition name="saveSdlAnalysis"		value="saveSdlAnalysis" />
	    <transition name="success"				value="end" />
 	</activity>

 	<!-- 경주승식별 조회 -->
	<activity name="searchSdlAnalysis"	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success"		value="end" />
	    <property name="resultkey"		value="dsOutSdl" />
	    <property name="sqlkey"			value="tbee_sdl_analysis_fe001" />
		<property name="param-count"	value="3" />
	    <property name="dao"			value="boadao" />
		<property name="param0"			value="STR_DT" />
		<property name="param1"			value="END_DT" />
		<property name="param2"			value="POOL_CD" />
	</activity>

	<!-- 금액별 발매건수 조회 -->
	<activity name="searchSdlDtl"		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
		<transition name="success"		value="end" />
		<property name="resultkey"		value="dsOutSdlDtl" />
		<property name="sqlkey"			value="tbee_sdl_analysis_fe002" />
		<property name="param-count"	value="6" />
	    <property name="dao"			value="boadao" />
		<property name="param0"			value="RACE_DT" />
		<property name="param1"			value="RACE_NO" />
		<property name="param2"			value="POOL_CD" />
		<property name="param3"			value="RACE_DT" />
		<property name="param4"			value="RACE_NO" />
		<property name="param5"			value="POOL_CD" />
	</activity>

	<!-- 경주SDL분석자료 저장-->
	<activity name="saveSdlAnalysis"	class="snis.boa.fairness.e05070040.activity.SaveSdlAnalysis">
    	<transition name="success"		value="searchSdlAnalysis" />
	</activity>

	<transaction-manager id="tx_boa" commit="true" />
</service>