<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02020040" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerGrdStat|dsOutRacerAll" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerGrdStat"      	   value="searchRacerGrdStat" />    
    <transition name="success" value="end" />
  </activity>

  <!-- 선수등급현황 조회 -->
  <activity name="searchRacerGrdStat" class="snis.boa.organization.e02020040.activity.SearchRacerGrdStat">
    <transition name="success" 		value="end" />
	<property name="resultkey" 		value="dsOutRacerGrdStat" />
  </activity>

  <!-- 기수별 등급 현황조회 -->
  <activity name="searchRacer" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsOutRacerAll" />
    <property name="sqlkey" 				value="tbeb_racer_grd_fb002" />
    <property name="dao" 					value="boadao" />
    <property name="param-count" 			value="1"    />
    <property name="param0"  				value="NM_KOR"  />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

