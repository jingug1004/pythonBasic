<?xml version="1.0" encoding="EUC-KR"?>
<service name="e03030010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerInvw|dsOutRacerInvw_rpt" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerInvw" value="searchRacerInvw" />
    <transition name="SaveRacerInvw" value="SaveRacerInvw" />
    <transition name="searchRacerInvw_rpt" value="searchRacerInvw_rpt" />    
    <transition name="success" value="end" />
  </activity>
  
  <!-- 출주선수 면담정보  조회 -->
  <activity name="searchRacerInvw" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerInvw" />
    <property name="sqlkey" value="tbec_racer_invw_fc001" />
    <property name="param-count" value="8" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="STND_YEAR" />
    <property name="param4" value="MBR_CD" />
    <property name="param5" value="TMS" />
    <property name="param6" value="NM_KOR" />
    <property name="param7" value="RACER_NO" />
  </activity>

  <!-- 출주선수 면담정보 저장 -->
  <activity name="SaveRacerInvw" class="snis.boa.racer.e03030010.activity.SaveRacerInvw">
    <transition name="success" value="searchRacerInvw" />
  </activity>

  <!-- 출주선수 면담 레포트정보  조회 -->
  <activity name="searchRacerInvw_rpt" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerInvw_rpt" />
    <property name="sqlkey" value="tbec_racer_invw_fc002" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="NM_KOR" />
    <property name="param4" value="RACER_NO" />
  </activity>
    
  <transaction-manager id="tx_boa" commit="true" />
</service>

