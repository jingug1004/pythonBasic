<?xml version="1.0" encoding="EUC-KR"?>
<service name="e06010140" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutPropellaList|dsOutMaintenance|dsOutChkProDup|dsOutDisusePropeller|dsOutChkProvide|dsOutMaintenance" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPropella"      	value="searchPropella" />
    <transition name="savePropellerMgr"     value="savePropellerMgr" />
    <transition name="checkPropellaNo"      value="checkPropellaNo" />
    <transition name="searchDisusePropella" value="searchDisusePropella" />
    <transition name="chkProvidePro"      	value="chkProvidePro" />
    <transition name="searchMaintenance"    value="searchMaintenance" />
    <transition name="success" value="end" />
  </activity>

  <!-- 프로펠러 목록 조회. -->
  <activity name="searchPropella" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPropellaList" />
    <property name="sqlkey" value="tbef_propeller_mgr_new_ff001" />
    <property name="param-count" value="21" />
    <property name="param0"  value="MBR_CD" />
	<property name="param1"  value="SEARCH_GUBUN" />
	<property name="param2"  value="SEARCH_SDT" />
	<property name="param3"  value="SEARCH_EDT" />
	<property name="param4"  value="SEARCH_GUBUN" />
	<property name="param5"  value="SEARCH_SDT" />
	<property name="param6"  value="SEARCH_EDT" />
	<property name="param7"  value="SEARCH_GUBUN" />
	<property name="param8"  value="SEARCH_MOT_NO" />
	<property name="param9"  value="SEARCH_MOT_NO" />
	<property name="param10"  value="SEARCH_MOT_NO" />
	<property name="param11"  value="REG_TYPE_CD" />
	<property name="param12"  value="REG_TYPE_CD" />
	<property name="param13"  value="REG_TYPE_CD" />
	<property name="param14"  value="DISUSE_TYPE_CD" />
	<property name="param15"  value="DISUSE_TYPE_CD" />
	<property name="param16"  value="DISUSE_TYPE_CD" />
	<property name="param17"  value="CHK_NOT_DISUSE" />
	<property name="param18"  value="CHK_NOT_DISUSE" />
	<property name="param19"  value="CHK_NOT_REG" />
	<property name="param20"  value="CHK_NOT_REG" />
    <property name="dao" value="boadao" />
  </activity>
  
    <!-- 프로펠러 저장. -->
  <activity name="savePropellerMgr" class="snis.boa.equipment.e06010140.activity.SavePropellerMgrNew">
    <transition name="success" value="searchPropella" />
  </activity>
  
  <!-- 프로펠러 번호  중복체크 . -->
  <activity name="checkPropellaNo" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutChkProDup" />
    <property name="sqlkey" value="tbef_propeller_mgr_new_ff002" />
    <property name="param-count" value="2" />
    <property name="param0"  value="PROPELLER_NO" />
    <property name="param1"  value="PROPELLER_SEQ" />
    <property name="dao" value="boadao" />
  </activity>
  
    <!-- 반납 프로펠러. -->
  <activity name="searchDisusePropella" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchMaintenance" />
    <property name="resultkey" value="dsOutDisusePropeller" />
    <property name="sqlkey" value="tbef_propeller_mgr_new_ff003" />
    <property name="param-count" value="1" />
    <property name="param0"  value="MOT_NO" />
    <property name="dao" value="boadao" />
  </activity>
  
  <!-- 정기점검내역. -->
  <activity name="searchMaintenance" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutMaintenance" />
    <property name="sqlkey" value="tbef_propeller_maintenance_new_ff001" />
    <property name="param-count" value="1" />
    <property name="param0"  value="PROPELLER_SEQ" />
    <property name="dao" value="boadao" />
  </activity>
  
    <!-- 지급 프로펠러 존재 확인.. -->
  <activity name="chkProvidePro" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutChkProvide" />
    <property name="sqlkey" value="tbef_propeller_mgr_new_ff004" />
    <property name="param-count" value="2" />
    <property name="param0"  value="PROPELLER_NO" />
    <property name="param1"  value="MOT_NO" />
    <property name="dao" value="boadao" />
  </activity>
  

  <transaction-manager id="tx_boa" commit="true" />
</service>

