<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
확정검사
 -->
<service name="e06010050" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutEquipInsp|dsOutEquipInspDetail" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEquipInsp"      value="searchEquipInsp" />
    <transition name="saveEquipInsp"    value="saveEquipInsp" />
    <transition name="success" value="end" />
  </activity>

  <!-- 확정검사 조회   -->
  <activity name="searchEquipInsp" class="snis.boa.equipment.e06010050.activity.SearchEquipInsp">
    <transition name="success" value="end" />
  </activity>
 

  <!-- 확정검사  저장 -->
  <activity name="saveEquipInsp" class="snis.boa.equipment.e06010050.activity.SaveEquipInsp">
    <transition name="success" value="searchEquipInsp" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

