<?xml version="1.0" encoding="EUC-KR"?>
<service name="e09020001" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacer|dsOutRacerViol|dsRaceViolAnalysis" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceRacer"	value="searchRaceRacer" />
    <transition name="searchRacerViol"	value="searchRacerViol" />
    <transition name="searchRacerViolAnalysis"	value="searchRacerViolAnalysis" />
    <transition name="saveAcctEduRacer"	value="saveAcctEduRacer" />
    <transition name="saveRacerViol"	value="saveRacerViol" />
    <transition name="saveRaceViolAnalysis"	value="saveRaceViolAnalysis" />
    <transition name="success" 			value="end" />
  </activity>

  <!-- searchRaceRacer -->
  <activity name="searchRaceRacer" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchRacerViolAnalysis" />
    <property name="resultkey" 				value="dsOutRacer" />
    <property name="sqlkey" 				value="tbed_race_voil_edu_s02" />    
    <property name="dao" 					value="boadao" />
    <property name="param-count" 			value="17" />
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="TMS" />
    <property name="param2" 				value="MBR_CD" />
    
    <property name="param3" 				value="STND_YEAR" />
    <property name="param4" 				value="MBR_CD" />
    <property name="param5" 				value="TMS" />
    <property name="param6" 				value="DAY_ORD" />
    
    <property name="param7" 				value="STND_YEAR" />
    <property name="param8" 				value="MBR_CD" />
    <property name="param9" 				value="TMS" />
    <property name="param10" 				value="DAY_ORD" />
    <property name="param11" 				value="RACE_NO" />
    
    <property name="param12" 				value="STND_YEAR" />
    <property name="param13" 				value="MBR_CD" />
    <property name="param14" 				value="TMS" />
    <property name="param15" 				value="DAY_ORD" />
    <property name="param16" 				value="RACE_NO" />
  </activity>  
  
  <!-- searchRacerViol -->
  <activity name="searchRacerViol" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsOutRacerViol" />
    <property name="sqlkey" 				value="tbed_race_voil_edu_s03" />    
    <property name="dao" 					value="boadao" />
    <property name="param-count" 			value="6" />
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="MBR_CD" />
    <property name="param2" 				value="TMS" />
    <property name="param3" 				value="DAY_ORD" />
    <property name="param4" 				value="RACE_NO" />
    <property name="param5" 				value="RACER_NO" />
  </activity>
  
  <!-- searchRacerViol -->
  <activity name="searchRacerViolAnalysis" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRaceViolAnalysis" />
    <property name="sqlkey" 				value="tbed_race_voil_analysis_s01" />    
    <property name="dao" 					value="boadao" />
    <property name="param-count" 			value="5" />
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="MBR_CD" />
    <property name="param2" 				value="TMS" />
    <property name="param3" 				value="DAY_ORD" />
    <property name="param4" 				value="RACE_NO" />
  </activity>
  
  <!-- saveAcctEduRacer -->
  <activity name="saveAcctEduRacer" class="snis.boa.referere.e09010001.activity.SaveAcctEduRacer">
    <transition name="success" 				value="saveRacerViol" />    
  </activity>  
  
  <!-- saveRacerViol -->
  <activity name="saveRacerViol" class="snis.boa.referere.e09020001.activity.SaveRacerViol">
    <transition name="success" 	 value="saveRaceViolAnalysis" />    
  </activity>  
  
    <!-- saveRaceViolAnalysis -->
  <activity name="saveRaceViolAnalysis" class="snis.boa.referere.e09020001.activity.SaveRaceViolAnalysis">
    <transition name="success" 	 value="end" />    
  </activity>  
  <transaction-manager id="tx_boa" commit="true" />
</service>