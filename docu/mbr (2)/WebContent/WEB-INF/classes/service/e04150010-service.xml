<?xml version="1.0" encoding="EUC-KR"?>
<service name="e04150010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerBadMoter|dsOutFL|dsOutRacerFL|dsOutFLYear|dsOutFLRacer|dsOutFLRacerDetail" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerBadMoter"	value="searchRacerBadMoter" />
    <transition name="searchFL"				value="searchFL" />
    <transition name="searchFLYear"			value="searchFLYear" />
    <transition name="searchFLRacer"		value="searchFLRacer" />
    <transition name="searchFLRacerDetail"	value="searchFLRacerDetail" />
    <transition name="searchRacerFL"		value="searchRacerFL" />
    <transition name="success"				value="end" />
  </activity>
  
  <!-- 선수와 모터상관 관계 -->
  <activity name="searchRacerBadMoter" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success"		value="end" />
    <property name="resultkey"		value="dsOutRacerBadMoter" />
    <property name="sqlkey"			value="tbed_refe_etc_fd002" />
    <property name="param-count"	value="6" />
    <property name="dao"			value="boadao" />
    <property name="param0"			value="STND_YEAR"/>
    <property name="param1"			value="STND_YEAR"/>
    <property name="param2"			value="MBR_CD"/>
    <property name="param3"			value="BAD_RATE"/>
    <property name="param4"			value="STND_YEAR"/>
    <property name="param5"			value="MBR_CD"/>
  </activity>
  
 <!-- 출발위반현황 조회 -->
 <activity name="searchFL" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutFL" />
    <property name="sqlkey" value="tbed_print_fd012" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="TMS"/>
   </activity>
  
 <!-- 선수별 출발위반 전체 조회 -->
 <activity name="searchRacerFL" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerFL" />
    <property name="sqlkey" value="tbed_refe_etc_fd001" />
    <property name="param-count" value="1" />
    <property name="param0" value="RACER_NO"/>
    <property name="dao" value="boadao" />
   </activity>
  
 <!-- 연간별 출발위반 통계 -->
 <activity name="searchFLYear" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutFLYear" />
    <property name="sqlkey" value="tbed_refe_etc_fd003" />
    <property name="param-count" value="0" />
    <property name="dao" value="boadao" />
   </activity>
  
 <!-- 선수별 출발위반 통계 -->
 <activity name="searchFLRacer" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchFLRacerDetail" />
    <property name="resultkey" value="dsOutFLRacer" />
    <property name="sqlkey" value="tbed_refe_etc_fd004" />
    <property name="param-count" value="1" />
    <property name="param0" value="RACER_NM"/>
    <property name="dao" value="boadao" />
   </activity>
  
 <!-- 선수별 출발위반 세부내역 조회 -->
 <activity name="searchFLRacerDetail" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutFLRacerDetail" />
    <property name="sqlkey" value="tbed_refe_etc_fd005" />
    <property name="param-count" value="1" />
    <property name="param0" value="RACER_NM"/>
    <property name="dao" value="boadao" />
   </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>