<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02030035" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutArrangeNoti|dsOutArrangeNotiList" />
    <transition name="success" value="EventRouter" />
  </activity>

  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchArrangeNoti"       	value="searchArrangeNoti" />
    <transition name="searchArrangeNotiList"    value="searchArrangeNotiList" />
    <transition name="searchArrangeNotiListAdd" value="searchArrangeNotiListAdd" />
    <transition name="saveArrangeNoti"          value="saveArrangeNoti" />    
    <transition name="saveArrangeNotiList"      value="saveArrangeNotiList" />    
    <transition name="success" value="end" />
  </activity>

  <!-- 주선선수통보 조회 -->
  <activity name="searchArrangeNoti" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutArrangeNoti" />
    <property name="sqlkey" value="tbeb_arrange_noti_fb001" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="STND_YEAR" />
    <property name="param3" value="MBR_CD" />
  </activity>
  
  <!-- 주선선수통보 조회(SEQ == 1) -->
  <activity name="searchArrangeNotiList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutArrangeNotiList" />
    <property name="sqlkey" value="tbeb_arrange_noti_fb002" />
    <property name="param-count" value="8" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="SEQ" />
    <property name="param4" value="STND_YEAR" />
    <property name="param5" value="MBR_CD" />
    <property name="param6" value="TMS" />
    <property name="param7" value="SEQ" />
  </activity>
  
  <!-- 주선선수통보 조회(SEQ > 1) -->
  <activity name="searchArrangeNotiListAdd" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutArrangeNotiList" />
    <property name="sqlkey" value="tbeb_arrange_noti_fb003" />
    <property name="param-count" value="8" />
    <property name="dao" value="boadao" />
    <property name="param0" value="WORKDAY" />
    <property name="param1" value="STND_YEAR" />
    <property name="param2" value="TMS" />
    <property name="param3" value="SEQ" />
    <property name="param4" value="STND_YEAR" />
    <property name="param5" value="MBR_CD" />
    <property name="param6" value="TMS" />
    <property name="param7" value="SEQ" />
  </activity>
  
  <!-- 주선선수통보 저장 -->
  <activity name="saveArrangeNoti" class="snis.boa.organization.e02030035.activity.SaveArrangeNoti">
    <transition name="success" value="searchArrangeNoti" />
  </activity>

  <!-- 주선선수통보 담당자적요 저장 -->
  <activity name="saveArrangeNotiList" class="snis.boa.organization.e02030035.activity.SaveArrangeNotiList">
    <transition name="success" value="searchArrangeNoti" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

