<?xml version="1.0" encoding="EUC-KR"?>
<service name="e06010150" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutMaintenance|dsOutMotPropeller" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchMaintenance"    value="searchMaintenance" />
    <transition name="searchMaintenanceAll" value="searchMaintenanceAll" />
    <transition name="saveMaintenance"    	value="saveMaintenance" />
    <transition name="searchRacerPropella"  value="searchRacerPropella" />
    <transition name="success" value="end" />
  </activity>

  
  <!-- 정기점검내역. -->
  <activity name="searchMaintenance" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchRacerPropella" />
    <property name="resultkey" value="dsOutMaintenance" />
    <property name="sqlkey" value="tbef_propeller_maintenance_new_ff002" />
    <property name="param-count" value="9" />
    <property name="param0"  value="MBR_CD" />
    <property name="param1"  value="SEARCH_SDT" />
    <property name="param2"  value="SEARCH_EDT" />
    <property name="param3"  value="NM_KOR" />
    <property name="param4"  value="NM_KOR" />
    <property name="param5"  value="NM_KOR" />
    <property name="param6"  value="PROPELLER_NO" />
    <property name="param7"  value="PROPELLER_NO" />
    <property name="param8"  value="PROPELLER_NO" />
    <property name="dao" value="boadao" />
  </activity>
  
  <!-- 정기점검대상 조회. -->
  <activity name="searchMaintenanceAll" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchRacerPropella" />
    <property name="resultkey" value="dsOutMaintenance" />
    <property name="sqlkey" value="tbef_propeller_maintenance_new_ff003" />
    <property name="param-count" value="5" />
    <property name="param0"  value="SEARCH_SDT" />
    <property name="param1"  value="MBR_CD" />
    <property name="param2"  value="MBR_CD" />
    <property name="param3"  value="SEARCH_SDT" />
    <property name="param4"  value="SEARCH_SDT" />
    <property name="dao" value="boadao" />
  </activity>

<!-- 선수 보유 프로펠러목록 조회. -->
  <activity name="searchRacerPropella" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutMotPropeller" />
    <property name="sqlkey" value="tbef_propeller_mgr_new_ff005" />
    <property name="param-count" value="0" />
    <property name="dao" value="boadao" />
  </activity>
  
      <!-- 프로펠러 저장. -->
  <activity name="saveMaintenance" class="snis.boa.equipment.e06010150.activity.SavePropellerMaintenanceNew">
    <transition name="success" value="searchMaintenance" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

