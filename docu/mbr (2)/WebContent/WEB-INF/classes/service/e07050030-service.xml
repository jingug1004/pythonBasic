<?xml version="1.0" encoding="EUC-KR"?>

<service name="e07050030" initial="SetResultKeys">

  <activity name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" 	value="resultkey.properties" />
    <property name="resultKeyList" 			value="dsList|dsRacerList" />
    <transition name="success" 				value="EventRouter" />
  </activity>

  <activity name="EventRouter" 					class="com.posdata.glue.biz.activity.PosDefaultRouter">
  	<transition name="searchList"  				value="searchList" />
  	<transition name="searchInsuExist"  		value="searchInsuExist" />
  	<transition name="saveInsHstList"  			value="saveInsHstList" />
    <transition name="success" 					value="end" />
  </activity>         
  
  
  <!-- 선수연금보험 재가입내역 목록조회 -->
  <activity name="searchList" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsList" />
    <property name="sqlkey"					value="e07050030_s01" />
    <property name="param-count" 			value="0" />  
    <property name="dao" 					value="boadao" />
  </activity>    
  
  
  <!-- 선수연금보험 재가입내역 목록조회 -->
  <activity name="searchInsuExist" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRacerList" />
    <property name="sqlkey"					value="e07050010_s02" />
    <property name="param-count" 			value="1" />  
	<property name="param0" 				value="RACER_NO"/>
    <property name="dao" 					value="boadao" />
  </activity>    
  
  <activity name="saveInsHstList"           class="snis.boa.financial.e07050030.activity.SaveInsHstList">
	<transition name="success"              value="end" />
	<transition name="bizException"         value="bizError"/>
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />

</service>



