    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerPrz" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="SearchRacerPrz" value="SearchRacerPrz" /> 
    <transition name="success" value="end" />
  </activity>

  <!-- 지정연습편성  정보  조회 -->
  <activity name="SearchRacerPrz" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerPrz" />
    <property name="sqlkey" value="tbeg_racer_award_his_fc001" />
    <property name="param-count" value="7" />
    <property name="dao" value="boadao" />
    <property name="param0" value="GBN" />
    <property name="param1" value="GBN" />
    <property name="param2" value="GBN" />
    <property name="param3" value="STND_YEAR" />
    <property name="param4" value="RMK" />
    <property name="param5" value="NM_KOR" />
    <property name="param6" value="RACER_NO" />
  </activity>

  <!-- 입/포상 선수 정보  조회 -->
  <activity name="SearchRacerPrz_" class="snis.boa.racer.e03060010.activity.SearchRacerPrz">
    <transition name="success" value="end" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

