<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05100040" initial="SetResultKeys">
    
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutDopingRslt|dsOutDopingYn" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchDopingRslt"	value="searchDopingRslt" />
    <transition name="searchDopingYn"	value="searchDopingYn" />
    <transition name="saveDopingRslt"	value="saveDopingRslt" />
    <transition name="success"			value="end" />
  </activity>
 
  <!-- 약물검사결과 조회 -->
 <activity name="searchDopingRslt" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutDopingRslt" />
    <property name="sqlkey" value="tbee_risk_mng_fd401" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="TMS"/>
  </activity>
 
  <!-- 약물검사여부 조회 -->
 <activity name="searchDopingYn" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutDopingYn" />
    <property name="sqlkey" value="tbee_risk_mng_fd402" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="TMS"/>
  </activity>
 
  <!-- 약물검사결과  등록 -->
  <activity name="saveDopingRslt" class="snis.boa.fairness.e05100040.activity.SaveDopingRslt">
    <transition name="success" value="end" />
  </activity>
  <transaction-manager id="tx_boa" commit="true" />
</service>