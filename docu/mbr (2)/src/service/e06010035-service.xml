<?xml version="1.0" encoding="EUC-KR"?>
<!-- 회차별 프로펠러 등록 관리  -->
<service name="e06010035" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutPropeller" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchPropeller"      value="searchPropeller" />
    <transition name="savePropeller"    	value="savePropeller" />
    <transition name="success"				value="end" />
  </activity>

  <!-- 회차별 프로펠러 사용 조회  -->
  <activity name="searchPropeller" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPropeller" />
    <property name="sqlkey" value="tbef_propeller_ff001" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="TMS"/>
  </activity>

  <!-- 회차별 프로펠러 사용 저장-->
  <activity name="savePropeller" class="snis.boa.equipment.e06010035.activity.SavePropeller">
    <transition name="success" value="searchPropeller" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

