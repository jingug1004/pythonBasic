<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05110010" initial="SetResultKeys">
    
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsSaleRslt|dsRaceRslt|dsSpecialDocu" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceSale"		value="searchRaceSale" />
    <transition name="searchRaceResult"		value="searchRaceResult" />
    <transition name="saveCustomerGuess"	value="saveCustomerGuess" />
    <transition name="searchSpecialDocu"	value="searchSpecialDocu" />
    <transition name="success"			value="end" />
  </activity>
 
 	<!-- 고객 예상순위 저장 -->
  <activity name="saveCustomerGuess"		class="snis.boa.fairness.e05110010.activity.SaveCustomerGuess">
   	<transition name="success"		    value="end" />
  </activity>
 
  <!-- 공정관리-경주상황보고-매출현황 조회 -->
  <activity name="searchRaceSale" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchRaceResult" />
    <property name="resultkey" value="dsSaleRslt" />
    <property name="sqlkey" value="tbes_sale_search_se001" />
    <property name="param-count" value="27" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/> 
    <property name="param1" value="MEET_CD"/> 
    <property name="param2" value="TMS"/> 
    <property name="param3" value="DAY_ORD"/> 
    <property name="param4" value="STND_YEAR"/> 
    <property name="param5" value="MEET_CD"/> 
    <property name="param6" value="TMS"/>
    <property name="param7" value="STND_YEAR"/> 
    <property name="param8" value="MEET_CD"/> 
    <property name="param9" value="TMS"/> 
    <property name="param10" value="STND_YEAR"/> 
    <property name="param11" value="MEET_CD"/> 
    <property name="param12" value="TMS"/> 
    <property name="param13" value="STND_YEAR"/> 
    <property name="param14" value="MEET_CD"/> 
    <property name="param15" value="TMS"/> 
    <property name="param16" value="DAY_ORD"/>
    <property name="param17" value="STND_YEAR"/> 
    <property name="param18" value="MEET_CD"/> 
    <property name="param19" value="TMS"/> 
    <property name="param20" value="STND_YEAR"/> 
    <property name="param21" value="MEET_CD"/> 
    <property name="param22" value="TMS"/> 
    <property name="param23" value="DAY_ORD"/>
    <property name="param24" value="STND_YEAR"/> 
    <property name="param25" value="MEET_CD"/> 
    <property name="param26" value="TMS"/> 
  </activity>
  
    <!-- 공정관리-경주상황보고-경주결과 조회 -->
  <activity name="searchRaceResult" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchSpecialDocu" />
    <property name="resultkey" value="dsRaceRslt" />
    <property name="sqlkey" value="tbee_raceresult_search_se001" />
    <property name="param-count" value="12" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/> 
    <property name="param1" value="MBR_CD"/> 
    <property name="param2" value="TMS"/> 
    <property name="param3" value="DAY_ORD"/> 
    <property name="param4" value="STND_YEAR"/> 
    <property name="param5" value="MBR_CD"/> 
    <property name="param6" value="TMS"/>
    <property name="param7" value="DAY_ORD"/>
    <property name="param8" value="STND_YEAR"/> 
    <property name="param9" value="MBR_CD"/> 
    <property name="param10" value="TMS"/> 
    <property name="param11" value="DAY_ORD"/> 
  </activity>

	<!-- 공정관리-경주상황보고-특이사항 조회 -->
  <activity name="searchSpecialDocu" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsSpecialDocu" />
    <property name="sqlkey" value="tbee_special_docu_se001" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/> 
    <property name="param1" value="MBR_CD"/> 
    <property name="param2" value="TMS"/> 
    <property name="param3" value="DAY_ORD"/> 
  </activity>  
  
  <transaction-manager id="tx_boa" commit="true" />
</service>