<?xml version="1.0" encoding="EUC-KR"?>
<service name="e03130020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRaceResult|dsOutVoilCnt|dsOutRacerEduData" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceResult" value="searchRaceResult" />
    <transition name="searchVoilCnt" value="searchVoilCnt" />
    <transition name="searchRacerEduData" value="searchRacerEduData" />
    <transition name="saveRacerEduData" value="saveRacerEduData"/>
    <transition name="success" value="end" />
  </activity>
  
  <!-- 경주성적(경주별) 조회 -->
  <activity name="searchRaceResult" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchVoilCnt" />
    <property name="resultkey" value="dsOutRaceResult" />
    <property name="sqlkey" value="tbec_racer_edu_data_fc006" />
    <property name="param-count" value="19" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="DAY_ORD" />
    <property name="param4" value="RACE_NO" />
    <property name="param5" value="STND_YEAR" />
    <property name="param6" value="MBR_CD" />
    <property name="param7" value="TMS" />
    <property name="param8" value="DAY_ORD" />
    <property name="param9" value="RACE_NO" />
    <property name="param10" value="STND_YEAR" />
    <property name="param11" value="MBR_CD" />
    <property name="param12" value="TMS" />
    <property name="param13" value="DAY_ORD" />
    <property name="param14" value="STND_YEAR" />
    <property name="param15" value="MBR_CD" />
    <property name="param16" value="TMS" />
    <property name="param17" value="DAY_ORD" />
    <property name="param18" value="RACE_NO" />
  </activity>

  <!-- 사고점(경주별) 조회 -->
  <activity name="searchVoilCnt" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchRacerEduData" />
    <property name="resultkey" value="dsOutVoilCnt" />
    <property name="sqlkey" value="tbec_racer_edu_data_fc007" />
    <property name="param-count" value="8" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="STND_YEAR" />
    <property name="param4" value="MBR_CD" />
    <property name="param5" value="TMS" />
    <property name="param6" value="DAY_ORD" />
    <property name="param7" value="RACE_NO" />
  </activity>

  <!-- 선수교육자료 특이사항 조회 -->
  <activity name="searchRacerEduData" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerEduData" />
    <property name="sqlkey" value="tbec_racer_edu_data_fc008" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="DAY_ORD" />
    <property name="param4" value="RACE_NO" />
  </activity>

  <!-- 선수교육자료 특이사항 저장 -->
  <activity name="saveRacerEduData" class="snis.boa.racer.e03130020.activity.SaveRacerEduData">
    <transition name="success" value="end" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>
