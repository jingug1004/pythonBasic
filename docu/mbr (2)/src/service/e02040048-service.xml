<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02040048" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRaceGap" />
    <transition name="success" value="EventRouter" />
  </activity>

  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceGap"  		value="searchRaceGap" />
    <transition name="saveRaceGap"  		value="saveRaceGap" />
    <transition name="success" value="end" />
  </activity>

  <!-- 경주간격 조회 -->
  <activity name="searchRaceGap" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRaceGap" />
    <property name="sqlkey" value="tbeb_race_gap_fb001" />
    <property name="param-count" value="0" />
    <property name="dao" value="boadao" />
  </activity>
  
  <!-- 경주간격 저장 -->
  <activity name="saveRaceGap" class="snis.boa.organization.e02040048.activity.SaveRaceGap">
    <transition name="success" value="end" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

