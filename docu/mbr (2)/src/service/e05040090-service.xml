<?xml version="1.0" encoding="EUC-KR"?>
<service name="e05040090" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutIntroRunTime|dsOutIntroRunTime_Detail" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchIntroRunTime"   		value="searchIntroRunTime" />
    <transition name="searchIntroRunTime_Detail"    value="searchIntroRunTime_Detail" />
    <transition name="success"            			value="end" />
  </activity>
  
  <!-- 모터별 소개항주 현황 조회 -->
  <activity name="searchIntroRunTime" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutIntroRunTime" />
    <property name="sqlkey" value="tbee_intro_run_tm_fe001" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="MBR_CD"/>
    <property name="param1" value="FROM_YEAR"/>
    <property name="param2" value="FROM_TMS"/>
    <property name="param3" value="TO_YEAR"/>
    <property name="param4" value="TO_TMS"/>
  </activity>
  
  <!-- 모터별 소개항주 현황 상세조회 -->
  <activity name="searchIntroRunTime_Detail" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutIntroRunTime_Detail" />
    <property name="sqlkey" value="tbee_intro_run_tm_fe002" />
    <property name="param-count" value="6" />
    <property name="dao" value="boadao" />
    <property name="param0" value="MBR_CD"/>
    <property name="param1" value="FROM_YEAR"/>
    <property name="param2" value="FROM_TMS"/>
    <property name="param3" value="TO_YEAR"/>
    <property name="param4" value="TO_TMS"/>
    <property name="param5" value="MOT_NO"/>
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>