<?xml version="1.0" encoding="EUC-KR"?>
<service name="e08010002" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsGoalList|dsRateList|dsBasList|dsSeq" />
    <transition name="success" value="EventRouter" />
  </activity>
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchGoal"  			value="searchGoal" />
    <transition name="searchRate"   		value="searchRate" />  
    <transition name="searchBas"   			value="searchBas" />  
    <transition name="searchRateSeq"   		value="searchRateSeq" /> 
    <transition name="searchRateSeqCombo"   value="searchRateSeqCombo" />
    <transition name="saveTqcGoalBas"   	value="saveTqcGoalBas" /> 
    <transition name="success" 				value="end" />
  </activity>
  <!-- 목표  조회 -->
  <activity name="searchGoal" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchRate" />
    <property name="resultkey" 				value="dsGoalList" />
    <property name="sqlkey" 				value="tbtq_goal_fa001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="STND_YEAR" />
    <property name="dao" 					value="boadao" />    
  </activity> 
    <!-- 비중  조회 -->
  <activity name="searchRate" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchBas" />
    <property name="resultkey" 				value="dsRateList" />
    <property name="sqlkey" 				value="tbtq_rate_fa001" />
    <property name="param-count" 			value="1" />
     <property name="param0" 				value="STND_YEAR" />
    <property name="dao" 					value="boadao" />    
  </activity> 
    <!-- 평가기준  조회 -->
  <activity name="searchBas" 				class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchRateSeq" />
    <property name="resultkey" 				value="dsBasList" />
    <property name="sqlkey" 				value="tbtq_bas_fa001" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="STND_YEAR" />
    <property name="dao" 					value="boadao" />    
  </activity>
  <!-- 비중seq combo -->
  <activity name="searchRateSeq" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsSeq" />
    <property name="sqlkey" 				value="tbtq_rate_fa002" />
    <property name="param-count" 			value="0" />
    <property name="dao" 					value="boadao" />    
  </activity>
  <!-- 비중  combo조회 -->
  <activity name="searchRateSeqCombo" 		class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsRateList" />
    <property name="sqlkey" 				value="tbtq_rate_fa003" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="SEQ" />
    <property name="dao" 					value="boadao" />    
  </activity>  
  <!-- 저장 -->
  <activity name="saveTqcGoalBas" 			class="snis.boa.tqc.e08010002.activity.SaveTqcGoalBas">
    <transition name="success" 	value="end" />
  </activity> 
   
  <transaction-manager id="tx_boa" commit="true" />
</service>