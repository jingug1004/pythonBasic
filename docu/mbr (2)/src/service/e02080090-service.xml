<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02080090" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutFLList" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
	<transition name="searchFL"  			value="searchFL" /> 
	<transition name="saveFL"  			    value="saveFL" /> 
    <transition name="success" value="end" />
  </activity>

  <!-- 출발위반현황 리스트 -->
  <activity name="searchFL" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutFLList" />
    <property name="sqlkey" value="tbeb_organ_fb017" />
    <property name="param-count" value="4" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="STR_DT" />
    <property name="param3" value="END_DT" />
    <property name="dao" value="boadao" />
  </activity>

  <!-- 출발위반현황 제재 저장 -->
  <activity name="saveFL" class="snis.boa.organization.e02080090.activity.SaveFL">
    <transition name="success" value="end" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

