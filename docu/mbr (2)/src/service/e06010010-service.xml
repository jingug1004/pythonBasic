<?xml version="1.0" encoding="EUC-KR"?>
<!-- 장비 관리  -->
<service name="e06010010" initial="SetResultKeys">  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">      <property name="staticResultKeyFile" value="resultkey.properties" />    <property name="resultKeyList" value="dsOutEquip|dsOutEquipLast" />    <transition name="success" value="EventRouter" />  </activity>
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">    <transition name="searchEquip"      value="searchEquip" />    <transition name="searchEquipLast"  value="searchEquipLast" />    <transition name="saveEquip"    value="saveEquip" />    <transition name="success" value="end" />  </activity>
  <!-- 장비 조회  -->  <activity name="searchEquip" class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success" value="searchEquipLast" />    <property name="resultkey" value="dsOutEquip" />    <property name="sqlkey" value="tbef_equip_ff001" />    <property name="param-count" value="6" />    <property name="dao" value="boadao" />    <property name="param0" value="STND_YEAR"/>    <property name="param1" value="STND_YEAR"/>    <property name="param2" value="MBR_CD"/>    <property name="param3" value="EQUIP_TPE_CD"/>    <property name="param4" value="USE_YN"/>    <property name="param5" value="USE_YN"/>  </activity>    <!-- 장비 조회  -->  <activity name="searchEquipLast" class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success" value="end" />    <property name="resultkey" value="dsOutEquipLast" />    <property name="sqlkey" value="tbef_equip_ff002" />    <property name="param-count" value="6" />    <property name="dao" value="boadao" />    <property name="param0" value="STND_YEAR"/>    <property name="param1" value="STND_YEAR"/>    <property name="param2" value="MBR_CD"/>    <property name="param3" value="EQUIP_TPE_CD"/>    <property name="param4" value="USE_YN"/>    <property name="param5" value="USE_YN"/>  </activity>
  <!-- 장비 저장-->  <activity name="saveEquip" class="snis.boa.equipment.e06010010.activity.SaveEquip">    <transition name="success" value="searchEquip" />  </activity>
  <transaction-manager id="tx_boa" commit="true" /></service>

