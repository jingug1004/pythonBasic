<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02080070" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutEntryCourseList|dsOutStrategyList|dsOutRaceRegNoList|dsOutMaxRaceRegNoCnt" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
	<transition name="searchEntryCourse"  			value="searchEntryCourse" /> 
	<transition name="searchEntryCourseStrategy"  	value="searchEntryCourseStrategy" /> 
	<transition name="searchRaceRegNo"  			value="searchRaceRegNo" /> 
	<transition name="searchMaxRaceRegNoCnt"  		value="searchMaxRaceRegNoCnt" /> 
    <transition name="success" value="end" />
  </activity>

  <!-- 코스별입상률 -->
  <activity name="searchEntryCourse" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchEntryCourseStrategy" />
    <property name="resultkey" value="dsOutEntryCourseList" />
    <property name="sqlkey" value="tbeb_organ_fb015" />
    <property name="param-count" value="8" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="ST_MTHD_CD" />
    <property name="param3" value="ST_MTHD_CD" />
    <property name="param4" value="STR_DT" />
    <property name="param5" value="END_DT" />
    <property name="param6" value="ST_MTHD_CD" />
    <property name="param7" value="ST_MTHD_CD" />
    <property name="dao" value="boadao" />
  </activity>

  <!-- 코스별전법 -->
  <activity name="searchEntryCourseStrategy" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchRaceRegNo" />
    <property name="resultkey" value="dsOutStrategyList" />
    <property name="sqlkey" value="tbeb_organ_fb015_" />
    <property name="param-count" value="4" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="ST_MTHD_CD" />
    <property name="param3" value="ST_MTHD_CD" />
    <property name="dao" value="boadao" />
  </activity>

  <!-- 정번별 코스 취득률 -->
  <activity name="searchRaceRegNo" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRaceRegNoList" />
    <property name="sqlkey" value="tbeb_organ_fb016" />
    <property name="param-count" value="4" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="ST_MTHD_CD" />
    <property name="param3" value="ST_MTHD_CD" />
    <property name="dao" value="boadao" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

