<?xml version="1.0" encoding="EUC-KR"?>
<service name="e08010009" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsRateCompareList|dsCntCompareList" />
    <transition name="success" value="EventRouter" />
  </activity>
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRateCompareList"  value="searchRateCompareList" />
    <transition name="searchCntCompareList"  value="searchCntCompareList" />
    <transition name="success" 				  value="end" />
  </activity>
  <activity name="searchRateCompareList"   	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="searchCntCompareList" />
    <property name="resultkey" 				value="dsRateCompareList" />
    <property name="sqlkey" 				value="tbtq_comparison_fa001" />
    <property name="param-count" 			value="14" />
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="STND_YEAR" />   
    <property name="param2" 				value="STND_YEAR" />
    <property name="param3" 				value="STND_YEAR" /> 
    <property name="param4" 				value="STND_YEAR" />
    <property name="param5" 				value="STND_YEAR" />  
    <property name="param6" 				value="STND_YEAR" />
    <property name="param7" 				value="BF_YEAR" /> 
    <property name="param8" 				value="BF_YEAR" />
    <property name="param9" 				value="BF_YEAR" />   
    <property name="param10" 				value="BF_YEAR" />
    <property name="param11" 				value="BF_YEAR" /> 
    <property name="param12" 				value="BF_YEAR" />
    <property name="param13" 				value="BF_YEAR" />  
    <property name="dao" 					value="boadao" />    
  </activity>
  <activity name="searchCntCompareList"   	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 				value="end" />
    <property name="resultkey" 				value="dsCntCompareList" />
    <property name="sqlkey" 				value="tbtq_comparison_fa002" />
    <property name="param-count" 			value="2" />
    <property name="param0" 				value="STND_YEAR" />
    <property name="param1" 				value="BF_YEAR" />   
   <property name="dao" 					value="boadao" />    
  </activity>
  <transaction-manager id="tx_boa" commit="true" />
</service>