<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
포상금 등록  
 -->
<service name="e07040020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutPartsCom" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchAwardList"      value="searchAwardList" />
    <transition name="saveAwardDesc"    value="saveAwardDesc" />
    <transition name="success" value="end" />
  </activity>

  <!-- 포상금내역  조회      -->
  <activity name="searchAwardList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPartsCom" />
    <property name="sqlkey" value="tbeg_racer_award_his_ff001" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="AWARD_YEAR"/>
    <property name="param1" value="AWARD_MONTH"/>
    <property name="param2" value="AWARD_MONTH"/>
    <property name="param3" value="RACER_NM"/>
  </activity>>
  
 
  <!-- 포상금  저장 -->
  <activity name="saveAwardDesc" class="snis.boa.financial.e07040020.activity.SaveAwardDesc">
    <transition name="success" value="searchAwardList" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

