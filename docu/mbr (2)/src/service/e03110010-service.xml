<?xml version="1.0" encoding="EUC-KR"?>
<service name="e03110010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutCertIssu|dsOutCertIssu_rpt" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchCertIssu" value="searchCertIssu" />
    <transition name="SaveCertIssu" value="SaveCertIssu" />
    <transition name="SaveCertConfirm"  	value="SaveCertConfirm" />   
	<transition name="SaveCertPrintCheck"  	value="SaveCertPrintCheck" />   
    <transition name="searchCertIssu_rpt" value="searchCertIssu_rpt" />
    <transition name="success" value="end" />
  </activity>
  
  <!-- 증명서발급정보  조회 -->
  <activity name="searchCertIssu" class="snis.boa.racer.e03110010.activity.SearchCertIssu">
    <transition name="success" value="end" />
  </activity>

  <!-- 증명서발급정보 저장 -->
  <activity name="SaveCertIssu" class="snis.boa.racer.e03110010.activity.SaveCertIssu">
    <transition name="success" value="end" />
  </activity>

  <!-- 증명서 발급 신청 확인  -->
  <activity name="SaveCertConfirm" class="snis.boa.racer.e03110010.activity.SaveCertConfirm">
    <transition name="success" value="end" />
  </activity>
  
  
  <!-- 증명서 발급 Check Update-->
  <activity name="SaveCertPrintCheck" class="snis.boa.racer.e03110010.activity.SaveCertPrintCheck">
    <transition name="success" value="end" />
  </activity>
  

  <!-- 증명서레포트정보  조회 -->
  <activity name="searchCertIssu_rpt" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutCertIssu_rpt" />
    <property name="sqlkey" value="tbec_cert_issu_history_fc003" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="ISSU_YEAR" />
    <property name="param1" value="RACER_NO" />
    <property name="param2" value="ISSU_YEAR" />
    <property name="param3" value="SEQ" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

