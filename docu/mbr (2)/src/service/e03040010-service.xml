<?xml version="1.0" encoding="EUC-KR"?>
<service name="e03040010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerArrange|dsOutPrtSeq|dsOutMotLot|dsOutBoatLot|dsOutRacerArrange_rpt|dsOutSPrtMax|dsPrtSeq" />
    <transition name="success" value="EventRouter" />
  </activity>
   
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEquipmentArrange" 	value="searchEquipmentArrange" />
    <transition name="searchEquipmentPrintSeq" 	value="searchEquipmentPrintSeq" />
    <transition name="SaveEquipmentArrange" 	value="SaveEquipmentArrange" />    
    <transition name="SaveEquipmentConfirm" 	value="SaveEquipmentConfirm" />
    <transition name="SaveEquipmentUnConfirm" 	value="SaveEquipmentUnConfirm" />
    <transition name="SaveEquipmentRandom" 		value="SaveEquipmentRandom" />
    <transition name="searchEquipmentMotLot" 	value="searchEquipmentMotLot" />
    <transition name="searchEquipmentBoatLot" 	value="searchEquipmentBoatLot" />
    <transition name="searchEquipmentArrange_rpt" value="searchEquipmentArrange_rpt" />
    <transition name="searchRandomMax" 			value="searchRandomMax" />
    <transition name="searchRandomResult" 		value="searchRandomResult" />
    <transition name="searchRacerChangeList" 	value="searchRacerChangeList" />
    <transition name="searchEquipmentArrageList" value="searchEquipmentArrageList" />
    <transition name="saveEquipmentArrangeRslt" value="saveEquipmentArrangeRslt" />    
    <transition name="success" value="end" />
  </activity>
  
  <!-- 모터/보트 배정  조회 -->
  <activity name="searchEquipmentArrange" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchEquipmentPrintSeq" />
    <property name="resultkey" value="dsOutRacerArrange" />
    <property name="sqlkey" value="tbeb_arrange_fc001" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="NM_KOR" />
    <property name="param4" value="EXCL_YN" />
  </activity>

  <!-- 모터/보트 배정  조회 -->
  <activity name="searchEquipmentPrintSeq" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutPrtSeq" />
    <property name="sqlkey" value="tbec_equip_prt_fc005" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="EXCL_YN" />
  </activity>

  <!-- 모터/보트 배정정보 저장 -->
  <activity name="SaveEquipmentArrange" class="snis.boa.racer.e03040010.activity.SaveEquipmentArrange">
    <transition name="success" value="end" />
  </activity>


  <!-- 모터/보트 배정 확정 -->
  <activity name="SaveEquipmentConfirm" class="snis.boa.racer.e03040010.activity.SaveEquipmentConfirm">
    <transition name="success" value="end" />
  </activity>

  <!-- 모터/보트 배정 확정취소 -->
  <activity name="SaveEquipmentUnConfirm" class="snis.boa.racer.e03040010.activity.SaveEquipmentUnConfirm">
    <transition name="success" value="end" />
  </activity>

  <!-- 모터/보트 랜덤추첨 정보 저장 -->
  <activity name="SaveEquipmentRandom" class="snis.boa.racer.e03040010.activity.SaveEquipmentRandom">
    <transition name="success" value="end" />
  </activity>
  
  <!-- 모터추첨대상  조회 -->
  <activity name="searchEquipmentMotLot" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchEquipmentBoatLot" />
    <property name="resultkey" value="dsOutMotLot" />
    <property name="sqlkey" value="tbef_equip_lot_fc001" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="EXCL_YN" />
  </activity>

  <!-- 보트추첨대상  조회 -->
  <activity name="searchEquipmentBoatLot" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutBoatLot" />
    <property name="sqlkey" value="tbef_equip_lot_fc002" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="EXCL_YN" />
  </activity>

  <!-- 모터/보트 배정 레포트정보 조회 -->
  <activity name="searchEquipmentArrange_rpt" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerArrange_rpt" />
    <property name="sqlkey" value="tbeb_arrange_fc002" />
    <property name="param-count" value="6" />
    <property name="dao" value="boadao" />
    <property name="param0" value="PRT_TYPE" />
    <property name="param1" value="STND_YEAR" />
    <property name="param2" value="MBR_CD" />
    <property name="param3" value="TMS" />
    <property name="param4" value="NM_KOR" />
    <property name="param5" value="EXCL_YN" />
  </activity>

  <!-- 랜덤출력저장  Max값 조회 -->
  <activity name="searchRandomMax" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutSPrtMax" />
    <property name="sqlkey" value="tbec_equip_prt_fc002" />
    <property name="param-count" value="3" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
  </activity> 

  <!-- 모터/보트 배정 랜덤출력저장  조회 -->
  <activity name="searchRandomResult" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerArrange" />
    <property name="sqlkey" value="tbec_equip_prt_fc003" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="PRT_SEQ" />
  </activity> 
  
  <!-- 모터/보트 랜덤추첨 정보 저장 -->
  <activity name="searchRacerChangeList" class="snis.boa.racer.e03040010.activity.SearchRacerChangeList">
    <transition name="success" value="end" />
  </activity>
  
  <!-- 모터/보트 배정 랜덤출력리스트 조회 -->
  <activity name="searchRandomResult" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerArrange" />
    <property name="sqlkey" value="tbec_equip_prt_fc003" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="MBR_CD" />
    <property name="param2" value="TMS" />
    <property name="param3" value="PRT_SEQ" />
  </activity> 
  
  <!-- 모터/보트 배정 확정 -->
  <activity name="saveEquipmentArrangeRslt" class="snis.boa.racer.e03040010.activity.SaveEquipmentArrangeRslt">
    <transition name="success" value="end" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

