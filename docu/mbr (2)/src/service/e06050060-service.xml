<?xml version="1.0" encoding="EUC-KR"?>
<service name="e06050060" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsBFYear|dsSuppList" />
    <transition name="success" value="EventRouter" />
  </activity>  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchBFYearList"  	value="searchBFYearList" />    
    <transition name="searchSuppBalance"  	value="searchSuppBalance" />    
    <transition name="procBringForward"		value="procBringForward" />    
    <transition name="success" 				value="end" />
  </activity>


  <!-- 이월 리스트  조회      -->
  <activity name="searchBFYearList" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="end" />
    <property name="resultkey" 		value="dsBFYear" />
    <property name="sqlkey" 		value="e06050060_s01" />
    <property name="param-count" 	value="0" />
    <property name="dao" 			value="boadao" />
  </activity>
  

  <!-- 해당년도 소모품 현재고 수량 조회      -->
  <activity name="searchSuppBalance" 	class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" 		value="end" />
    <property name="resultkey" 		value="dsSuppList" />
    <property name="sqlkey" 		value="e06050060_s02" />
    <property name="param-count" 	value="1" />
    <property name="dao" 			value="boadao" />
    <property name="param0" 		value="BF_YEAR"/>
  </activity>
  
  <!-- 이월처리 -->
  <activity name="procBringForward" class="snis.boa.equipment.e06050060.activity.ProcBringForward">
    <transition name="success" 		value="searchBFYearList" />
  </activity>    <transaction-manager id="tx_boa" commit="true" />
</service>

