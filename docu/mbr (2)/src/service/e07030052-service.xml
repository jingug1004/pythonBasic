<?xml version="1.0" encoding="EUC-KR"?>
<service name="e07030052" initial="SetResultKeys">
	<activity 		name="SetResultKeys" 			class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    	<property 	name="staticResultKeyFile" 		value="resultkey.properties" />
    	<property 	name="resultKeyList" 			value="dsRcptIssu" />
    	<transition name="success" 					value="EventRouter" />
  	</activity>
  
	<activity 		name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
		<transition name="searchRcptIssu"  			value="searchRcptIssu" />  
    	<transition name="updateRcptIssu"  			value="updateRcptIssu" />  
    	<transition name="success" 					value="end" />
  </activity>

	<!-- 사업소득원천징수영수증 발급신청 조회 -->
  	<activity 		name="searchRcptIssu" 			class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    	<transition name="success"					value="end" />
		<property 	name="resultkey" 				value="dsRcptIssu" />
	    <property   name="sqlkey"					value="tbeg_rcpt_issu_ff001" />
    	<property   name="param-count" 				value="2" />
	    <property   name="param0" 					value="STR_DT" />
	    <property   name="param1" 					value="END_DT" />
        <property name="dao" 						value="boadao" />
  	</activity>
  	
  	<!-- 사업소득원천징수영수증 발급신청 변경 -->
  	<activity 		name="updateRcptIssu" 			class="snis.boa.financial.e07030052.activity.UpdateRcptIssu">
    	<transition name="success"					value="end" />
  	</activity>
  	
  	<transaction-manager id="tx_boa" commit="true" />
</service>
