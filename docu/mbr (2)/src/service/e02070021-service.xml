<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02070021" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutTrngExptRacer|dsOutRacerSanc|dsOutRacerIjry|dsOutRacerWant|dsTrngRqst" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerSanc"        value="searchRacerSanc" />
    <transition name="searchTrngExptRacer"    value="searchTrngExptRacer" />
    <!--   --><transition name="saveTrngExptRacer"      value="saveTrngExptRacer" /> -->
    <transition name="searchTrngExptRacerCandi" value="searchTrngExptRacerCandi" />
    <transition name="searchRacerIjryCandi"     value="searchRacerIjryCandi" />
    <transition name="searchRacerWantCandi"     value="searchRacerWantCandi" />
    <transition name="searchTrngRqst"     		value="searchTrngRqst" />
    <transition name="saveTrngRqst"     		value="saveTrngRqst" />
    <transition name="success" value="end" />
  </activity>

  <!-- 제재내용 조회 -->
  <activity name="searchRacerSanc" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerSanc" />
    <property name="sqlkey" value="tbee_racer_sanc_fb001" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="1" />
    <property name="param0" value="SANC_ISSUE_NO" />
  </activity>

  <!-- 훈련예정선수 조회 -->
  <activity name="searchTrngExptRacer" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutTrngExptRacer" />
    <property name="sqlkey" value="tbeb_trng_expt_racer_fb102" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="5" />
    <property name="param0" value="TRNG_RSN_CD" />
    <property name="param1" value="NM_KOR" />
    <property name="param2" value="TRNG_EXPT_STAT_CD" />
    <property name="param3" value="STR_DT" />
    <property name="param4" value="END_DT" />
  </activity>


  <!-- 훈련예정선수(제재선수) 조회 -->
  <activity name="searchTrngExptRacerCandi" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutTrngExptRacer" />
    <property name="sqlkey" value="tbeb_trng_expt_racer_fb002" />
    <property name="dao" value="boadao" />
    <property name="param-count" value="6" />
    <property name="param0" value="NM_KOR" />
    <property name="param1" value="TRNG_EXPT_STAT_CD" />
    <property name="param2" value="STR_DT" />
    <property name="param3" value="END_DT" />
    <property name="param4" value="NOT_YET" />
    <property name="param5" value="NOT_YET" />
  </activity>
  
  <!-- 부상선수정보  조회 -->
  <activity name="searchRacerIjryCandi" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchRacerSanc" />
    <property name="resultkey" value="dsOutRacerIjry" />
    <property name="sqlkey" value="tbeb_trng_expt_racer_fb006" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR" />
    <property name="param1" value="NM_KOR" />
    <property name="param2" value="NOT_YET" />
    <property name="param3" value="NOT_YET" />
  </activity>
  
  <!-- 희망선수정보(전체선수) 조회 -->
  <activity name="searchRacerWantCandi" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchTrngRqst" />
    <property name="resultkey" value="dsOutRacerWant" />
    <property name="sqlkey" value="tbeb_trng_expt_racer_fb007" />
    <property name="param-count" value="2" />
    <property name="dao" value="boadao" />
    <property name="param0" value="RACER_NO" />
    <property name="param1" value="NM_KOR" />
  </activity>
  
  <!-- 훈련예정선수 저장 
  <activity name="saveTrngExptRacer" class="snis.boa.organization.e02070021.activity.SaveTrngExptRacer">
    <transition name="success" value="searchTrngExptRacer" />
  </activity>
  -->
  <!-- IWORKS_SFR009 2013.12.22-->
  <!-- 훈련신청정보 조회 (커뮤니티) -->
  <activity name="searchTrngRqst" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsTrngRqst" />
    <property name="sqlkey" value="tbeb_trng_rqst_fb001" />
    <property name="param-count" value="6" />
    <property name="dao" value="boadao" />
    <property name="param0" value="TRNG_STRT_DT" />
    <property name="param1" value="TRNG_END_DT" />
    <property name="param2" value="TRNG_STRT_DT" />
    <property name="param3" value="TRNG_END_DT" />
    <property name="param4" value="RACER_NO" />
    <property name="param5" value="NM_KOR" />
  </activity>
  
  <!-- IWORKS_SFR009 2013.12.22-->
  <!-- 훈련신청정보 저장 (커뮤니티) -->												
  <activity name="saveTrngRqst" class="snis.boa.organization.e02070021.activity.SaveTrngRqst">
    <transition name="success" value="end" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

