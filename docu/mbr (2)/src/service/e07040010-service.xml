<?xml version="1.0" encoding="EUC-KR"?>
<service name="e07040010" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutUnColList" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" 				class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchUnCollList"  	value="searchUnCollList" />    
    <transition name="searchUnCollTpeList"  value="searchUnCollTpeList" />
    <transition name="saveUnCollList"   	value="saveUnCollList" />    
    <transition name="success" 				value="end" />
  </activity>


  <!-- 미수금 내역  조회      -->
  <activity name="searchUnCollList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutUnColList" />
    <property name="sqlkey" value="tbeg_uncollected_ff001" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="PAY_MONTH"/>
    <property name="param2" value="PAY_MONTH"/>
    <property name="param3" value="NM_KOR"/>
    <property name="param4" value="COLL_TPE"/>
  </activity> 
  
  <!-- 미수금 내역  조회      -->
  <activity name="searchUnCollTpeList" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutUnColList" />
    <property name="sqlkey" value="tbeg_uncollected_ff002" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="PAY_MONTH"/>
    <property name="param2" value="PAY_MONTH"/>
    <property name="param3" value="NM_KOR"/>
  </activity> 
  
  <!-- 미수금 내역저장 -->
  <activity name="saveUnCollList" class="snis.boa.financial.e07040010.activity.SaveUnCollList">
    <transition name="success" 	value="end" />
  </activity>

	<transaction-manager id="tx_boa" commit="true" />
</service>

