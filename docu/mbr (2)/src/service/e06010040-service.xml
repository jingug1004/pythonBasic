<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
모터 보트 추첨 대상 등록  
 -->
<service name="e06010040" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutEquip|dsOutEquipRaceRecord|dsOutEquipReprHis|dsOutReprPrint" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchEquip"      	value="searchEquip" />
    <transition name="searchMotRaceRecord"	value="searchMotRaceRecord" />
    <transition name="searchBoatRaceRecord"	value="searchBoatRaceRecord" />
    <transition name="searchEquipReprHis"	value="searchEquipReprHis" />
    <transition name="searchReprPrint"		value="searchReprPrint" />
    <transition name="saveEquipReprHis"		value="saveEquipReprHis" />
    <transition name="success" value="end" />
  </activity>

  <!-- 장비 조회    -->
  <activity name="searchEquip" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutEquip" />
    <property name="sqlkey" value="tbef_equip_repr_his_ff001" />
    <property name="param-count" value="7" />
    <property name="dao" value="boadao" />
    <property name="param0" value="REPR_STR_DT"/>
    <property name="param1" value="REPR_END_DT"/>
    <property name="param2" value="EQUIP_TPE_CD"/>
    <property name="param3" value="EQUIP_NO"/>
    <property name="param4" value="MBR_CD"/>
    <property name="param5" value="USE_YN"/>
    <property name="param6" value="USE_YN"/>
  </activity>
  
  <!-- 모터  경주 기록 조회     -->
  <activity name="searchMotRaceRecord" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutEquipRaceRecord" />
    <property name="sqlkey" value="tbef_equip_repr_his_ff020" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="EQUIP_NO"/>
  </activity>
  
  <!-- 보트    경주 기록 조회     -->
  <activity name="searchBoatRaceRecord" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutEquipRaceRecord" />
    <property name="sqlkey" value="tbef_equip_repr_his_ff021" />
    <property name="param-count" value="1" />
    <property name="dao" value="boadao" />
    <property name="param0" value="EQUIP_NO"/>
  </activity>
  
  <!-- 장비 정비 이력  조회      -->
  <activity name="searchEquipReprHis" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutEquipReprHis" />
    <property name="sqlkey" value="tbef_equip_repr_his_ff003" />
    <property name="param-count" value="9" />
    <property name="dao" value="boadao" />
    <property name="param0" value="REPR_STR_DT"/>
    <property name="param1" value="REPR_END_DT"/>
    <property name="param2" value="EQUIP_TPE_CD"/>
    <property name="param3" value="EQUIP_NO"/>
    <property name="param4" value="REPR_TPE_CD"/>
    <property name="param5" value="MBR_CD"/>
    <property name="param6" value="REPR_PARTS"/>
    <property name="param7" value="USE_YN"/>
    <property name="param8" value="USE_YN"/>
  </activity>>
  
  <!-- 모터정비내역  조회    -->
  <activity name="searchReprPrint" class="com.posdata.glue.miplatform.biz.activity.PosSearch">    <transition name="success" value="end" />    <property name="resultkey" value="dsOutReprPrint" />    <property name="sqlkey" value="tbef_equip_repr_his_ff004" />    <property name="param-count" value="5" />    <property name="dao"    value="boadao" />    <property name="param0" value="DISP_YN"/>
    <property name="param1" value="REPR_STR_DT"/>
    <property name="param2" value="REPR_END_DT"/>    <property name="param3" value="EQUIP_TPE_CD"/>    <property name="param4" value="MBR_CD"/>  </activity>
  
  <!-- 장비 정비 이력  저장 -->
  <activity name="saveEquipReprHis" class="snis.boa.equipment.e06010040.activity.SaveEquipReprHis">
    <transition name="success" value="searchEquipReprHis" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

