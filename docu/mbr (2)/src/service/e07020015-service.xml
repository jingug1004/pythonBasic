<?xml version="1.0" encoding="EUC-KR"?>
<!-- 상금 계획 및 분석 조회  -->
<service name="e07020015" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerPriz|dsOutRacePriz" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="calcRacerPriz"   value="calcRacerPriz" />
    <transition name="calcRacePriz"    value="calcRacePriz" />
    <transition name="success" value="end" />
  </activity>

  <!-- 선수별 상금 계획 및 분석 조회 -->
  <activity name="calcRacerPriz" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="calcRacePriz" />
    <property name="resultkey" value="dsOutRacerPriz" />
    <property name="sqlkey" value="tbeg_race_priz_ff003" />
    <property name="param-count" value="9" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="STND_YEAR"/>
    <property name="param3" value="MBR_CD"/>
    <property name="param4" value="STND_YEAR"/>
    <property name="param5" value="MBR_CD"/>
    <property name="param6" value="STND_YEAR"/>
    <property name="param7" value="STND_YEAR"/>
    <property name="param8" value="STND_YEAR"/>
    <property name="dao" value="boadao" />
  </activity>
  
  <!-- 경주등급별 상금 계획 및 분석 조회 -->
  <activity name="calcRacePriz" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacePriz" />
    <property name="sqlkey" value="tbeg_race_priz_ff004" />
    <property name="param-count" value="9" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="STND_YEAR"/>
    <property name="param3" value="MBR_CD"/>
    <property name="param4" value="STND_YEAR"/>
    <property name="param5" value="MBR_CD"/>
    <property name="param6" value="STND_YEAR"/>
    <property name="param7" value="STND_YEAR"/>
    <property name="param8" value="STND_YEAR"/>
    <property name="dao" value="boadao" />
  </activity>
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

