<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02080020" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRacerList" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
	<transition name="searchIntroRunTm"  			value="searchIntroRunTm" /> 
	<transition name="searchRaceTm"  			    value="searchRaceTm" /> 
    <transition name="success" value="end" />
  </activity>

  <!-- 소개항주리스트 -->
  <activity name="searchIntroRunTm" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerList" />
    <property name="sqlkey" value="tbeb_organ_fb012" />
    <property name="param-count" value="3" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="SEQ" />
    <property name="dao" value="boadao" />
  </activity>

  <!-- 완주타임리스트 -->
  <activity name="searchRaceTm" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRacerList" />
    <property name="sqlkey" value="tbeb_organ_fb013" />
    <property name="param-count" value="3" />
    <property name="param0" value="STR_DT" />
    <property name="param1" value="END_DT" />
    <property name="param2" value="SEQ" />
    <property name="dao" value="boadao" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

