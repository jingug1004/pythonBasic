<?xml version="1.0" encoding="EUC-KR"?>
<service name="e02080210" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsRacer|dsPerioNo|dsMaxRaceRegNo" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRacerStts"       		value="searchRacerStts" />
    <transition name="searchPerioNoStts"          	value="searchPerioNoStts" />
    <transition name="searchMaxRaceRegNo"       	value="searchMaxRaceRegNo" />
    <transition name="success" value="end" />
  </activity>

  <!-- 대상 경주 선수별 통합성적  조회 -->
  <activity name="searchRacerStts" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchPerioNoStts" />
    <property name="resultkey" value="dsRacer" />
    <property name="sqlkey" value="tbeb_race_tms_fb112" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="GRP_CD" />
    <property name="param1" value="CD" />
    <property name="param2" value="RACE_FROM_DT" />
    <property name="param3" value="RACE_TO_DT" />
  </activity>

    <!-- 대상 경주 기수별 통합성적  조회 -->
  <activity name="searchPerioNoStts" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="searchMaxRaceRegNo" />
    <property name="resultkey" value="dsPerioNo" />
    <property name="sqlkey" value="tbeb_race_tms_fb113" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="GRP_CD" />
    <property name="param1" value="CD" />
    <property name="param2" value="RACE_FROM_DT" />
    <property name="param3" value="RACE_TO_DT" />
  </activity>
  
  	<!-- 대상경주 최고 정번  조회 -->
   <activity name="searchMaxRaceRegNo" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsMaxRaceRegNo" />
    <property name="sqlkey" value="tbeb_race_tms_fb111" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="GRP_CD" />
    <property name="param1" value="CD" />
    <property name="param2" value="RACE_FROM_DT" />
    <property name="param3" value="RACE_TO_DT" />
  </activity>
  
  
  
  <transaction-manager id="tx_boa" commit="true" />
</service>

