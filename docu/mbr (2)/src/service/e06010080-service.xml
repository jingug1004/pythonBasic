<?xml version="1.0" encoding="EUC-KR"?>
<!-- 
출주검사표출력
 -->
<service name="e06010080" initial="SetResultKeys">
  <activity name="SetResultKeys" class="com.posdata.glue.miplatform.biz.activity.PosSetResultKeyList">  
    <property name="staticResultKeyFile" value="resultkey.properties" />
    <property name="resultKeyList" value="dsOutRaceIms|dsOutRaceOrgan|dsOutReportMemo" />
    <transition name="success" value="EventRouter" />
  </activity>
  
  <activity name="EventRouter" class="com.posdata.glue.biz.activity.PosDefaultRouter">
    <transition name="searchRaceIms"		value="searchRaceIms" />
    <transition name="searchRaceOrgan"		value="searchRaceOrgan" />  
    <transition name="searchReportMemo"		value="searchReportMemo" />  
    <transition name="saveTiltPrint"		value="saveTiltPrint" />  
    <transition name="saveRunInspPrint" 	value="saveRunInspPrint" />  
    <transition name="saveReportMemo" 		value="saveReportMemo" />
    <transition name="success" value="end" />  
  </activity>

  <!-- 출주검사표  조회      -->
  <activity name="searchRaceIms" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRaceIms" />
    <property name="sqlkey" value="tbef_run_insp_print_ff001" />
    <property name="param-count" value="4" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="TMS"/>
    <property name="param3" value="DAY_ORD"/>
  </activity>>
  
  <!-- 출주검사표 상세  조회      -->
  <activity name="searchRaceOrgan" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutRaceOrgan" />
    <property name="sqlkey" value="tbef_run_insp_print_ff002" />
    <property name="param-count" value="5" />
    <property name="dao" value="boadao" />
    <property name="param0" value="STND_YEAR"/>
    <property name="param1" value="MBR_CD"/>
    <property name="param2" value="TMS"/>
    <property name="param3" value="DAY_ORD"/>
    <property name="param4" value="RACE_NO"/>
  </activity>>
  
  <!-- 출주검사표  점검내용조회      -->
  <activity name="searchReportMemo" class="com.posdata.glue.miplatform.biz.activity.PosSearch">
    <transition name="success" value="end" />
    <property name="resultkey" value="dsOutReportMemo" />
    <property name="sqlkey" value="tbef_run_insp_print_ff003" />
    <property name="param-count" value="0" />
    <property name="dao" value="boadao" />
  </activity>
  
  <activity name="saveTiltPrint" class="snis.boa.equipment.e06010080.activity.SaveTiltPrint">
    <transition name="success" value="searchRaceIms" />
  </activity>
  
  <activity name="saveRunInspPrint" class="snis.boa.equipment.e06010080.activity.SaveRunInspPrint">
    <transition name="success" value="searchRaceIms" />
  </activity>
  
  <activity name="saveReportMemo" class="snis.boa.equipment.e06010080.activity.SaveReportMemo">
    <transition name="success" value="end" />
  </activity>

  <transaction-manager id="tx_boa" commit="true" />
</service>

