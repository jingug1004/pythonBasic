    <script>
    function openVersion() {
    	$.ajax({
			url: '/admin/version/getVersion',
			processData: false,
			contentType: false,
			type: 'GET',
			success: function(data){
				bootbox.prompt({
		    		title: "버전을 입력하세요.",
		    		value: data.versionName,
		    		callback: function (result) {
		    			if(result) {
		    				data.versionName = result;
			    			$.ajax({
			    				url: "/admin/version/update",
			    				contentType: 'application/json',
			    			    mimeType: 'application/json',
			    			    dataType: 'json', 
			    				data: JSON.stringify(data),
			    				type: 'POST',
			    				success: function(result){
			    					if(result.resultCode == 200) {
			    						bootbox.alert(result.message, function() {});
			    					} else {
			    						bootbox.alert(result.message, function() {});
			    					}
			    				},
			    				error: function(){
			    					bootbox.alert("오류가 발생였습니다.\n잠시 후 다시 시도해 주세요.", function() {});
			    				}
			    			});
		    			}
		   			}
				});
			},
			 error: function(){
				 bootbox.alert("오류가 발생였습니다.\n잠시 후 다시 시도해 주세요.", function() {});
			 }
		});
    }
    
    function openAuto(auto) {
    	var title;
    	if(auto == 'N') {
    		title = "신규가입 발급 쿠폰 코드를 입력하세요";
    	} else if(auto == 'P') {
    		title = "첫 구매 발급 쿠폰 코드를 입력하세요.";
    	}
    	$.ajax({
			url: '/admin/coupon/getAuto?auto='+auto,
			processData: false,
			contentType: false,
			type: 'GET',
			success: function(data){
				bootbox.prompt({
		    		title: title,
		    		value: data,
		    		callback: function (result) {
		    			if(result) {
		    				var formData = new FormData();
		    				formData.append("code", result);
		    				formData.append("auto", auto);
			    			$.ajax({
			    				url: "/admin/coupon/auto",
			    				processData: false,
			    				contentType: false,
			    				data: formData,
			    				type: 'POST',
			    				success: function(result){
			    					if(result.resultCode == 200) {
			    						bootbox.alert(result.message, function() {});
			    					} else {
			    						bootbox.alert(result.message, function() {});
			    					}
			    				},
			    				error: function(){
			    					bootbox.alert("오류가 발생였습니다.\n잠시 후 다시 시도해 주세요.", function() {});
			    				}
			    			});
		    			}
		   			}
				});
			},
			 error: function(){
				 bootbox.alert("오류가 발생였습니다.\n잠시 후 다시 시도해 주세요.", function() {});
			 }
		});
    }
	</script>