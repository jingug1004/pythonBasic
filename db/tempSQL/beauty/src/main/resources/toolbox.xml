<?xml version="1.0" encoding="UTF-8"?>
<toolbox>
	<tool>
		<key>datetool</key>
		<scope>request</scope>
		<class>org.apache.velocity.tools.generic.DateTool</class>
	</tool>
	<tool>
		<key>datecmptool</key>
		<scope>request</scope>
		<class>org.apache.velocity.tools.generic.ComparisonDateTool</class>
	</tool>
	<tool>
		<key>displaytool</key>
		<scope>request</scope>
		<class>org.apache.velocity.tools.generic.DisplayTool</class>
	</tool>
	<tool>
		<key>esctool</key>
		<scope>application</scope>
		<class>org.apache.velocity.tools.generic.EscapeTool</class>
	</tool>
	<tool>
	    <key>listtool</key>
	    <scope>application</scope>
	    <class>org.apache.velocity.tools.generic.ListTool</class>
	</tool>
	<tool>
        <key>number</key>
	    <scope>application</scope>
	    <class>org.apache.velocity.tools.generic.NumberTool</class>
	    <parameter name="format" value="#0.0" />
	</tool>
	<tool>
  		<key>math</key>
  		<scope>application</scope>
  		<class>org.apache.velocity.tools.generic.MathTool</class>
	</tool>
</toolbox>